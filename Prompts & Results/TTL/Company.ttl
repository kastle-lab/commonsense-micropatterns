@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/> .ex:Company rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:label "Company"@en ;    rdfs:comment "A legal entity that provides goods and services to consumers."@en .ex:hasCompanyName rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Company ;    rdfs:range xsd:string ;    rdfs:label "has company name"@en ;    rdfs:comment "The official name of the company."@en .ex:isLocatedIn rdf:type owl:ObjectProperty ;    rdfs:domain ex:Company ;    rdfs:range ex:Location ;    rdfs:label "is located in"@en ;    rdfs:comment "The location where the company is based."@en .ex:operatesInIndustry rdf:type owl:ObjectProperty ;    rdfs:domain ex:Company ;    rdfs:range ex:Industry ;    rdfs:label "operates in industry"@en ;    rdfs:comment "The industry in which the company operates."@en .


