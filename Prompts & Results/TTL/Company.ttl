@prefix ex: <http://example.com/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2000/10/XMLSchema#> .ex:Company rdf:type rdfs:Class .ex:company1 rdf:type ex:Company ;  ex:companyName "Company One"^^xsd:string ;  ex:establishedDate "2000-01-01"^^xsd:date ;  ex:hasLocation "Location of Company One"^^xsd:string .

@prefix : <http://example.org/> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Company a rdfs:Class ;  rdfs:label "Company"@en ;  rdfs:comment "An organization that produces, buy, or sells goods or services"@en .:hasName a rdf:Property ;  rdfs:label "has name"@en ;  rdfs:comment "The name of the company"@en ;  rdfs:domain :Company ;  rdfs:range rdfs:Literal .:hasAddress a rdf:Property ;  rdfs:label "has address"@en ;  rdfs:comment "The address of the company"@en ;  rdfs:domain :Company ;  rdfs:range rdfs:Literal .:offersService a rdf:Property ;  rdfs:label "offers service"@en ;  rdfs:comment "The service(s) that the company offers"@en ;  rdfs:domain :Company ;  rdfs:range rdfs:Literal .:employsPerson a rdf:Property ;  rdfs:label "employs person"@en ;  rdfs:comment "The person(s) that the company employs"@en ;  rdfs:domain :Company ;  rdfs:range foaf:Person .

