@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://www.example.org/#> .ex:City rdfs:subClassOf owl:Thing ;         rdfs:label "City"^^xsd:string ;        rdfs:comment "A large and permanent human settlement"^^xsd:string .ex:hasPopulation rdfs:domain ex:City ;                 rdfs:range xsd:integer ;                 rdfs:label "hasPopulation"^^xsd:string ;                 rdfs:comment "Represents the population number of a city"^^xsd:string .ex:hasMayor rdfs:domain ex:City ;            rdfs:range ex:Person ;            rdfs:label "hasMayor"^^xsd:string ;            rdfs:comment "Defines the mayor of a city"^^xsd:string .ex:isLocatedIn rdfs:domain ex:City ;               rdfs:range ex:Country ;               rdfs:label "isLocatedIn"^^xsd:string ;               rdfs:comment "Indicates the country where the city is located"^^xsd:string .ex:hasArea rdfs:domain ex:City ;           rdfs:range xsd:decimal ;           rdfs:label "hasArea"^^xsd:string ;           rdfs:comment "Defines the city's area size in square kilometers"^^xsd:string .ex:City rdfs:subClassOf [  a owl:Restriction ;  owl:onProperty ex:isLocatedIn ;  owl:someValuesFrom ex:Country].ex:City rdfs:subClassOf [  a owl:Restriction ;  owl:onProperty ex:hasArea ;  owl:someValuesFrom xsd:decimal].ex:City rdfs:subClassOf [  a owl:Restriction ;  owl:onProperty ex:hasPopulation ;  owl:someValuesFrom xsd:integer].ex:City rdfs:subClassOf [  a owl:Restriction ;  owl:onProperty ex:hasMayor ;  owl:someValuesFrom ex:Person].

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix geo: <http://www.geonames.org/ontology#> .geo:City rdf:type owl:Class ;    rdfs:subClassOf geo:GeonamesFeature ;    rdfs:label "City"@en ;    rdfs:comment "A large and permanent human settlement."@en .geo:population rdf:type owl:DatatypeProperty ;    rdfs:domain geo:City ;    rdfs:range xsd:integer ;    rdfs:label "population"@en ;    rdfs:comment "The number of inhabitants of a city."@en .geo:country rdf:type owl:ObjectProperty ;    rdfs:domain geo:City ;    rdfs:range geo:Country ;    rdfs:label "country"@en ;    rdfs:comment "The country wherein a city is located."@en .geo:area rdf:type owl:DatatypeProperty ;    rdfs:domain geo:City ;    rdfs:range xsd:float ;    rdfs:label "area"@en ;    rdfs:comment "The total land, water, and urban area of a city in kmÂ²."@en .geo:elevation rdf:type owl:DatatypeProperty ;    rdfs:domain geo:City ;    rdfs:range xsd:float ;    rdfs:label "elevation"@en ;    rdfs:comment "The elevation of a city above sea level in meters."@en .geo:timeZone rdf:type owl:ObjectProperty ;    rdfs:domain geo:City ;    rdfs:range geo:TimeZone ;    rdfs:label "time zone"@en ;    rdfs:comment "The time zone a city falls within."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.@prefix owl: <http://www.w3.org/2002/07/owl#>.@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.@prefix city: <http://example.org/ontology/city#>.city:City rdf:type owl:Class.city:cityName rdf:type owl:DatatypeProperty.city:cityName rdfs:domain city:City.city:cityName rdfs:range xsd:string.city:country rdf:type owl:ObjectProperty.city:country rdfs:domain city:City.city:country rdfs:range city:Country.city:Country rdf:type owl:Class.city:population rdf:type owl:DatatypeProperty.city:population rdfs:domain city:City.city:population rdfs:range xsd:integer.city:area rdf:type owl:DatatypeProperty.city:area rdfs:domain city:City.city:area rdfs:range xsd:float.city:established rdf:type owl:DatatypeProperty.city:established rdfs:domain city:City.city:established rdfs:range xsd:date.city:City rdfs:comment "This class represents all cities in the world".city:City rdfs:label "City".


