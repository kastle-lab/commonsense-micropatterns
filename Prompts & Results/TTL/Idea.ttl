
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix idea: <http://www.example.com/idea#> .idea:    a owl:Ontology ;    rdfs:label "The ontology of Idea"@en .idea:Idea    a owl:Class ;    rdfs:label "Idea"@en ;    rdfs:comment "An abstract concept or mental representation"@en .idea:hasCreator    a owl:ObjectProperty ;    rdfs:label "has creator"@en ;    rdfs:domain idea:Idea ;    rdfs:range idea:Creator .idea:hasDateCreated    a owl:DatatypeProperty ;    rdfs:label "has date created"@en ;    rdfs:domain idea:Idea ;    rdfs:range xsd:dateTime .idea:hasDescription    a owl:DatatypeProperty ;    rdfs:label "has description"@en ;    rdfs:domain idea:Idea ;    rdfs:range xsd:string .idea:Creator    a owl:Class ;    rdfs:label "Creator"@en ;    rdfs:comment "The person or entity that creates an Idea"@en .idea:hasName    a owl:DatatypeProperty ;    rdfs:label "has name"@en ;    rdfs:domain idea:Creator ;    rdfs:range xsd:string .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix idea: <http://example.com/idea#> .idea:Idea rdf:type rdfs:Class ;rdfs:comment "Class that represents the concept of Idea" .idea:hasAuthor rdf:type rdf:Property ;               rdfs:range idea:Person ;               rdfs:domain idea:Idea ;               rdfs:comment "Relates an Idea with its Author" .idea:hasDate rdf:type rdf:Property ;              rdfs:range xsd:dateTime ;              rdfs:domain idea:Idea ;              rdfs:comment "Indicates the date when the Idea was produced" .idea:hasDescription rdf:type rdf:Property ;                    rdfs:range xsd:string ;                    rdfs:domain idea:Idea ;                    rdfs:comment "A brief description of the Idea" .idea:hasName rdf:type rdf:Property ;             rdfs:range xsd:string ;             rdfs:domain idea:Idea ;             rdfs:comment "Name or title of the Idea" .idea:Person rdf:type rdfs:Class ;            rdfs:comment "Class that represents people, potential authors of ideas" .idea:hasName rdf:type rdf:Property ;             rdfs:range xsd:string ;             rdfs:domain idea:Person ;             rdfs:comment "Name of the Person" .idea:hasAge rdf:type rdf:Property ;            rdfs:range xsd:integer ;            rdfs:domain idea:Person ;            rdfs:comment "Age of the Person" .





@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix idea: <http://www.example.com/idea#> .idea:     rdf:type rdfs:Class ;    rdfs:label "Idea" ;    rdfs:comment "This is a class for capturing Ideas" .idea:hasTitle    rdf:type rdf:Property ;    rdfs:domain idea: ;    rdfs:range xsd:string ;    rdfs:label "has Title" ;    rdfs:comment "The title or headline of the Idea" .idea:hasDescription    rdf:type rdf:Property ;    rdfs:domain idea: ;    rdfs:range xsd:string ;    rdfs:label "has Description" ;    rdfs:comment "Detailed description of the Idea" .idea:hasAuthor     rdf:type rdf:Property ;    rdfs:domain idea: ;    rdfs:range foaf:Person ;    rdfs:label "has Author" ;    rdfs:comment "Person who came up with the Idea" .idea:hasDate     rdf:type rdf:Property ;    rdfs:domain idea: ;    rdfs:range xsd:date ;    rdfs:label "has Date" ;    rdfs:comment "Date when the Idea was proposed" .idea:isRelatedTo    rdf:type rdf:Property ;    rdfs:domain idea: ;    rdfs:range idea: ;    rdfs:label "is related to" ;    rdfs:comment "Other ideas that are related to this Idea" .idea:hasCategory     rdf:type rdf:Property ;    rdfs:domain idea: ;    rdfs:range xsd:string ;    rdfs:label "has category" ;    rdfs:comment "Category or theme the Idea belongs to" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix idea: <http://www.example.org/idea#> .idea:     rdf:type owl:Ontology ;    rdfs:comment "Ontology of the Concept Idea"^^xsd:string ;    rdfs:label "Idea Ontology"^^xsd:string .idea:Idea    rdf:type owl:Class ;    rdfs:label "Idea"^^xsd:string ;    rdfs:comment "Concept of Idea"^^xsd:string .idea:hasOriginator    rdf:type owl:ObjectProperty ;    rdfs:label "has originator"^^xsd:string ;    rdfs:comment "The person or entity that originated the idea"^^xsd:string ;    rdfs:domain idea:Idea ;    rdfs:range idea:Originator .idea:Originator    rdf:type owl:Class ;    rdfs:label "Originator"^^xsd:string ;    rdfs:comment "The person or entity that generated an Idea"^^xsd:string .idea:hasDate    rdf:type owl:DatatypeProperty ;    rdfs:label "has date"^^xsd:string ;    rdfs:comment "The date when the idea was conceived"^^xsd:string ;    rdfs:domain idea:Idea ;    rdfs:range xsd:date .idea:isRelatedTo    rdf:type owl:ObjectProperty ;    rdfs:label "is related to"^^xsd:string ;    rdfs:comment "The notion that one idea is linked or relates to another"^^xsd:string ;    rdfs:domain idea:Idea ;    rdfs:range idea:Idea .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix idea: <http://www.example.org/idea#> .# Idea classidea:Idea rdf:type owl:Class ;  rdfs:subClassOf owl:Thing ;  rdfs:comment "Entity representing an idea" ;  rdfs:label "Idea"@en .# Creator classidea:Creator rdf:type owl:Class ;  rdfs:subClassOf owl:Thing ;  rdfs:comment "Entity representing the creator of an idea" ;  rdfs:label "Creator"@en .# Property of ideaidea:hasCreator rdf:type owl:ObjectProperty ;  rdfs:domain idea:Idea ;  rdfs:range idea:Creator ;  rdfs:comment "Property linking an idea to its creator" ;  rdfs:label "has creator"@en .# Property of ideaidea:hasDescription rdf:type owl:DatatypeProperty ;  rdfs:domain idea:Idea ;  rdfs:range xsd:string ;  rdfs:comment "Property linking an idea to its description" ;  rdfs:label "has description"@en .# Property of ideaidea:hasName rdf:type owl:DatatypeProperty ;  rdfs:domain idea:Idea ;  rdfs:range xsd:string ;  rdfs:comment "Property linking an idea to its name" ;  rdfs:label "has name"@en .# Property of creatoridea:hasName rdf:type owl:DatatypeProperty ;  rdfs:domain idea:Creator ;  rdfs:range xsd:string ;  rdfs:comment "Property linking a creator to its name" ;  rdfs:label "has name"@en .










@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .<http://example.com/ontology#Idea>  rdf:type rdfs:Class ;  rdfs:label "Idea"@en ;  rdfs:comment "An abstract or general notion underlying a human reasoning or decision."@en .<http://example.com/ontology#hasIdea>  rdf:type rdf:Property ;  rdfs:domain <http://example.com/ontology#Person> ;  rdfs:range <http://example.com/ontology#Idea> ;  rdfs:label "has idea"@en ;  rdfs:comment "A person can have an idea."@en .  <http://example.com/ontology#inspiredBy>  rdf:type rdf:Property ;  rdfs:domain <http://example.com/ontology#Idea> ;  rdfs:range <http://example.com/ontology#Event> ;  rdfs:label "inspired by"@en ;  rdfs:comment "An idea can be inspired by an event."@en .<http://example.com/ontology#leadTo>  rdf:type rdf:Property ;  rdfs:domain <http://example.com/ontology#Idea> ;  rdfs:range <http://example.com/ontology#Action> ;  rdfs:label "lead to"@en ;  rdfs:comment "An idea can lead to an action."@en .


