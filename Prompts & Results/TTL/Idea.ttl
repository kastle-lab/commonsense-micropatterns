@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://example.org/#> .:Idea rdf:type rdfs:Class .:Idea rdfs:label "Idea" .:Idea rdfs:comment "A concept or mental impression" .:IdeaOriginator  rdf:type rdf:Property ;  rdfs:domain :Idea ;  rdfs:label "originator" ;  rdfs:range :Person ;  rdfs:comment "Person who originated or thought of the idea" .:IdeaCreationDate  rdf:type rdf:Property ;  rdfs:domain :Idea ;  rdfs:label "creation date" ;  rdfs:range :Date ;  rdfs:comment "Date when the idea was created or thought of" . :IdeaSummary  rdf:type rdf:Property ;  rdfs:domain :Idea ;  rdfs:label "summary" ;  rdfs:comment "An overview or brief description of the idea" ;  rdfs:range rdfs:Literal .


