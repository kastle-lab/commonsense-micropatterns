
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix pro: <http://www.example.org/ontology/program#> .pro:Program rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:label "Program" ;    rdfs:comment "A computer software application that executes specific tasks or activities" .pro:hasProgramName rdf:type owl:DatatypeProperty ;    rdfs:domain pro:Program ;    rdfs:range xsd:string ;    rdfs:label "hasProgramName" ;    rdfs:comment "The name of the program" .pro:hasVersion rdf:type owl:DatatypeProperty ;    rdfs:domain pro:Program ;    rdfs:range xsd:string ;    rdfs:label "hasVersion" ;    rdfs:comment "The version of the program" .pro:hasDeveloper rdf:type owl:ObjectProperty ;    rdfs:domain pro:Program ;    rdfs:range pro:Developer ;    rdfs:label "hasDeveloper" ;    rdfs:comment "The developer of the program" .pro:Developer rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:label "Developer" ;    rdfs:comment "A person or entity that develops a program" .pro:hasDeveloperName rdf:type owl:DatatypeProperty ;    rdfs:domain pro:Developer ;    rdfs:range xsd:string ;    rdfs:label "hasDeveloperName" ;    rdfs:comment "The name of the developer" .pro:hasLanguage rdf:type owl:DatatypeProperty ;    rdfs:domain pro:Program ;    rdfs:range xsd:string ;    rdfs:label "hasLanguage" ;    rdfs:comment "The programming language of the program" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://www.example.com/program#> .:Program rdf:type rdfs:Class ;         rdfs:label "Program" ;         rdfs:comment "A planned series of events or procedures" .:ProgramCode rdf:type rdfs:Class ;            rdfs:label "Program Code" ;            rdfs:comment "The code representation of a program" ;            rdfs:subClassOf :Program .:Algorithm rdf:type rdfs:Class ;           rdfs:label "Algorithm" ;           rdfs:comment "A process or set of rules followed in calculations or problem-solving" ;           rdfs:subClassOf :Program .:hasProgramCode rdf:type rdf:Property ;                rdfs:label "has program code" ;                rdfs:comment "The property that associates a program with its code" ;                rdfs:range :ProgramCode ;                rdfs:domain :Program .:hasAlgorithm rdf:type rdf:Property ;              rdfs:label "has algorithm" ;              rdfs:comment "The property that associates a program with its algorithm" ;              rdfs:range :Algorithm ;              rdfs:domain :Program .:name rdf:type rdf:Property ;      rdfs:label "name" ;      rdfs:comment "The name of something" ;      rdfs:range xsd:string ;      rdfs:domain :Program .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix pr: <http://example.com/schema/Program#> .@prefix ex: <http://example.com/resource/> .ex:Program1 a pr:Program ;    pr:name "Program 1"@en ;    pr:description "This is a computer program."@en ;    pr:code "print('Hello, World!')" ;    pr:language "Python" ;    pr:version "1.0"^^xsd:float ;    pr:created "2010-10-10"^^xsd:date .ex:Program2 a pr:Program ;    pr:name "Program 2"@en ;    pr:description "This is another computer program."@en ;    pr:code "console.log('Hello, World!');" ;    pr:language "JavaScript" ;    pr:version "2.0"^^xsd:float ;    pr:created "2015-10-10"^^xsd:date .pr:Program a rdfs:Class ;    rdfs:label "Computer program"@en ;    rdfs:comment "Computer applications or code that instruct a computer to perform specific tasks."@en ;    rdfs:subClassOf rdfs:Resource .pr:name a rdf:Property ;    rdfs:label "Name"@en ;    rdfs:comment "The name of the computer program."@en ;    rdfs:domain pr:Program ;    rdfs:range xsd:string .pr:description a rdf:Property ;    rdfs:label "Description"@en ;    rdfs:domain pr:Program ;    rdfs:range xsd:string .pr:code a rdf:Property ;    rdfs:label "Code"@en ;    rdfs:comment "The code of the computer program."@en ;    rdfs:domain pr:Program ;    rdfs:range xsd:string .pr:language a rdf:Property ;    rdfs:label "Language"@en ;    rdfs:comment "The programming language of the computer program."@en ;    rdfs:domain pr:Program ;    rdfs:range xsd:string .pr:version a rdf:Property ;    rdfs:label "Version"@en ;    rdfs:comment "The version of the computer program."@en ;    rdfs:domain pr:Program ;    rdfs:range xsd:float .pr:created a rdf:Property ;    rdfs:label "Created"@en ;    rdfs:comment "The date the computer program was created."@en ;    rdfs:domain pr:Program ;    rdfs:range xsd:date .




@prefix prog: <http://example.org/programontology#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .# Classesprog:Program a rdfs:Class ;    rdfs:label "Program"@en ;    rdfs:comment "Software program or application"@en ;    .prog:Version a rdfs:Class ;    rdfs:label "Version"@en ;    rdfs:comment "Version of a program"@en ;    .prog:Developer a rdfs:Class ;    rdfs:label "Developer"@en ;    rdfs:comment "Developer or author of a program"@en ;    .prog:Language a rdfs:Class ;    rdfs:label "Language"@en ;    rdfs:comment "Programming language used to write a program"@en ;    .# Object Propertiesprog:hasVersion a rdf:Property ;    rdfs:label "has version"@en ;    rdfs:comment "Links a program to its versions"@en ;    rdfs:domain prog:Program ;    rdfs:range prog:Version ;    .prog:hasDeveloper a rdf:Property ;    rdfs:label "has developer"@en ;    rdfs:comment "Links a program to its developer"@en ;    rdfs:domain prog:Program ;    rdfs:range prog:Developer ;    .prog:hasLanguage a rdf:Property ;    rdfs:label "has language"@en ;    rdfs:comment "Links a program to the language used for its development"@en ;    rdfs:domain prog:Program ;    rdfs:range prog:Language ;    .# Data Propertiesprog:hasName a rdf:Property ;    rdfs:label "has name"@en ;    rdfs:comment "Program's name"@en ;    rdfs:domain prog:Program ;    rdfs:range xsd:string ;    .prog:hasReleaseDate a rdf:Property ;    rdfs:label "has release date"@en ;    rdfs:comment "Program's release date"@en ;    rdfs:domain prog:Version ;    rdfs:range xsd:date ;    .



@prefix : <http://www.example.org/#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:Program   rdfs:subClassOf :Software ;  rdfs:label "Program"^^xsd:string .:hasDeveloper   a rdf:Property ;  rdfs:label "has developer"^^xsd:string ;  rdfs:domain :Program ;  rdfs:range :Developer .:Developer  a rdfs:Class ;  rdfs:label "Developer"^^xsd:string .:writtenIn  a rdf:Property ;  rdfs:label "is written in"^^xsd:string ;  rdfs:domain :Program ;  rdfs:range :ProgrammingLanguage .:ProgrammingLanguage  a rdfs:Class ;  rdfs:label "Programming Language"^^xsd:string .:hasVersion   a rdf:Property ;  rdfs:label "has version"^^xsd:string ;  rdfs:domain :Program ;  rdfs:range xsd:string .:hasReleaseDate   a rdf:Property ;  rdfs:label "has release date"^^xsd:string ;  rdfs:domain :Program ;  rdfs:range xsd:date .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix prg: <http://www.example.org/program#> .prg:Program rdf:type rdfs:Class .prg:hasName rdf:type rdf:Property ;    rdfs:domain prg:Program ;    rdfs:range xsd:string .prg:writtenIn rdf:type rdf:Property ;    rdfs:domain prg:Program ;    rdfs:range prg:ProgrammingLanguage .prg:hasAuthor rdf:type rdf:Property ;    rdfs:domain prg:Program ;    rdfs:range prg:SoftwareDeveloper .prg:hasVersion rdf:type rdf:Property ;    rdfs:domain prg:Program ;    rdfs:range xsd:integer .prg:ProgrammingLanguage rdf:type rdfs:Class .prg:SoftwareDeveloper rdf:type rdfs:Class .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix prog: <http://www.example.org/ontology/program#> .prog:Program rdf:type rdfs:Class ;    rdfs:label "Program"^^xsd:string ;    rdfs:comment "A program is an executable software that instructs a computer to perform specific tasks."^^xsd:string .prog:hasCode rdf:type rdf:Property ;    rdfs:label "has code"^^xsd:string ;    rdfs:comment "Property showing that a program has code instructions."^^xsd:string ;    rdfs:domain prog:Program ;    rdfs:range prog:Code.prog:Code rdf:type rdfs:Class ;    rdfs:label "Code"^^xsd:string ;    rdfs:comment "Code is a set of instructions given to a computer to perform a specific task."^^xsd:string .prog:writtenIn rdf:type rdf:Property ;    rdfs:label "written in"^^xsd:string ;    rdfs:comment "Property indicating the programming language a code is written in."^^xsd:string ;    rdfs:domain prog:Code ;    rdfs:range prog:ProgrammingLanguage.prog:ProgrammingLanguage rdf:type rdfs:Class ;    rdfs:label "Programming Language"^^xsd:string ;    rdfs:comment "A programming language is a formal language designed to communicate instructions to a machine, particularly a computer."^^xsd:string. prog:hasInput rdf:type rdf:Property ;    rdfs:label "has input"^^xsd:string ;    rdfs:comment "Property showing that a program accepts some input."^^xsd:string ;    rdfs:domain prog:Program ;    rdfs:range prog:Input.prog:Input rdf:type rdfs:Class ;    rdfs:label "Input"^^xsd:string ;    rdfs:comment "Input is data that a program accepts for processing."^^xsd:string .prog:hasOutput rdf:type rdf:Property ;    rdfs:label "has output"^^xsd:string ;    rdfs:comment "Property showing that a program produces some output."^^xsd:string ;    rdfs:domain prog:Program ;    rdfs:range prog:Output.prog:Output rdf:type rdfs:Class ;    rdfs:label "Output"^^xsd:string ;    rdfs:comment "Output is data that a program produces after processing."^^xsd:string .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix prg: <http://www.example.org/program#> .### Ontology surrounding the concept of Program## Classesprg:Programm rdf:type owl:Class ;   rdfs:comment "A series of coded software instructions to control the operation of a computer or other machine." ;   rdfs:label "Program" .prg:Software rdf:type owl:Class ;   rdfs:comment "The programs and other operating information used by a computer." ;   rdfs:label "Software" ;   rdfs:subClassOf prg:Program .prg:Script rdf:type owl:Class ;   rdfs:comment "A program or sequence of instructions that is interpreted or carried out by another program rather than by the computer processor." ;   rdfs:label "Script" ;   rdfs:subClassOf prg:Program .prg:Algorithm rdf:type owl:Class ;   rdfs:comment "A process or set of rules to be followed in calculations or other problem-solving operations, especially by a computer." ;   rdfs:label "Algorithm" ;   rdfs:subClassOf prg:Program .## Propertiesprg:isWrittenIn rdf:type owl:ObjectProperty ;   rdfs:comment "Specifies the programming language that was used to write the program." ;   rdfs:label "is written in" ;   rdfs:domain prg:Program ;   rdfs:range prg:ProgrammingLanguage .prg:hasAuthor rdf:type owl:ObjectProperty ;   rdfs:comment "Specifies the author of the program." ;   rdfs:label "has author" ;   rdfs:domain prg:Program ;   rdfs:range prg:Programmer .#######################################


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .ex:Program rdf:type owl:Class .ex:Software ex:subClassOf ex:Program .ex:Executable ex:subClassOf ex:Program .ex:SourceCode ex:subClassOf ex:Program .ex:hasVersion rdf:type owl:DatatypeProperty .ex:hasVersion rdfs:domain ex:Program .ex:hasVersion rdfs:range xsd:string .ex:hasLanguage rdf:type owl:DatatypeProperty .ex:hasLanguage rdfs:domain ex:Program .ex:hasLanguage rdfs:range xsd:string .ex:runsOn rdf:type owl:ObjectProperty .ex:runsOn rdfs:domain ex:Program .ex:runsOn rdfs:range ex:OperatingSystem .ex:OperatingSystem rdf:type owl:Class .ex:Linux ex:subClassOf ex:OperatingSystem .ex:Windows ex:subClassOf ex:OperatingSystem .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix prg: <http://example.com/program/> .prg:Program rdf:type rdfs:Class ;    rdfs:label "Program" ;    rdfs:comment "A collection of instructions that performs a specific task when executed by a computer." .prg:hasName rdf:type rdf:Property ;    rdfs:label "has name" ;    rdfs:comment "The name of the program." ;    rdfs:domain prg:Program ;    rdfs:range xsd:string .prg:hasDeveloper rdf:type rdf:Property ;    rdfs:label "has developer" ;    rdfs:comment "The developer of the program." ;    rdfs:domain prg:Program ;    rdfs:range xsd:string .prg:hasVersion rdf:type rdf:Property ;    rdfs:label "has version" ;    rdfs:comment "The version of the program." ;    rdfs:domain prg:Program ;    rdfs:range xsd:decimal .prg:hasLanguage rdf:type rdf:Property ;    rdfs:label "has language" ;    rdfs:comment "The programming language used in the program." ;    rdfs:domain prg:Program ;    rdfs:range xsd:string .prg:hasPlatform rdf:type rdf:Property ;    rdfs:label "has platform" ;    rdfs:comment "The platform on which the program runs." ;    rdfs:domain prg:Program ;    rdfs:range xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://example.org/program#> .:Program rdf:type rdfs:Class ;   rdfs:label "Program"@en ;   rdfs:comment "A class representing a program or software."@en .:hasName rdf:type owl:DatatypeProperty ;   rdfs:domain :Program ;   rdfs:range xsd:string ;   rdfs:label "has name"@en ;   rdfs:comment "The name of the program."@en .:hasVersion rdf:type owl:DatatypeProperty ;   rdfs:domain :Program ;   rdfs:range xsd:string ;   rdfs:label "has version"@en ;   rdfs:comment "The version of the program."@en .:isWrittenIn rdf:type owl:ObjectProperty ;   rdfs:domain :Program ;   rdfs:range :ProgrammingLanguage ;   rdfs:label "is written in"@en ;   rdfs:comment "The programming language the program is written in."@en .:ProgrammingLanguage rdf:type rdfs:Class ;   rdfs:label "Programming Language"@en ;   rdfs:comment "A class representing a programming language."@en .:hasAuthor rdf:type owl:ObjectProperty ;   rdfs:domain :Program ;   rdfs:range :Author ;   rdfs:label "has author"@en ;   rdfs:comment "The author of the program."@en .:Author rdf:type rdfs:Class ;   rdfs:label "Author"@en ;   rdfs:comment "A class representing an author."@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix program: <http://example.org/program#> .program:Program rdf:type rdfs:Class ;rdfs:label "Program"@en ;rdfs:comment "A set of related measures, events, or activities with a particular long-term aim"@en .program:hasName rdf:type owl:DatatypeProperty ;rdfs:range xsd:string ;rdfs:domain program:Program ;rdfs:label "has name"@en ;rdfs:comment "The name of the program"@en .program:hasDescription rdf:type owl:DatatypeProperty ;rdfs:range xsd:string ;rdfs:domain program:Program ;rdfs:label "has description"@en ;rdfs:comment "Detailed information about a program"@en .program:hasStartDate rdf:type owl:DatatypeProperty ;rdfs:range xsd:date ;rdfs:domain program:Program ;rdfs:label "has start date"@en ;rdfs:comment "The start date of the program"@en .program:hasEndDate rdf:type owl:DatatypeProperty ;rdfs:range xsd:date ;rdfs:domain program:Program ;rdfs:label "has end date"@en ;rdfs:comment "The end date of the program"@en .program:hasDuration rdf:type owl:DatatypeProperty ;rdfs:range xsd:duration ;rdfs:domain program:Program ;rdfs:label "has duration"@en ;rdfs:comment "The duration of the program"@en .program:isPartOf rdf:type owl:ObjectProperty ;rdfs:range program:Program ;rdfs:domain program:Program ;rdfs:label "is part of"@en ;rdfs:comment "To designate that a program belongs to another larger program"@en .program:hasPart rdf:type owl:ObjectProperty ;rdfs:domain program:Program ;rdfs:range program:Program ;rdfs:label "has part"@en ;rdfs:comment "To indicate that a program contains another smaller program"@en .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix : <http://www.example.org/program#> .:Program a rdfs:Class ;    rdfs:comment "Any sequence of instructions that can be interpreted by a computer" ;    rdfs:label "Program" .:hasProgramName a owl:DatatypeProperty ;    rdfs:domain :Program ;    rdfs:range xsd:string ;    rdfs:comment "Denotes the name of a program." ;    rdfs:label "has program name" .:hasVersion a owl:DatatypeProperty ;    rdfs:domain :Program ;    rdfs:range xsd:string ;    rdfs:comment "Denotes the version of a program." ;    rdfs:label "has version" .:hasDeveloper a owl:ObjectProperty ;    rdfs:domain :Program ;    rdfs:range :Developer ;    rdfs:comment "Denotes the developer of a program." ;    rdfs:label "has developer" .:Developer a rdfs:Class ;    rdfs:comment "The individual or organization responsible for creating a program." ;    rdfs:label "Developer" .:hasDeveloperName a owl:DatatypeProperty ;    rdfs:domain :Developer ;    rdfs:range xsd:string ;    rdfs:comment "Denotes the name of a developer." ;    rdfs:label "has developer name" .

