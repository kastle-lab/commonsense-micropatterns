@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix dbo: <http://dbpedia.org/ontology/> .@prefix home: <http://example.com/home#> .home:     a dbo:Place ;    rdfs:label "Home"@en ;    rdfs:comment "A place where an individual or family resides"@en .home:Room     a rdfs:Class ;    rdfs:label "Room"@en ;    rdfs:comment "A distinct area within a home, such as a bedroom or kitchen"@en ;    rdfs:subClassOf home: .home:Bedroom     a rdfs:Class ;    rdfs:label "Bedroom"@en ;    rdfs:comment "A room for sleeping in"@en ;    rdfs:subClassOf home:Room .home:Kitchen     a rdfs:Class ;    rdfs:label "Kitchen"@en ;    rdfs:comment "A room used for cooking and food preparation"@en ;    rdfs:subClassOf home:Room .home:Bathroom     a rdfs:Class ;    rdfs:label "Bathroom"@en ;    rdfs:comment "A room containing a bathtub or a shower and usually also a washbasin and a toilet"@en ;    rdfs:subClassOf home:Room .home:Furniture     a rdfs:Class ;    rdfs:label "Furniture"@en ;    rdfs:comment "Items that can be moved and used to make a house, office, or other area ready for living or working"@en .  home:Bed     a rdfs:Class ;    rdfs:label "Bed"@en ;    rdfs:comment "A piece of furniture for sleep or rest, typically a framework with a mattress"@en ;    rdfs:subClassOf home:Furniture .home:Table     a rdfs:Class ;    rdfs:label "Table"@en ;    rdfs:comment "A piece of furniture with a flat top and one or more legs, providing a level surface for eating, writing, or working"@en ;    rdfs:subClassOf home:Furniture .home:Person    a dbo:Person ;    rdfs:label "Person"@en ;    rdfs:comment "A human being regarded as an individual"@en .home:hasRoom    a owl:ObjectProperty ;    rdfs:domain home: ;    rdfs:range home:Room ;    rdfs:label "has room"@en ;    rdfs:comment "Relates a home to its rooms"@en .home:hasFurniture     a owl:ObjectProperty ;    rdfs:domain home:Room ;    rdfs:range home:Furniture ;    rdfs:label "has furniture"@en ;    rdfs:comment "Relates a room to its furniture"@en .home:residesIn     a owl:ObjectProperty ;     rdfs:domain home:Person ;     rdfs:range home: ;     rdfs:label "resides in"@en ;     rdfs:comment "Relates a person to the home they live in"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix home: <http://example.org/home.owl#> .home:Home a owl:Class ;  rdfs:label "Home" ;  rdfs:comment "A place where someone lives" .home:Bedroom a owl:Class ;  rdfs:label "Bedroom" ;  rdfs:subClassOf home:Home ;  rdfs:comment "A room within a home for sleeping" .home:Kitchen a owl:Class ;  rdfs:label "Kitchen" ;  rdfs:subClassOf home:Home ;  rdfs:comment "A room within a home for cooking and food preparation" .home:Bathroom a owl:Class ;  rdfs:label "Bathroom" ;  rdfs:subClassOf home:Home ;  rdfs:comment "A room within a home used for personal hygiene activities" .home:LivingRoom a owl:Class ;  rdfs:label "Living Room" ;  rdfs:subClassOf home:Home ;  rdfs:comment "A room in a home used for common social or leisure activities" .home:hasRoom a owl:ObjectProperty ;  rdfs:domain home:Home ;  rdfs:range home:Room ;  rdfs:label "has room" ;  rdfs:comment "Relates a home to its rooms".home:Room a owl:Class ;  rdfs:label "Room" ;  rdfs:comment "A part or division of a building enclosed by walls, floor, and ceiling".  home:Furniture a owl:Class ;  rdfs:label "Furniture" ;  rdfs:comment "Large movable equipment, such as tables and chairs, used to make a house, office, or other space suitable for living or working".home:hasFurniture a owl:ObjectProperty ;  rdfs:domain home:Room ;  rdfs:range home:Furniture ;  rdfs:label "has furniture" ;  rdfs:comment "Relates a room to its furniture".



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix home: <http://example.org/home#> .home:Building a rdfs:Class ;    rdfs:comment "A structure with walls and a roof." ;    rdfs:label "Building" .home:Apartment a rdfs:Class ;    rdfs:label "Apartment" ;    rdfs:comment "A suite of rooms forming one residence, typically in a building containing a number of these." ;    rdfs:subClassOf home:Building .    home:House a rdfs:Class ;    rdfs:comment "A building for human habitation, especially one that is lived in by a family or small group of people." ;    rdfs:label "House" ;    rdfs:subClassOf home:Building .    home:Room a rdfs:Class ;    rdfs:comment "A part or division of a building enclosed by walls, floor, and ceiling." ;    rdfs:label "Room" .home:Kitchen a rdfs:Class ;    rdfs:comment "A room or area where food is prepared and cooked." ;    rdfs:label "Kitchen" ;    rdfs:subClassOf home:Room .home:Bathroom a rdfs:Class ;    rdfs:comment "A room containing a bathtub or a shower and usually also a washbasin and a toilet." ;    rdfs:label "Bathroom" ;    rdfs:subClassOf home:Room .home:hasRoom a rdf:Property ;    rdfs:domain home:Building ;    rdfs:range home:Room ;    rdfs:label "has room" ;    rdfs:comment "The property that relates a building with its rooms." .





