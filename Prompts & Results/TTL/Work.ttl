

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix wo: <http://example.com/work_ontology#> .wo:Work a rdfs:Class ;    rdfs:label "Work"@en ;    rdfs:comment "A class representing any type of work or employment."@en .wo:Job a rdfs:Class ;    rdfs:subClassOf wo:Work ;    rdfs:label "Job"@en ;    rdfs:comment "A regular remunerative position."@en .wo:FreelanceJob a rdfs:Class ;    rdfs:subClassOf wo:Job ;    rdfs:label "Freelance Job"@en ;    rdfs:comment "A job in which a person works for themselves, rather than for a company."@en .wo:FullTimeJob a rdfs:Class ;    rdfs:subClassOf wo:Job ;    rdfs:label "Full-time Job"@en ;    rdfs:comment "A job that requires working for a full set of hours per week."@en .wo:PartTimeJob a rdfs:Class ;    rdfs:subClassOf wo:Job ;    rdfs:label "Part-time Job"@en ;    rdfs:comment "A job that requires working for less than the normal hours per week."@en .wo:title a rdf:Property ;    rdfs:domain wo:Work ;    rdfs:range xsd:string ;    rdfs:comment "A title or name of the work."@en .wo:organization a rdf:Property ;    rdfs:domain wo:Work ;    rdfs:range xsd:string ;    rdfs:comment "The organization or company the work is for."@en .wo:location a rdf:Property ;    rdfs:domain wo:Work ;    rdfs:range xsd:string ;    rdfs:comment "The location where the work takes place."@en .wo:description a rdf:Property ;    rdfs:domain wo:Work ;    rdfs:range xsd:string ;    rdfs:comment "Description of the work."@en .wo:startDate a rdf:Property ;    rdfs:domain wo:Work ;    rdfs:range xsd:date ;    rdfs:comment "Date when the work starts."@en .wo:endDate a rdf:Property ;    rdfs:domain wo:Work ;    rdfs:range xsd:date ;    rdfs:comment "Date when the work ends."@en .wo:salary a rdf:Property ;    rdfs:domain wo:Job ;    rdfs:range xsd:decimal ;    rdfs:comment "The remuneration for the job."@en .wo:hoursPerWeek a rdf:Property ;    rdfs:domain wo:Job ;    rdfs:range xsd:integer ;    rdfs:comment "The number of hours needed to work per week for the job."@en .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http:/example.org/work#> .# Defining Classeswork:Work rdf:type owl:Class .work:Employment rdf:type owl:Class .work:Location rdf:type owl:Class .work:Role rdf:type owl:Class .# Defining Subclasseswork:FullTime rdf:type owl:Class ;    rdfs:subClassOf work:Employment .work:PartTime rdf:type owl:Class ;    rdfs:subClassOf work:Employment .# Defining Propertieswork:hasRole rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Role .work:atLocation rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Location .work:hasEmploymentType rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Employment .work:name rdf:type owl:DatatypeProperty ;    rdfs:domain work:Location ;    rdfs:range xsd:string .work:description rdf:type owl:DatatypeProperty ;    rdfs:domain work:Role ;    rdfs:range xsd:string .work:hoursPerWeek rdf:type owl:DatatypeProperty ;    rdfs:domain work:Employment ;    rdfs:range xsd:decimal .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/work#> .ex:Work  rdf:type owl:Class ;  rdfs:label "Work" ;  rdfs:subClassOf owl:Thing ;.ex:Employee  rdf:type owl:Class ;  rdfs:label "Employee" ;  rdfs:subClassOf ex:Work ;.ex:Employer  rdf:type owl:Class ;  rdfs:label "Employer" ;  rdfs:subClassOf ex:Work ;.ex:tasks  rdf:type owl:ObjectProperty ;  rdfs:label "tasks" ;  rdfs:range ex:Task ;.ex:Task  rdf:type owl:Class ;  rdfs:label "Task" ;  rdfs:subClassOf ex:Work ;.ex:role  rdf:type owl:DatatypeProperty ;  rdfs:label "role" ;  rdfs:range xsd:string ;.ex:salary  rdf:type owl:DatatypeProperty ;  rdfs:label "salary" ;  rdfs:range xsd:float ;.ex:WorkPeriod  rdf:type owl:Class ;  rdfs:label "WorkPeriod" ;  rdfs:subClassOf ex:Work ;.ex:startDate  rdf:type owl:DatatypeProperty ;  rdfs:label "startDate" ;  rdfs:range xsd:date ;.ex:endDater rdf:type owl:DatatypeProperty ;  rdfs:label "endDate" ;  rdfs:range xsd:date ;.





@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http://example.org/work#> .work:Work rdf:type owl:Class .work:hasJobTitle rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:string .    work:hasSalary rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:decimal .work:hasStartDate rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:date .work:Employer rdf:type owl:Class .work:Work rdfs:subClassOf [  rdf:type owl:Restriction ;  owl:onProperty work:hasEmployer ;  owl:someValuesFrom work:Employer] .work:hasEmployer rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Employer .work:hasLocation rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:string .    work:isFullTime rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:boolean .work:isRemote rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:boolean .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix work: <http://example.org/work/> .work:Job a owl:Class ;    rdfs:label "Job"@en ;    rdfs:comment "An employment position held by a person"@en .work:Employee a owl:Class ;    rdfs:label "Employee"@en ;    rdfs:comment "A person who works under an employment contract"@en .work:Employer a owl:Class ;    rdfs:label "Employer"@en ;    rdfs:comment "A person or organization employing people"@en .work:hasJob a owl:ObjectProperty ;    rdfs:domain work:Employee ;    rdfs:range work:Job ;    rdfs:label "has job"@en ;    rdfs:comment "Links an employee to the job they have"@en .work:employs a owl:ObjectProperty ;    rdfs:domain work:Employer ;    rdfs:range work:Employee ;    rdfs:label "employs"@en ;    rdfs:comment "Links an employer to the people they employ"@en .work:jobTitle a owl:DatatypeProperty ;    rdfs:domain work:Job ;    rdfs:range xsd:string ;    rdfs:label "job title"@en ;    rdfs:comment "The title of a job"@en .work:salary a owl:DatatypeProperty ;    rdfs:domain work:Job ;    rdfs:range xsd:decimal ;    rdfs:label "salary"@en ;    rdfs:comment "The salary for a job"@en .work:fullName a owl:DatatypeProperty ;    rdfs:domain work:Employee ;    rdfs:range xsd:string ;    rdfs:label "full name"@en ;    rdfs:comment "The full name of an employee"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http://example.org/work#> .<http://example.org/work>    rdf:type owl:Ontology ;    rdfs:comment "An ontology surrounding the concept of Work" ;    rdfs:label "Work Ontology" .work:Work    rdf:type owl:Class ;    rdfs:label "Work" ;    rdfs:comment "This class describes all types of work or professional activities" .work:Employee    rdf:type owl:Class ;    rdfs:label "Employee" ;    rdfs:comment "An individual who performs work for a wage or salary" .work:Employer    rdf:type owl:Class ;    rdfs:label "Employer" ;    rdfs:comment "An individual or entity that hires and pays employees for their work" .work:JobTitle    rdf:type owl:Class ;    rdfs:label "Job Title" ;    rdfs:comment "The formal designation of a worker's role in an organization" .work:hasJob    rdf:type owl:ObjectProperty ;    rdfs:domain work:Employee ;    rdfs:range work:Work ;    rdfs:label "has job" ;    rdfs:comment "Relates an individual to the work they perform" .work:employs    rdf:type owl:ObjectProperty ;    rdfs:domain work:Employer ;    rdfs:range work:Employee ;    rdfs:label "employs" ;    rdfs:comment "Relates an employer to the individual they employ" .work:jobTitle    rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:string ;    rdfs:label "job title" ;    rdfs:comment "The title of a specific Job or Work" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http://www.example.com/work#> .work:Work  rdf:type rdfs:Class .work:fullTime  rdf:type rdfs:Class ;  rdfs:subClassOf work:Work .work:partTime   rdf:type rdfs:Class ;  rdfs:subClassOf work:Work . work:jobTitle  rdf:type rdf:Property ;  rdfs:domain work:Work ;  rdfs:range xsd:string .work:employer  rdf:type rdf:Property ;  rdfs:domain work:Work ;  rdfs:range xsd:string .work:employee    rdf:type rdf:Property ;    rdfs:domain work:Work ;    rdfs:range xsd:string .work:startDate  rdf:type rdf:Property ;  rdfs:domain work:Work ;  rdfs:range xsd:date .work:endDate  rdf:type rdf:Property ;  rdfs:domain work:Work ;  rdfs:range xsd:date . work:Job1  rdf:type work:fullTime ;  work:jobTitle "Software Developer" ;  work:employer "TechCompany Ltd." ;  work:employee "John Doe" ;  work:startDate "2021-01-01"^^xsd:date ;  work:endDate " "^^xsd:date . work:Job2  rdf:type work:partTime ;  work:jobTitle "Data Analyst" ;  work:employer "Analytics Ltd." ;  work:employee "Jane Doe" ;  work:startDate "2020-05-01"^^xsd:date ;  work:endDate "2022-01-31"^^xsd:date .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix work: <http://example.org/work#> .work:Work rdf:type rdfs:Class ;    rdfs:comment "A class representing all kinds of work"^^xsd:string ;    rdfs:label "Work"^^xsd:string .work:Job rdf:type rdfs:Class ;    rdfs:subClassOf work:Work ;    rdfs:label "Job"^^xsd:string ;    rdfs:comment "A class representing formal employment"^^xsd:string .work:Task rdf:type rdfs:Class ;    rdfs:subClassOf work:Work ;    rdfs:label "Task"^^xsd:string ;    rdfs:comment "A class representing a specific piece of work or assignment"^^xsd:string .work:Volunteer rdf:type rdfs:Class ;    rdfs:subClassOf work:Work ;    rdfs:label "Volunteer"^^xsd:string ;    rdfs:comment "A class representing unpaid work, often for a charitable cause"^^xsd:string .work:startDate rdf:type owl:DatatypeProperty ;    rdfs:range xsd:dateTime ;    rdfs:domain work:Work ;    rdfs:label "start date"^^xsd:string ;    rdfs:comment "The start date of a work"^^xsd:string .work:endDate rdf:type owl:DatatypeProperty ;    rdfs:range xsd:dateTime ;    rdfs:domain work:Work ;    rdfs:label "end date"^^xsd:string ;    rdfs:comment "The end date of a work"^^xsd:string .work:worker rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Worker ;    rdfs:label "worker"^^xsd:string ;    rdfs:comment "The person who does the work"^^xsd:string .work:Worker rdf:type owl:Class ;    rdfs:label "Worker"^^xsd:string ;    rdfs:comment "A class representing person who is doing work"^^xsd:string .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix work: <http://www.example.org/ontology/work#> .work:Work rdf:type owl:Class .work:hasEmployer rdf:type owl:ObjectProperty ;rdfs:domain work:Work ;rdfs:range work:Employer .work:hasEmployee rdf:type owl:ObjectProperty ;rdfs:domain work:Employer ;rdfs:range work:Employee .work:hasPosition rdf:type owl:ObjectProperty ;rdfs:domain work:Work ;rdfs:range work:Position .work:Employee rdf:type owl:Class .work:Employer rdf:type owl:Class .work:Position rdf:type owl:Class .work:hasStartDate rdf:type owl:DatatypeProperty ;rdfs:domain work:Work ;rdfs:range xsd:date .work:hasEndDate rdf:type owl:DatatypeProperty ;rdfs:domain work:Work ;rdfs:range xsd:date .work:hasFullName rdf:type owl:DatatypeProperty ;rdfs:domain work:Employee ;rdfs:range xsd:string .work:hasCompanyName rdf:type owl:DatatypeProperty ;rdfs:domain work:Employer ;rdfs:range xsd:string .work:hasJobTitle rdf:type owl:DatatypeProperty ;rdfs:domain work:Position ;rdfs:range xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http://www.example.org/work#> .work:Work rdf:type owl:Class ;    rdfs:label "Work" ;    rdfs:comment "Different types of work or jobs" .work:hasEmployee rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Employee ;    rdfs:label "has employee" ;    rdfs:comment "The employee who is doing the work" .work:Employee rdf:type owl:Class ;    rdfs:label "Employee" ;    rdfs:comment "Individual who works for a company or organization" .work:hasPosition rdf:type owl:ObjectProperty ;    rdfs:domain work:Employee ;    rdfs:range work:Position ;    rdfs:label "has position" ;    rdfs:comment "The position or job title of the employee" .work:Position rdf:type owl:Class ;    rdfs:label "Position" ;    rdfs:comment "The specific role or title that an employee holds" .work:hasSalary rdf:type owl:DatatypeProperty ;    rdfs:domain work:Position ;    rdfs:range xsd:decimal ;    rdfs:label "has salary" ;    rdfs:comment "The salary for a specific position" .work:hasDuration rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:duration ;    rdfs:label "has duration" ;    rdfs:comment "The duration of the work" .


@prefix schema: <http://schema.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .<http://example.com/ontology/Work>  rdf:type owl:Ontology ;  rdfs:comment "Ontology for the domain of Work" .schema:JobPosting  rdf:type rdfs:Class ;  rdfs:subClassOf schema:Work ;  rdfs:comment "A listing that describes a job opening" .  schema:Employee  rdf:type rdfs:Class ;  rdfs:subClassOf schema:Work ;  rdfs:comment "An individual who provides work to a organization" .schema:Employer  rdf:type rdfs:Class ;  rdfs:subClassOf schema:Work ;  rdfs:comment "An organization or individual that hires employees" .schema:Organization  rdf:type rdfs:Class ;  rdfs:subClassOf schema:Work ;  rdfs:comment "An organized body of people with a common function" .schema:hasJobPosting  rdf:type owl:ObjectProperty ;  rdfs:domain schema:Organization ;  rdfs:range schema:JobPosting ;  rdfs:comment "A property indicating job postings of an organization" .schema:employedBy  rdf:type owl:ObjectProperty ;  rdfs:domain schema:Employee ;  rdfs:range schema:Organization ;  rdfs:comment "A property indicating the organization that an employee works for" .schema:employs  rdf:type owl:ObjectProperty ;  rdfs:domain schema:Employer ;  rdfs:range schema:Employee ;  rdfs:comment "A property indicating the employees who work for an employer" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix work: <http://www.example.org/ontology/work#> .work:Work rdf:type rdfs:Class ;          rdfs:label "Work"@en ;          rdfs:comment "Universal class for all kind of work activities"@en .work:Employee rdf:type rdfs:Class ;               rdfs:subClassOf work:Work ;             rdfs:label "Employee"@en ;             rdfs:comment "An individual who performs work for an employer"@en .work:Employer rdf:type rdfs:Class ;               rdfs:subClassOf work:Work ;             rdfs:label "Employer"@en ;             rdfs:comment "An entity that hires employees to perform work"@en .work:hasEmployee rdf:type rdf:Property ;                   rdfs:label "has employee"@en ;                 rdfs:comment "A relationship between an employer and its employee"@en ;                 rdfs:domain work:Employer ;                 rdfs:range work:Employee .work:performWork rdf:type rdf:Property ;                   rdfs:label "performs work"@en ;                 rdfs:comment "A relationship indicating the employee performs work for the employer"@en ;                 rdfs:domain work:Employee ;                 rdfs:range work:Employer .work:salary rdf:type rdf:Property ;              rdfs:label "salary"@en ;            rdfs:comment "The remuneration an employee receives for their work"@en ;            rdfs:domain work:Employee ;            rdf:range xsd:decimal .



@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http://example.com/work#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .work:Work a owl:Class .work:hasTitle a owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:string .work:hasStartTime a owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:dateTime .work:hasEndTime a owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:dateTime .work:isPerformedBy a owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Worker .work:Worker a owl:Class .work:hasName a owl:DatatypeProperty ;    rdfs:domain work:Worker ;    rdfs:range xsd:string .work:hasAge a owl:DatatypeProperty ;    rdfs:domain work:Worker ;    rdfs:range xsd:integer .work:WorksIn a owl:ObjectProperty ;    rdfs:domain work:Worker ;    rdfs:range work:Work .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Work rdf:type rdfs:Class .ex:work1 rdf:type ex:Work ;    ex:hasTitle "Example Work"^^xsd:string ;    ex:hasStartDate "2020-01-01"^^xsd:date ;    ex:hasEndDate "2020-12-31"^^xsd:date ;    ex:isPerformedBy ex:employee1 .ex:employee1 rdf:type ex:Employee ;    ex:hasName "John Doe"^^xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/stuff/1.0/> .ex:Work rdf:type rdfs:Class .ex:hasTitle rdf:type rdf:Property ;   rdfs:domain ex:Work ;   rdfs:range xsd:string .ex:hasAuthor rdf:type rdf:Property ;   rdfs:domain ex:Work ;   rdfs:range xsd:string .ex:hasDate rdf:type rdf:Property ;   rdfs:domain ex:Work ;   rdfs:range xsd:dateTime .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix work: <http://example.com/work#> .work:Work rdf:type owl:Class ;   rdfs:comment "Representation of Work or tasks" ;   rdfs:subClassOf rdfs:Resource .work:Employee rdf:type owl:Class ;   rdfs:comment "Person who performs a Work" ;   rdfs:subClassOf rdfs:Resource .work:jobTitle rdf:type owl:DatatypeProperty ;    rdfs:domain work:Work ;    rdfs:range xsd:string ;    rdfs:comment "Title or name of the Work" .work:hasEmployee rdf:type owl:ObjectProperty ;    rdfs:domain work:Work ;    rdfs:range work:Employee ;    rdfs:comment "Link a Work to an Employee who performs it" .work:employeeName rdf:type owl:DatatypeProperty ;    rdfs:domain work:Employee ;    rdfs:range xsd:string ;    rdfs:comment "Name of the Employee" .  work:employeeID rdf:type owl:DatatypeProperty ;    rdfs:domain work:Employee ;    rdfs:range xsd:int ;    rdfs:comment "Identifier of the Employee" . 


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix work: <http://example.com/work#> .work:Work rdfs:subClassOf rdfs:Resource ;          rdfs:label "Work"@en ;          rdfs:comment "A description of a work, such as a job or task."@en ;          rdfs:seeAlso work:Task .work:Employer rdfs:subClassOf rdfs:Resource ;              rdfs:label "Employer"@en ;              rdfs:comment "An entity that employs individuals for work."@en .work:Employee rdfs:subClassOf rdfs:Resource ;              rdfs:label "Employee"@en ;              rdfs:comment "An individual who is employed for work by an employer."@en .work:employedBy rdf:type owl:ObjectProperty ;                rdfs:label "employed by"@en ;                rdfs:comment "Links an employee to their employer."@en ;                rdfs:domain work:Employee ;                rdfs:range work:Employer .

