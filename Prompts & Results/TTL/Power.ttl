
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix power: <http://example.org/power#> .power:Electric rdf:type rdfs:Class;    rdfs:comment "Electrical power or energy" .power:Mechanical rdf:type rdfs:Class;    rdfs:comment "Mechanical power or energy" .power:Nuclear rdf:type rdfs:Class;    rdfs:wattage "Nuclear power or energy" .power:Solar rdf:type rdfs:Class;    rdfs:comment "Solar power or energy" .power:Wind rdf:type rdfs:Class;    rdfs:comment "Wind power or energy" .power:Hydraulic rdf:type rdfs:Class;    rdfs:comment "Hydraulic power or energy" .power:Geothermal rdf:type rdfs:Class;    rdfs:comment "Geothermal power or energy" .power:Wattage rdf:type rdf:Property;    rdfs:comment "Measurement of power output" ;    rdfs:domain power:Power ;    rdfs:range xsd:float .power:Source rdf:type rdf:Property;    rdfs:comment "Source of the power" ;    rdfs:domain power:Power ;    rdfs:range rdfs:Resource .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org#> .# Category Classesex:Power rdf:type rdfs:Class.ex:ElectricalPower rdf:type rdfs:Class .ex:SolarPower rdf:type rdfs:Class .ex:MagneticPower rdf:type rdfs:Class .ex:NuclearPower rdf:type rdfs:Class .ex:WavePower rdf:type rdfs:Class .ex:HumanPower rdf:type rdfs:Class .# Subclass Relationsex:Power rdfs:subClassOf ex:Energy.ex:ElectricalPower rdfs:subClassOf ex:Power.ex:SolarPower rdfs:subClassOf ex:Power.ex:MagneticPower rdfs:subClassOf ex:Power.ex:NuclearPower rdfs:subClassOf ex:Power.ex:WavePower rdfs:subClassOf ex:Power.ex:HumanPower rdfs:subClassOf ex:Power.# Propertiesex:generates rdf:type rdf:Property ;              rdfs:domain ex:Power ;             rdfs:range ex:Energy .ex:consumes rdf:type rdf:Property ;            rdfs:domain ex:Power ;            rdfs:range ex:Energy .ex:transformsFrom rdf:type rdf:Property ;                  rdfs:domain ex:Power ;                  rdfs:range ex:Energy .ex:transformsTo rdf:type rdf:Property ;                rdfs:domain ex:Power ;                rdfs:range ex:Energy .# Individuals and Their Class Membershipex:WindTurbine rdf:type ex:WindPower .# Individuals and Their Property Relationshipsex:WindTurbine ex:generates ex:ElectricalPower .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/power#> .ex:Power rdf:type rdfs:Class .rdfs:label "Power" .ex:ElectricalPower rdf:type rdfs:SubClassOf ;  rdfs:label "Electrical power" ;  rdfs:subClassOf ex:Power .ex:MechanicalPower rdf:type rdfs:SubClassOf ;  rdfs:label "Mechanical power" ;  rdfs:subClassOf ex:Power .ex:SolarPower rdf:type rdfs:SubClassOf ;  rdfs:label "Solar power" ;  rdfs:subClassOf ex:Power .ex:hasSource rdf:type rdf:Property ;  rdfs:label "has source" ;  rdfs:domain ex:Power ;  rdfs:range ex:Source .ex:Source rdf:type rdfs:Class ;  rdfs:label "Source" .ex:NaturalSource rdf:type rdfs:SubClassOf ;  rdfs:label "Natural source" ;  rdfs:subClassOf ex:Source .ex:ArtificialSource rdf:type rdfs:SubClassOf ;  rdfs:label "Artificial source" ;  rdfs:subClassOf ex:Source .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix power: <http://www.example.org/power#> .power:Power rdf:type rdfs:Class .power:Mechanical rdf:type rdfs:Class ;    rdfs:subClassOf power:Power .power:Electrical rdf:type rdfs:Class ;    rdfs:subClassOf power:Power .power:Nuclear rdf:type rdfs:Class ;    rdfs:subClassOf power:Power .power:amount rdf:type rdf:Property ;    rdfs:domain power:Power ;    rdfs:range xsd:decimal .power:unit rdf:type rdf:Property ;    rdfs:domain power:Power ;    rdfs:range rdfs:Literal .power:source rdf:type rdf:Property ;    rdfs:domain power:Power ;    rdfs:range rdfs:Resource . power:Transformer rdf:type rdfs:Class ;    rdfs:subClassOf rdfs:Resource .power:turbine rdf:type rdfs:Class ;    rdfs:subClassOf rdfs:Resource .power:solarPanel rdf:type rdfs:Class ;    rdfs:subClassOf rdfs:Resource .power:Solar rdf:type power:Power ;    power:amount "500.0"^^xsd:decimal ;    power:unit "Watts" ;    power:source power:solarPanel .power:Turbine rdf:type power:Power ;    power:amount "2000.0"^^xsd:decimal ;    power:unit "Watts" ;    power:source power:turbinepower:Transformer rdf:type power:Power ;    power:amount "10000.0"^^xsd:decimal ;    power:unit "Watts" ;    power:source power:Transformer .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix power: <http://example.org/power#> .power:PowerConcept    a rdfs:Class ;    rdfs:label "Power Concept" ;    rdfs:comment "Class of all Power Concepts" .power:ElectricalPower    a rdfs:Class ;    rdfs:label "Electrical Power" ;    rdfs:subClassOf power:PowerConcept ;    rdfs:comment "Class of all electrical power concepts" .power:MechanicalPower    a rdfs:Class ;    rdfs:label "Mechanical Power" ;    rdfs:subClassOf power:PowerConcept ;    rdfs:comment "Class of all mechanical power concepts" .power:hasMagnitude    a rdf:Property ;    rdfs:label "has magnitude" ;    rdfs:domain power:PowerConcept ;    rdfs:range xsd:float ;    rdfs:comment "Property for the magnitude of power" .power:hasUnit    a rdf:Property ;    rdfs:label "has unit" ;    rdfs:domain power:PowerConcept ;    rdfs:range rdfs:Literal ;    rdfs:comment "Property for the unit of power" .power:isUsedIn    a rdf:Property ;    rdfs:label "is used in" ;    rdfs:domain power:PowerConcept ;    rdfs:range rdfs:Resource ;    rdfs:comment "Property for the equipment or resource that uses the power concept" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://www.example.org/power#> .:Power a rdfs:Class ;    rdfs:comment "The class of all things related to Power" .:source a rdf:Property ;    rdfs:domain :Power ;    rdfs:range xsd:string ;    rdfs:comment "defines the source of power" .:amount a rdf:Property ;    rdfs:domain :Power ;    rdfs:range xsd:integer ;    rdfs:comment "defines the amount of power" .:unit a rdf:Property ;    rdfs:domain :Power ;    rdfs:range xsd:string ;    rdfs:comment "defines the unit of power" .:Electricity a :Power ;    :source "Generators" ;    :amount "1000"^^xsd:integer ;    :unit "Watt" .:Solar a :Power ;    :source "Sunlight" ;    :amount "700"^^xsd:integer ;    :unit "Watt" .:Nuclear a :Power ;    :source "Nuclear fusion" ;    :amount "1000000"^^xsd:integer ;    :unit "Watt" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www3.org/2000/01/rdf-schema#> .@prefix power: <http://example.org/power#> .power:     rdf:type rdfs:Class ;    rdfs:comment "Class representing the concept of power" .power:MechanicalPower      rdf:type rdfs:Class ;    rdfs:subClassOf power: ;    rdfs:comment "Power generated by mechanical means" .power:ElectricalPower     rdf:type rdfs:Class ;    rdfs:subClassOf power: ;    rdfs:comment "Power generated by electricity" .power:NuclearPower     rdf:type rdfs:Class ;    rdfs:subClassOf power: ;    rdfs:comment "Power generated by nuclear energy" .power:SolarPower     rdf:type rdfs:Class ;    rdfs:subClassOf power: ;    rdfs:comment "Power generated by solar energy" .power:WindPower     rdf:type rdfs:Class ;    rdfs:subClassOf power: ;    rdfs:comment "Power generated by wind energy" .power:HydroPower     rdf:type rdfs:Class ;    rdfs:subClassOf power: ;    rdfs:comment "Power generated by water energy" .power:Magnitude     rdf:type rdf:Property ;    rdfs:domain power: ;    rdfs:range rdfs:Literal ;    rdfs:comment "Property representing the magnitude of power" .power:Unit     rdf:type rdf:Property ;    rdfs:domain power: ;    rdfs:range rdfs:Literal ;    rdfs:comment "Property representing the unit of power measurement" .power:GeneratedBy     rdf:type rdf:Property ;    rdfs:domain power: ;    rdfs:range rdfs:Resource ;    rdfs:comment "Property representing the source generating the power" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix power: <http://www.example.org/power#> .# Classespower:Power a owl:Class .power:MechanicalPower a owl:Class ;  rdfs:subClassOf power:Power .power:ElectricalPower a owl:Class ;  rdfs:subClassOf power:Power .power:SolarPower a owl:Class ;  rdfs:subClassOf power:Power .# Propertiespower:isGeneratedBy a owl:ObjectProperty ;  rdfs:domain power:Power ;  rdfs:range power:PowerSource .power:isMeasuredIn a owl:DatatypeProperty ;  rdfs:domain power:Power ;  rdfs:range xsd:string .# Individualspower:CoalPlant a power:PowerSource ;  power:isGeneratedBy power:ElectricalPower .power:HydroPlant a power:PowerSource ;  power:isGeneratedBy power:MechanicalPower .power:Sun a power:PowerSource ;  power:isGeneratedBy power:SolarPower .# Note: PowerSource is assumed to be a pre-existent class in this ontology.

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix power: <http://www.example.com/power#> .power:    rdf:type owl:Ontology ;    rdfs:comment "An ontology dedicated to the Power field"@en .power:Power    rdf:type owl:Class ;    rdfs:label "Power"@en ;    rdfs:comment "Class representing power in general"@en .power:MechanicalPower    rdf:type owl:Class ;    rdfs:subClassOf power:Power ;    rdfs:label "Mechanical Power"@en ;    rdfs:comment "Power used or produced by mechanical systems"@en .power:ElectricalPower    rdf:type owl:Class ;    rdfs:subClassOf power:Power ;    rdfs:label "Electrical Power"@en ;    rdfs:comment "Power used or produced by electrical systems"@en .power:hasUnit    rdf:type owl:DatatypeProperty ;    rdfs:range xsd:string ;    rdfs:domain power:Power ;    rdfs:label "has unit"@en ;    rdfs:comment "The unit of measurement for the power"@en .power:hasValue    rdf:type owl:DatatypeProperty ;    rdfs:range xsd:decimal ;    rdfs:domain power:Power ;    rdfs:label "has value"@en ;    rdfs:comment "The value or amount of the power"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix op: <http://www.example.com/ontology/power#> .op:Power a owl:Class ;  rdfs:label "Power" ;  rdfs:comment "Power in various contexts, such as physical power, political power, etc." .op:PhysicalPower a owl:Class ;  rdfs:subClassOf op:Power ;  rdfs:label "Physical power" ;  rdfs:comment "Physical power, expressed as energy over time." .op:PoliticalPower a owl:Class ;  rdfs:subClassOf op:Power ;  rdfs:label "Political power" ;  rdfs:comment "Political power, representing influence over others." .op:hasPower a owl:ObjectProperty ;  rdfs:domain op:Individual ;  rdfs:range op:Power ;  rdfs:label "has power" ;  rdfs:comment "A property indicating that an individual or group has some form of power." .op:Individual a owl:Class ;  rdfs:label "Individual" ;  rdfs:comment "An individual or group that can hold some form of power." .op:amountOfPower a owl:DatatypeProperty ;  rdfs:domain op:PhysicalPower ;  rdfs:range xsd:decimal ;  rdfs:label "amount of power" ;  rdfs:comment "The quantitative measure of physical power." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix power: <http://example.org/power#> .power:     rdf:type owl:Ontology ;    rdfs:comment "Ontology about the concept of Power" .power:Power     rdf:type owl:Class ;    rdfs:label "The concept of Power" ;    rdfs:comment "The capacity or ability to direct or influence the behavior of others or the course of events".power:PhysicalPower     rdf:type owl:Class ;    rdfs:subClassOf power:Power ;    rdfs:label "Physical Power" ;    rdfs:comment "The capacity to do work or cause physical change".power:SocialPower     rdf:type owl:Class ;    rdfs:subClassOf power:Power ;    rdfs:label "Social Power" ;    rdfs:comment "The degree to which an individual or organization can influence others".power:PoliticalPower     rdf:type owl:Class ;    rdfs:subClassOf power:Power ;    rdfs:label "Political Power" ;    rdfs:comment "The capacity to influence or control the behavior of people, typically through governance".power:hasPower     rdf:type owl:ObjectProperty ;    rdfs:domain owl:Thing ;    rdfs:range power:Power ;    rdfs:label "has power" ;    rdfs:comment "An object property to show that a person or organization possesses power" . power:amountOfPower     rdf:type owl:DatatypeProperty ;      rdfs:domain power:Power ;     rdfs:range xsd:integer ;    rdfs:label "amount of power" ;    rdfs:comment "A data property to describe the quantity or intensity of power" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com#> .ex:Power rdf:type rdfs:Class ;    rdfs:label "Power" ;    rdfs:comment "A measure of one's ability to control or influence something" .ex:PoliticalPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power ;    rdfs:label "Political Power" ;    rdfs:comment "Power used in the context of political relations, such as government and citizens" .ex:EconomicPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power ;    rdfs:label "Economic Power" ;    rdfs:comment "Power used in economic contexts, such as corporations and markets" .ex:MilitaryPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power ;    rdfs:label "Military Power" ;    rdfs:comment "Power defined by military strength and strategic capabilities" .ex:SuperPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power ;    rdfs:label "Super Power" ;    rdfs:comment "Extraordinary abilities beyond the capabilities of normal entities" .ex:PowerSource rdf:type rdfs:Class ;    rdfs:label "Power Source" ;    rdfs:comment "The origin or source from which power is derived" .ex:hasPower rdf:type rdf:Property ;    rdfs:label "has Power" ;    rdfs:domain ex:Entity ;    rdfs:range ex:Power .ex:derivedFrom rdf:type rdf:Property ;    rdfs:label "derived from" ;    rdfs:domain ex:Power ;    rdfs:range ex:PowerSource .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ont: <http://www.example.org/ontology#> .ont:Power a rdfs:Class ;    rdfs:label "Power"@en ;    rdfs:comment "Something that is owned or possessed, which gives the ability or capacity to do something or act in a particular way"@en .ont:PhysicalPower a rdfs:Class ;    rdfs:subClassOf ont:Power ;    rdfs:label "Physical Power"@en ;    rdfs:comment "Power that refers to strength and energy in an action or a movement, often related to the body or a physical object"@en .ont:SocialPower a rdfs:Class ;    rdfs:subClassOf ont:Power ;    rdfs:label "Social Power"@en ;    rdfs:comment "Power that is related to interpersonal relationships, often reflected in the influence or control over others within a social group"@en.ont:ElectricalPower a rdfs:Class ;    rdfs:subClassOf ont:Power ;    rdfs:label "Electrical Power"@en ;    rdfs:comment "Power which refers to the rate, per unit time, at which electrical energy is transferred by an electric circuit"@en.ont:PoliticalPower a rdfs:Class ;    rdfs:subClassOf ont:Power ;    rdfs:label "Political Power"@en ;    rdfs:comment "Power that is related to or held by a person or group in a political context, often associated with governance and decision-making in a society"@en.



@prefix ex: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .ex:Power a owl:Class ;   rdfs:label "Power"@en ;   rdfs:comment "The capacity or ability to direct or influence the behavior of others or the course of events"@en .ex:ElectricalPower a owl:Class ;   rdfs:subClassOf ex:Power ;   rdfs:label "Electrical Power"@en ;   rdfs:comment "Power that is generated by electricity"@en .ex:PoliticalPower a owl:Class ;   rdfs:subClassOf ex:Power ;   rdfs:label "Political Power"@en ;   rdfs:comment "Power that is related togovt"@en .ex:PhysicalPower a owl:Class ;   rdfs:subClassOf ex:Power ;   rdfs:label "Physical Power"@en ;   rdfs:comment "Power that is related to physical strength and energy"@en .ex:SolarPower a owl:Class ;   rdfs:subClassOf ex:ElectricalPower ;   rdfs:label "Solar Power"@en ;   rdfs:comment "Power that is generated by the sun's rays"@en .ex:WindPower a owl:Class ;   rdfs:subClassOf ex:ElectricalPower ;   rdfs:label "Wind Power"@en ;   rdfs:comment "Power that is generated by wind"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/power/> .ex:Power rdf:type rdfs:Class .ex:MechanicalPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power .ex:ElectricalPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power .ex:HydroPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:MechanicalPower .ex:WindPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:MechanicalPower .ex:SolarPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:ElectricalPower .ex:NuclearPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:ElectricalPower .ex:powerGeneration rdf:type rdf:Property ;    rdfs:domain ex:Power ;    rdfs:range rdfs:Literal .ex:capacity rdf:type rdf:Property ;    rdfs:domain ex:Power ;    rdfs:range rdfs:Literal .ex:affectEnvironment rdf:type rdf:Property ;    rdfs:domain ex:Power ;    rdfs:range rdfs:Literal .



@prefix ex: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .ex:Country rdf:type rdfs:Class .ex:hasPowerOver rdf:type rdf:Property ;    rdfs:domain ex:Country ;    rdfs:range ex:Country .ex:USA rdf:type ex:Country ;    ex:hasPowerOver ex:CountryX .ex:CountryX rdf:type ex:Country .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/ontology#> .ex:Power a rdfs:Class ;    rdfs:label "Power" ;    rdfs:comment "A quantity or attribute of a physical system or body; an object's capacity to perform work."ex:hasPower a rdf:Property ;    rdfs:label "has power" ;    rdfs:comment "The property that connects an object to its power." ;    rdfs:domain ex:Object ;    rdfs:range ex:Power .    ex:MeasuredPower a ex:Power ;    rdfs:label "Measured Power" ;    rdfs:comment "A value of power that has been determined by measurement." ;    ex:UnitOfPower a rdfs:Class ;    rdfs:label "Unit of Power" ;    rdfs:comment "The unit in which power is measured." ;    ex:hasUnitOfPower a rdf:Property ;    rdfs:label "has unit of power" ;    rdfs:comment "The property that connects a measured power to its unit." ;    rdfs:domain ex:MeasuredPower ;    rdfs:range ex:UnitOfPower .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://www.example.org/ontology#> .ex:Power a rdfs:Class ;    rdfs:label "Power"@en ;    rdfs:comment "The class of all Power Entities"@en ;    rdfs:subClassOf ex:PhysicalQuantity .ex:hasPower a rdf:Property ;    rdfs:label "has power"@en ;    rdfs:comment "A property to represent the power of an entity"@en ;    rdfs:domain ex:Machine ;    rdfs:range ex:Power .ex:Machine a rdfs:Class ;    rdfs:label "Machine"@en ;    rdfs:comment "The class of all Machine Entities"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .ex:Power a rdfs:Class .ex:hasPower a rdf:Property ;    rdfs:domain ex:Entity ;    rdfs:range ex:Power .ex:Entity a owl:Class .ex:MechanicalPower a owl:Class ;    rdfs:subClassOf ex:Power .    ex:ElectricalPower a owl:Class ;    rdfs:subClassOf ex:Power .ex:entity1 a ex:Entity ;    ex:hasPower ex:MechanicalPower .ex:entity2 a ex:Entity ;    ex:hasPower ex:ElectricalPower .


@prefix : <http://example.org/power#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:ElectricalPower a owl:Class ;    rdfs:label "Electrical Power" ;    rdfs:comment "Amount of work done by electrical current in a specific amount of time" .:MechanicalPower a owl:Class ;    rdfs:label "Mechanical Power" ;    rdfs:comment "Amount of work done or energy transferred in a specific amount of time" .:ImpactPower a owl:Class ;    rdfs:label "Impact Power" ;    rdfs:comment "Ability to generate high force rapidly" .:CalculatesPower a owl:ObjectProperty ;    rdfs:label "Calculates Power" ;    rdfs:comment "Defines the entity capable of calculating power" ;    rdfs:domain :Entity ;    rdfs:range  [a owl:Class ; owl:unionOf (:ElectricalPower :MechanicalPower :ImpactPower)].:Entity a owl:Class ;    rdfs:label "Entity" ;    rdfs:comment "An abstract entity that can calculate power" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/> .ex:Power rdf:type rdfs:Class .ex:ElectricalPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power .ex:MechanicalPower rdf:type rdfs:Class ;    rdfs:subClassOf ex:Power .ex:hasPower rdf:type rdf:Property ;    rdfs:domain ex:Machine ;    rdfs:range ex:Power .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:ElectricalPower a rdfs:Class ;    rdfs:label "Electrical Power"^^xsd:string ;    rdfs:comment "The amount of electrical power in Watt" .ex:MechanicalPower a rdfs:Class ;    rdfs:label "Mechanical Power"^^xsd:string ;    rdfs:comment "The amount of mechanical power in Watt" .ex:MagneticPower a rdfs:Class ;    rdfs:label "Magnetic Power"^^xsd:string ;    rdfs:comment "The amount of magnetic power in Watt" .ex:Measurement a rdfs:Class ;    rdfs:label "Measurement"^^xsd:string ;    rdfs:comment "A power measurement" .ex:unit a rdfs:Property ;    rdfs:label "unit"^^xsd:string ;    rdfs:domain ex:Measurement ;    rdfs:range xsd:string .ex:value a rdfs:Property ;    rdfs:label "value"^^xsd:string ;    rdfs:domain ex:Measurement ;    rdfs:range xsd:decimal .ex:hasMeasurement a rdfs:Property ;    rdfs:label "has measurement"^^xsd:string ;    rdfs:domain ex:ElectricalPower, ex:MechanicalPower, ex:MagneticPower ;    rdfs:range ex:Measurement .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix power: <http://example.org/power#> .power:Power a rdfs:Class .power:hasUnit a rdf:Property ;    rdfs:domain power:Power ;    rdfs:range xsd:string .power:hasValue a rdf:Property ;    rdfs:domain power:Power ;    rdfs:range xsd:float .

