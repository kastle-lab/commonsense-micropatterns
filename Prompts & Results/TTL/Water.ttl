@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix water: <http://example.org/water#> .# Classeswater:Water rdf:type rdfs:Class ;    rdfs:comment "The liquid substance that is vital for all known forms of life"^^xsd:string ;    rdfs:label "Water"^^xsd:string .water:SaltWater rdf:type rdfs:Class ;    rdfs:subClassOf water:Water ;    rdfs:comment "Water from the sea or ocean, with a high salt content"^^xsd:string ;    rdfs:label "Salt Water"^^xsd:string .water:FreshWater rdf:type rdfs:Class ;    rdfs:subClassOf water:Water ;    rdfs:comment "Water that is naturally occurring and has low concentrations of dissolved salts and minerals"^^xsd:string ;    rdfs:label "Fresh Water"^^xsd:string .# Propertieswater:isSourceOf rdf:type rdf:Property ;    rdfs:domain water:Water ;    rdfs:range rdfs:Resource ;    rdfs:comment "The source from which the water originated"^^xsd:string ;    rdfs:label "is source of"^^xsd:string .water:hasTemperature rdf:type rdf:Property ;    rdfs:domain water:Water ;    rdfs:range xsd:decimal ;    rdfs:comment "The temperature of the water in degrees Celsius"^^xsd:string ;    rdfs:label "has temperature"^^xsd:string .water:hasPH rdf:type rdf:Property ;    rdfs:domain water:Water ;    rdfs:range xsd:decimal ;    rdfs:comment "The pH level of the water"^^xsd:string ;    rdfs:label "has pH"^^xsd:string .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix waterOnt: <http://example.com/waterOnt#> .waterOnt:Water a rdfs:Class ;    rdfs:label "Water"@en ;    rdfs:comment "H2O, a transparent, tasteless, odorless, and nearly colorless chemical substance, which is the main constituent of Earth's hydrosphere and the fluids of all known living organisms"@en .waterOnt:isSaltWater a rdf:Property ;    rdfs:label "is salt water"@en ;    rdfs:domain waterOnt:Water ;    rdfs:range xsd:boolean .waterOnt:isFreshWater a rdf:Property ;    rdfs:label "is fresh water"@en ;    rdfs:domain waterOnt:Water ;    rdfs:range xsd:boolean .waterOnt:freezingPoint a rdf:Property ;    rdfs:label "freezing point"@en ;    rdfs:domain waterOnt:Water ;    rdfs:range xsd:float .waterOnt:boilingPoint a rdf:Property ;    rdfs:label "boiling point"@en ;    rdfs:domain waterOnt:Water ;    rdfs:range xsd:float . waterOnt:WaterEntity a waterOnt:Water ;    waterOnt:isSaltWater false ;    waterOnt:isFreshWater true ;    waterOnt:freezingPoint 0.0 ;    waterOnt:boilingPoint 100.0 .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix water: <http://www.example.com/water#> .water:     rdf:type rdfs:Class ;    rdfs:label "Water"@en;    rdfs:comment "The liquid that forms the seas, lakes, rivers, and rain and is the basis of the fluids of living organisms."@en .water:State    rdf:type rdfs:Class ;    rdfs:label "State"@en ;    rdfs:comment "The different physical states in which water can exist: solid (ice), liquid (water), and gas (steam or vapor)."@en .    water:Source      rdf:type rdfs:Class ;    rdfs:label "Source"@en ;    rdfs:comment "The origin points or places where water is sourced from, such as rivers, lakes, oceans, wells, etc."@en .water:hasState    rdf:type rdf:Property ;    rdfs:domain water: ;    rdfs:range water:State ;    rdfs:label "has state"@en ;    rdfs:comment "The property that relates water to its physical state."@en .water:hasSource    rdf:type rdf:Property ;    rdfs:domain water: ;    rdfs:range water:Source ;    rdfs:label "has source"@en ;    rdfs:comment "The property that relates water to its source."@en .## Instances of water in different stateswater:IcelandGlacierWater    rdf:type water: ;    water:hasState water:Solid ;    water:hasSource "Glacier"^^xsd:string ;    rdfs:label "Iceland Glacier water"@en .water:AmazonRiverWater    rdf:type water: ;    water:hasState water:Liquid ;    water:hasSource "River"^^xsd:string ;    rdfs:label "Amazon River water"@en .water:BoilingWater    rdf:type water: ;    water:hasState water:Gas ;    water:hasSource "Kettle"^^xsd:string ;    rdfs:label "Boiling water"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix water: <http://www.example.org/water#> .water:Water rdf:type owl:Class ;    rdfs:comment "A liquid that forms the seas, lakes, rivers, and rain, and is the basis of the fluids of living organisms."@en ;    rdfs:label "Water"@en .water:FreshWater rdf:type owl:Class ;    rdfs:subClassOf water:Water ;    rdfs:comment "Water that is not salty, especially when considered as a natural resource."@en ;    rdfs:label "Fresh water"@en .    water:SaltWater rdf:type owl:Class ;    rdfs:subClassOf water:Water ;    rdfs:comment "Water from a sea or ocean that has a high salt content."@en ;    rdfs:label "Salt water"@en .water:isUsedIn rdf:type owl:ObjectProperty ;    rdfs:domain water:Water ;    rdfs:range water:Use ;    rdfs:comment "Relates water to its uses."@en ;    rdfs:label "is used in"@en .water:Use rdf:type owl:Class ;    rdfs:comment "An application or use of water."@en ;    rdfs:label "Use"@en .water:Drinking rdf:type owl:Class ;    rdfs:subClassOf water:Use ;    rdfs:comment "The act of consuming water by drinking."@en ;    rdfs:label "Drinking"@en .water:Bathing rdf:type owl:Class ;    rdfs:subClassOf water:Use ;    rdfs:comment "The act of using water to wash the body."@en ;    rdfs:label "Bathing"@en .water:Agriculture rdf:type owl:Class ;    rdfs:subClassOf water:Use ;    rdfs:comment "The use of water for growing crops and raising animals."@en ;    rdfs:label "Agriculture"@en .water:IndustrialUse rdf:type owl:Class ;    rdfs:subClassOf water:Use ;    rdfs:comment "The use of water in various industrial applications."@en ;    rdfs:label "Industrial use"@en .



