
@prefix : <http://www.example.org/#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .:Day a owl:Class ;    rdfs:label "Day"@en ;    rdfs:comment "A 24-hour period of time during which the earth completes one rotation on its axis"@en ;    rdfs:subClassOf owl:Thing .:hasHours a owl:ObjectProperty ;    rdfs:label "has hours"@en ;    rdfs:domain :Day ;    rdfs:range :Hour .:Hour a owl:Class ;    rdfs:label "Hour"@en ;    rdfs:subClassOf owl:Thing .    :hasMinutes a owl:ObjectProperty ;    rdfs:label "has minutes"@en ;    rdfs:domain :Hour ;    rdfs:range :Minute .    :Minute a owl:Class ;    rdfs:label "Minute"@en ;    rdfs:subClassOf owl:Thing .    :hasSeconds a owl:ObjectProperty ;    rdfs:label "has seconds"@en ;    rdfs:domain :Minute ;    rdfs:range :Second .:Second a owl:Class ;    rdfs:label "Second"@en ;    rdfs:subClassOf owl:Thing .:hasPartOfDay a owl:ObjectProperty ;    rdfs:label "has part of day"@en ;    rdfs:domain :Day ;    rdfs:range :PartOfDay .:PartOfDay a owl:Class ;    rdfs:subClassOf owl:Thing .:Morning a owl:Class ;    rdfs:label "Morning"@en ;    rdfs:subClassOf :PartOfDay .:Afternoon a owl:Class ;    rdfs:label "Afternoon"@en ;    rdfs:subClassOf :PartOfDay .    :Evening a owl:Class ;    rdfs:label "Evening"@en ;    rdfs:subClassOf :PartOfDay .:Night a owl:Class ;    rdfs:label "Night"@en ;    rdfs:subClassOf :PartOfDay .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.com/day#> .day:Day rdf:type rdfs:Class .day:hasName rdf:type rdf:Property ;  rdfs:domain day:Day ;  rdfs:range xsd:string .day:hasTimeOfDay rdf:type rdf:Property ;  rdfs:domain day:Day ;  rdfs:range xsd:string .day:MORNING rdf:type day:Day ;  day:hasName "Morning" ;  day:hasTimeOfDay "6 AM to 12 PM"^^xsd:string .day:AFTERNOON rdf:type day:Day ;  day:hasName "Afternoon" ;  day:hasTimeOfDay "12 PM to 4 PM"^^xsd:string .day:EVENING rdf:type day:Day ;  day:hasName "Evening" ;  day:hasTimeOfDay "4 PM to 8 PM"^^xsd:string .day:NIGHT rdf:type day:Day ;  day:hasName "Night" ;  day:hasTimeOfDay "8 PM to 6 AM"^^xsd:string .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/day#> .day:Day rdf:type owl:Class ;    rdfs:comment "Class represents a generic day concept" ;    rdfs:subClassOf rdfs:Resource .day:hasTime rdf:type owl:ObjectProperty ;    rdfs:comment "Property represents time segments in a day (morning, afternoon, evening, night)" ;    rdfs:domain day:Day ;    rdfs:range day:Time .day:Time rdf:type owl:Class ;    rdfs:comment "Class represents time periods in a day." ;    rdfs:subClassOf rdfs:Resource .day:Morning rdf:type owl:NamedIndividual, day:Time .day:Afternoon rdf:type owl:NamedIndividual, day:Time .day:Evening rdf:type owl:NamedIndividual, day:Time .day:Night rdf:type owl:NamedIndividual, day:Time .day:hasActivity rdf:type owl:ObjectProperty ;    rdfs:comment "Property represents the activity done during a specific time of a day" ;    rdfs:domain day:Time ;    rdfs:range day:Activity .day:Activity rdf:type owl:Class ;    rdfs:comment "Class represents a type of activity that can be performed in a day." ;    rdfs:subClassOf rdfs:Resource .day:Work rdf:type owl:NamedIndividual, day:Activity .day:Sleep rdf:type owl:NamedIndividual, day:Activity .day:Eat rdf:type owl:NamedIndividual, day:Activity .day:Play rdf:type owl:NamedIndividual, day:Activity .day:hasDate rdf:type owl:DatatypeProperty ;    rdfs:comment "Property represents the specific date for a day" ;    rdfs:domain day:Day ;    rdfs:range xsd:date .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/day#> .day:Day a rdfs:Class ;    rdfs:label "Day" ;    rdfs:comment "A Day in a week or year" .day:hasDate a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:date ;    rdfs:label "has date" ;    rdfs:comment "The specific date of the day." .day:hasDayOfWeek a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:string ;    rdfs:label "has day of week" ;    rdfs:comment "The day of the week." .day:hasDayOfYear a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:string ;    rdfs:label "has day of year" ;    rdfs:comment "The day of the year." .day:Monday a day:Day ;    day:hasDate "2022-10-10"^^xsd:date ;    day:hasDayOfWeek "Monday" ;    day:hasDayOfYear "283" .day:Tuesday a day:Day ;    day:hasDate "2022-10-11"^^xsd:date ;    day:hasDayOfWeek "Tuesday" ;    day:hasDayOfYear "284" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.com/day#> .day:Day rdf:type rdfs:Class .day:isPartOfWeek rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range day:Week .day:hasTemperature rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:float .day:hasWeatherCondition rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range day:WeatherCondition .day:sunriseTime rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:dateTime .day:sunsetTime rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:dateTime .day:Week rdf:type rdfs:Class .day:WeatherCondition rdf:type rdfs:Class .day:monday rdf:type day:Day ;    day:isPartOfWeek day:Week ;    day:hasTemperature "20.0"^^xsd:float ;    day:hasWeatherCondition day:Sunny ;    day:sunriseTime "2022-04-01T06:15:00Z"^^xsd:dateTime ;    day:sunsetTime "2022-04-01T19:30:00Z"^^xsd:dateTime .day:Sunny rdf:type day:WeatherCondition .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/day#> .# Class definitionsday:Day rdf:type rdfs:Class ;    rdfs:comment "Class representing a single day."@en .day:Weekday rdf:type rdfs:Class ;    rdfs:comment "Class representing a weekday, e.g., Monday, Tuesday, etc."@en .day:Weekend rdf:type rdfs:Class ;    rdfs:comment "Class representing a day in the weekend, e.g., Saturday, Sunday."@en .# Subclass relationsday:Weekday rdfs:subClassOf day:Day ;    rdfs:comment "A weekday is a subclass of day."@en .    day:Weekend rdfs:subClassOf day:Day ;    rdfs:comment "A weekend day is a subclass of day."@en .# Property definitionsday:hasDate rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:date ;    rdfs:comment "Property specifying the date of a day."@en .day:hasName rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range rdfs:Literal ;    rdfs:comment "Property specifying the name of a day."@en .# Instance definitionsday:Monday rdf:type day:Weekday ;    rdfs:label "Monday"@en ;    day:hasName "Monday" ;    day:hasDate "2022-04-11"^^xsd:date .day:Tuesday rdf:type day:Weekday ;    rdfs:label "Tuesday"@en ;    day:hasName "Tuesday" ;    day:hasDate "2022-04-12"^^xsd:date .day:Saturday rdf:type day:Weekend ;    rdfs:label "Saturday"@en ;    day:hasName "Saturday" ;    day:hasDate "2022-04-16"^^xsd:date .



@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix day: <http://example.org/DayOntology#> .day:Day a rdfs:Class ;  rdfs:comment "Represents a day" ;  rdfs:label "Day" .day:hasDate a rdf:Property ;  rdfs:comment "The date of the day" ;  rdfs:label "hasDate" ;  rdfs:domain day:Day ;  rdfs:range xsd:date .day:hasDayOfWeek a rdf:Property ;  rdfs:comment "The day of the week" ;  rdfs:label "hasDayOfWeek" ;  rdfs:domain day:Day ;  rdfs:range rdfs:Literal .day:hasTemperature a rdf:Property ;  rdfs:comment "The average temperature of the day" ;  rdfs:label "hasTemperature" ;  rdfs:domain day:Day ;  rdfs:range xsd:float .day:hasWeatherCondition a rdf:Property ;  rdfs:comment "The weather condition of the day" ;  rdfs:label "hasWeatherCondition" ;  rdfs:domain day:Day ;  rdfs:range rdfs:Literal .day:hasDaylightDuration a rdf:Property ;  rdfs:comment "The duration of daylight during the day" ;  rdfs:label "hasDaylightDuration" ;  rdfs:domain day:Day ;  rdfs:range xsd:duration .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/ontology/day#> .day:Day rdf:type rdfs:Class .day:hasDayPart rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range day:DayPart .day:Morning rdf:type day:DayPart .day:Afternoon rdf:type day:DayPart .day:Evening rdf:type day:DayPart .day:Night rdf:type day:DayPart .day:hasStartTime rdf:type rdf:Property ;    rdfs:domain day:DayPart ;    rdfs:range xsd:time .day:hasEndTime rdf:type rdf:Property ;    rdfs:domain day:DayPart ;    rdfs:range xsd:time .  day:hasTemperature rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:float .day:hasWeatherCondition rdf:type rdf:Property ;    rdfs:domain day:Day ;    rdfs:range day:WeatherCondition .day:Sunny rdf:type day:WeatherCondition .day:Cloudy rdf:type day:WeatherCondition .day:Rainy rdf:type day:WeatherCondition .day:Snowy rdf:type day:WeatherCondition .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://www.example.org/Day#> .:Day a owl:Class ;  rdfs:label "Day" ;  rdfs:comment "A period of 24 hours starting from 12:00 AM".  :startsAt a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:time ;  rdfs:label "starts at" ;  rdfs:comment "The time a day starts at".  :endsAt a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:time ;  rdfs:label "ends at" ;  rdfs:comment "The time a day ends at".  :hasDaylight a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:boolean ;  rdfs:label "has daylight" ;  rdfs:comment "The day has a period of daylight".  :hasTemperature a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:float ;  rdfs:label "has temperature" ;  rdfs:comment "The average temperature during the day".  :hasHumidity a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:float ;  rdfs:label "has humidity" ;  rdfs:comment "The average humidity during the day".:hasSunrise a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:time ;  rdfs:label "has sunrise" ;  rdfs:comment "The time of sunrise during the day".  :hasSunset a rdf:Property ;  rdfs:domain :Day ;  rdfs:range xsd:time ;  rdfs:label "has sunset" ;  rdfs:comment "The time of sunset during the day".

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/day#> .day:Day a rdfs:Class ;    rdfs:label "Day" ;    rdfs:comment "A 24-hour period, from midnight to the next midnight." .day:hasDayNumber a rdf:Property ;    rdfs:range xsd:integer ;    rdfs:domain day:Day ;    rdfs:label "has day number" ;    rdfs:comment "The day of the month." .day:hasWeekday a rdf:Property ;    rdfs:range rdfs:Literal ;    rdfs:domain day:Day ;    rdfs:label "has weekday" ;    rdfs:comment "The day of the week." .day:Weather a rdfs:Class ;    rdfs:label "Weather" ;    rdfs:comment "The state of the atmosphere at a place and time." .day:hasWeather a rdf:Property ;    rdfs:range day:Weather ;    rdfs:domain day:Day ;    rdfs:label "has weather" ;    rdfs:comment "The weather of the day." .day:hasTemperature a rdf:Property ;    rdfs:range xsd:integer ;    rdfs:domain day:Weather ;    rdfs:label "has temperature" ;    rdfs:comment "The temperature of the day." .day:hasPrecipitation a rdf:Property ;    rdfs:range xsd:integer ;    rdfs:domain day:Weather ;    rdfs:label "has precipitation" ;    rdfs:comment "The amount of rain, snow, etc. that falls." .day:Event a rdfs:Class ;    rdfs:label "Event" ;    rdfs:comment "Something that occurs in a certain place during a particular interval of time." .day:hasEvent a rdf:Property ;    rdfs:range day:Event ;    rdfs:domain day:Day ;    rdfs:label "has event" ;    rdfs:comment "The event of the day." .day:EventType a rdf:Property ;    rdfs:range rdfs:Literal ;    rdfs:domain day:Event ;    rdfs:label "event type" ;    rdfs:comment "The type of the event." .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/day#> .day:Day a rdfs:Class ;   rdfs:comment "The class of all days" .day:hasDate a rdf:Property ;    rdfs:domain day:Day ;     rdfs:range xsd:date ;    rdfs:comment "The date of the day" .    day:hasDayName a rdf:Property ;    rdfs:domain day:Day ;     rdfs:range xsd:string ;    rdfs:comment "The name of the day" .day:hasDayOfTheWeek a rdf:Property ;    rdfs:domain day:Day ;     rdfs:range xsd:string ;    rdfs:comment "The day of the week" .day:hasPartOfDay a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range day:PartOfDay ;    rdfs:comment "A specific part of the day" .day:PartOfDay a rdfs:Class ;    rdfs:comment "The class of all parts of the day (morning, afternoon, evening, and night)" .day:Morning a day:PartOfDay ;    rdfs:comment "The part of day from sunrise until midday" .day:Afternoon a day:PartOfDay ;    rdfs:comment "The part of day after midday and before evening" .day:Evening a day:PartOfDay ;    rdfs:comment "The part of day from sunset until bedtime" .day:Night a day:PartOfDay ;   rdfs:comment "The part of day when the sun is below the horizon" .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Day a rdfs:Class ;     rdfs:label "Day" ;     rdfs:comment "Represents a day in a week or a calendar" ;     rdfs:subClassOf xsd:string .ex:Monday a ex:Day ;  rdfs:label "Monday" ;  rdfs:comment "Represents Monday, first day of the week in many cultures" .ex:Tuesday a ex:Day ;  rdfs:label "Tuesday" ;  rdfs:comment "Represents Tuesday, the second day of the week in many cultures" .ex:Wednesday a ex:Day ;  rdfs:label "Wednesday" ;  rdfs:comment "Represents Wednesday, the third day of the week in many cultures" .ex:Thursday a ex:Day ;  rdfs:label "Thursday" ;  rdfs:comment "Represents Thursday, the fourth day of the week in many cultures" .ex:Friday a ex:Day ;  rdfs:label "Friday" ;  rdfs:comment "Represents Friday, the fifth day of the week in many cultures" .ex:Saturday a ex:Day ;  rdfs:label "Saturday" ;  rdfs:comment "Represents Saturday, the sixth day of the week in many cultures" .ex:Sunday a ex:Day ;  rdfs:label "Sunday" ;  rdfs:comment "Represents Sunday, the seventh day of the week in many cultures" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix myOnto: <http://www.example.com/myOntology#> .myOnto:Day rdf:type owl:Class ;    rdfs:comment "Day" ;    rdfs:label "Day" .myOnto:hasDayOfWeek rdf:type owl:DatatypeProperty ;    rdfs:comment "Property that relates a Day to a day of the week" ;    rdfs:label "hasDayOfWeek" ;    rdfs:domain myOnto:Day ;    rdfs:range xsd:string .myOnto:hasDayOfMonth rdf:type owl:DatatypeProperty ;    rdfs:comment "Property that relates a Day to a day of the month" ;    rdfs:label "hasDayOfMonth" ;    rdfs:domain myOnto:Day ;    rdfs:range xsd:int .myOnto:hasDayOfYear rdf:type owl:DatatypeProperty ;    rdfs:comment "Property that relates a Day to a day of the year" ;    rdfs:label "hasDayOfYear" ;    rdfs:domain myOnto:Day ;    rdfs:range xsd:int .myOnto:hasMonth rdf:type owl:DatatypeProperty ;    rdfs:comment "Property that relates a Day to a month" ;    rdfs:label "hasMonth" ;    rdfs:domain myOnto:Day ;    rdfs:range xsd:string .myOnto:hasYear rdf:type owl:DatatypeProperty ;    rdfs:comment "Property that relates a Day to a year" ;    rdfs:label "hasYear" ;    rdfs:domain myOnto:Day ;    rdfs:range xsd:int .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix day: <http://example.org/day#> .day:Day a rdfs:Class ;    rdfs:comment "Represents the concept of a day" ;    rdfs:label "Day" .day:hasDate a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:date ;    rdfs:comment "The date of the day" ;    rdfs:label "has date" .day:hasDayOfWeek a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range rdfs:Literal ;    rdfs:comment "The day of the week" ;    rdfs:label "has day of week" .day:isWeekday a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:boolean ;    rdfs:comment "Indicates whether the day is a weekday (Monday-Friday)" ;    rdfs:label "is weekday" .day:isWeekend a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:boolean ;    rdfs:comment "Indicates whether the day is a weekend day (Saturday or Sunday)" ;    rdfs:label "is weekend" .day:hasTemperature a rdf:Property ;    rdfs:domain day:Day ;    rdfs:range xsd:float ;    rdfs:comment "On average, the temperature of the day" ;    rdfs:label "has temperature" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix day: <http://example.org/day#> .day:Day a rdfs:Class ;    rdfs:label "Day"@en ;    rdfs:comment "A class representing a day in the context of calendar"@en .day:hasDate a rdf:Property ;    rdfs:label "has date"@en ;    rdfs:comment "Property relating Day to a specific date"@en ;    rdfs:domain day:Day ;    rdfs:range xsd:date .day:hasDayOfWeek a rdf:Property ;    rdfs:label "has day of week"@en ;    rdfs:comment "Property relating Day to a specific day of week"@en ;    rdfs:domain day:Day ;    rdfs:range xsd:string .day:beginningOfDay a rdf:Property ;    rdfs:label "beginning of day"@en ;    rdfs:comment "Property specifying the beginning time of the day"@en ;    rdfs:domain day:Day ;    rdfs:range xsd:dateTime .day:endOfDay a rdf:Property ;    rdfs:label "end of day"@en ;    rdfs:comment "Property specifying the end time of the day"@en ;    rdfs:domain day:Day ;    rdfs:range xsd:dateTime .

