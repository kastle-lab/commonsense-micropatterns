// Context Classpublic class TrafficLight {   private State state;   public void setState(State state) {      this.state = state;   }   public void changeState() {      state.handleStateChange(this);   }}// Abstract State Classpublic abstract class State {   public abstract void handleStateChange(TrafficLight trafficLight);}// Concrete State Classespublic class RedLightState extends State {   @Override   public void handleStateChange(TrafficLight trafficLight) {      System.out.println("Red Light. You should stop.");      trafficLight.setState(new GreenLightState());   }}public class GreenLightState extends State {   @Override   public void handleStateChange(TrafficLight trafficLight) {      System.out.println("Green Light. You could go.");      trafficLight.setState(new YellowLightState());   }}public class YellowLightState extends State {   @Override   public void handleStateChange(TrafficLight trafficLight) {      System.out.println("Yellow Light. You should prepare to stop.");      trafficLight.setState(new RedLightState());   }}
