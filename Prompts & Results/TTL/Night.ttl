@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.com/ontology/time#> .@prefix astro: <http://www.example.com/ontology/astro#> .time:Night rdf:type owl:Class ;    rdfs:subClassOf time:PeriodOfDay ;    rdfs:label "Night" ;    rdfs:comment "The period of time from sunset to sunrise when it is dark" .time:PeriodOfDay rdf:type owl:Class ;    rdfs:label "Period of Day" ;    rdfs:comment "Different periods during a 24-hour day" .astro:Sunset rdf:type owl:Class ;    rdfs:label "Sunset" ;    rdfs:comment "The time in the evening when the sun disappears or daylight fades" .astro:Sunrise rdf:type owl:Class ;    rdfs:label "Sunrise" ;    rdfs:comment "The time in the morning when the sun appears or daylight begins" .time:hasStart rdf:type owl:ObjectProperty ;    rdfs:domain time:Night ;    rdfs:range astro:Sunset ;    rdfs:label "has start" ;    rdfs:comment "Indicates the starting point of the night" .time:hasEnd rdf:type owl:ObjectProperty ;    rdfs:domain time:Night ;    rdfs:range astro:Sunrise ;    rdfs:label "has end" ;    rdfs:comment "Indicates the ending point of the night" .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix time: <http://www.w3.org/2006/time#> .<urn:absolute:night>  a owl:Class ;  rdfs:label "Night" ;  rdfs:subClassOf time:TemporalEntity ;  rdfs:comment "The period of time from sunset to sunrise." .  <urn:absolute:hasStartTime>  a owl:DatatypeProperty ;  rdfs:label "Has start time" ;  rdfs:domain <urn:absolute:night> ;  rdfs:range xsd:time .  <urn:absolute:hasEndTime>  a owl:DatatypeProperty ;  rdfs:label "Has end time" ;  rdfs:domain <urn:absolute:night> ;  rdfs:range xsd:time .<urn:absolute:hasMoonPhase>  a owl:DatatypeProperty ;  rdfs:label "Has moon phase" ;  rdfs:domain <urn:absolute:night> ;  rdfs:range xsd:string .  <urn:absolute:isDark>  a owl:DatatypeProperty ;  rdfs:label "Is dark" ;  rdfs:domain <urn:absolute:night> ;  rdfs:range xsd:boolean .  <urn:absolute:partOf>  a owl:ObjectProperty ;  rdfs:label "Part of" ;  rdfs:domain <urn:absolute:night> ;  rdfs:range <urn:absolute:day-night-cycle> .<urn:absolute:day-night-cycle>   a owl:Class;  rdfs:label "Day-Night Cycle" ;  rdfs:comment "24 Hours period including Day and Night."

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix night: <http://www.example.org/night#> .night:Night rdf:type owl:Class ;    rdfs:label "Night"@en ;    rdfs:comment "The period of every day when it is dark outside, often associated with sleep and rest"@en .night:startsAt rdf:type owl:DatatypeProperty ;    rdfs:label "starts at"@en ;    rdfs:comment "The time when the night starts"@en ;    rdfs:range xsd:time ;    rdfs:domain night:Night .night:endsAt rdf:type owl:DatatypeProperty ;    rdfs:label "ends at"@en ;    rdfs:comment "The time when the night ends"@en ;    rdfs:range xsd:time ;    rdfs:domain night:Night .night:moonPhase rdf:type owl:DatatypeProperty ;    rdfs:label "moon phase"@en ;    rdfs:comment "The phase of the moon during the night"@en ;    rdfs:range xsd:string ;    rdfs:domain night:Night .night:isAstronomicalNight rdf:type owl:DatatypeProperty ;    rdfs:label "is astronomical night"@en ;    rdfs:comment "A boolean indicating if the night is an astronomical night"@en ;    rdfs:range xsd:boolean ;    rdfs:domain night:Night .night:hasStarVisibility rdf:type owl:DatatypeProperty ;    rdfs:label "has star visibility"@en ;    rdfs:comment "A boolean indicating if the stars are visible during the night"@en ;    rdfs:range xsd:boolean ;    rdfs:domain night:Night .


