@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix night: <http://www.example.org/night#> .# Class Definitionsnight:TimeOfDay rdf:type owl:Class .night:Night rdf:type owl:Class ;  rdfs:subClassOf night:TimeOfDay .night:SleepingTime rdf:type owl:Class ;  rdfs:subClassOf night:Night .night:StargazingTime rdf:type owl:Class ;  rdfs:subClassOf night:Night .# Datatype Properties night:startTime rdf:type owl:DatatypeProperty ;  rdf:domain night:TimeOfDay ;  rdf:range xsd:time .night:endTime rdf:type owl:DatatypeProperty ;  rdf:domain night:TimeOfDay ;  rdf:range xsd:time .# Object Properties night:hasActivity rdf:type owl:ObjectProperty ;  rdf:domain night:TimeOfDay ;  rdf:range [ rdf:type owl:Class ] .night:isPartOf rdf:type owl:ObjectProperty ;  rdf:domain night:TimeOfDay ;  rdf:range [ rdf:type owl:Class ] . # Individuals night:Midnight rdf:type night:Night ;  night:startTime "00:00:00"^^xsd:time ;  night:endTime "01:00:00"^^xsd:time .night:LateNight rdf:type night:Night,                night:SleepingTime,                night:StargazingTime ;  night:startTime "01:01:00"^^xsd:time ;  night:endTime "04:00:00"^^xsd:time . night:EarlyMorning rdf:type night:Night,                   night:SleepingTime ;  night:startTime "04:01:00"^^xsd:time ;  night:endTime "06:00:00"^^xsd:time .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix night: <http://www.example.org/ontologies/night#> .night:Night rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:comment "The period from sunset to sunrise in each twenty-four hours".night:hasPart rdf:type owl:ObjectProperty ;    rdfs:domain night:Night ;    rdfs:range owl:Thing ;    rdfs:comment "A part of night, eg. early night, midnight, late night".night:isTimeOf rdf:type owl:ObjectProperty ;    rdfs:domain night:Night ;    rdfs:range owl:Thing ;    rdfs:comment "A relation indicating what a certain time of night is related to, eg. Night is time of sleep".night:earlyNight rdf:type owl:Class ;    rdfs:subClassOf night:Night ;    rdfs:comment "The period just after sunset".night:midnight rdf:type owl:Class ;    rdfs:subClassOf night:Night ;    rdfs:comment "The middle of the night; specifically, the moment of time that is equidistant between dusk and dawn".night:lateNight rdf:type owl:Class ;    rdfs:subClassOf night:Night ;    rdfs:comment "The period just before sunrise".night:sleep rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:comment "A condition of body and mind such as that which typically recurs for several hours every night, in which the nervous system is relatively inactive, the eyes closed, the postural muscles relaxed, and consciousness practically suspended".night:sleep rdf:type owl:thing ;    night:isTimeOf night:Night .night:darkness rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:comment "The partial or total absence of light".night:darkness rdf:type owl:thing ;    night:isTimeOf night:Night .night:astronomy rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:comment "The branch of science which deals with celestial objects, space, and the physical universe as a whole, often practiced at night".night:astronomy rdf:type owl:thing ;    night:isTimeOf night:Night .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix night: <http://www.example.org/night#> .night:   rdf:type owl:Ontology ;  rdfs:comment "Ontology of the Night" .night:Night   rdf:type owl:Class ;  rdfs:comment "The class of Nights" ;  rdfs:subClassOf owl:Thing .night:start   rdf:type owl:DatatypeProperty ;  rdfs:domain night:Night ;  rdfs:range xsd:dateTime ;  rdfs:comment "The start time of the Night, indicated with a dateTime value" .night:end   rdf:type owl:DatatypeProperty ;  rdfs:domain night:Night ;  rdfs:range xsd:dateTime ;  rdfs:comment "The end time of the Night, indicated with a dateTime value" .night:hasMoonPhase   rdf:type owl:ObjectProperty ;  rdfs:domain night:Night ;  rdfs:range night:MoonPhase ;  rdfs:comment "The moon phase of the Night" .night:MoonPhase   rdf:type owl:Class ;  rdfs:comment "The class of moon phases" ;  rdfs:subClassOf owl:Thing .night:hasWeather   rdf:type owl:ObjectProperty ;  rdfs:domain night:Night ;  rdfs:range night:Weather ;  rdfs:comment "The weather during the Night" .night:Weather   rdf:type owl:Class ;  rdfs:comment "The class of weather types" ;  rdfs:subClassOf owl:Thing .night:hasTemperature   rdf:type owl:DatatypeProperty ;  rdfs:domain night:Night ;  rdfs:range xsd:float ;  rdfs:comment "The temperature during the Night, shown as a floating point number" .night:hasStarsVisible   rdf:type owl:DatatypeProperty ;  rdfs:domain night:Night ;  rdfs:range xsd:boolean ;  rdfs:comment "Indicates if stars are visible during the Night" .night:hasAurora   rdf:type owl:DatatypeProperty ;  rdfs:domain night:Night ;  rdfs:range xsd:boolean ;  rdfs:comment "Indicates if an aurora is visible during the Night" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix night: <http://example.org/night/> .night: has rdf:type owl:Ontology ;  rdfs:comment "Ontology of the Night"^^xsd:string ;  rdfs:label "Night" .night:Star has rdf:type owl:Class ;  rdfs:comment "Stars visible during the night."^^xsd:string ;  rdfs:label "Star" .night:Moon has rdf:type owl:Class ;  rdfs:comment "The moon as seen during night."^^xsd:string ;  rdfs:label "Moon" .night:Sky has rdf:type owl:Class ;  rdfs:comment "The sky during night time."^^xsd:string ;  rdfs:label "Sky" .night:isPartOf has rdf:type owl:ObjectProperty ;  rdfs:domain night:Star ;  rdfs:range night:Sky ;  rdfs:comment "Indicates that a star is part of the night sky."^^xsd:string ;  rdfs:label "is part of" .night:isVisibileIn has rdf:type owl:ObjectProperty ;  rdfs:domain night:Moon ;  rdfs:range night:Sky ;  rdfs:comment "Indicates that the moon is visible in the night sky."^^xsd:string ;  rdfs:label "is visible in" .night:Twinkle has rdf:type owl:DatatypeProperty ;  rdfs:domain night:Star ;  rdfs:range xsd:boolean ;  rdfs:comment "Indicates if a star twinkle during the night."^^xsd:string ;  rdfs:label "Twinkle" .night:Phase has rdf:type owl:DatatypeProperty ;  rdfs:domain night:Moon ;  rdfs:range xsd:string ;  rdfs:comment "Indicates the phase of the moon during the night."^^xsd:string ;  rdfs:label "Phase" .

