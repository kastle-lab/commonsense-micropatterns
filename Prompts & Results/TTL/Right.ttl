@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.com/> .#Defining Right as a classex:Right rdf:type rdfs:Class ;             rdfs:comment "Representation of Rights or entitlements" .#Defining instances of Rightex:FreedomOfSpeech rdf:type ex:Right ;         rdfs:comment "An example of a legal or moral right" .#To represent 'Right' as a directionex:Direction rdf:type rdfs:Class ;                      rdfs:comment "Direction representation" .ex:Right rdf:type ex:Direction ;         rdfs:comment "An instance depicting 'Right' direction" .
Class: :Right    Annotations: rdfs:label "Right"@enObjectProperty: :hasRight    Annotations: rdfs:label "has the right"@en    Domain: :Person    Range: :RightClass: :Person    Annotations: rdfs:label "Person"@en
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix ex: <http://example.org/> .ex:Person a rdf:Class . ex:Right a rdf:Class .ex:hasRight a rdf:Property ;    rdf:domain ex:Person ;    rdf:range ex:Right .ex:RightName a rdf:Property ;    rdf:domain ex:Right ;    rdf:range rdf:PlainLiteral .ex:john a ex:Person .ex:freedomOfSpeech a ex:Right ;    ex:RightName "Freedom of Speech" .ex:john ex:hasRight ex:freedomOfSpeech . 

@prefix : <http://example.org/#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .:Right a owl:Class ;    rdfs:label "Right" ;    rdfs:comment "A class representing a certain privilege or entitlement" .:providesRight a owl:ObjectProperty ;    rdfs:domain :Entity ;    rdfs:range :Right ;    rdfs:label "providesRight" ;    rdfs:comment "A property representing the act of giving or providing a right" .:Entity a owl:Class ;    rdfs:label "Entity" ;    rdfs:comment "A class representing any individual or organization that can provide rights" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/> .ex:Right rdf:type rdfs:Class .ex:hasRight rdf:type rdf:Property ;    rdfs:domain ex:Person ;    rdfs:range ex:Right .# Create some examplesex:Person1 rdf:type ex:Person .ex:Right1 rdf:type ex:Right .ex:Person1 ex:hasRight ex:Right1 .# Optionally you can give some labels to the instances:ex:Person1 rdfs:label "Bob" .ex:Right1 rdfs:label "Right to speech" .

