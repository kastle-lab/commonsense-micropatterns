@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Story rdf:type owl:Class .ex:title rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Story ;    rdfs:range xsd:string .ex:hasCharacter rdf:type owl:ObjectProperty ;    rdfs:domain ex:Story ;    rdfs:range ex:Character .ex:Character rdf:type owl:Class .ex:hasName rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Character ;    rdfs:range xsd:string .ex:Story1 rdf:type ex:Story ;    ex:title "The Great Adventure" .ex:Character1 rdf:type ex:Character ;    ex:hasName "John Doe" .ex:Story1 ex:hasCharacter ex:Character1 .

@prefix dct: <http://purl.org/dc/terms/> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix sot: <http://example.org/story-ontology#> .### Classes sot:Story a rdfs:Class ;    rdfs:comment "A narrative or tale, real or imagined" .sot:Character a rdfs:Class ;    rdfs:comment "An actor or person appearing in a story" .sot:Setting a rdfs:Class ;    rdfs:comment "The place and time where a story takes place" .sot:Event a rdfs:Class ;    rdfs:comment "A significant occurrence within a story" .### Properties sot:hasCharacter a rdf:Property ;    rdfs:domain sot:Story ;    rdfs:range sot:Character ;    rdfs:comment "Indicates a character appearing in a story" .sot:hasSetting a rdf:Property ;    rdfs:domain sot:Story ;    rdfs:range sot:Setting ;    rdfs:comment "Indicates where and when a story takes place" .sot:hasEvent a rdf:Property ;    rdfs:domain sot:Story ;    rdfs:range sot:Event ;    rdfs:comment "Indicates a significant event within a story" .### Individuals sot:story1 a sot:Story ;    sot:hasCharacter sot:character1 ;    sot:hasSetting sot:setting1 ;    sot:hasEvent sot:event1 ;    dct:title "Example Story" ;    dct:creator "John Smith" .sot:character1 a sot:Character ;    foaf:name "John Doe" .sot:setting1 a sot:Setting ;    dct:description "A small town in 1940s" .sot:event1 a sot:Event ;    dct:description "An unexpected discovery" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .# Class definitionex:Story a rdfs:Class ;    rdfs:label "Story" ;    rdfs:comment "A narrative or tale, either true or fictitious" .# Property definitionsex:title a rdf:Property ;    rdfs:label "title" ;    rdfs:comment "The title of the story" ;    rdfs:domain ex:Story ;    rdfs:range xsd:string .ex:author a rdf:Property ;    rdfs:label "author" ;    rdfs:comment "The author of the story" ;    rdfs:domain ex:Story ;    rdfs:range xsd:string .ex:publicationDate a rdf:Property ;    rdfs:label "publication date" ;    rdfs:comment "The date the story was published" ;    rdfs:domain ex:Story ;    rdfs:range xsd:date .# Example instanceex:sampleStory a ex:Story ;    ex:title "Sample Story" ;    ex:author "Sample Author" ;    ex:publicationDate "2022-01-01"^^xsd:date .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix story: <http://example.org/story#> .story:Story a rdfs:Class ;    rdfs:label "Story" ;    rdfs:comment "A narrative or tale, real or imagined."@en .story:title a rdf:Property ;    rdfs:label "title" ;    rdfs:comment "The title of the story."@en ;    rdfs:domain story:Story ;    rdfs:range xsd:string .story:author a rdf:Property ;    rdfs:label "author" ;    rdfs:comment "The author of the story."@en ;    rdfs:domain story:Story ;    rdfs:range xsd:string .story:character a rdf:Property ;    rdfs:label "character" ;    rdfs:comment "A character in the story."@en ;    rdfs:domain story:Story ;    rdfs:range story:Character .story:Character a rdfs:Class ;    rdfs:label "Character" ;    rdfs:comment "A person, animal, or other being that takes part in the action of a story."@en .story:name a rdf:Property ;    rdfs:label "name" ;    rdfs:comment "The name of the character."@en ;    rdfs:domain story:Character ;    rdfs:range xsd:string .

@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .@prefix :        <http://example.com/story#> .:Story rdf:type rdfs:Class ;       rdfs:comment "Class representing a story" .:title rdf:type rdf:Property ;       rdfs:comment "Title of the story" ;       rdfs:domain :Story ;       rdfs:range rdfs:Literal .:author rdf:type rdf:Property ;        rdfs:comment "Author of the story" ;        rdfs:domain :Story ;        rdfs:range rdfs:Literal .:charactarizes rdf:type rdf:Property ;               rdfs:comment "Characterizes the object in the story" ;               rdfs:domain :Story .:Object rdf:type rdfs:Class ;        comment "An object that the story characterizes" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix story: <http://www.example.org/story/> .story:Story a rdfs:Class ;            rdfs:label "Story" ;            rdfs:comment "Represents a story or narrative" .story:title a rdf:Property ;            rdfs:domain story:Story ;            rdfs:range xsd:string ;            rdfs:label "title" ;            rdfs:comment "The title of the story" .story:author a rdf:Property ;            rdfs:domain story:Story ;            rdfs:range xsd:string ;            rdfs:label "author" ;            rdfs:comment "The author of the story" .story:character a rdf:Property ;            rdfs:domain story:Story ;            rdfs:range xsd:string ;            rdfs:label "character" ;            rdfs:comment "A character in the story" .story:plot a rdf:Property ;            rdfs:domain story:Story ;            rdfs:range xsd:string ;            rdfs:label "plot" ;            rdfs:comment "The plot of the story" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://www.example.org/story#> .# Classes definition ex:Story a rdfs:Class .ex:Character a rdfs:Class .ex:Setting a rdfs:Class .ex:Theme a rdfs:Class .# Properties definition ex:hasCharacter a rdf:Property ;    rdfs:domain ex:Story ;    rdfs:range ex:Character .ex:hasSetting a rdf:Property ;    rdfs:domain ex:Story ;    rdfs:range ex:Setting .ex:hasTheme a rdf:Property ;    rdfs:domain ex:Story ;    rdfs:range ex:Theme .# Example of story <http://www.example.org/story/little_red_riding_hood> a ex:Story ;    ex:hasCharacter <http://www.example.org/character/little_red_riding_hood>,                    <http://www.example.org/character/grandmother>,                    <http://www.example.org/character/wolf> ;    ex:hasSetting <http://www.example.org/setting/forest> ;    ex:hasTheme <http://www.example.org/theme/good_vs_evil> .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Story rdfs:subClassOf rdfs:Class .ex:title rdf:type rdf:Property ;         rdfs:domain ex:Story ;         rdfs:range xsd:string .ex:content rdf:type rdf:Property ;          rdfs:domain ex:Story ;          rdfs:range xsd:string .ex:author rdf:type rdf:Property ;         rdfs:domain ex:Story ;         rdfs:range ex:Person .ex:Person rdfs:subClassOf rdfs:Class .ex:name rdf:type rdf:Property ;       rdfs:domain ex:Person ;       rdfs:range xsd:string .ex:birthDate rdf:type rdf:Property ;            rdfs:domain ex:Person ;            rdfs:range xsd:date .# Example of usageex:story1 rdf:type ex:Story ;         ex:title "The Two Towers" ;         ex:content "Some content here..." ;         ex:author ex:person1 .ex:person1 rdf:type ex:Person ;          ex:name "J. R. R. Tolkien" ;          ex:birthDate "1892-01-03"^^xsd:date .


