
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://money.org#> .money: 

@prefix ex: <http://example.org/ex#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .ex:Money rdfs:comment "The concept of a medium of exchange in the form of coins or banknotes" .ex:PaperMoney rdfs:subClassOf ex:Money ;              rdfs:comment "Banknotes, a type of money printed on paper" .ex:Coin rdfs:subClassOf ex:Money ;        rdfs:comment "A type of money in the form of a small round piece of metal" .ex:DigitalCurrency rdfs:subClassOf ex:Money ;                  rdfs:comment "A form of money available only in digtal form, not physical" .ex:Currency rdfs:comment "A system of money in general use in a particular country" .ex:hasCurrency rdfs:domain ex:Country ;               rdfs:range ex:Currency ;               rdfs:comment "Relates a country to its system of money" .ex:hasValue rdfs:domain ex:Money ;            rdfs:range xsd:decimal ;            rdfs:comment "Specifies the value of a particular form of money" .ex:CurrencyConversion rdfs:comment "A process that allows you to determine the equivalent amount of money in another currency" .ex:ConvertsTo rdfs:domain ex:CurrencyConversion ;              rdfs:range ex:Currency ;              rdfs:comment "The target currency for a conversion" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix money: <http://example.com/ontology/money#> .money:   rdf:type owl:Ontology ;  rdfs:comment "An ontology about the concept of money" .money:Money   rdf:type owl:Class ;  rdfs:comment "A medium that can be exchanged for goods and services and is used as a measure of their values on the market, including but not limited to coin and paper money as well as demand deposits." .money:hasCurrency  rdf:type owl:ObjectProperty ;  rdfs:domain money:Money ;  rdfs:range money:Currency ;  rdfs:comment "The currency in which a given amount of money is denominated." .money:hasValue  rdf:type owl:DatatypeProperty ;  rdfs:domain money:Money ;  rdfs:range xsd:float ;  rdfs:comment "The numerical value of a given amount of money." .money:Currency   rdf:type owl:Class ;  rdfs:comment "A system of money (monetary units) in common use, especially for people in a nation." .money:Currency rdfs:subClassOf   [ rdf:type owl:Restriction ;    owl:onProperty money:hasCurrency ;    owl:someValuesFrom [      rdf:type owl:Class ;      owl:unionOf (         money:Coins        money:PaperMoney        money:DemandDeposits      ) ] ].money:Coins   rdf:type owl:Class ;  rdfs:comment "Metallic forms of money." .money:PaperMoney  rdf:type owl:Class ;  rdfs:comment "Paper notes used as money." .money:DemandDeposits  rdf:type owl:Class ;  rdfs:comment "Deposit account balances from which withdrawals can be made on demand." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://example.org/money#> .money:Currency rdf:type rdfs:Class ;    rdfs:label "Currency" ;    rdfs:comment "A system of money in general use in a particular country." .money:Value rdf:type rdfs:Class ;    rdfs:label "Value" ;    rdfs:comment "The value of a particular currency, usually measured against another currency." .money:ExchangeRate rdf:type rdfs:Class ;    rdfs:label "Exchange Rate" ;    rdfs:comment "The value of one currency for the purpose of conversion to another." .money:hasCurrency rdf:type rdf:Property ;    rdfs:label "has currency" ;    rdfs:domain money:Currency ;    rdfs:range xsd:string .money:hasValue rdf:type rdf:Property ;    rdfs:label "has value" ;    rdfs:domain money:Value ;    rdfs:range xsd:decimal .money:hasExchangeRate rdf:type rdf:Property ;    rdfs:label "has exchange rate" ;    rdfs:domain money:ExchangeRate ;    rdfs:range xsd:decimal .money:Dollar rdf:type money:Currency ;    money:hasCurrency "Dollar" ;    money:hasValue "1"^^xsd:decimal ;    money:hasExchangeRate "1"^^xsd:decimal .money:Euro rdf:type money:Currency ;    money:hasCurrency "Euro" ;    money:hasValue "0.85"^^xsd:decimal ;    money:hasExchangeRate "0.85"^^xsd:decimal .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://example.com/money#> .# Class Definitionmoney:Money rdf:type owl:Class .# SubClass Of Moneymoney:DigitalMoney rdf:type owl:Class ;  rdfs:subClassOf money:Money .money:PhysicalMoney rdf:type owl:Class ;  rdfs:subClassOf money:Money .# Property Definitionmoney:hasCurrency rdf:type owl:DatatypeProperty ;  rdfs:range xsd:string ;  rdfs:domain money:Money .money:hasAmount rdf:type owl:DatatypeProperty ;  rdfs:range xsd:decimal ;  rdfs:domain money:Money .money:hasSource rdf:type owl:ObjectProperty ;  rdfs:range money:Money ;  rdfs:domain money:Money .money:hasDestination rdf:type owl:ObjectProperty ;  rdfs:range money:Money ;  rdfs:domain money:Money .# Individual Definitionmoney:Bitcoin rdf:type owl:NamedIndividual, money:DigitalMoney ;  money:hasCurrency "BTC"^^xsd:string ;  money:hasAmount "100.0"^^xsd:decimal .money:Dollar rdf:type owl:NamedIndividual, money:PhysicalMoney ;  money:hasCurrency "USD"^^xsd:string ;  money:hasAmount "50.0"^^xsd:decimal .


@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://www.example.org/money#> .money:Currency  rdf:type rdfs:Class ;  rdfs:label "Currency"@en ;  rdfs:comment "An entity which represents a system of money in common use, especially in a nation."@en.money:CurrencyCode  rdf:type rdfs:Class ;  rdfs:label "Currency Code"@en ;  rdfs:comment "An abbreviated form of currency used in financial transaction and representation"@en.money:hasCurrencyCode  rdf:type rdf:Property ;  rdfs:label "has currency code"@en ;  rdfs:comment "The property which represents the currency code of a given currency"@en ;  rdfs:domain money:Currency ;  rdfs:range money:CurrencyCode .money:hasValue  rdf:type rdf:Property ;  rdfs:label "has value"@en ;  rdfs:comment "The property which represents the nominal value of a currency"@en ;  rdfs:domain money:Currency ;  rdfs:range xsd:decimal .money:Banknote  rdf:type rdfs:Class ;  rdfs:label "Banknote"@en ;  rdfs:comment "A type of negotiable instrument known as a promissory note, made by a bank, payable to the bearer on demand."@en;  rdfs:subClassOf money:Currency .money:Coin  rdf:type rdfs:Class ;  rdfs:label "Coin"@en ;  rdfs:comment "A small, flat, and usually round piece of metal issued by a government as a form of money."@en;  rdfs:subClassOf money:Currency .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix money: <http://example.org/money#> .money:Money   rdfs:subClassOf rdfs:Resource ;  rdfs:label "Money"@en ;  rdfs:comment "Representation of all types of money including currency, digital money, etc."@en .  money:Currency   rdfs:subClassOf money:Money ;  rdfs:label "Currency"@en ;  rdfs:comment "Physical or tangible form of money usually issued by government."@en .    money:DigitalMoney   rdfs:subClassOf money:Money ;  rdfs:label "Digital Money"@en ;  rdfs:comment "Virtual or intangible form of money, used in online transactions."@en .money:hasValue   rdfs:range xsd:decimal ;  rdfs:domain money:Money ;  rdfs:label "has value"@en ;  rdfs:comment "Determines the value of the money."@en .money:hasCurrencyCode   rdfs:range xsd:string ;  rdfs:domain money:Currency ;  rdfs:label "has currency code"@en ;  rdfs:comment "Identifies the currency by its international code."@en .money:hasCurrencyName   rdfs:range xsd:string ;  rdfs:domain money:Currency ;  rdfs:label "has currency name"@en ;  rdfs:comment "Identifies the currency by its name."@en .money:isIssuedBy   rdfs:range xsd:string ;  rdfs:domain money:Currency ;  rdfs:label "is issued by"@en ;  rdfs:comment "Determines the issuing authority of the currency."@en . money:DigitalMoneyType   rdfs:range xsd:string ;  rdfs:domain money:DigitalMoney ;  rdfs:label "Digital Money Type"@en ;  rdfs:comment "Determines the type of digital money."@en . money:belongsToUser   rdfs:range xsd:string ;  rdfs:domain money:DigitalMoney ;  rdfs:label "belongs to user"@en ;  rdfs:comment "Determines the owner of the digital money."@en .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://example.org/money#> .money:Money rdfs:subClassOf rdfs:Resource .money:Money rdfs:label "Money" .money:Currency rdf:type rdfs:Class .money:Currency rdfs:label "Currency" .money:Currency rdfs:subClassOf money:Money .money:Coin rdf:type rdfs:Class .money:Coin rdfs:label "Coin" .money:Coin rdfs:subClassOf money:Money .money:Banknote rdf:type rdfs:Class .money:Banknote rdfs:label "Banknote" .money:Banknote rdfs:subClassOf money:Money .money:hasCurrency rdf:type rdf:Property .money:hasCurrency rdfs:domain money:Money .money:hasCurrency rdfs:range money:Currency .money:hasCurrency rdfs:label "has currency" .money:hasValue rdf:type rdf:Property .money:hasValue rdfs:domain money:Money .money:hasValue rdfs:range xsd:decimal .money:hasValue rdfs:label "has value" .money:hasMaterial rdf:type rdf:Property .money:hasMaterial rdfs:domain money:Money .money:hasMaterial rdfs:range rdfs:Literal .money:hasMaterial rdfs:label "has material" .money:US_Dollar rdf:type money:Currency .money:US_Dollar rdfs:label "US Dollar" .money:Euro rdf:type money:Currency .money:Euro rdfs:label "Euro" .




@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://somewhere/Money#> .money:Currency rdf:type rdfs:Class .money:Note rdf:type rdfs:Class .money:Coin rdf:type rdfs:Class .money:denomination rdf:type rdf:Property ;                  rdfs:domain money:Currency ;                  rdfs:range xsd:decimal .money:country rdf:type rdf:Property ;              rdfs:domain money:Currency ;              rdfs:range xsd:string .money:material rdf:type rdf:Property ;               rdfs:domain money:Coin ;               rdfs:range xsd:string .money:noteValue rdf:type rdf:Property ;                rdfs:domain money:Note ;                rdfs:range xsd:decimal .money:coinValue rdf:type rdf:Property ;                rdfs:domain money:Coin ;                rdfs:range xsd:decimal .

@prefix : <http://example.org/money#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:Money rdf:type rdfs:Class ;        rdfs:label "Money"@en ;       rdfs:comment "A medium of exchange for goods and services, which includes currency notes, coins, bank deposits."@en .:Currency rdf:type rdfs:Class ;           rdfs:label "Currency"@en ;           rdfs:subClassOf :Money ;          rdfs:comment "Physical or digital form of money that is used as a medium of exchange in a particular country or region."@en .:DigitalCurrency rdf:type rdfs:Class ;                  rdfs:label "Digital Currency"@en ;                 rdfs:subClassOf :Currency ;                 rdfs:comment "A type of currency that is non-physical and represented only in digital format."@en .:Coin rdf:type rdfs:Class ;       rdfs:label "Coin"@en ;       rdfs:subClassOf :Currency ;      rdfs:comment "Physical form of money, typically made of metal and minted by an sovereign authority."@en .:BankDeposit rdf:type rdfs:Class ;              rdfs:label "Bank Deposit"@en ;              rdfs:subClassOf :Money ;             rdfs:comment "A type of money that is placed into a banking institution for safekeeping."@en .:hasValue rdf:type rdf:Property ;          rdfs:label "has value"@en ;          rdfs:comment "Property that indicates the value of the corresponding form of money."@en ;          rdfs:range xsd:decimal . :hasCurrencyName rdf:type rdf:Property ;                rdfs:label "has currency name"@en ;                rdfs:comment "Property that indicates the name or type of the currency."@en ;                rdfs:range xsd:string . :hasCountry rdf:type rdf:Property ;            rdfs:label "has country"@en ;            rdfs:comment "Property that indicates the country where the currency is used."@en ;            rdfs:range xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://www.example.org/money#> .money:     a owl:Ontology ;    rdfs:comment "An ontology about money."@en .money:Money    a owl:Class ;    rdfs:comment "The super-class of all types of money."@en ;    rdfs:label "Money"@en .money:currency    a owl:DatatypeProperty ;    rdfs:domain money:Money ;    rdfs:range xsd:string ;    rdfs:comment "The currency of the money e.g. USD, EUR, etc."@en ;    rdfs:label "currency"@en .money:amount    a owl:DatatypeProperty ;    rdfs:domain money:Money ;    rdfs:range xsd:decimal ;    rdfs:comment "The numerical amount of the money."@en ;    rdfs:label "amount"@en .money:PaperMoney    a owl:Class ;    rdfs:subClassOf money:Money ;    rdfs:comment "Money in the form of paper notes."@en ;    rdfs:label "Paper Money"@en .money:Coin    a owl:Class ;    rdfs:subClassOf money:Money ;    rdfs:comment "Money in the form of coins."@en ;    rdfs:label "Coin"@en .money:DigitalMoney    a owl:Class ;    rdfs:subClassOf money:Money ;    rdfs:comment "Money in a digital or electronic form."@en ;    rdfs:label "Digital Money"@en .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://example.org/money#> .money:Money rdf:type owl:Class ;  rdfs:comment "Class representing money"^^xsd:string ;  rdfs:label "Money"^^xsd:string .money:Currency rdf:type owl:Class ;  rdfs:comment "Class representing different currencies"^^xsd:string ;  rdfs:label "Currency"^^xsd:string .money:hasCurrency rdf:type owl:ObjectProperty ;  rdfs:domain money:Money ;  rdfs:range money:Currency ;  rdfs:comment "Property connecting money and its currency"^^xsd:string ;  rdfs:label "has currency"^^xsd:string .money:Amount rdf:type owl:DatatypeProperty ;  rdfs:domain money:Money ;  rdfs:range xsd:decimal ;  rdfs:comment "Property representing the amount of money"^^xsd:string ;  rdfs:label "amount"^^xsd:string .money:Code rdf:type owl:DatatypeProperty ;  rdfs:domain money:Currency ;  rdfs:range xsd:string ;  rdfs:comment "Property representing the code of currency"^^xsd:string ;  rdfs:label "code"^^xsd:string .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://example.org/money#> .money:Money rdf:type rdfs:Class ;    rdfs:label "Money" ;    rdfs:comment "The medium that a system of exchange uses" .money:Currency rdf:type rdfs:Class ;    rdfs:label "Currency" ;    rdfs:comment "A system of money in general use in a particular country" ;    rdfs:subClassOf money:Money .money:Coin rdf:type rdfs:Class ;    rdfs:label "Coin" ;    rdfs:comment "A flat, typically round piece of metal with an official stamp, used as money" ;    rdfs:subClassOf money:Money .money:Banknote rdf:type rdfs:Class ;    rdfs:label "Banknote" ;    rdfs:comment "A printed piece of paper that is worth a specific amount of money" ;    rdfs:subClassOf money:Money .money:value rdf:type rdf:Property ;    rdfs:label "value" ;    rdfs:comment "The numerical value of the money" ;    rdfs:domain money:Money ;    rdfs:range xsd:decimal .money:hasCurrency rdf:type rdf:Property ;    rdfs:label "hasCurrency" ;    rdfs:comment "The currency of the money" ;    rdfs:domain money:Money ;    rdfs:range money:Currency .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://www.example.org/money#> .ex:Money rdf:type rdfs:Class .ex:hasCurrency rdf:type rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range ex:Currency .ex:hasAmount rdf:type rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range xsd:decimal .ex:Currency rdf:type rdfs:Class .# Instancesex:USD rdf:type ex:Currency ;    rdfs:label "US Dollar" .ex:myWallet rdf:type ex:Money ;    ex:hasCurrency ex:USD ;    ex:hasAmount "100.00"^^xsd:decimal .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://example.org/money#> .:Currency a rdfs:Class ;  rdfs:label "Currency"@en .:Banknote rdf:type rdfs:Class ;  rdfs:subClassOf :Currency ;  rdfs:label "Banknote"@en .:Coin rdf:type rdfs:Class ;  rdfs:subClassOf :Currency ;  rdfs:label "Coin"@en .:hasCurrency rdf:type rdf:Property ;   rdfs:domain :Person ;   rdfs:range :Currency ;   rdfs:label "has currency"@en .:hasValue rdf:type rdf:Property ;  rdfs:domain :Currency ;  rdfs:range xsd:double ;  rdfs:label "has value"@en .:Person rdf:type owl:Class ;  rdfs:label "Person"@en .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://www.example.org/money/> .ex:Currency a rdfs:Class ;  rdfs:label "Currency" ;  rdfs:comment "The class of all currencies".ex:moneyIssuedBy a rdfs:Property ;  rdfs:domain ex:Currency ;  rdfs:range ex:Country ;  rdfs:label "money issued by" ;  rdfs:comment "A property that links a currency to the country that issued it".ex:hasValue a rdfs:Property ;  rdfs:domain ex:Currency ;  rdfs:label "has value" ;  rdfs:comment "A property that links a currency to its numerical value".ex:Country a rdfs:Class ;  rdfs:label "Country" ;  rdfs:comment "The class of all countries".ex:numericalValue a rdfs:Datatype ;  rdfs:label "Numerical value" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix ex: <http://example.org/money#> .ex:Money a rdfs:Class .ex:hasCurrency a rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range xsd:string .ex:hasAmount a rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range xsd:decimal .ex:Transaction a rdfs:Class ;    rdfs:subClassOf ex:Money .ex:Transfer a rdfs:Class ;    rdfs:subClassOf ex:Transaction .ex:Payment a rdfs:Class ;    rdfs:subClassOf ex:Transaction .ex:transfers a rdf:Property ;    rdfs:domain ex:Transfer ;    rdfs:range ex:Money .ex:pays a rdf:Property ;    rdfs:domain ex:Payment ;    rdfs:range ex:Money .

@prefix ex: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .ex:Money rdf:type rdfs:Class .ex:hasCurrency rdf:type rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range rdfs:Literal .ex:hasAmount rdf:type rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range xsd:decimal .

@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix myvoc: <http://www.example.org/myvocab#> .myvoc:Money a owl:Class ;    rdfs:label "Money"@en ;    rdfs:comment "A medium that can be exchanged for goods and services"@en ;    .myvoc:hasCurrency a owl:ObjectProperty ;    rdfs:label "has currency"@en ;    rdfs:domain myvoc:Money ;    rdfs:range myvoc:Currency ;    .myvoc:Currency a owl:Class ;    rdfs:label "Currency"@en ;    rdfs:comment "A system of money in common use"@en ;    .

@prefix ex: <http://example.com/> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .ex:Money a owl:Class ;    rdfs:label "Money" ;    rdfs:subClassOf rdfs:Literal ;    rdfs:comment "The object that represents a value or currency."@en .ex:hasValue a owl:DatatypeProperty ;    rdfs:label "has value" ;    rdfs:range xsd:decimal ;    rdfs:domain ex:Money ;    rdfs:comment "The numerical value of a money."@en .ex:hasCurrency a owl:DatatypeProperty ;    rdfs:label "has currency" ;    rdfs:range xsd:string ;    rdfs:domain ex:Money ;    rdfs:comment "The currency of a money."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/money#> .# Define classesex:Currency a rdfs:Class ;    rdfs:label "Currency" .    ex:Transaction a rdfs:Class ;    rdfs:label "Transaction" .# Define propertiesex:hasCurrency a rdf:Property ;    rdfs:domain ex:Transaction ;    rdfs:range ex:Currency ;    rdfs:label "has currency" .ex:hasAmount a rdf:Property ;    rdfs:domain ex:Transaction ;    rdfs:range xsd:decimal ;    rdfs:label "has amount" .# Define instances of currencyex:USD a ex:Currency ;    rdfs:label "United States Dollar" .ex:EUR a ex:Currency ;    rdfs:label "Euro" .# Define a transaction instanceex:Transaction1 a ex:Transaction ;    ex:hasCurrency ex:USD ;    ex:hasAmount "1000.5"^^xsd:decimal .    

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.org/stuff/1.0/> .ex:Money rdf:type owl:Class ;    rdfs:label "Money"@en ;    rdfs:comment "A medium of exchange for goods and services, universally accepted in transactions"@en ;    rdfs:subClassOf ex:EconomicConcept .    ex:hasCurrency rdf:type owl:ObjectProperty ;    rdfs:domain ex:Money ;    rdfs:range ex:Currency ;    rdfs:label "has currency"@en ;    rdfs:comment "The specific type of money used in a region or country"@en .ex:Currency rdf:type owl:Class ;    rdfs:label "Currency"@en ;    rdfs:comment "A system of money specific to a particular region or country"@en .

@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix my: <http://www.example.org/ontology/money#> .my:Currency a rdfs:Class;    rdfs:comment "The currency in which the money value is represented.".my:Value a rdfs:Class;    rdfs:comment "The numerical value of the money instance.".my:Money a rdfs:Class ;    rdfs:label "Money" ;    rdfs:comment "Class representing Money. Includes value and currency as properties." ;    rdfs:subClassOf owl:Thing;    rdfs:subClassOf [ a owl:Restriction ; owl:onProperty my:hasValue ; owl:allValuesFrom my:Value ] ;    rdfs:subClassOf [ a owl:RestrictedProperty ; owl:onProperty my:hasCurrency ; owl:allValuesFrom my:Currency ] .my:hasCurrency a owl:ObjectProperty ;    rdfs:label "hasCurrency" ;    rdfs:comment "Relates a Money instance to its currency." ;    rdfs:domain my:Money ;    rdfs:range my:Currency .my:hasValue a owl:DatatypeProperty ;    rdfs:label "hasValue" ;    rdfs:comment "Relates a Money instance to its value." ;    rdfs:domain my:Money ;    rdfs:range xsd:decimal .my:myCurrency a my:Currency ;    rdfs:comment "An instance of currency.".my:myValue a my:Value ;    rdfs:comment "An instance of value." xsd:decimal "100.00" .my:myMoney a my:Money;    my:hasCurrency my:myCurrency ;    my:hasValue my:myValue .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .ex:Money rdf:type owl:Class .ex:hasDenomination rdf:type owl:DatatypeProperty ;                  rdfs:domain ex:Money ;                  rdfs:range xsd:decimal .ex:isCurrencyOf rdf:type owl:ObjectProperty ;                rdfs:domain ex:Money ;                rdfs:range ex:Country .ex:Country rdf:type owl:Class .

@prefix : <http://example.org/> .:Person a rdfs:Class .:hasMoney a rdf:Property ;    rdfs:domain :Person ;    rdfs:range xsd:decimal .:Money a rdfs:Class .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://example.org/money#> .:Money rdf:type rdfs:Class ;    rdfs:comment "An entity that represents a medium of exchange." .:hasCurrency rdf:type rdf:Property ;    rdfs:domain :Money ;    rdfs:range :Currency ;    rdfs:comment "The currency of the money." .:amount rdf:type rdf:Property ;    rdfs:domain :Money ;    rdfs:range rdf:decimal ;    rdfs:comment "The numeric amount of the money." .:Currency rdf:type rdfs:Class ;    rdfs:comment "An entity that represents a type of currency." .    :USD rdf:type :Currency ;    rdfs:comment "United States dollar." .:GBP rdf:type :Currency ;    rdfs:comment "British pound sterling." .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix money: <http://example.org/money#> .money:Money rdf:type rdfs:Class ;    rdfs:comment "Class to represent Money" ;    rdfs:label "Money" .money:Currency rdf:type rdfs:Class ;    rdfs:comment "Class to represent different types of Currencies" ;    rdfs:label "Currency" .money:Amount rdf:type rdf:Property ;    rdf:domain money:Money ;    rdf:range xsd:decimal ;    rdfs:comment "Decimal value of the money." ;    rdfs:label "Amount" .    money:hasCurrency rdf:type rdf:Property ;    rdf:domain money:Money ;    rdf:range money:Currency ;    rdfs:comment "The currency of the money." ;    rdfs:label "hasCurrency" .money:CurrencyName rdf:type rdf:Property ;    rdf:domain money:Currency ;    rdf:range xsd:string ;    rdfs:comment "The name of the currency." ;    rdfs:label "CurrencyName" .
@prefix : <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:Money rdf:type rdfs:Class ;    rdfs:label "Money" ;    rdfs:comment "A medium of exchange in the form of coins and banknotes" .:hasAmount rdf:type rdf:Property ;    rdfs:label "has amount" ;    rdfs:comment "The numerical amount of money" ;    rdfs:domain :Money ;    rdfs:range xsd:decimal .:hasCurrency rdf:type rdf:Property ;    rdfs:label "has currency" ;    rdfs:comment "The type of currency of money" ;    rdfs:domain :Money ;    rdfs:range :Currency .:Currency rdf:type rdfs:Class ;    rdfs:label "Currency" ;    rdfs:comment "A system of money in common use, especially for people in a nation" .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com#> .ex:Money a rdfs:Class.ex:hasAmount a rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range xsd:decimal .ex:hasCurrency a rdf:Property ;    rdfs:domain ex:Money ;    rdfs:range rdfs:Literal .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/#> .ex:Money a rdfs:Class ;     rdfs:label "Money" ;    rdfs:comment "Entity that acts as a medium of exchange, a measure of value, a unit of account, and a store of value." ;    rdfs:subClassOf ex:FinancialEntity .ex:Currency a rdfs:Class ;    rdfs:label "Currency" ;    rdfs:comment "A system of money in general use in a particular country." ;    rdfs:subClassOf ex:Money .ex:DigitalCurrency a rdfs:Class ;    rdfs:label "Digital currency" ;    rdfs:comment "A type of currency available in digital form, represents money." ;    rdfs:subClassOf ex:Money .ex:hasValue a rdfs:Property ;    rdfs:label "has value" ;    rdfs:comment "Denotes the numerical value associated with a unit of money." ;    rdfs:domain ex:Money ;    rdfs:range rdfs:Literal .

@prefix ex: <http://example.org#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .ex:Money rdf:type owl:Class .ex:Currency rdf:type owl:Class ;    rdfs:subClassOf ex:Money .ex:TransactionMethod rdf:type owl:Class ;    rdfs:subClassOf ex:Money .ex:Source rdf:type owl:Class ;    rdfs:subClassOf ex:Money .ex:Bank rdf:type owl:Class ;    rdfs:subClassOf ex:Source .ex:Individual rdf:type owl:Class ;    rdfs:subClassOf ex:Source .ex:hasCurrency rdf:type owl:ObjectProperty ;    rdfs:domain ex:Money ;    rdfs:range ex:Currency .ex:hasTransactionMethod rdf:type owl:ObjectProperty ;    rdfs:domain ex:Money ;    rdfs:range ex:TransactionMethod .ex:hasSource rdf:type owl:ObjectProperty ;    rdfs:domain ex:Money ;    rdfs:range ex:Source .

