@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://example.org/> .# Defining the class 'Year' :Year rdf:type owl:Class ;      rdfs:label "Year"@en ;            rdfs:comment "Representation of a Year"@en ;            rdfs:subClassOf xsd:gYear .# Defining properties of 'Year'      :hasYear rdf:type owl:DatatypeProperty ;         rdfs:label "has year"@en ;         rdfs:comment "The specific year of an event or object"@en ;         rdfs:range xsd:gYear ;         rdfs:domain owl:Thing .                        :inDecade rdf:type owl:ObjectProperty ;          rdfs:label "in decade"@en ;          rdfs:comment "Decade in which the year falls"@en ;          rdfs:range :Decade ;          rdfs:domain :Year .:inCentury rdf:type owl:ObjectProperty ;           rdfs:label "in century"@en ;           rdfs:comment "Century in which the year falls"@en ;           rdfs:range :Century ;           rdfs:domain :Year .# Additional classes for properties of 'Year' :Decade rdf:type owl:Class ;        rdfs:label "Decade"@en ;        rdfs:comment "A period of ten years"@en .        :Century rdf:type owl:Class ;         rdfs:label "Century"@en ;         rdfs:comment "A period of one hundred years"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com/> .ex:Year rdf:type rdfs:Class ;   rdfs:label "Year" ;   rdfs:comment "The class of all years" .ex:year rdf:type rdf:Property ;   rdfs:label "year" ;   rdfs:comment "The property that associates an entity with a year" ;   rdfs:domain ex:Entity ;   rdfs:range ex:Year .ex:Entity rdf:type rdfs:Class ;   rdfs:label "Entity" ;   rdfs:comment "The class of all entities" .ex:Entity ex:year "2020"^^xsd:integer .ex:beginningOfYear rdf:type rdf:Property ;   rdfs:label "beginning of year" ;   rdfs:comment "The property that signifies the beginning of a year" ;   rdfs:domain ex:Year ;   rdfs:range xsd:date .ex:endOfYear rdf:type rdf:Property ;   rdfs:label "end of year" ;   rdfs:comment "The property that signifies the end of a year" ;   rdfs:domain ex:Year ;   rdfs:range xsd:date .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com#> .ex:Year a rdfs:Class ;    rdfs:label "Year"^^xsd:string ;    rdfs:comment "A specific year in the Gregorian calendar."@en ;    rdfs:subClassOf xsd:integer .ex:hasYear a rdf:Property ;    rdfs:label "has year"@en ;    rdfs:comment "The property indicative of a specific year"@en ;    rdfs:domain ex:Date ;    rdfs:range ex:Year .ex:isYearOf a rdf:Property ;    rdfs:label "is year of"@en ;    rdfs:comment "The property indicative of a date that falls within a specific year"@en ;    rdfs:domain ex:Year ;    rdfs:range ex:Date .ex:Date a rdfs:Class ;    rdfs:label "Date"^^xsd:string ;    rdfs:comment "A specific date in the Gregorian calendar."@en .ex:isLeapYear a rdf:Property ;    rdfs:label "is leap year"@en ;    rdfs:comment "A property that determines whether a year is a leap year"@en ;    rdfs:domain ex:Year ;    rdfs:range xsd:boolean .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix year: <http://example.org/year#> .year:Year      a rdfs:Class ;      rdfs:comment "A representation of a Calendar Year."@en .year:hasMonth      a rdf:Property ;      rdfs:domain year:Year ;      rdfs:range year:Month ;      rdfs:comment "Relates a year to its constituting months."@en .year:Month      a rdfs:Class ;      rdfs:comment "A representation of a Calendar Month."@en .year:hasDay      a rdf:Property ;      rdfs:domain year:Month ;      rdfs:range year:Day ;      rdfs:comment "Relates a month to its constituting days."@en .year:Day      a rdfs:Class ;      rdfs:comment "A representation of a Calendar Day."@en .year:isLeapYear      a rdf:Property ;      rdfs:domain year:Year ;      rdfs:range xsd:boolean ;      rdfs:comment "Indicates if a year is a leap year or not."@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix yr: <http://example.com/year#> .# Year classyr:Year rdf:type rdfs:Class ;  rdfs:label "Year"@en ;  rdfs:comment "The class of all years"@en .# Year propertiesyr:hasYearNumber rdf:type rdf:Property ;  rdfs:label "Year number"@en ;  rdfs:domain yr:Year ;  rdfs:range xsd:integer ;  rdfs:comment "The property that holds the number of a year"@en .yr:hasMonth rdf:type rdf:Property ;  rdfs:label "Month"@en ;  rdfs:domain yr:Year ;  rdfs:range rdfs:Literal ;  rdfs:comment "The property that holds month of a year"@en .# Exampleyr:Year2005 rdf:type yr:Year ;  yr:hasYearNumber 2005;  yr:hasMonth "December"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix yearOnto: <http://example.org/ontology/year#> .yearOnto:     rdf:type rdfs:Class ;    rdfs:label "Year"@en ;    rdfs:comment "A class representing a period of 365 or 366 days, in the Gregorian calendar, divided into 12 calendar months, and reckoned from January 1 to December 31"@en .yearOnto:yearID    rdf:type rdf:Property ;    rdfs:label "Year ID"@en ;    rdfs:comment "A unique identifier for a year"@en ;    rdfs:domain yearOnto: ;    rdfs:range xsd:string .yearOnto:number     rdf:type rdf:Property ;    rdfs:label "Year Number"@en ;    rdfs:comment "The numbering of the year"@en ;    rdfs:domain yearOnto: ;    rdfs:range xsd:integer .yearOnto:isLeapYear    rdf:type rdf:Property ;    rdfs:label "Is Leap Year?"@en ;    rdfs:comment "Indicates whether the year is a leap year"@en ;    rdfs:domain yearOnto: ;    rdfs:range xsd:boolean .

@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Year a rdfs:Class ;  rdfs:label "Year" ;  rdfs:comment "A year is a period of 365 or 366 days, in the Gregorian calendar, divided into 12 months, and is a unit of time used by humans in which to measure durations and ages." .ex:hasMonth a rdf:Property ;  rdfs:label "hasMonth" ;  rdfs:comment "A property that links a year to its 12 constituent months" ;  rdfs:domain ex:Year ;  rdfs:range ex:Month .ex:Month a rdfs:Class ;  rdfs:label "Month" ;  rdfs:comment "A month is a unit of time, used with calendars, which is approximately as long as a natural period related to the motion of the Moon.".ex:hasDay rdfs:Property ;  rdfs:label "hasDay" ;  rdfs:comment "A property that links a month to its constituent days. Depending on the month, this can be between 28 and 31." ;  rdfs:domain ex:Month ;  rdfs:range ex:Day .ex:Day a rdfs:Class ;  rdfs:label "Day" ;  rdfs:comment "A day is approximately the period of time during which the Earth completes one rotation with respect to the Sun.".ex:Year2022 a ex:Year ;  rdfs:label "2022" ;  ex:hasMonth ex:January2022 .ex:January2022 a ex:Month ;  rdfs:label "January" ;  ex:hasDay ex:January12022 .ex:January12022 a ex:Day ;  rdfs:label "January 1, 2022" .  ex:isLeapYear a rdf:Property ;  rdfs:label "isLeapYear" ;  rdfs:comment "A property that indicates whether a given year is a leap year, which has an additional day - February 29" ;  rdfs:domain ex:Year ;  rdfs:range xsd:boolean .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/#> .ex:year rdf:type owl:Class ;ex:hasYear rdf:type owl:ObjectProperty ;    rdfs:domain ex:Calendar ;    rdfs:range ex:Year ;    rdfs:comment "Link a calendar to a year" ;    rdfs:label "has year" .ex:Calendar rdf:type owl:Class ;    rdfs:comment "A representation of time, including a year" ;    rdfs:label "Calendar" .ex:yearInCentury rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Year ;    rdfs:range xsd:integer ;    rdfs:comment "The year within the century, e.g., 20 for the year 2020" ;    rdfs:label "year in century" .ex:Year2020 a ex:Year ;    ex:yearInCentury 20 .

