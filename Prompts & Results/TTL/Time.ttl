@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix time: <http://www.w3.org/2006/time#> .time:Time a owl:Class ;    rdfs:label "Time" ;    rdfs:subClassOf owl:Thing .        time:hasDateTime a owl:ObjectProperty ;    rdfs:label "hasDateTime" ;    rdfs:domain time:Time ;    rdfs:range xsd:dateTime .    time:hasTimezone a owl:ObjectProperty ;    rdfs:label "hasTimezone" ;    rdfs:domain time:Time ;    rdfs:range xsd:string .    time:hasHour a owl:DatatypeProperty ;    rdfs:label "hasHour" ;    rdfs:domain time:Time ;    rdfs:range xsd:int .    time:hasMinute a owl:DatatypeProperty ;    rdfs:label "hasMinute" ;    rdfs:domain time:Time ;    rdfs:range xsd:int .    time:hasSecond a owl:DatatypeProperty ;    rdfs:label "hasSecond" ;    rdfs:domain time:Time ;    rdfs:range xsd:int .time:Instant a owl:Class ;    rdfs:label "Instant" ;    rdfs:subClassOf time:Time ;    rdfs:comment "A point in time" .time:Interval a owl:Class ;    rdfs:label "Interval" ;    rdfs:subClassOf time:Time ;    rdfs:comment "A span of time between two instants" . time:hasBeginning a owl:DatatypeProperty ;    rdfs:label "hasBeginning" ;    rdfs:domain time:Interval ;    rdfs:range time:Instant .    time:hasEnd a owl:DatatypeProperty ;    rdfs:label "hasEnd" ;    rdfs:domain time:Interval ;    rdfs:range time:Instant .


