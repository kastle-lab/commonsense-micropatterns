
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/ontology/time#> .time: a owl:Ontology ;  rdfs:label "Time Ontology" ;  rdfs:comment "An ontology dedicated to Time."time:Time a rdfs:Class ;  rdfs:label "Time" ;  rdfs:comment "The point or period at which things occur." ;  rdfs:subClassOf rdfs:Resource .  time:Date a rdfs:Class ;  rdfs:label "Date" ;  rdfs:comment "Identifies a specific day in a month and year." ;  rdfs:subClassOf time:Time .  time:Year a rdfs:Class ;  rdfs:label "Year" ;  rdfs:comment "A year as per the standard Gregorian calendar." ;  rdfs:subClassOf time:Time .time:Month a rdfs:Class ;  rdfs:label "Month" ;  rdfs:comment "A month in a year as per the standard Gregorian calendar." ;  rdfs:subClassOf time:Time .time:Day a rdfs:Class ;  rdfs:label "Day" ;  rdfs:comment "A day in a month and a year." ;  rdfs:subClassOf time:Time .  time:Hour a rdfs:Class ;  rdfs:label "Hour" ;  rdfs:comment "An hour in a day." ;  rdfs:subClassOf time:Time .time:Minute a rdfs:Class ;  rdfs:label "Minute" ;  rdfs:comment "A minute in an hour." ;  rdfs:subClassOf time:Time .time:Second a rdfs:Class ;  rdfs:label "Second" ;  rdfs:comment "A Second in a minute." ;  rdfs:subClassOf time:Time .time:hasDate a rdf:Property ;  rdfs:label "has date" ;  rdfs:comment "Relates a Time to a specific date." ;  rdfs:range time:Date ;  rdfs:domain time:Time .time:hasYear a rdf:Property ;  rdfs:label "has year" ;  rdfs:comment "Relates a Time or Date to a specific year." ;  rdfs:range time:Year ;  rdfs:domain [ owl:unionOf ( time:Time time:Date ) ] .time:hasMonth a rdf:Property ;  rdfs:label "has month" ;  rdfs:comment "Relates a Time or Date to a specific month." ;  rdfs:range time:Month ;  rdfs:domain [ owl:unionOf ( time:Time time:Date ) ] .time:hasDay a rdf:Property ;  rdfs:label "has 