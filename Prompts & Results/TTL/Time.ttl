@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.w3.org/2006/time#> .<http://example.com/ontology/Time> rdf:type owl:Ontology ;owl:imports <http://www.w3.org/2006/time#> ;                    time:hasTemporalDurationDescription :Day, :Hour, :Minute, :Second ;                   time:unitType "second"^^xsd:string .:Day a time:TemporalUnit ;    time:unitType "day"^^xsd:string ;    time:numericDuration "24"^^xsd:float.:Hour a time:TemporalUnit ;    time:unitType "hour"^^xsd:string ;    time:numericDuration "1"^^xsd:float.:Minute a time:TemporalUnit ;    time:unitType "minute"^^xsd:string ;    time:numericDuration "1"^^xsd:float.    :Second a time:TemporalUnit ;    time:unitType "second"^^xsd:string ;    time:numericDuration "1"^^xsd:float.                       


@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix time: <http://www.ontology.com/time#> .time:Instant  a rdfs:Class ;  rdfs:comment "A specific point in time." .time:Interval  a rdfs:Class ;  rdfs:comment "A duration of time, has a start and end." .time:DateTime  a rdfs:Datatype ;  rdfs:subClassOf xsd:dateTime ;  rdfs:comment "The date-time representation as per the XML Schema Datatype definition." .time:dateTimeStamp  a owl:DatatypeProperty ;  rdfs:domain time:Instant ;  rdfs:range time:DateTime ;  rdfs:comment "The representation of a time instant as a date-time stamp." .time:hasBeginning  a owl:ObjectProperty ;  rdfs:domain time:Interval ;  rdfs:range time:Instant ;  rdfs:comment "The start of a time interval." .time:hasEnd  a owl:ObjectProperty ;  rdfs:domain time:Interval ;  rdfs:range time:Instant ;  rdfs:comment "The end of a time interval." .time:duration  a owl:DatatypeProperty ;  rdfs:domain time:Interval ;  rdfs:range xsd:duration ;  rdfs:comment "The duration of a time interval." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xml: <http://www.w3.org/XML/1998/namespace> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.w3.org/2006/time#> .<http://www.example.com/ontologies/time#>    a owl:Ontology ;    rdfs:comment "An ontology of time" ;    rdfs:label "Time Ontology" .time:TemporalEntity    a owl:Class ;    rdfs:comment "A class representing temporal entities" ;    rdfs:label "Temporal Entity" .time:Instant    a owl:Class ;    rdfs:subClassOf time:TemporalEntity ;    rdfs:comment "A class representing temporal instants" ;    rdfs:label "Instant" .time:Interval    a owl:Class ;    rdfs:subClassOf time:TemporalEntity ;    rdfs:comment "A class representing temporal intervals" ;    rdfs:label "Interval" .time:hasBeginning    a owl:ObjectProperty ;    rdfs:domain time:Interval ;    rdfs:range time:Instant ;    rdfs:comment "A property linking a temporal interval to its beginning instant" ;    rdfs:label "has beginning" .time:hasEnd    a owl:ObjectProperty ;    rdfs:domain time:Interval ;    rdfs:range time:Instant ;    rdfs:comment "A property linking a temporal interval to its end instant" ;    rdfs:label "has end" .time:inXSDDateTime    a owl:DatatypeProperty ;    rdfs:domain time:Instant ;    rdfs:range xsd:dateTime ;    rdfs:comment "A property representing the date and time of an instant in xsd:dateTime format" ;    rdfs:label "in xsd:dateTime" .




@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/time#> .time:TimePeriod rdf:type owl:Class ;                rdfs:comment "A period of time with a known duration" ;                rdfs:label "Time Period"@en .time:hasDuration rdf:type owl:DatatypeProperty ;                 rdfs:range xsd:duration ;                 rdfs:domain time:TimePeriod ;                 rdfs:comment "The duration of a time period" ;                 rdfs:label "has duration"@en .time:startedAt rdf:type owl:DatatypeProperty ;               rdfs:range xsd:dateTime ;               rdfs:domain time:TimePeriod ;               rdfs:comment "The start point of a time period" ;               rdfs:label "started at"@en .time:endedAt rdf:type owl:DatatypeProperty ;             rdfs:range xsd:dateTime ;             rdfs:domain time:TimePeriod ;             rdfs:comment "The end point of a time period" ;             rdfs:label "ended at"@en .time:Instant rdf:type owl:Class ;             rdfs:comment "A specific point in time" ;             rdfs:label "Instant"@en .time:isTimePointOf rdf:type owl:ObjectProperty ;                   rdfs:range time:TimePeriod ;                   rdfs:domain time:Instant ;                   rdfs:comment "Associates a timepoint to a time period" ;                   rdfs:label "is time point of"@en .time:inDateTime rdf:type owl:DatatypeProperty ;                rdfs:range xsd:dateTime ;                rdfs:domain time:Instant ;                rdfs:comment "The date and time of an instant" ;                rdfs:label "in date and time"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/time#> .time:     rdf:type owl:Ontology ;    rdfs:comment "Ontology dedicated to the concept of Time" ;    rdfs:label "Time Ontology" .time:Time     rdf:type owl:Class ;    rdfs:comment "The general concept of time." ;    rdfs:subClassOf owl:Thing .time:DateTime    rdf:type owl:Class ;    rdfs:comment "A specific point in time." ;    rdfs:subClassOf time:Time .time:Duration     rdf:type owl:Class ;    rdfs:comment "A length or span of time." ;    rdfs:subClassOf time:Time .time:Instant     rdf:type owl:Class ;    rdfs:comment "An exact moment in time." ;    rdfs:subClassOf time:DateTime .time:Interval     rdf:type owl:Class ;    rdfs:comment "A span of time defined by two instants." ;    rdfs:subClassOf time:Duration .time:inDateTime     rdf:type owl:DatatypeProperty ;    rdfs:domain time:Instant ;    rdfs:range xsd:dateTime .time:hasDuration     rdf:type owl:ObjectProperty ;    rdfs:domain owl:Thing ;    rdfs:range time:Duration .time:begin     rdf:type owl:DatatypeProperty ;    rdfs:domain time:Interval ;    rdfs:range time:Instant .time:end     rdf:type owl:DatatypeProperty ;    rdfs:domain time:Interval ;    rdfs:range time:Instant .time:days     rdf:type owl:DatatypeProperty ;    rdfs:domain time:Duration ;    rdfs:range xsd:int .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.w3.org/2006/time#> .time:TemporalUnit rdfs:comment "A unit of temporal extent.".time:Instant rdfs:comment "A zero-dimensional temporal region, or a position on a temporal coordinate.".time:Interval rdfs:comment "A one-dimensional temporal region.".time:day a time:TemporalUnit ;    rdfs:comment "A temporal unit representing a day." .time:month a time:TemporalUnit ;    rdfs:comment "A temporal unit representing a month." .time:year a time:TemporalUnit ;    rdfs:comment "A temporal unit representing a year." .time:hour a time:TemporalUnit ;    rdfs:comment "A temporal unit representing an hour." .time:minute a time:TemporalUnit ;    rdfs:comment "A temporal unit representing a minute." .time:second a time:TemporalUnit ;    rdfs:comment "A temporal unit representing a second." .time:microsecond a time:TemporalUnit ;    rdfs:comment "A temporal unit representing a microsecond." .time:hasTemporalUnit a rdf:Property ;    rdfs:domain time:Instant ;    rdfs:range time:TemporalUnit ;    rdfs:comment "Indicates the unit of measure associated with a time instant." .






@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/time#> .time: a rdfs:Class ;    rdfs:label "Time"@en ;    rdfs:comment "Abstract entity that determines the occurrence of phenomena"@en .time:DateTime a rdfs:Class ;    rdfs:subClassOf time: ;    rdfs:label "DateTime"@en ;    rdfs:comment "Specific point in time"@en .time:Instant a rdfs:Class ;    rdfs:subClassOf time:DateTime ;    rdfs:label "Instant"@en ;    rdfs:comment "A specific moment in time"@en .time:Interval a rdfs:Class ;    rdfs:subClassOf time:DateTime ;    rdfs:label "Interval"@en ;    rdfs:comment "A period of time"@en .time:inXSDDateTime a rdf:Property;    rdfs:domain time:DateTime ;    rdfs:range xsd:dateTime ;    rdfs:label "inXSDDateTime"@en ;    rdfs:comment "Expressing time in xsd:dateTime format"@en .time:hasBeginning a rdf:Property;    rdfs:domain time:Interval ;    rdfs:range time:Instant ;    rdfs:label "hasBeginning"@en ;    rdfs:comment "The starting moment of a time interval"@en .time:hasEnd a rdf:Property;    rdfs:domain time:Interval ;    rdfs:range time:Instant ;    rdfs:label "hasEnd"@en ;    rdfs:comment "The ending moment of a time interval"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://example.org/time#> .time:Time    rdf:type owl:Class ;    rdfs:comment "The concept of time as a continuous sequence" ;    rdfs:label "Time" .time:Instant    rdf:type owl:Class ;    rdfs:subClassOf time:Time ;    rdfs:comment "A specific point in time" ;    rdfs:label "Instant" .time:Interval    rdf:type owl:Class ;    rdfs:subClassOf time:Time ;    rdfs:comment "A duration of time" ;    rdfs:label "Interval" .time:DateTime    rdf:type owl:DatatypeProperty ;    rdfs:domain time:Instant ;    rdfs:range xsd:dateTime ;    rdfs:comment "The date and time of an instant" ;    rdfs:label "DateTime" .time:hasBeginning    rdf:type owl:ObjectProperty ;    rdfs:domain time:Interval ;    rdfs:range time:Instant ;    rdfs:comment "The beginning of a time interval" ;    rdfs:label "hasBeginning" .time:hasEnd    rdf:type owl:ObjectProperty ;    rdfs:domain time:Interval ;    rdfs:range time:Instant ;    rdfs:comment "The end of a time interval" ;    rdfs:label "hasEnd" .time:duration    rdf:type owl:DatatypeProperty ;    rdfs:domain time:Interval ;    rdfs:range xsd:duration ;    rdfs:comment "The duration of a time interval" ;    rdfs:label "duration" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix time: <http://www.example.org/time#> .time:Time a rdfs:Class ;    rdfs:label "Time"@en ;    rdfs:comment "Entity that represents the concept of time"@en .time:Event a rdfs:Class ;    rdfs:label "Event"@en ;    rdfs:comment "A unit of time or happening within a period of time"@en ;    rdfs:subClassOf time:Time .time:Instant a rdfs:Class ;    rdfs:label "Instant"@en ;    rdfs:comment "A specific point in time"@en ;    rdfs:subClassOf time:Time .time:Interval a rdfs:Class ;    rdfs:label "Interval"@en ;    rdfs:comment "A span of time with a start and an end"@en ;    rdfs:subClassOf time:Time .time:hasTime rdf:type owl:DatatypeProperty ;    rdfs:domain time:Event ;    rdfs:range xsd:dateTime ;    rdfs:label "has time"@en ;    rdfs:comment "Property that indicates the time an event took place"@en .time:startTime rdf:type owl:DatatypeProperty ;    rdfs:domain time:Interval ;    rdfs:range xsd:dateTime ;    rdfs:label "start time"@en ;    rdfs:comment "Property that indicates the time an interval starts"@en .time:endTime rdf:type owl:DatatypeProperty ;    rdfs:domain time:Interval ;    rdfs:range xsd:dateTime ;    rdfs:label "end time"@en ;    rdfs:comment "Property that indicates the time an interval ends"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://timeontology.com/ns#> .time:Time    rdf:type owl:Class ;    rdfs:label "Time" ;    rdfs:comment "A point or period in which things happen." .time:SpecificTime    rdf:type owl:Class ;    rdfs:subClassOf time:Time ;    rdfs:label "Specific Time" ;    rdfs:comment "A specific point in time." .time:TimeInterval    rdf:type owl:Class ;    rdfs:subClassOf time:Time ;    rdfs:label "Time Interval" ;    rdfs:comment "A period of time." .time:hasTime    rdf:type owl:ObjectProperty ;    rdfs:label "has time" ;    rdfs:comment "Relates something to its time." ;    rdfs:domain owl:Thing ;    rdfs:range time:Time .time:atTime    rdf:type owl:ObjectProperty ;    rdfs:label "at time" ;    rdfs:comment "Relates something to a specific time." ;    rdfs:subPropertyOf time:hasTime ;    rdfs:domain owl:Thing ;    rdfs:range time:SpecificTime .time:during    rdf:type owl:ObjectProperty ;    rdfs:label "during" ;    rdfs:comment "Relates something to a time interval." ;    rdfs:subPropertyOf time:hasTime ;    rdfs:domain owl:Thing ;    rdfs:range time:TimeInterval .time:inFuture    rdf:type owl:ObjectProperty ;    rdfs:label "in future" ;    rdfs:comment "Relates something to a point or period in time that is in the future." ;    rdfs:subPropertyOf time:hasTime ;    rdfs:domain owl:Thing ;    rdfs:range time:Time .time:inPast    rdf:type owl:ObjectProperty ;    rdfs:label "in past" ;    rdfs:comment "Relates something to a point or period in time that is in the past." ;    rdfs:subPropertyOf time:hasTime ;    rdfs:domain owl:Thing ;    rdfs:range time:Time .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.w3.org/2006/time#> .@prefix ex: <http://example.com/time#> .ex:TimeEvent a rdfs:Class ;    rdfs:label "Time Event"@en ;    rdfs:comment "A class for representing time-related events."@en .ex:title a rdf:Property ;    rdfs:label "title"@en ;    rdfs:comment "The title of the event."@en ;    rdfs:domain ex:TimeEvent ;    rdfs:range xsd:string .ex:startTime a rdf:Property ;    rdfs:label "start time"@en ;    rdfs:comment "The start time of the event."@en ;    rdfs:domain ex:TimeEvent ;    rdfs:range time:DateTime .ex:endTime a rdf:Property ;    rdfs:label "end time"@en ;    rdfs:comment "The end time of the event."@en ;    rdfs:domain ex:TimeEvent ;    rdfs:range time:DateTime .ex:description a rdf:Property ;    rdfs:label "description"@en ;    rdfs:comment "A description of the event."@en ;    rdfs:domain ex:TimeEvent ;    rdfs:range xsd:string .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.w3.org/2006/time#> .time:Time    rdf:type rdfs:Class ;   rdfs:subClassOf rdfs:Resource ;   rdfs:comment "A temporal entity i.e., a position or extent in time."@en ;   rdfs:label "Time".time:inXSDDateTime  rdf:type owl:DatatypeProperty;  rdfs:domain time:Time;  rdfs:range xsd:dateTime;  rdfs:comment "Property relating a time entity to its xsd:dateTime representation"@en ;  rdfs:label "in XSD Date Time" .time:hasBeginning    rdf:type owl:ObjectProperty ;   rdfs:domain time:Time ;   rdfs:range time:Instant ;   rdfs:comment "The beginning of a temporal entity"@en ;   rdfs:label "has beginning" .time:hasEnd     rdf:type owl:ObjectProperty ;    rdfs:domain time:Time ;    rdfs:range time:Instant ;    rdfs:comment "The end of a temporal entity"@en ;    rdfs:label "has end" .time:Instant    rdf:type rdfs:Class ;   rdfs:subClassOf time:Time ;   rdfs:comment "A temporal instant"@en ;   rdfs:label "Instant" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix time: <http://www.w3.org/TR/owl-time#> .@prefix ex: <http://example.com#> .ex:myTime a time:Instant ;  time:inXSDDateTime "2021-12-11T12:00:00Z"^^<http://www.w3.org/2001/XMLSchema#dateTime> .ex:myInterval a time:Interval ;  time:hasBeginning ex:myTime ;  time:inXSDDuration "PT1H"^^<http://www.w3.org/2001/XMLSchema#duration> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://www.example.org/time#> .:TimePeriod rdf:type rdfs:Class .:hasStartTime rdf:type rdf:Property ;    rdfs:domain :TimePeriod ;    rdfs:range xsd:dateTime .:hasEndTime rdf:type rdf:Property ;    rdfs:domain :TimePeriod ;    rdfs:range xsd:dateTime .:hasDuration rdf:type rdf:Property ;    rdfs:domain :TimePeriod ;    rdfs:range xsd:duration .


// Namespace Declarations.@prefix rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl:<http://www.w3.org/2002/07/owl#> .@prefix xsd:<http://www.w3.org/2001/XMLSchema#> .@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#> .@prefix time:<http://www.example.com/time#> .// Time class definition.time:Time rdf:type owl:Class ;    rdfs:label "Time" ;    rdfs:comment "Concept of Time" .// TimeInterval, TimeInstant subclasses of Time.time:TimeInterval rdf:type owl:Class ;    rdfs:subClassOf time:Time ;    rdfs:label "TimeInterval" ;    rdfs:comment "A particular interval of Time" .time:TimeInstant rdf:type owl:Class ;    rdfs:subClassOf time:Time ;    rdfs:label "TimeInstant" ;    rdfs:comment "A specific point in Time" .// startTime and endTime property of TimeInterval.time:startTime rdf:type owl:DatatypeProperty ;    rdfs:domain time:TimeInterval ;    rdfs:range xsd:dateTime ;    rdfs:label "startTime" ;    rdfs:comment "The start time of a TimeInterval".    time:endTime rdf:type owl:DatatypeProperty ;    rdfs:domain time:TimeInterval ;    rdfs:range xsd:dateTime ;    rdfs:label "endTime" ;    rdfs:comment "The end time of a TimeInterval" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.w3.org/2006/time#> .time:TemporalEntity a rdfs:Class ;  rdfs:label "Temporal Entity" ;  rdfs:comment "The abstract superclass of Temporal Positions and Temporal Intervals, all of which can be associated with numbers in several ways" .time:TemporalPosition a rdfs:Class ;  rdfs:subClassOf time:TemporalEntity ;  rdfs:label "Temporal Position" ;  rdfs:comment "An abstract reference to a position within a temporal coordinate system" .time:TemporalInterval a rdfs:Class ;  rdfs:subClassOf time:TemporalEntity ;  rdfs:label "Temporal Interval" ;  rdfs:comment "An interval of time that is bounded by two instants" .time:inXSDDateTimeStamp a rdf:Property ;  rdfs:domain time:TemporalPosition ;  rdfs:range xsd:dateTimeStamp ;  rdfs:label "in XSD DateTime Stamp" ;  rdfs:comment "Connects a temporal position with a dateTimeStamp as defined in XML Schema" .time:hasBeginning a rdf:Property ;  rdfs:domain time:TemporalInterval ;  rdfs:range time:TemporalPosition ;  rdfs:label "has Beginning" ;  rdfs:comment "The temporal interval has the specified temporal position as its beginning" .time:hasEnd a rdf:Property ;  rdfs:domain time:TemporalInterval ;  rdfs:range time:TemporalPosition ;  rdfs:label "has End" ;  rdfs:comment "The temporal interval has the specified temporal position as its end" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix time: <http://www.w3.org/2006/time#> .time:Instant a rdfs:Class ;    rdfs:label "Instant" ;    rdfs:comment "An instant is a point on the time-line" .time:Interval a rdfs:Class ;    rdfs:label "Interval" ;    rdfs:comment "A time interval is a duration on the time-line that is bounded by two instants" .time:DateTimeDescription a rdfs:Class ;    rdfs:label "DateTimeDescription" ;    rdfs:comment "A description of date and time structured according to Gregorian calendar" .time:inXSDDateTime a rdf:Property ;    rdfs:range time:DateTimeDescription ;    rdfs:domain time:Instant ;    rdfs:label "in XSD datetime" ;    rdfs:comment "Indicates the DateTimeDescription of the instant according to the xsd:dateTime format" .time:hasBeginning a rdf:Property ;    rdfs:range time:Instant ;    rdfs:domain time:Interval ;    rdfs:label "has beginning" ;    rdfs:comment "Relation between a time interval and an instant at which it begins" .time:hasEnd a rdf:Property ;    rdfs:range time:Instant ;    rdfs:domain time:Interval ;    rdfs:label "has end" ;    rdfs:comment "Relation between a time interval and an instant at which it ends" .time:inXSDDuration a rdf:Property ;    rdfs:range rdfs:Literal ;    rdfs:domain time:Interval ;    rdfs:label "in XSD duration" ;    rdfs:comment "Indicates the duration of the interval in xsd:duration format" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix time: <http://example.org/time#> .time:Time a owl:Class ;    rdfs:label "Time"@en ;    rdfs:comment "Represents time concept"@en .    time:hours a owl:DatatypeProperty ;    rdfs:domain time:Time ;    rdfs:range xsd:integer ;    rdfs:label "has hours"@en ;    rdfs:comment "Property representing hours"@en .time:minutes a owl:DatatypeProperty ;    rdfs:domain time:Time ;    rdfs:range xsd:integer ;    rdfs:label "has minutes"@en ;    rdfs:comment "Property representing minutes"@en .    time:seconds a owl:DatatypeProperty ;    rdfs:domain time:Time ;    rdfs:range xsd:integer ;    rdfs:label "has seconds"@en ;    rdfs:comment "Property representing seconds"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix time: <http://www.example.org/ontology/time#> .# Class definitionstime:Time rdf:type rdfs:Class ;    rdfs:comment "A point or period in time" ;    rdfs:label "Time" .time:Hour rdf:type rdfs:Class ;    rdfs:subClassOf time:Time ;    rdfs:comment "A unit of time equal to 60 minutes" ;    rdfs:label "Hour" .time:Minute rdf:type rdfs:Class ;    rdfs:subClassOf time:Time ;    rdfs:comment "A unit of time equal to 60 seconds" ;    rdfs:label "Minute" .# Property definitionstime:hasHour rdf:type rdf:Property ;    rdfs:domain time:Time ;    rdfs:range time:Hour ;    rdfs:comment "Relates a point or period in time to an hour of the day" ;    rdfs:label "has hour" .             time:hasMinute rdf:type rdf:Property ;    rdfs:domain time:Time ;    rdfs:range time:Minute ;    rdfs:comment "Relates a point or period in time to a minute of an hour" ;    rdfs:label "has minute" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://example.org/> .:JohnDoe :hasBirthTime :aSpecificTime .:aSpecificTime a owl:Time ;   :hasTimeStamp "1990-01-01T00:00:00Z"^^xsd:dateTime .

@prefix : <http://www.example.org/> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:TimeInterval a rdfs:Class ;    rdfs:label "Time Interval" ;    rdfs:comment "A specific duration of time." .:hasStartTime a owl:ObjectProperty ;    rdfs:label "has start time" ;    rdfs:comment "The time when the interval begins." ;    rdfs:domain :TimeInterval ;    rdfs:range xsd:dateTime .:hasEndTime a owl:ObjectProperty ;    rdfs:label "has end time" ;    rdfs:comment "The time when the interval ends." ;    rdfs:domain :TimeInterval ;    rdfs:range xsd:dateTime .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/time#> .time:Time     a rdfs:Class ;     rdfs:label "Time" ;     rdfs:comment "Class representing notion of time" .time:hasHour     a rdf:Property ;     rdfs:label "has hour" ;     rdfs:domain time:Time ;     rdfs:range xsd:integer ;     rdfs:comment "Property representing hour segment of time" .time:hasMinute     a rdf:Property ;     rdfs:label "has minute" ;     rdfs:domain time:Time ;     rdfs:range xsd:integer ;     rdfs:comment "Property representing minute segment of time" .time:hasSecond     a rdf:Property ;     rdfs:label "has second" ;     rdfs:domain time:Time ;     rdfs:range xsd:integer ;     rdfs:comment "Property representing second segment of time" .
Class: TimePoint  Description: Represents a point in time.Class: Past  SubClassOf: TimePoint  Description: Represents a time point in the past.Class: Present  SubClassOf: TimePoint  Description: Represents the present time point.Class: Future  SubClassOf: TimePoint  Description: Represents a time point in the future.ObjectProperty: follows  Domain: TimePoint  Range: TimePoint  Description: Represents the relation "follows" between two time points. ObjectProperty: precedes  InverseOf: follows  Domain: TimePoint  Range: TimePoint  Description: Represents the relation "precedes" between two time points. 
@prefix : <http://www.example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:hasTime rdf:type owl:DatatypeProperty ;        rdfs:range xsd:dateTime .:Event rdf:type owl:Class ;       rdfs:subClassOf [         rdf:type owl:Restriction ;         owl:onProperty :hasTime ;         owl:someValuesFrom xsd:dateTime       ] .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/ontology/time#> .time:Time a rdfs:Class ;          rdfs:comment "This is a fundamental class representing time" .time:Hour a rdfs:Class ;           rdfs:subClassOf time:Time ;           rdfs:comment "This represents the smaller unit of time, the hour" .time:Minute a rdfs:Class ;             rdfs:subClassOf time:Hour ;             rdfs:comment "This represents the smaller unit of time, the minute. 60 minutes make an hour." .time:lengthInHours a rdf:Property ;                   rdfs:domain time:Time ;                   rdfs:range xsd:float ;                   rdfs:comment "This property denotes the length of a certain time span in terms of hours" .time:lengthInMinutes a rdf:Property ;                   rdfs:domain time:Time ;                   rdfs:range xsd:float ;                   rdfs:comment "This property denotes the length of a certain time span in terms of minutes" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix time: <http://www.example.org/ontology/time#> .time:TimePeriod rdf:type rdfs:Class ;  rdfs:label "Time Period" ;  rdfs:comment "A specific length or portion of time." .time:hasStartTime rdf:type rdf:Property ;  rdfs:label "has start time" ;  rdfs:comment "The time at which a time period begins." ;  rdfs:domain time:TimePeriod ;  rdfs:range rdfs:Literal .time:hasEndTime rdf:type rdf:Property ;  rdfs:label "has end time" ;  rdfs:comment "The time at which a time period ends." ;  rdfs:domain time:TimePeriod ;  rdfs:range rdfs:Literal .time:duration rdf:type rdf:Property ;  rdfs:label "duration" ;  rdfs:comment "The duration or length of a time period." ;  rdfs:domain time:TimePeriod ;  rdfs:range rdfs:Literal .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix time: <http://www.example.org/ontology/time#> .time:Hour rdf:type owl:Class .time:Minute rdf:type owl:Class .time:Second rdf:type owl:Class .time:hasHour rdf:type owl:DatatypeProperty ;             rdfs:domain time:Time ;             rdfs:range xsd:int .      time:hasMinute rdf:type owl:DatatypeProperty ;               rdfs:domain time:Time ;               rdfs:range xsd:int .time:hasSecond rdf:type owl:DatatypeProperty ;               rdfs:domain time:Time ;               rdfs:range xsd:int ._:a_time_instance rdf:type time:Time ;                  time:hasHour "10"^^xsd:int ;                  time:hasMinute "30"^^xsd:int ;                  time:hasSecond "15"^^xsd:int .
