
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix dbo: <http://dbpedia.org/ontology/> .<http://example.org/Business> rdf:type owl:Ontology ;                             rdfs:comment "An ontology for business"^^xsd:string ;                             rdfs:label "BusinessOntology"^^xsd:string .<http://example.org/Business#Company> rdf:type owl:Class ;                                      rdfs:subClassOf dbo:Organisation .                                      <http://example.org/Business#Employee> rdf:type owl:Class ;                                      rdfs:subClassOf foaf:Person .                                      <http://example.org/Business#Product> rdf:type owl:Class .<http://example.org/Business#Service> rdf:type owl:Class .<http://example.org/Business#employs> rdf:type owl:ObjectProperty ;                                     rdfs:domain <http://example.org/Business#Company> ;                                     rdfs:range <http://example.org/Business#Employee> .                                     <http://example.org/Business#provides> rdf:type owl:ObjectProperty ;                                       rdfs:domain <http://example.org/Business#Company> ;                                       rdfs:range <http://example.org/Business#Product> .                                       <http://example.org/Business#offers> rdf:type owl:ObjectProperty ;                                     rdfs:domain <http://example.org/Business#Company> ;                                     rdfs:range <http://example.org/Business#Service> .<http://example.org/Business#operatesIn> rdf:type owl:ObjectProperty ;                                         rdfs:domain <http://example.org/Business#Company> ;                                         rdfs:range dbo:Place .                                      <http://example.org/Business#hasCEO> rdf:type owl:DatatypeProperty ;                                    rdfs:domain <http://example.org/Business#Company> ;                                    rdfs:range foaf:Person .


# Set the base@base <http://example.org/business/> .# Set the business prefix@prefix business: <http://example.org/business/> .# Set the rdf prefix@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .# Set the rdfs prefix@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .# Set the owl prefix@prefix owl: <http://www.w3.org/2002/07/owl#> .# Define the Business classbusiness:Business rdf:type rdfs:Class ;    rdfs:label "Business"@en ;    rdfs:comment "An organisation or economic system where goods and services are exchanged"@en .# Define the Employee classbusiness:Employee rdf:type rdfs:Class ;    rdfs:label "Employee"@en ;    rdfs:comment "A person who is hired to provide services to a company on a regular basis"@en .# Define the Employer classbusiness:Employer rdf:type rdfs:Class ;    rdfs:label "Employer"@en ;    rdfs:comment "A person or organization that employs people"@en .# Define the Product classbusiness:Product rdf:type rdfs:Class ;    rdfs:label "Product"@en ;    rdfs:comment "A thing or person that is the result of an action or process"@en .# Define the Service classbusiness:Service rdf:type rdfs:Class ;    rdfs:label "Service"@en ;    rdfs:comment "The action of helping or doing work for someone"@en .# Define the worksFor propertybusiness:worksFor rdf:type rdf:Property ;    rdfs:label "works for"@en ;    rdfs:comment "The business that an employee works for"@en ;    rdfs:domain business:Employee ;    rdfs:range business:Business .# Define the employs propertybusiness:employs rdf:type rdf:Property ;    rdfs:label "employs"@en ;    rdfs:comment "The employee of a business"@en ;    rdfs:domain business:Business ;    rdfs:range business:Employee .# Define the offersProduct propertybusiness:offersProduct rdf:type rdf:Property ;    rdfs:label "offers product"@en ;    rdfs:comment "The product that a business offers"@en ;    rdfs:domain business:Business ;    rdfs:range business:Product .# Define the offersService propertybusiness:offersService rdf:type rdf:Property ;    rdfs:label "offers service"@en ;    rdfs:comment "The service that a business offers"@en ;    rdfs:domain business:Business ;    rdfs:range business:Service .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix business: <http://example.org/business#> .business:Business rdf:type owl:Class .business:BusinessSector rdf:type owl:Class .business:Location rdf:type owl:Class .business:Employee rdf:type owl:Class .business:Product rdf:type owl:Class .business:Service rdf:type owl:Class .business:hasSector rdf:type owl:ObjectProperty ;    rdfs:domain business:Business ;    rdfs:range business:BusinessSector .business:hasLocation rdf:type owl:ObjectProperty ;    rdfs:domain business:Business ;    rdfs:range business:Location .business:hasEmployee rdf:type owl:ObjectProperty ;    rdfs:domain business:Business ;    rdfs:range business:Employee .business:producesProduct rdf:type owl:ObjectProperty ;    rdfs:domain business:Business ;    rdfs:range business:Product .business:providesService rdf:type owl:ObjectProperty ;    rdfs:domain business:Business ;    rdfs:range business:Service .business:empName rdf:type owl:DatatypeProperty ;    rdfs:domain business:Employee ;    rdfs:range xsd:string .business:locName rdf:type owl:DatatypeProperty ;    rdfs:domain business:Location ;    rdfs:range xsd:string .business:prodName rdf:type owl:DatatypeProperty ;    rdfs:domain business:Product ;    rdfs:range xsd:string .business:serviceName rdf:type owl:DatatypeProperty ;    rdfs:domain business:Service ;    rdfs:range xsd:string .business:sectorName rdf:type owl:DatatypeProperty ;    rdfs:domain business:BusinessSector ;    rdfs:range xsd:string .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix business: <http://example.com/business#> .business:Business rdf:type rdfs:Class .business:Employee rdf:type rdfs:Class ;business:Manager rdf:type rdfs:Class ;  rdfs:subClassOf business:Employee .  business:HasEmployee rdf:type rdf:Property ;  rdfs:domain business:Business ;  rdfs:range business:Employee .  business:WorksFor rdf:type rdf:Property ;  rdfs:domain business:Employee ;  rdfs:range business:Business .business:CompanyName rdf:type rdf:Property ;  rdfs:domain business:Business ;  rdfs:range xsd:string .# Individual businesses business:DeltaInc rdf:type business:Business ;  business:CompanyName "Delta Inc." .business:GammaCorp rdf:type business:Business ;  business:CompanyName "Gamma Corp." .# Individual employeesbusiness:JohnDoe rdf:type business:Employee ;  business:WorksFor business:DeltaInc .business:JaneSmith rdf:type business:Manager ;  business:WorksFor business:GammaCorp . 



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix business: <http://example.com/business#> .business:Business a rdfs:Class ;            rdfs:comment "A class representing all businesses" ;            rdfs:label "Business" .business:industry a rdf:Property ;            rdfs:domain business:Business ;            rdfs:range business:Industry ;            rdfs:label "industry" ;            rdfs:comment "The industry that a business operates in" .business:Industry a rdfs:Class ;              rdfs:comment "A class representing all industries" ;              rdfs:label "Industry" .business:hasEmployee a rdf:Property ;            rdfs:domain business:Business ;            rdfs:range business:Employee ;            rdfs:label "has employee" ;            rdfs:comment "A relation between a business and its employees" .business:Employee a rdfs:Class ;            rdfs:comment "A class representing employees" ;            rdfs:label "Employee" .business:producesProduct a rdf:Property ;            rdfs:domain business:Business ;            rdfs:range business:Product ;            rdfs:label "produces product" ;            rdfs:comment "A relation between a business and the products it produces" .business:Product a rdfs:Class ;            rdfs:comment "A class representing products" ;            rdfs:label "Product" .business:hasHeadquarters a rdf:Property ;             rdfs:domain business:Business ;             rdfs:range business:Location ;             rdfs:comment "A relation between a business and its headquarters" ;             rdfs:label "has headquarters" .business:Location a rdfs:Class ;             rdfs:comment "A class representing physical locations" ;             rdfs:label "Location" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix biz: <http://example.com/biz#> .biz:Business rdf:type rdfs:Class .biz:Employee rdf:type rdfs:Class .biz:Product rdf:type rdfs:Class .biz:Service rdf:type rdfs:Class .biz:hasEmployee rdf:type rdf:Property ;    rdfs:domain biz:Business ;     rdfs:range biz:Employee .biz:offersProduct rdf:type rdf:Property ;    rdfs:domain biz:Business ;     rdfs:range biz:Product .biz:offersService rdf:type rdf:Property ;    rdfs:domain biz:Business ;    rdfs:range biz:Service .biz:name rdf:type rdf:Property ;    rdfs:domain [ rdf:type owl:Class ; owl:unionOf (biz:Business biz:Employee biz:Product biz:Service) ] ;    rdfs:range xsd:string .biz:revenue rdf:type rdf:Property ;    rdfs:domain biz:Business ;    rdfs:range xsd:float .biz:salary rdf:type rdf:Property ;    rdfs:domain biz:Employee ;    rdfs:range xsd:float .biz:price rdf:type rdf:Property ;    rdfs:domain [ rdf:type owl:Class ; owl:unionOf (biz:Product biz:Service) ] ;    rdfs:range xsd:float .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix biz: <http://example.com/business#> .biz:Business rdfs:subClassOf owl:Thing ;    rdfs:comment "A commercial organization which provides goods or services" ;    rdfs:label "Business" .biz:Employee rdfs:subClassOf owl:Thing ;    rdfs:comment "A person who works for a business" ;    rdfs:label "Employee" .biz:Manager rdfs:subClassOf biz:Employee ;    rdfs:comment "A person who has control over the activities of a part of a business" ;    rdfs:label "Manager" .biz:Customer rdfs:subClassOf owl:Thing ;    rdfs:comment "A person or an entity who purchases goods or services from a business" ;    rdfs:label "Customer" .biz:Product rdfs:subClassOf owl:Thing ;    rdfs:comment "A thing that a business sells to its customers" ;    rdfs:label "Product" .biz:Service rdfs:subClassOf owl:Thing ;    rdfs:comment "A task or assistance that a business performs for its customers" ;    rdfs:label "Service" .biz:employs rdfs:domain biz:Business ;    rdfs:range biz:Employee ;    rdfs:comment "Associates a business with its employees" ;    rdfs:label "employs" .biz:managedBy rdfs:domain biz:Business ;    rdfs:range biz:Manager ;    rdfs:comment "Associates a business with its manager" ;    rdfs:label "managedBy" .biz:sellsTo rdfs:domain biz:Business ;    rdfs:range biz:Customer ;    rdfs:comment "Associates a business with its customers" ;    rdfs:label "sellsTo" .biz:produces rdfs:domain biz:Business ;    rdfs:range biz:Product ;    rdfs:comment "Associates a business with the products it produces" ;    rdfs:label "produces" .biz:provides rdfs:domain biz:Business ;    rdfs:range biz:Service ;    rdfs:comment "Associates a business with the services it provides" ;    rdfs:label "provides" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix business: <http://mybusiness.org#> .business:Business rdf:type owl:Class .business:hasName rdf:type owl:DatatypeProperty ;  rdfs:range xsd:string ;  rdfs:domain business:Business .business:hasLocation rdf:type owl:DatatypeProperty ;  rdfs:range xsd:string ;  rdfs:domain business:Business .business:Employee rdf:type owl:Class .business:employs rdf:type owl:ObjectProperty ;  rdfs:range business:Employee ;  rdfs:domain business:Business .business:hasRole rdf:type owl:DatatypeProperty ;  rdfs:range xsd:string ;  rdfs:domain business:Employee .business:Department rdf:type owl:Class .business:hasDepartment rdf:type owl:ObjectProperty ;  rdfs:range business:Department ;  rdfs:domain business:Business .business:Product rdf:type owl:Class .business:hasProduct rdf:type owl:ObjectProperty ;  rdfs:range business:Product ;  rdfs:domain business:Business .business:Customer rdf:type owl:Class .business:serves rdf:type owl:ObjectProperty ;  rdfs:range business:Customer ;  rdfs:domain business:Business .business:Investor rdf:type owl:Class .business:isFundedBy rdf:type owl:ObjectProperty ;  rdfs:range business:Investor ;  rdfs:domain business:Business .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/business#> .ex:Business rdf:type rdfs:Class ;    rdfs:comment "A commercial activity engaged in as a means of livelihood or profit, or an entity which engages in such activities" .ex:isInvolvedIn rdf:type rdf:Property ;    rdfs:range ex:Business ;    rdfs:domain ex:Individual ;    rdfs:comment "Relates an individual to a business in which they are involved." .ex:OperatesIn rdf:type rdf:Property ;    rdfs:range ex:Location ;    rdfs:domain ex:Business ;    rdfs:comment "Relates a business to a location in which it operates." .ex:Location rdf:type rdfs:Class;    rdfs:comment "Geographic location in which a business operates." .ex:Individual rdf:type rdfs:Class ;    rdfs:comment "An individual is someone who is involved in a business" .    ex:Product rdf:type rdfs:Class ;    rdfs:comment "A thing produced by the business for use or sale." . ex:Produces rdf:type rdf:Property ;    rdfs:range ex:Product ;    rdfs:domain ex:Business ;    rdfs:comment "Relates a business to a product it produces." . ex:hasRevenue rdf:type rdf:Property;    rdfs:range xsd:decimal ;    rdfs:domain ex:Business ;    rdfs:comment "Relates a business to its revenue." . ex:hasEmployee rdf:type rdf:Property;    rdfs:range ex:Individual;    rdfs:domain ex:Business ;    rdfs:comment "Relates a business to its employees." .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix business: <http://example.org/business#> .business:Business rdf:type rdfs:Class ;  rdfs:label "Business"^^xsd:string ;  rdfs:comment "The class of all business entities." .business:hasEmployee rdf:type rdf:Property ;  rdfs:label "has employee"^^xsd:string ;  rdfs:range business:Employee ;  rdfs:domain business:Business .business:Employee rdf:type rdfs:Class ;  rdfs:label "Employee"^^xsd:string ;  rdfs:comment "The class of all employees." ;business:hasPosition rdf:type rdf:Property ;  rdfs:range business:Position ;  rdfs:domain business:Employee ;business:Position rdf:type rdfs:Class ;  rdfs:label "Position"^^xsd:string ;  rdfs:comment "The class of all business positions." .business:Partnership rdf:type rdfs:Class ;  rdfs:label "Partnership"^^xsd:string ;  rdfs:comment "The class of all business partnerships." .business:hasPartner rdf:type rdf:Property ;  rdfs:range business:Partnership ;  rdfs:domain business:Business .

@prefix : <http://example.org/> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .:Business rdf:type rdfs:Class .:Employee rdf:type rdfs:Class .:Manager rdf:type rdfs:Class ;    rdfs:subClassOf :Employee .:Client rdf:type rdfs:Class .:Product rdf:type rdfs:Class .:Service rdf:type rdfs:Class .:Supplier rdf:type rdfs:Class .    :owns rdf:type rdf:Property ;    rdfs:domain :Manager ;    rdfs:range :Business .:hires rdf:type rdf:Property ;    rdfs:domain :Manager ;    rdfs:range :Employee .:sells rdf:type rdf:Property ;    rdfs:domain :Business ;    rdfs:range :Product .:provides rdf:type rdf:Property ;    rdfs:domain :Business ;    rdfs:range :Service .:buysFrom rdf:type rdf:Property ;    rdfs:domain :Business ;    rdfs:range :Supplier .:serves rdf:type rdf:Property ;    rdfs:domain :Business ;    rdfs:range :Client .

Script@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com/> .ex:Business rdf:type rdfs:Class .ex:product rdf:type rdf:Property ;    rdfs:domain ex:Business ;    rdfs:range ex:Product .ex:Product rdf:type rdfs:Class .ex:industry rdf:type rdf:Property ;    rdfs:domain ex:Business ;    rdfs:range ex:Industry .ex:Industry rdf:type rdfs:Class .ex:location rdf:type rdf:Property ;    rdfs:domain ex:Business ;    rdfs:range ex:Location .ex:Location rdf:type rdfs:Class .ex:employee rdf:type rdf:Property ;    rdfs:domain ex:Business ;    rdfs:range ex:Employee .ex:Employee rdf:type rdfs:Class .ex:profit rdf:type rdf:Property ;    rdfs:domain ex:Business ;    rdfs:range xsd:decimal .ex:owner rdf:type rdf:Property ;    rdfs:domain ex:Business ;    rdfs:range ex:Person .ex:Person rdf:type rdfs:Class .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix business: <http://www.example.org/business#> .business:Business a rdfs:Class ;  rdfs:label "Business" ;  rdfs:comment "A commercial activity or profession." .business:Product a rdfs:Class ;  rdfs:label "Product" ;  rdfs:comment "A tangible or intangible good or service produced by a business." .business:Service a rdfs:Class ;  rdfs:label "Service" ;  rdfs:comment "An intangible good provided by a business." ;  rdfs:subClassOf business:Product .business:Supplier a rdfs:Class ;  rdfs:label "Supplier" ;  rdfs:comment "A person or company that provides goods or services to a business." .business:produces a rdf:Property ;  rdfs:label "produces" ;  rdfs:domain business:Business ;  rdfs:range business:Product ;  rdfs:comment "Indicates the products a business produces." .business:supplies a rdf:Property ;  rdfs:label "supplies" ;  rdfs:domain business:Supplier ;  rdfs:range business:Product ;  rdfs:comment "Indicates the products a supplier provides to businesses." .business:name a rdf:Property ;  rdfs:label "name" ;  rdfs:domain business:Business ;  rdfs:range xsd:string ;  rdfs:comment "The name of the business." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix business: <http://example.org/business#> .business:Business rdf:type owl:Class ;    rdfs:comment "A category representing a business or firm" .business:hasOwner rdf:type owl:ObjectProperty ;    rdfs:range business:Owner ;    rdfs:domain business:Business ;    rdfs:comment "Property linking a business to its owner" .business:Owner rdf:type owl:Class ;    rdfs:comment "Individual or entity that owns a business" .business:Employee rdf:type owl:Class ;    rdfs:comment "Individual who works for a business" .business:hasEmployee rdf:type owl:ObjectProperty ;    rdfs:range business:Employee ;    rdfs:domain business:Business ;    rdfs:comment "Property linking a business to its employees" .business:Product rdf:type owl:Class ;    rdfs:comment "Goods or services offered by a business" .business:offersProduct rdf:type owl:ObjectProperty ;    rdfs:range business:Product ;    rdfs:domain business:Business ;    rdfs:comment "Property linking a business to the goods or services it offers" .business:Location rdf:type owl:Class ;    rdfs:comment "Physical location of a business" .business:hasLocation rdf:type owl:ObjectProperty ;    rdfs:range business:Location ;    rdfs:domain business:Business ;    rdfs:comment "Property linking a business to its physical location".business:revenue rdf:type owl:DatatypeProperty ;    rdfs:range xsd:decimal ;    rdfs:domain business:Business ;    rdfs:comment "Property representing a business's revenue" .business:name rdf:type owl:DatatypeProperty ;    rdfs:range xsd:string ;    rdfs:domain business:Business ;    rdfs:comment "Property representing a business's name" .    business:establishedDate rdf:type owl:DatatypeProperty ;    rdfs:range xsd:date ;    rdfs:domain business:Business ;    rdfs:comment "Property representing a business's established date" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ont: <http://www.example.org/ontology#> .# Class Definitionsont:Business rdf:type rdfs:Class ;    rdfs:label "Business"@en ;    rdfs:comment "A commercial entity engaged in economic activity"@en .ont:ContactInfo rdf:type rdfs:Class ;    rdfs:label "Contact Information"@en ;    rdfs:comment "Information used to reach a Business"@en .ont:Address rdf:type rdfs:Class ;    rdfs:label "Address"@en ;    rdfs:comment "Physical location of a Business"@en .# Property Definitionsont:hasName rdf:type rdf:Property ;    rdfs:label "has name"@en ;    rdfs:comment "The name of the Business"@en ;    rdfs:domain ont:Business ;    rdfs:range xsd:string .ont:hasContactInfo rdf:type rdf:Property ;    rdfs:label "has contact info"@en ;    rdfs:comment "The contact information of the Business"@en ;    rdfs:domain ont:Business ;    rdfs:range ont:ContactInfo .ont:hasAddress rdf:type rdf:Property ;    rdfs:label "has address"@en ;    rdfs:comment "The address of the Business"@en ;    rdfs:domain ont:Business ;    rdfs:range ont:Address .# Individual Instancesont:business1 rdf:type ont:Business ;    ont:hasName "Example Business Inc." ;    ont:hasContactInfo ont:contact1 ;    ont:hasAddress ont:address1 .ont:contact1 rdf:type ont:ContactInfo ;    rdfs:label "+1234567890" .ont:address1 rdf:type ont:Address ;    rdfs:label "123 Main St, City, Country" .

@prefix : <http://example.org/business#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Company a rdfs:Class ;    rdfs:label "Company" ;    rdfs:comment "Entity that represents a commercial institution" .:Employee a rdfs:Class ;    rdfs:label "Employee" ;    rdfs:comment "Individual who works under a contract of employment" .    :employs a rdfs:Property ;    rdfs:label "employs" ;    rdfs:comment "Factor that relates a company with its employees" ;    rdfs:domain :Company ;    rdfs:range :Employee .:IBM a :Company ;    rdfs:label "IBM" .:JohnDoe a :Employee ;    rdfs:label "John Doe" .:IBM :employs :JohnDoe .


