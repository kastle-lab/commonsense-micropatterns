@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix job: <http://example.org/job/> .job:Job a rdfs:Class ;        rdfs:label "Job"@en ;    rdfs:comment "A Job is a specific role or occupation within an organization" .    job:title a rdf:Property ;    rdfs:label "title"@en ;    rdfs:comment "The title of a job e.g Software Engineer, Data Analyst etc"@en ;    rdfs:domain job:Job ;    rdfs:range xsd:string .job:hasSalary a rdf:Property ;    rdfs:label "has salary"@en ;    rdfs:comment "The amount of money an employee is paid for performing the job"@en ;    rdfs:domain job:Job ;        rdfs:range xsd:double .job:location a rdf:Property ;    rdfs:label "location"@en ;    rdfs:comment "The physical location where the job is performed"@en ;    rdfs:domain job:Job ;        rdfs:range xsd:string .    job:Company a rdfs:Class ;        rdfs:label "Company"@en ;    rdfs:comment "A Company is a legal entity made up of an association of people carrying on a commercial or industrial enterprise"@en .job:hasCompany a rdf:Property ;    rdfs:label "has company"@en ;    rdfs:comment "The company that provides the job"@en ;    rdfs:domain job:Job ;        rdfs:range job:Company .job:requiresSkill a rdf:Property ;    rdfs:label "requires skill"@en ;    rdfs:comment "A particular capability or expertise required to perform the job"@en ;    rdfs:domain job:Job ;        rdfs:range xsd:string .job:JobPosting a rdfs:Class ;        rdfs:label "JobPosting"@en ;    rdfs:comment "An offering of a job"@en .job:postedJob a rdf:Property ;    rdfs:label "posted job"@en ;    rdfs:comment "The job that is being offered in the job posting"@en ;    rdfs:domain job:JobPosting ;        rdfs:range job:Job .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix job: <http://example.com/ontology/job#> .# Class Definitionsjob:Job rdf:type owl:Class ;    rdfs:comment "A Job or position of employment"@en ;    rdfs:subClassOf rdfs:Resource .job:JobTitle rdf:type owl:Class ;    rdfs:comment "The title of the job"@en ;    rdfs:subClassOf rdfs:Resource .job:JobDescription rdf:type owl:Class ;    rdfs:comment "A brief description about the job"@en ;    rdfs:subClassOf rdfs:Resource .job:Employer rdf:type owl:Class ;    rdfs:comment "The company or organization that provides the job"@en ;    rdfs:subClassOf rdfs:Resource .job:Location rdf:type owl:Class ;    rdfs:comment "The geographical location of the job"@en ;    rdfs:subClassOf rdfs:Resource .job:Salary rdf:type owl:Class ;    rdfs:comment "The pay or wage for the job"@en ;    rdfs:subClassOf rdfs:Resource .# Property Definitionsjob:hasJobTitle rdf:type owl:DatatypeProperty ;    rdfs:domain job:Job ;    rdfs:range job:JobTitle ;    rdfs:comment "Relates a job to its title"@en .job:hasJobDescription rdf:type owl:DatatypeProperty ;    rdfs:domain job:Job ;    rdfs:range job:JobDescription ;    rdfs:comment "Relates a job to its description"@en .job:hasEmployer rdf:type owl:ObjectProperty ;    rdfs:domain job:Job ;    rdfs:range job:Employer ;    rdfs:comment "Relates a job to its employer"@en .job:hasLocation rdf:type owl:ObjectProperty ;    rdfs:domain job:Job ;    rdfs:range job:Location ;    rdfs:comment "Relates a job to its location"@en .job:hasSalary rdf:type owl:DatatypeProperty ;    rdfs:domain job:Job ;    rdfs:range job:Salary ;    rdfs:comment "Relates a job to its salary"@en .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix job: <http://www.example.org/job#> .job:Job a rdfs:Class ;    rdfs:label "Job" ;    rdfs:comment "The entity class Job represents a work or occupation carried out for a specific period of time, in return for monetary compensation."@en .job:hasTitle a rdf:Property;    rdfs:label "has title" ;    rdfs:comment "The property that describes the title of a job."@en ;    rdfs:domain job:Job ;    rdfs:range xsd:string.job:hasSalary a rdf:Property;    rdfs:label "has salary" ;    rdfs:comment "The property that describes the salary of a job."@en ;    rdfs:domain job:Job ;    rdfs:range xsd:int.job:performsDuty a rdf:Property;    rdfs:label "performs duty" ;    rdfs:comment "The property that describes the responsibilities of a job."@en ;    rdfs:domain job:Job ;    rdfs:range xsd:string.job:requiresSkill a rdf:Property;    rdfs:label "requires skill" ;    rdfs:comment "The property that describes the skills required for a job."@en ;    rdfs:domain job:Job ;    rdfs:range xsd:string.job:isLocatedIn a rdf:Property;    rdfs:label "is located in" ;    rdfs:comment "The property that describes the location of a job."@en ;    rdfs:domain job:Job ;    rdfs:range xsd:string.job:hasDuration a rdf:Property;    rdfs:label "has duration" ;    rdfs:comment "The property that describes the duration of a job."@en ;    rdfs:domain job:Job ;    rdfs:range xsd:int.job:Company a rdfs:Class ;    rdfs:label "Company" ;    rdfs:comment "Entity class representing a company where a job can be."@en .job:isFromCompany a rdf:Property ;    rdfs:label "is from company" ;    rdfs:comment "Property that represents the relationship of a job to a company."@en ;    rdfs:domain job:Job ;    rdfs:range job:Company.


