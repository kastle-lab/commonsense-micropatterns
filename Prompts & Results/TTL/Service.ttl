
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix serviceOnt: <http://www.example.org/service-ontology#> .serviceOnt:Service a rdfs:Class ;    rdfs:label "Service"^^xsd:string ;    rdfs:comment "A service provided to customers by a company or organization"^^xsd:string .serviceOnt:name a rdf:Property ;    rdfs:label "has name"^^xsd:string ;    rdfs:domain serviceOnt:Service ;    rdfs:range xsd:string .serviceOnt:type a rdf:Property ;    rdfs:label "has type"^^xsd:string ;    rdfs:domain serviceOnt:Service ;    rdfs:range serviceOnt:ServiceType .serviceOnt:ServiceType a rdfs:Class ;    rdfs:label "Service type"^^xsd:string ;    rdfs:comment "The type or category of a service"^^xsd:string .serviceOnt:Provider a rdfs:Class ;    rdfs:label "Service provider"^^xsd:string ;    rdfs:comment "The organization or company that provides the service"^^xsd:string .serviceOnt:providesService a rdf:Property ;    rdfs:label "provides service"^^xsd:string ;    rdfs:domain serviceOnt:Provider ;    rdfs:range serviceOnt:Service .serviceOnt:Customer a rdfs:Class ;    rdfs:label "Customer"^^xsd:string ;    rdfs:comment "The individual or organization that uses the service"^^xsd:string .serviceOnt:usesService a rdf:Property ;    rdfs:label "uses service"^^xsd:string ;    rdfs:domain serviceOnt:Customer ;    rdfs:range serviceOnt:Service .serviceOnt:Price a rdfs:Class ;    rdfs:label "Price"^^xsd:string ;    rdfs:comment "The cost of a service"^^xsd:string .serviceOnt:hasPrice a rdf:Property ;    rdfs:label "has price"^^xsd:string ;    rdfs:domain serviceOnt:Service ;    rdfs:range serviceOnt:Price .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/service#> .ex:Service a rdfs:Class ;    rdfs:comment "The class of all services" .   ex:name a rdf:Property ;    rdfs:range xsd:string ;    rdfs:domain ex:Service ;    rdfs:comment "Property representing the name of the service" . ex:offeredBy a rdf:Property ;    rdfs:range ex:ServiceProvider ;    rdfs:domain ex:Service ;    rdfs:comment "Property linking the service to its provider" .ex:price a rdf:Property ;    rdfs:range xsd:decimal ;    rdfs:domain ex:Service ;    rdfs:comment "Property representing the price of the service" .ex:ServiceProvider a rdfs:Class ;    rdfs:comment "The class of all service providers" .ex:provides a rdf:Property ;    rdfs:range ex:Service ;    rdfs:domain ex:ServiceProvider ;    rdfs:comment "Property representing the services provided by the service provider" .ex:location a rdf:Property ;    rdfs:range xsd:string ;    rdfs:domain ex:ServiceProvider ;    rdfs:comment "Property representing the location of the service provider" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix svc: <http://example.com/service#> .# Classessvc:Service rdf:type rdfs:Class ;    rdfs:comment "A class representing all services."@en ;    rdfs:label "Service"@en .    svc:Customer rdf:type rdfs:Class ;    rdfs:comment "A class representing all customers who use services."@en ;    rdfs:label "Customer"@en .svc:ServiceProvider rdf:type rdfs:Class ;    rdfs:comment "A class representing all service providers."@en ;    rdfs:label "Service Provider"@en .# Propertiessvc:provides rdf:type rdf:Property ;    rdfs:domain svc:ServiceProvider ;    rdfs:range svc:Service ;    rdfs:comment "The property connecting service provider and the service they provide."@en ;    rdfs:label "provides"@en .svc:usesService rdf:type rdf:Property ;    rdfs:domain svc:Customer ;    rdfs:range svc:Service ;    rdfs:comment "The property connecting customers and the service they use."@en ;    rdfs:label "uses service"@en .svc:hasCustomer rdf:type rdf:Property ;    rdfs:domain svc:ServiceProvider ;    rdfs:range svc:Customer ;    rdfs:comment "The property connecting service provider and their customers."@en ;    rdfs:label "has customer"@en .    # Individualsvc:ServiceProvider1 rdf:type svc:ServiceProvider ;    svc:provides svc:Service1 ;    svc:hasCustomer svc:Customer1 .svc:Service1 rdf:type svc:Service .svc:Customer1 rdf:type svc:Customer ;    svc:usesService svc:Service1 .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix service: <http://www.example.org/service#> .service:    rdf:type owl:Ontology ;    rdfs:comment "A simple ontology for services" ;    rdfs:label "Service" .service:Service     rdf:type owl:Class ;    rdfs:label "Service" ;    rdfs:comment "The service offered" .service:Provider     rdf:type owl:Class ;    rdfs:label "Provider" ;    rdfs:comment "The provider of the service" .service:Consumer     rdf:type owl:Class ;    rdfs:label "Consumer" ;    rdfs:comment "The consumer of the service" .service:hasProvider     rdf:type owl:ObjectProperty ;    rdfs:domain service:Service ;    rdfs:range service:Provider ;    rdfs:label "has provider" ;    rdfs:comment "The entity providing the service" .service:hasConsumer     rdf:type owl:ObjectProperty ;    rdfs:domain service:Service ;    rdfs:range service:Consumer ;    rdfs:label "has consumer" ;    rdfs:comment "The entity consuming the service" .service:hasPrice     rdf:type owl:DatatypeProperty ;    rdfs:domain service:Service ;    rdfs:range xsd:decimal ;    rdfs:label "has price" ;    rdfs:comment "The price of the service" .service:hasDuration     rdf:type owl:DatatypeProperty ;    rdfs:domain service:Service ;    rdfs:range xsd:duration ;    rdfs:label "has duration" ;    rdfs:comment "The duration of the service" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix service: <http://example.com/ontology/service#> .service:Service a rdfs:Class ;    rdfs:label "Service"@en ;    rdfs:comment "A system supplying a public need such as transport, communications, or utilities."@en .service:name a rdf:Property ;    rdfs:label "Service Name"@en ;    rdfs:comment "The name of the service."@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:description a rdf:Property ;    rdfs:label "Service Description"@en ;    rdfs:comment "The description of the service."@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:provider a rdf:Property ;    rdfs:label "Service Provider"@en ;    rdfs:comment "The provider of the service."@en ;    rdfs:domain service:Service ;    rdfs:range service:ServiceProvider .service:ServiceProvider a rdfs:Class ;    rdfs:label "Service Provider"@en ;    rdfs:comment "An entity that provides service."@en .service:hasLocation a rdf:Property ;    rdfs:label "Service Location"@en ;    rdfs:comment "The location where the service is available or provided."@en ;    rdfs:domain service:Service ;    rdfs:range service:Location .service:Location a rdfs:Class ;    rdfs:label "Service Location"@en ;    rdfs:comment "A geospatial vector point where a service is available or provided."@en .service:hasOpeningHours a rdf:Property ;    rdfs:label "Service Opening Hours"@en ;    rdfs:comment "The opening hours of the service."@en ;    rdfs:domain service:Service ;    rdfs:range service:OpeningHours .service:OpeningHours a rdfs:Class ;    rdfs:label "Service Opening Hours"@en ;    rdfs:comment "A schedule indicating when a service is available."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix svc: <http://example.org/service#> .svc:Service rdf:type owl:Class ;rdfs:subClassOf owl:Thing ;rdfs:comment "A class representing services, which can be any activity or benefit that one party can offer to another."@en .svc:serviceName rdf:type owl:DatatypeProperty ;rdfs:domain svc:Service ;rdfs:range xsd:string ;rdfs:comment "The name of the service."@en .svc:serviceProvider rdf:type owl:ObjectProperty ;rdfs:domain svc:Service ;rdfs:range svc:ServiceProvider ;rdfs:comment "The provider of the service."@en .svc:ServiceProvider rdf:type owl:Class ;rdfs:subClassOf owl:Thing ;rdfs:comment "A class representing service providers."@en .svc:providerName rdf:type owl:DatatypeProperty ;rdfs:domain svc:ServiceProvider ;rdfs:range xsd:string ;rdfs:comment "The name of the service provider."@en .svc:providesService rdf:type owl:ObjectProperty ;rdfs:domain svc:ServiceProvider ;rdfs:range svc:Service ;rdfs:comment "Relates a service provider and a service that it provides."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix svc: <http://www.example.org/service#> .svc:Service rdf:type rdfs:Class ;             rdfs:label "Service"@en ;            rdfs:comment "A class representing the concept of service"@en .svc:ServiceProvider rdf:type rdfs:Class ;                  rdfs:label "Service Provider"@en ;                  rdfs:comment "A class representing the entity that provides a service"@en .svc:ServiceConsumer rdf:type rdfs:Class ;                  rdfs:label "Service Consumer"@en ;                  rdfs:comment "A class representing the entity that uses a service"@en .svc:serviceProvider rdf:type rdf:Property ;                   rdfs:domain svc:Service ;                   rdfs:range svc:ServiceProvider ;                   rdfs:label "service provider"@en ;                   rdfs:comment "The property linking a service with its provider"@en .svc:serviceConsumer rdf:type rdf:Property ;                   rdfs:domain svc:Service ;                   rdfs:range svc:ServiceConsumer ;                   rdfs:label "service consumer"@en ;                   rdfs:comment "The property linking a service with its consumer"@en .svc:serviceType rdf:type rdf:Property ;               rdfs:domain svc:Service ;               rdfs:range xsd:string ;               rdfs:label "service type"@en ;               rdfs:comment "The property indicating the type of a service"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .# Class definitionsex:Service rdf:type rdfs:Class .# Property definitionsex:hasServiceName rdf:type rdf:Property ;    rdfs:domain ex:Service ;    rdfs:range xsd:string .ex:hasServiceType rdf:type rdf:Property ;    rdfs:domain ex:Service ;    rdfs:range xsd:string .ex:providedBy rdf:type rdf:Property ;    rdfs:domain ex:Service ;    rdfs:range ex:Provider .ex:hasCost rdf:type rdf:Property ;    rdfs:domain ex:Service ;    rdfs:range xsd:decimal .# Individual instanceex:Service1 rdf:type ex:Service ;    ex:hasServiceName "Internet Service" ;    ex:hasServiceType "Telecommunication" ;    ex:providedBy ex:Provider1 ;    ex:hasCost "50.00"^^xsd:decimal .# Provider classex:Provider rdf:type rdfs:Class .# Provider individualex:Provider1 rdf:type ex:Provider .


@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix sv: <http://www.example.com/service#> .# Classessv:Service a owl:Class .sv:OnlineService a owl:Class ; rdfs:subClassOf sv:Service .sv:OfflineService a owl:Class ; rdfs:subClassOf sv:Service .sv:CustomerService a owl:Class ; rdfs:subClassOf sv:Service .sv:Provider a owl:Class .sv:Customer a owl:Class .# Propertiessv:hasCustomer a owl:ObjectProperty ;    rdfs:domain sv:Service ;    rdfs:range sv:Customer .sv:providedBy a owl:ObjectProperty ;    rdfs:domain sv:Service ;     rdfs:range sv:Provider .sv:hasLocation a owl:DatatypeProperty ;    rdfs:domain sv:OfflineService ;    rdfs:range xsd:string .# Individualssv:Netflix a sv:OnlineService ;    sv:providedBy sv:Netflix_Inc .sv:Netflix_Inc a sv:Provider .sv:PostalService a sv:OfflineService ;    sv:providedBy sv:PostOffice ;    sv:hasLocation "123 Anywhere Street"^^xsd:string .sv:PostOffice a sv:Provider .sv:Joe a sv:Customer ;    sv:hasCustomer sv:Netflix, sv:PostalService .



@prefix : <http://example.com/service#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .:Service a rdfs:Class ;  rdfs:comment "Class representing any type of service" .:serviceID a rdf:Property ;  rdfs:comment "Unique ID of the service" ;  rdfs:domain :Service ;  rdfs:range xsd:string .:serviceName a rdf:Property ;  rdfs:comment "Name of the service" ;  rdfs:domain :Service ;  rdfs:range xsd:string .:serviceDescription a rdf:Property ;  rdfs:comment "Description of the service" ;  rdfs:domain :Service ;  rdfs:range xsd:string .:serviceProvider a rdf:Property ;  rdfs:comment "The provider of the service" ;  rdfs:domain :Service ;  rdfs:range :ServiceProvider .:ServiceProvider a rdfs:Class ;  rdfs:comment "Class representing the provider of a service" .:providerID a rdf:Property ;  rdfs:comment "Unique ID of the service provider" ;  rdfs:domain :ServiceProvider ;  rdfs:range xsd:string .:providerName a rdf:Property ;  rdfs:comment "Name of the service provider" ;  rdfs:domain :ServiceProvider ;  rdfs:range xsd:string .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.@prefix service: <http://example.com/service#>.service:     rdf:type rdfs:Class ;    rdfs:comment "A type of professional or beneficial act"^^xsd:string ;    rdfs:label "Service"@en .service:TypeOfService     rdf:type rdfs:Class ;    rdfs:subClassOf service: ;    rdfs:comment "A subtype of service, more specifically defined by its nature or purpose"@en ;    rdfs:label "Type of Service"@en .service:ServiceProvider     rdf:type rdfs:Class ;    rdfs:comment "An individual, organization, or entity that provides a particular service"@en ;    rdfs:label "Service Provider"@en .service:ServiceUser     rdf:type rdfs:Class ;    rdfs:comment "An individual, organization, or entity that uses a particular service"@en ;    rdfs:label "Service User"@en .service:ServiceLocation     rdf:type rdfs:Class ;    rdfs:comment "The location where a service is performed or made available"@en ;    rdfs:label "Service Location"@en .service:ServiceCost    rdf:type rdfs:Class ;    rdfs:comment "The cost or price associated with a service"@en ;    rdfs:label "Service Cost"@en .service:provides     rdf:type rdf:Property ;    rdfs:domain service:ServiceProvider ;    rdfs:range service: ;    rdfs:comment "It links the provider to the service they offer"@en ;    rdfs:label "provides"@en .service:uses     rdf:type rdf:Property ;    rdfs:domain service:ServiceUser ;    rdfs:range service: ;    rdfs:comment "It links the user to the service they use"@en ;    rdfs:label "uses"@en .service:locatedAt     rdf:type rdf:Property ;    rdfs:domain service: ;    rdfs:range service:ServiceLocation ;    rdfs:comment "It links the service to the location where it is provided"@en ;    rdfs:label "located at"@en .service:hasCost     rdf:type rdf:Property ;    rdfs:domain service: ;    rdfs:range service:ServiceCost ;    rdfs:comment "It links the service to its cost"@en ;    rdfs:label "has cost"@en .


@prefix : <http://www.example.org/ontology#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .:Service a rdfs:Class ;  rdfs:comment "A class representing types of service" ;  rdfs:label "Service" .:ServiceProvider a rdfs:Class ;  rdfs:comment "A class representing entities that provide services" ;  rdfs:label "Service Provider" ;  rdfs:subClassOf :Service.:ServiceRecipient a rdfs:Class ;  rdfs:comment "A class representing entities that receive services" ;  rdfs:label "Service Recipient" ;  rdfs:subClassOf :Service.:hasService a rdf:Property ;  rdfs:domain :ServiceProvider ;  rdfs:range :Service ;  rdfs:subPropertyOf :provides.:provides a rdf:Property ;  rdfs:domain :ServiceProvider ;  rdfs:range :Service ;  rdfs:label "provides" ;  rdfs:comment "The provides relation between service provider and service".:receivesService a rdf:Property ;  rdfs:domain :ServiceRecipient ;  rdfs:range :Service ;  rdfs:label "receives service" ;  rdfs:comment "The relation between service recipients and the service they receive".:ServiceType a rdfs:Class ;  rdfs:comment "A class representing types of services provided" ;  rdfs:label "Service Type" ;  rdfs:subClassOf :Service.:hasServiceType a rdf:Property ;  rdfs:domain :Service ;  rdfs:range :ServiceType ;  rdfs:label "has service type" ;  rdfs:comment "A relationship showing the type of service provided".

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix service: <http://www.example.com/service#> .service:Service    a               rdfs:Class;    rdfs:comment   "A category of activities provided for the benefit of others."@en ;    rdfs:label     "Service"@en.service:name    a               rdf:Property;    rdfs:comment   "The name of the service."@en;    rdfs:domain    service:Service;    rdfs:range     xsd:string.service:description    a               rdf:Property;    rdfs:comment   "Detailed explanation about the service."@en;    rdfs:domain    service:Service;    rdfs:range     xsd:string.service:price    a               rdf:Property;    rdfs:comment   "The cost of the service."@en;    rdfs:domain    service:Service;    rdfs:range     xsd:double.service:provider    a               rdf:Property;    rdfs:comment   "The provider or company offering the service."@en;    rdfs:domain    service:Service;    rdfs:range     xsd:string.service:rating    a               rdf:Property;    rdfs:comment   "The rating of the service based on customer reviews."@en;    rdfs:domain    service:Service;    rdfs:range     xsd:double.


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix service: <http://example.com/service#> .service:Service rdf:type rdfs:Class ;    rdfs:label "Service"@en ;    rdfs:comment "An act of helping or doing work for someone"@en .service:name rdf:type rdf:Property ;    rdfs:label "service name"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:provider rdf:type rdf:Property ;    rdfs:label "service provider"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:startTime rdf:type rdf:Property ;    rdfs:label "start time"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:dateTime .service:endTime rdf:type rdf:Property ;    rdfs:label "end time"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:dateTime .service:cost rdf:type rdf:Property ;    rdfs:label "cost"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:float .service:rating rdf:type rdf:Property ;    rdfs:label "rating"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:float .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix service: <http://www.example.com/schema/service#> .service:Service a rdfs:Class ;    rdfs:label "Service"@en ;    rdfs:comment "A class representing services."@en .service:hasServiceName a rdf:Property ;    rdfs:label "has service name"@en ;    rdfs:comment "The property that indicates the name of the service."@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:hasServiceProvider a rdf:Property ;    rdfs:label "has service provider"@en ;    rdfs:comment "The property that indicates the provider of the service."@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:hasServiceCost a rdf:Property ;    rdfs:label "has service cost"@en ;    rdfs:comment "The property that indicates the cost of the service."@en ;    rdfs:domain service:Service ;    rdfs:range xsd:decimal .# Example instancesservice:Service1 a service:Service ;    service:hasServiceName "Internet Service" ;    service:hasServiceProvider "ISP Company" ;    service:hasServiceCost 20.00 .service:Service2 a service:Service ;    service:hasServiceName "Cleaning Service" ;    service:hasServiceProvider "Cleaning Company" ;    service:hasServiceCost 50.00 .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/> .ex:MechanicService        rdf:type rdfs:Class ;    rdfs:label "Mechanic service" ;    rdfs:comment "Service offered by mechanics for vehicle repair and maintenance." .ex:offeredBy    rdf:type rdf:Property ;    rdfs:domain ex:MechanicService ;    rdfs:range ex:Mechanic ;    rdfs:label "offered by" ;    rdfs:comment "The mechanic who offers the service." .ex:cost    rdf:type rdf:Property ;    rdfs:domain ex:MechanicService ;    rdfs:range xsd:float ;    rdfs:label "cost" ;    rdfs:comment "Cost of the mechanic service." .ex:serviceDescription    rdf:type rdf:Property ;    rdfs:domain ex:MechanicService ;    rdfs:range xsd:string ;    rdfs:label "service description" ;    rdfs:comment "Description of the mechanic service." .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ont: <http://example.com/ontology#> .ont:Service a rdfs:Class ;    rdfs:comment "Represents the concept of a Service provided by an entity" ;    rdfs:label "Service" .ont:provides a rdf:Property ;    rdfs:comment "Represents an entity providing a Service" ;    rdfs:label "provides" ;    rdfs:domain ont:Entity ;    rdfs:range ont:Service .ont:Entity a rdfs:Class ;    rdfs:comment "Represents entities that can provide Services" ;    rdfs:label "Entity" .ont:serviceName a rdf:Property ;    rdfs:comment "The name of the Service" ;    rdfs:label "serviceName" ;    rdfs:domain ont:Service ;    rdfs:range xsd:string .ont:serviceDescription a rdf:Property ;    rdfs:comment "Description of the Service" ;    rdfs:label "serviceDescription" ;    rdfs:domain ont:Service ;    rdfs:range xsd:string .
@prefix : <http://example.com/>@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>#Class definitions:Service  rdf:type  rdfs:Class .#Individual definitions:PaintingService rdf:type :Service .#Property Definitions:offersService rdf:type rdf:Property ;               rdfs:domain    :Organization ;               rdfs:range     :Service      .:servicePrice  rdf:type rdf:Property ;               rdfs:domain    :Service ;               rdfs:range     xsd:decimal  .#Sample usage:MyCompany  rdf:type       :Organization ;            :offersService :PaintingService . :PaintingService :servicePrice "500.00"^^xsd:decimal .
@prefix : <http://example.com/service#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Service a rdfs:Class ;    rdfs:comment "The class of all services" .:providesService a rdf:Property ;    rdfs:domain :Agent ;    rdfs:range :Service ;    rdfs:comment "Relates an agent to a service it provides" .:usesService a rdf:Property ;    rdfs:domain :Agent ;    rdfs:range :Service ;    rdfs:comment "Relates an agent to a service it uses" .:Agent a rdfs:Class ;    rdfs:comment "The class of all agents" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix : <http://example.com/ontology#> .:Service a owl:Class ;    rdfs:label "Service"@en ;    rdfs:comment "A class representing services, such as financial services, health services or subscription services."@en .:hasServiceProvider a owl:ObjectProperty ;    rdfs:domain :Service ;    rdfs:range :ServiceProvider ;    rdfs:label "has service provider"@en ;    rdfs:comment "A property representing the provider of a Service."@en .:hasServiceConsumer a owl:ObjectProperty ;    rdfs:domain :Service ;    rdfs:range :ServiceConsumer ;    rdfs:label "has service consumer"@en ;    rdfs:comment "A property representing the consumer of a Service."@en .:ServiceProvider a owl:Class ;    rdfs:label "Service Provider"@en ;    rdfs:comment "A class representing entities that provide Services."@en .:ServiceConsumer a owl:Class ;    rdfs:label "Service Consumer"@en ;    rdfs:comment "A class representing entities that consume Services."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix service: <http://example.com/service#> .# Defining Class 'Service'service:Service rdf:type rdfs:Class ;    rdfs:comment "A service of any kind that is provided to customers" ;    rdfs:label "Service"@en .# Defining Properties for 'Service'service:name rdf:type rdf:Property ;    rdfs:label "name"@en ;    rdfs:comment "The name of the service"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:string .service:price rdf:type rdf:Property ;    rdfs:label "price"@en ;    rdfs:comment "The price of the service"@en ;    rdfs:domain service:Service ;    rdfs:range xsd:decimal .service:provider rdf:type rdf:Property ;    rdfs:label "provider"@en ;    rdfs:comment "The entity that provides the service"@en ;    rdfs:domain service:Service ;    rdfs:range foaf:Organization .# Instance of 'Service'service:service1 rdf:type service:Service ;    service:name "Room Cleaning" ;    service:price "50.00" ;    service:provider foaf:organization1 .

