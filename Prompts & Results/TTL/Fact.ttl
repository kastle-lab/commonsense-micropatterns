
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://example.org/fact#> .fact:Fact rdf:type owl:Class .fact:hasFact rdf:type owl:ObjectProperty ;    rdfs:domain fact:Fact ;    rdfs:range fact:Fact .fact:isFactAbout rdf:type owl:ObjectProperty ;    rdfs:domain fact:Fact ;    rdfs:range owl:Thing .fact:hasFactText rdf:type owl:DatatypeProperty ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string .fact:hasFactSource rdf:type owl:ObjectProperty ;    rdfs:domain fact:Fact ;    rdfs:range owl:Thing .fact:FactSource rdf:type owl:Class .fact:hasSourceUrl rdf:type owl:DatatypeProperty ;    rdfs:domain fact:FactSource ;    rdfs:range xsd:anyURI .fact:hasSourceName rdf:type owl:DatatypeProperty ;    rdfs:domain fact:FactSource ;    rdfs:range xsd:string .fact:isSourceVerified rdf:type owl:DatatypeProperty ;    rdfs:domain fact:FactSource ;    rdfs:range xsd:boolean .fact:FactCategory rdf:type owl:Class .fact:isFactOfCategory rdf:type owl:ObjectProperty ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactCategory .fact:hasCategoryName rdf:type owl:DatatypeProperty ;    rdfs:domain fact:FactCategory ;    rdfs:range xsd:string .fact:hasDateVerified rdf:type owl:DatatypeProperty ;    rdfs:domain fact:Fact ;    rdfs:range xsd:date .




@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://www.example.com/fact#> .:Fact rdf:type rdfs:Class .:hasSubject rdf:type rdf:Property ;    rdfs:domain :Fact ;    rdfs:range :Subject .:hasObject rdf:type rdf:Property ;    rdfs:domain :Fact ;    rdfs:range :Object .:hasPredicate rdf:type rdf:Property ;    rdfs:domain :Fact ;    rdfs:range :Predicate .:hasTruthValue rdf:type rdf:Property ;    rdfs:domain :Fact ;    rdfs:range rdfs:Literal .:Subject rdf:type rdfs:Class .:Object rdf:type rdfs:Class .:Predicate rdf:type rdfs:Class .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix fact: <http://www.example.org/fact#> .fact:Fact a rdfs:Class ;    rdfs:label "Fact"@en ;    rdfs:comment "A statement that can be proven to be true or false"@en ;    rdfs:subClassOf rdfs:Resource .fact:FactType a rdfs:Class ;    rdfs:label "Fact Type"@en ;    rdfs:comment "Type of the fact, e.g., scientific, historical, etc."@en ;    rdfs:subClassOf rdfs:Resource .fact:FactTopic a rdfs:Class ;    rdfs:label "Fact Topic"@en ;    rdfs:comment "Topic of the fact, e.g., climate change, world war II, etc."@en ;    rdfs:subClassOf rdfs:Resource . fact:Source a rdfs:Class ;    rdfs:label "Source"@en ;    rdfs:comment "Entity from which information of fact is derived"@en ;    rdfs:subClassOf rdfs:Resource .fact:FactCheck a rdfs:Class ;    rdfs:label "Fact Check"@en ;    rdfs:comment "Process of verifying the accuracy and authenticity of a fact"@en ;    rdfs:subClassOf rdfs:Resource .fact:hasFactType a rdf:Property ;    rdfs:label "has fact type"@en ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactType .fact:hasFactTopic a rdf:Property ;    rdfs:label "has fact topic"@en ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactTopic . fact:hasSource a rdf:Property ;    rdfs:label "has source"@en ;    rdfs:domain fact:Fact ;    rdfs:range fact:Source .fact:isFactChecked a rdf:Property ;    rdfs:label "is fact checked"@en ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactCheck . 



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://www.example.org/fact_ontology#> .fact:Fact rdf:type rdfs:Class ;  rdfs:label "Fact"@en ;  rdfs:comment "A statement that can be proven to be true or false"@en .fact:hasSource rdf:type rdf:Property ;  rdfs:label "has source"@en ;  rdfs:comment "The source from which the fact is derived"@en ;  rdfs:range fact:Source ;  rdfs:domain fact:Fact .fact:Source rdf:type rdfs:Class ;  rdfs:label "Source"@en ;  rdfs:comment "The origin from which the fact is obtained"@en .fact:isVerified rdf:type rdf:Property ;  rdfs:label "is verified"@en ;  rdfs:comment "Indicates whether the fact is verified or not"@en ;  rdfs:range xsd:boolean ;  rdfs:domain fact:Fact .fact:factStatement rdf:type rdf:Property ;  rdfs:label "fact statement"@en ;  rdfs:comment "The statement representing fact"@en ;  rdfs:range xsd:string ;  rdfs:domain fact:Fact .fact:dateVerified rdf:type rdf:Property ;  rdfs:label "date verified"@en ;  rdfs:comment "The date when the fact was verified"@en ;  rdfs:range xsd:date ;  rdfs:domain fact:Fact .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix fact: <http://www.example.org/fact#> .#Defining Fact as a Classfact:Fact rdf:type owl:Class .#Properties related to Factfact:isTrue rdf:type owl:DatatypeProperty ;    rdfs:domain fact:Fact ;    rdfs:range xsd:boolean .fact:source rdf:type owl:DatatypeProperty ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string .fact:description rdf:type owl:DatatypeProperty ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string .fact:datePublished rdf:type owl:DatatypeProperty ;    rdfs:domain fact:Fact ;    rdfs:range xsd:date .fact:factChecker rdf:type owl:ObjectProperty ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactChecker .#Subclass of Factfact:VerifiedFact rdf:type owl:Class ;    rdfs:subClassOf fact:Fact .fact:UnverifiedFact rdf:type owl:Class ;    rdfs:subClassOf fact:Fact .#Fact-checker class and related propertyfact:FactChecker rdf:type owl:Class .fact:hasChecked rdf:type owl:ObjectProperty ;    rdfs:domain fact:FactChecker ;    rdfs:range fact:Fact .#Instancesfact:Fact_1 rdf:type fact:Fact ;    fact:isTrue "true"^^xsd:boolean ;    fact:source "National Geographic" ;    fact:description "The earth is the third planet from the sun." ;    fact:datePublished "2000-01-01"^^xsd:date ;    fact:factChecker fact:FactChecker_1 .fact:FactChecker_1 rdf:type fact:FactChecker ;    fact:hasChecked fact:Fact_1 .


@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix fact: <http://www.example.org/fact#> .fact:Fact     a rdfs:Class ;    rdfs:label "Fact" ;    rdfs:comment "An event or thing known to have happened or existed." .fact:source    a rdfs:Property ;    rdfs:label "source" ;    rdfs:comment "The origin from where the fact is derived or obtained." ;    rdfs:domain fact:Fact .fact:verificationStatus    a rdfs:Property ;    rdfs:label "verification status" ;    rdfs:comment "The state signifying whether a fact has been checked for truth and accuracy." ;    rdfs:domain fact:Fact .fact:hasEvidence     a rdfs:Property ;    rdfs:label "has evidence" ;    rdfs:comment "Relates a fact to the evidence supporting its truth." ;    rdfs:domain fact:Fact .fact:Evidence     a rdfs:Class ;    rdfs:label "Evidence" ;    rdfs:comment "Information or objects that support the truth or existence of a fact." .fact:Fact    rdfs:subClassOf fact:hasEvidence .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://example.org/fact#> .fact:Fact rdf:type rdfs:Class ;    rdfs:comment "Class representing a fact, a piece of information presented as having objective reality."@en .fact:hasValue rdf:type rdf:Property ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string ;    rdfs:comment "Property representing the value of a fact."@en .fact:hasSource rdf:type rdf:Property ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string ;    rdfs:comment "Property representing the source of a fact."@en .fact:hasDate rdf:type rdf:Property ;    rdfs:domain fact:Fact ;    rdfs:range xsd:date ;    rdfs:comment "Property representing the date when the fact was recorded."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://example.org/fact#> .fact:Fact rdf:type rdfs:Class .fact:hasSource rdf:type rdf:Property ;             rdfs:domain fact:Fact ;             rdfs:range xsd:string .fact:hasStatement rdf:type rdf:Property ;                 rdfs:domain fact:Fact ;                 rdfs:range xsd:string . fact:hasVeracity rdf:type rdf:Property ;                rdfs:domain fact:Fact ;                rdfs:range xsd:boolean . # Example of usage:fact:fact1 rdf:type fact:Fact ;          fact:hasSource "World Health Organization"^^xsd:string ;          fact:hasStatement "Smoking is a leading cause of cancer."^^xsd:string ;          fact:hasVeracity "true"^^xsd:boolean .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .#Definition of a Fact classex:Fact rdf:type rdfs:Class .ex:Fact rdfs:label "Fact" ;        rdfs:comment "A statement that can be proven to be true or false".#Properties for the Fact classex:hasStatement rdf:type rdf:Property ;               rdfs:domain ex:Fact ;               rdfs:range xsd:string ;               rdfs:comment "The statement of the fact" .ex:hasEvidence rdf:type rdf:Property ;               rdfs:domain ex:Fact ;               rdfs:range xsd:string ;               rdfs:comment "The evidence that supports the fact" .ex:isTrue rdf:type rdf:Property ;          rdfs:domain ex:Fact ;          rdfs:range xsd:boolean ;          rdfs:comment "Indicates whether the fact is true or not" . #Example instance of a Factex:fact1 rdf:type ex:Fact ;         ex:hasStatement "The earth revolves around the sun" ;         ex:hasEvidence "Scientific observations of celestial bodies" ;         ex:isTrue "True"^^xsd:boolean .


@prefix : <http://example.org/#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:Fact rdf:type rdf:Class .:hasAuthor rdf:type rdf:Property ;    rdf:domain :Fact ;    rdf:range :Author .:hasCategory rdf:type rdf:Property ;    rdf:domain :Fact ;    rdf:range :Category .    :hasDate rdf:type rdf:Property ;    rdf:domain :Fact ;    rdf:range xsd:date .    :Author rdf:type rdf:Class .:Category rdf:type rdf:Class .:fact_1 rdf:type :Fact ;    :hasAuthor :author_1 ;    :hasCategory :category_1 ;    :hasDate "2022-02-03"^^xsd:date .:author_1 rdf:type :Author ;    rdf:label "John Doe" .:category_1 rdf:type :Category ;    rdf:label "Science" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://example.org/fact#> .fact:Fact  rdf:type rdfs:Class ;  rdfs:comment "A representation of a concrete piece of information or known truth." ;  rdfs:label "Fact" .fact:isVerified  rdf:type rdf:Property ;  rdfs:domain fact:Fact ;  rdfs:range xsd:boolean ;  rdfs:comment "Indicates whether the fact has been verified as true or false." ;  rdfs:label "is verified" .fact:source  rdf:type rdf:Property ;  rdfs:domain fact:Fact ;  rdfs:range rdfs:Resource ;  rdfs:comment "The original source where the fact was obtained or referenced from." ;  rdfs:label "source" .fact:relatedTo  rdf:type rdf:Property ;  rdfs:domain fact:Fact ;  rdfs:range rdfs:Resource ;  rdfs:comment "Indicates the events, entities or concepts that this fact is related to." ;  rdfs:label "related to" .fact:dateVerified  rdf:type rdf:Property ;  rdfs:domain fact:Fact ;  rdfs:range xsd:date ;  rdfs:comment "The date when the fact was verified." ;  rdfs:label "date verified" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/ontology#> .ex:Fact    rdf:type rdfs:Class ;    rdfs:comment "A representation of a piece of information that is an objective reality or valid statement"^^xsd:string ;    rdfs:label "Fact"^^xsd:string .ex:assertedBy    rdf:type rdf:Property ;    rdfs:comment "Denotes the entity that asserts a fact"^^xsd:string ;    rdfs:label "assertedBy"^^xsd:string ;    rdfs:domain ex:Fact ;    rdfs:range rdf:resource .ex:validity    rdf:type rdf:Property ;    rdfs:comment "States the validity of a fact (True/False)"^^xsd:string ;    rdfs:label "validity"^^xsd:string ;    rdfs:domain ex:Fact ;    rdfs:range xsd:boolean .ex:Fact    rdfs:subClassOf rdf:Statement .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://www.example.com/fact#> .fact:Fact a rdfs:Class ;    rdfs:label "Fact" ;    rdfs:comment "A piece of information that is verified and true." .fact:hasId a rdf:Property ;    rdfs:label "hasId" ;    rdfs:comment "The unique identifier of a Fact." ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string .fact:hasSource a rdf:Property ;    rdfs:label "hasSource" ;    rdfs:comment "The verified source of the Fact." ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string .fact:isAbout a rdf:Property ;    rdfs:label "isAbout" ;    rdfs:comment "What the Fact is about, the subject of the Fact." ;    rdfs:domain fact:Fact ;    rdfs:range xsd:string .fact:FactDescription a rdfs:Class ;    rdfs:label "FactDescription" ;    rdfs:comment "Detailed description about the Fact." .fact:hasDescription a rdf:Property ;    rdfs:label "hasDescription" ;    rdfs:comment "The detailed description of the Fact." ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactDescription .

