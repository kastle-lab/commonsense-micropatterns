@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix fact: <http://example.org/fact#> .# Declare the class 'Fact'fact:Fact a owl:Class ;    rdfs:label "Fact" ;    rdfs:comment "Facts represent objective reality." .# Link Facts to a 'Source' class from where the facts were obtained.fact:hasSource a owl:ObjectProperty ;    rdfs:label "has source" ;    rdfs:comment "A link between a fact and its source." ;    rdfs:domain fact:Fact ;    rdfs:range fact:Source .# Link Facts to a 'FactChecker' class which validates them.fact:checkedBy a owl:ObjectProperty ;    rdfs:label "checked by" ;    rdfs:comment "A link between a fact and its checker." ;    rdfs:domain fact:Fact ;    rdfs:range fact:FactChecker .# Create subclasses 'VerifiedFact' and 'UnverifiedFact'fact:VerifiedFact a owl:Class ;    rdfs:label "Verified fact" ;    rdfs:comment "Represents facts that are authenticated." ;    rdfs:subClassOf fact:Fact .fact:UnverifiedFact a owl:Class ;    rdfs:label "Unverified fact" ;    rdfs:comment "Represents facts that are yet to be authenticated." ;    rdfs:subClassOf fact:Fact .# Declare the 'Source' and 'FactChecker' classesfact:Source a owl:Class ;    rdfs:label "Source" .fact:FactChecker a owl:Class ;    rdfs:label "Fact checker" .

@prefix : <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Fact a rdf:Class ;  rdfs:comment "A proposition that can be proven true or false."^^rdf:XMLLiteral ;  rdfs:label "Fact"^^rdf:XMLLiteral .:hasSubject a rdf:Property ;  rdfs:comment "The subject of a fact."^^rdf:XMLLiteral ;  rdfs:label "hasSubject"^^rdf:XMLLiteral ;  rdfs:domain :Fact .:hasPredicate a rdf:Property ;  rdfs:comment "The predicate of a fact."^^rdf:XMLLiteral ;  rdfs:label "hasPredicate"^^rdf:XMLLiteral ;  rdfs:domain :Fact .:hasObject a rdf:Property ;  rdfs:comment "The object of a fact."^^rdf:XMLLiteral ;  rdfs:label "hasObject"^^rdf:XMLLiteral ;  rdfs:domain :Fact .

@prefix : <http://example.org/#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .# Define 'Fact' class:Fact a rdfs:Class .# Define 'hasSubject' property related to 'Fact':hasSubject a rdf:Property ;    rdfs:domain :Fact ;    rdfs:range :Entity .# Define 'hasPredicate' property related to 'Fact':hasPredicate a rdf:Property ;    rdfs:domain :Fact ;    rdfs:range :Relation .# Define 'hasObject' property related to 'Fact':hasObject a rdf:Property ;    rdfs:domain :Fact ;    rdfs:range :Entity .# Define 'hasTruthValue' property related to 'Fact':hasTruthValue a rdf:Property ;    rdfs:domain :Fact ;    rdfs:range rdfs:Literal .

