
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix game: <http://example.org/game#> .game:Game rdf:type rdfs:Class ;    rdfs:label "Game"@en ;    rdfs:comment "A class representing games."@en ;    rdfs:subClassOf rdfs:Resource .game:VideoGame rdf:type rdfs:Class ;    rdfs:label "Video Game"@en ;    rdfs:comment "A game played by electronically manipulating images produced by a computer program on a television screen or other display screen."@en ;    rdfs:subClassOf game:Game .game:BoardGame rdf:type rdfs:Class ;    rdfs:label "Board Game"@en ;    rdfs:comment "A game played on a specifically designed board"@en ;    rdfs:subClassOf game:Game .game:CardGame rdf:type rdfs:Class ;    rdfs:label "Card Game"@en ;    rdfs:comment "A game using playing cards as the primary device"@en ;    rdfs:subClassOf game:Game .game:name rdf:type owl:DatatypeProperty ;    rdfs:domain game:Game ;    rdfs:range xsd:string ;    rdfs:label "name"@en ;    rdfs:comment "Denotes the name of the game."@en .game:hasPlayer rdf:type owl:ObjectProperty ;    rdfs:domain game:Game ;    rdfs:range game:Player ;    rdfs:label "has player"@en ;    rdfs:comment "Relates a game to its players."@en .game:Player rdf:type rdfs:Class ;    rdfs:label "Player"@en ;    rdfs:comment "Represents a person who plays a game."@en .game:playerName rdf:type owl:DatatypeProperty ;    rdfs:domain game:Player ;    rdfs:range xsd:string ;    rdfs:label "player name"@en ;    rdfs:comment "The name of the player."@en .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix game: <http://example.org/ontology/game#> .game:Game a rdfs:Class ;    rdfs:label "Game" ;    rdfs:comment "A structured form of play, usually undertaken for entertainment or fun." .game:VideoGame a rdfs:Class ;    rdfs:label "Video Game" ;    rdfs:comment "Electronic game that involves interaction with a user interface to generate visual feedback on a video device." ;    rdfs:subClassOf game:Game .game:BoardGame a rdfs:Class ;    rdfs:label "Board Game" ;    rdfs:comment "Game that involves counters or pieces moved or placed on a pre-marked surface or 'board', according to a set of rules." ;    rdfs:subClassOf game:Game .game:CardGame a rdfs:Class ;    rdfs:label "Card Game" ;    rdfs:comment "Any game using playing cards as the primary device with which the game is played." ;    rdfs:subClassOf game:Game .game:rule rdfs:Class ;    rdfs:label "Game rule" ;    rdfs:comment "Regulations to which the players must agree to follow for a game." .game:genre rdfs:Class ;    rdfs:label "Game genre" ;    rdfs:comment "Specific category of games related by similar gameplay characteristics." .game:hasRule a rdfs:Property ;    rdfs:label "has rule" ;    rdfs:domain game:Game ;    rdfs:range game:rule ;    rdfs:comment "Property that links a game to its rules." .game:hasGenre a rdfs:Property ;    rdfs:label "has genre" ;    rdfs:domain game:Game ;    rdfs:range game:genre ;    rdfs:comment "Property that links a game to its genre." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org#> .ex:Game  rdf:type rdfs:Class ;  rdfs:label "Game" ;  rdfs:comment "Class representing the concept of games" .ex:hasName  rdf:type rdf:Property ;  rdfs:domain ex:Game ;  rdfs:range xsd:string ;  rdfs:label "has name" ;  rdfs:comment "Relates a game to its name" .ex:hasGenre  rdf:type rdf:Property ;  rdfs:domain ex:Game ;  rdfs:range ex:Genre ;  rdfs:label "has genre" ;  rdfs:comment "Relates a game to its genre" .ex:Genre  rdf:type rdfs:Class ;  rdfs:label "Genre" ;  rdfs:comment "Class representing the genre of a game" .ex:Developer  rdf:type rdfs:Class ;  rdfs:label "Developer" ;  rdfs:comment "Class representing the developer of a game" .ex:developedBy  rdf:type rdf:Property ;  rdfs:domain ex:Game ;  rdfs:range ex:Developer ;  rdfs:label "developed by" ;  rdfs:comment "Relates a game to its developer" .ex:hasPlatform  rdf:type rdf:Property ;  rdfs:domain ex:Game ;  rdfs:range ex:Platform ;  rdfs:label "has platform" ;  rdfs:comment "Relates a game to the platform it can be played on" .ex:Platform  rdf:type rdfs:Class ;  rdfs:label "Platform" ;  rdfs:comment "Class representing the platform a game can be played on" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://www.example.org/game#> .game:Game rdf:type rdfs:Class ;    rdfs:label "Game" ;    rdfs:comment "A game is a structured form of play" .game:hasName rdf:type rdf:Property ;    rdfs:label "has name" ;    rdfs:domain game:Game ;    rdfs:range xsd:string .game:hasGenre rdf:type rdf:Property ;    rdfs:label "has genre" ;    rdfs:domain game:Game ;    rdfs:range game:Genre .game:Genre rdf:type rdfs:Class ;    rdfs:label "Genre" ;    rdfs:comment "Category or type of game" .game:Adventure rdf:type game:Genre ;    rdfs:label "Adventure" ;    rdfs:comment "Adventure game involves an interactive story, puzzle-solving and exploration" .game:Action rdf:type game:Genre ;    rdfs:label "Action" ;    rdfs:comment "Action game is a game that focuses on physical challenges" .game:hasPlatform rdf:type rdf:Property ;    rdfs:label "has platform" ;    rdfs:domain game:Game ;    rdfs:range game:Platform .game:Platform rdf:type rdfs:Class ;    rdfs:label "Platform" ;    rdfs:comment "Platform on which a game can be played" .game:PC rdf:type game:Platform ;    rdfs:label "PC" ;    rdfs:comment "Personal Computer" .game:Xbox rdf:type game:Platform ;    rdfs:label "Xbox" ;    rdfs:comment "Microsoft Xbox Console" .game:hasRating rdf:type rdf:Property ;    rdfs:label "has rating" ;    rdfs:domain game:Game ;    rdfs:range xsd:float .game:hasDeveloper rdf:type rdf:Property ;    rdfs:label "has developer" ;    rdfs:domain game:Game ;    rdfs:range game:Developer .game:Developer rdf:type rdfs:Class ;    rdfs:label "Developer" ;    rdfs:comment "Company that develops games" .game:EA rdf:type game:Developer ;    rdfs:label "EA" ;    rdfs:comment "Electronic Arts Inc." .game:Ubisoft rdf:type game:Developer ;    rdfs:label "Ubisoft" ;    rdfs:comment "Ubisoft Entertainment SA" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://www.example.org/Game#> .game:Game rdf:type rdfs:Class .game:name rdf:type rdf:Property ;    rdfs:domain game:Game ;    rdfs:range xsd:string .game:releaseYear rdf:type rdf:Property ;    rdfs:domain game:Game ;    rdfs:range xsd:year .game:genre rdf:type rdf:Property ;    rdfs:domain game:Game ;    rdfs:range game:Genre .game:Genre rdf:type rdfs:Class .    game:developer rdf:type rdf:Property ;    rdfs:domain game:Game ;    rdfs:range game:Developer .    game:Developer rdf:type rdfs:Class .game:GameReview rdf:type rdf:Class .    game:reviewOf rdf:type rdf:Property ;    rdfs:domain game:GameReview ;    rdfs:range game:Game .    game:reviewRating rdf:type rdf:Property ;    rdfs:domain game:GameReview ;    rdfs:range xsd:int .    game:reviewText rdf:type rdf:Property ;    rdfs:domain game:GameReview ;    rdfs:range xsd:string .    game:player rdf:type rdf:Property ;    rdfs:domain game:Game ;    rdfs:range game:Player .game:Player rdf:type rdfs:Class .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@base <http://example.com/game-ontology#> .<http://example.com/game-ontology#>  a owl:Ontology ;  rdfs:comment "An ontology about Game" .<#Game>  a owl:Class ;  rdfs:comment "The class of all games" .<#VideoGame>  a owl:Class ;  rdfs:subClassOf <#Game> ;  rdfs:comment "The subclass of games that are video games" .<#BoardGame>  a owl:Class ;  rdfs:subClassOf <#Game> ;  rdfs:comment "The subclass of games that are board games" .<#hasPlayer>  a owl:ObjectProperty ;  rdfs:domain <#Game> ;  rdfs:range <#Person> ;  rdfs:comment "The relation between a game and its players" .<#Person>  a owl:Class ;  rdfs:comment "The class of all people, who can be players in games" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://www.example.org/game/> .game:     rdf:type rdfs:Class ;    rdfs:label "Game"@en ;    rdfs:comment "An class representing a game in general."@en .game:name    rdf:type rdf:Property ;    rdfs:domain game: ;    rdfs:range xsd:string ;    rdfs:label "game name"@en ;    rdfs:comment "The name of the game."@en .game:developer    rdf:type rdf:Property ;    rdfs:domain game: ;    rdfs:range xsd:string ;    rdfs:label "game developer"@en ;    rdfs:comment "The developer of the game."@en .game:platform    rdf:type rdf:Property ;    rdfs:domain game: ;    rdfs:range xsd:string ;    rdfs:label "game platform"@en ;    rdfs:comment "The platform the game is available on."@en .    game:genre    rdf:type rdf:Property ;    rdfs:domain game: ;    rdfs:range xsd:string ;    rdfs:label "game genre"@en ;    rdfs:comment "The genre of the game."@en .game:releaseDate    rdf:type rdf:Property ;    rdfs:domain game: ;    rdfs:range xsd:date ;    rdfs:label "game release date"@en ;    rdfs:comment "The release date of the game."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix game: <http://example.com/game#> .game:Game rdf:type rdfs:Class ;          rdfs:comment "A higher concept of a game" .game:title rdf:type owl:DatatypeProperty ;           rdfs:domain game:Game ;           rdfs:range xsd:string ;           rdfs:comment "The title of the game" .game:genre rdf:type owl:DatatypeProperty ;           rdfs:domain game:Game ;           rdfs:range xsd:string ;           rdfs:comment "The genre of the game" . game:publisher rdf:type owl:ObjectProperty ;               rdfs:domain game:Game ;               rdfs:range game:Publisher ;               rdfs:comment "The publisher of the game" .        game:Publisher rdf:type rdfs:Class ;               rdfs:comment "The entity that publishes a game" .game:name rdf:type owl:DatatypeProperty ;          rdfs:domain game:Publisher ;          rdfs:range xsd:string ;          rdfs:comment "The name of the publisher" .game:releaseDate rdf:type owl:DatatypeProperty ;                 rdfs:domain game:Game ;                 rdfs:range xsd:date ;                 rdfs:comment "The release date of the game" .game:platform rdf:type owl:DatatypeProperty ;              rdfs:domain game:Game ;              rdfs:range xsd:string ;              rdfs:comment "The platform for which the game is designed" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://example.org/game/> .# Classesgame:Game rdf:type rdfs:Class ;    rdfs:label "Game" ;    rdfs:comment "A structured form of play, usually undertaken for entertainment or fun."@en .game:BoardGame rdf:type rdfs:Class ;    rdfs:label "Board Game" ;    rdfs:comment "A game that involves counters or pieces moved or placed on a pre-marked surface or 'board'."@en ;    rdfs:subClassOf game:Game .game:VideoGame rdf:type rdfs:Class ;    rdfs:label "Video Game" ;    rdfs:comment "An electronic game that involves interaction with a user interface to generate visual feedback on a video device such as a TV screen or computer monitor."@en ;    rdfs:subClassOf game:Game .# Propertiesgame:hasPlayer rdf:type rdf:Property ;    rdfs:label "has player" ;    rdfs:comment "The player of the game."@en ;    rdfs:domain game:Game ;    rdfs:range xsd:string .game:hasScore rdf:type rdf:Property ;    rdfs:label "has score" ;    rdfs:comment "The score of the game."@en ;    rdfs:domain game:Game ;    rdfs:range xsd:integer .game:hasRules rdf:type rdf:Property ;    rdfs:label "has rules" ;    rdfs:comment "The rules of the game."@en ;    rdfs:domain game:Game ;    rdfs:range xsd:string .game:hasTitle rdf:type rdf:Property ;    rdfs:label "has title" ;    rdfs:comment "The title of the game."@en ;    rdfs:domain game:Game ;    rdfs:range xsd:string .# Instancegame:Monopoly rdf:type game:BoardGame ;    game:hasTitle "Monopoly"@en ;    game:hasPlayer "John Doe" ;    game:hasScore 250 ;    game:hasRules "The players move around the board buying or trading properties, developing their properties with houses and hotels."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://example.org/game#> .# Class Definitionsgame:Game a rdfs:Class ;  rdfs:comment "A class representing a type of game." .game:BoardGame a rdfs:Class ;  rdfs:subClassOf game:Game ;  rdfs:comment "A subclass of Game representing board games." .game:VideoGame a rdfs:Class ;  rdfs:subClassOf game:Game ;  rdfs:comment "A subclass of Game representing video games." .game:Player a rdfs:Class ;  rdfs:comment "A class representing a Player involved in a game." .# Property Definitionsgame:hasPlayer a rdf:Property ;  rdfs:domain game:Game ;  rdfs:range game:Player ;  rdfs:comment "A property linking a game to its players." .game:gameTitle a rdf:Property ;  rdfs:domain game:Game ;  rdfs:range xsd:string ;  rdfs:comment "A property that gives the title of a game." .game:gameGenre a rdf:Property ;  rdfs:domain game:Game ;  rdfs:range xsd:string ;  rdfs:comment "A property that provides the genre of a game." .game:playerName a rdf:Property ;  rdfs:domain game:Player ;  rdfs:range xsd:string ;  rdfs:comment "A property that gives the name of a player." .  game:plays a rdf:Property ;  rdfs:domain game:Player ;  rdfs:range game:Game ;  rdfs:comment "A property linking a player to the games they play." . # This is a simple RDF representing games and players ontology.# It defines two subclasses of the Game class - BoardGame and VideoGame, # and properties related to games like gameTitle, gameGenre etc.# and properties related to players like playerName. It also expresses # relationships between games and players.


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix game: <http://example.org/game#> .game:Game a owl:Class ;  rdfs:label "Game"@en ;  rdfs:comment "A structured form of play, typically for enjoyment and sometimes used as an educational tool"@en .game:genre a owl:DatatypeProperty ;  rdfs:label "genre"@en ;  rdfs:domain game:Game ;  rdfs:range xsd:string ;  rdfs:comment "The genre of the game"@en .game:platform a owl:DatatypeProperty ;  rdfs:label "platform"@en ;  rdfs:domain game:Game ;  rdfs:range xsd:string ;  rdfs:comment "The platform on which the game can be played"@en .game:developer a owl:DatatypeProperty ;  rdfs:label "developer"@en ;  rdfs:domain game:Game ;  rdfs:range xsd:string ;  rdfs:comment "The developer of the game"@en .game:releaseDate a owl:DatatypeProperty ;  rdfs:label "release date"@en ;  rdfs:domain game:Game ;  rdfs:range xsd:date ;  rdfs:comment "The release date of the game"@en .game:Player a owl:Class ;  rdfs:label "Player"@en ;  rdfs:comment "A person who plays games" .game:plays a owl:ObjectProperty ;  rdfs:label "plays"@en ;  rdfs:domain game:Player ;  rdfs:range game:Game ;  rdfs:comment "The game that the player plays"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://example.com/game#> .game:Game  rdf:type rdfs:Class ;  rdfs:comment "The class of all games" .game:hasName  rdf:type rdf:Property ;  rdfs:domain game:Game ;  rdfs:range xsd:string ;  rdfs:comment "The name of the game" .game:hasGenre  rdf:type rdf:Property ;  rdfs:domain game:Game ;  rdfs:range game:Genre ;  rdfs:comment "The genre of the game" .game:hasPlayer  rdf:type rdf:Property ;  rdfs:domain game:Game ;  rdfs:range game:Player ;  rdfs:comment "The player of the game" .game:Genre  rdf:type rdfs:Class ;  rdfs:comment "The class of all game genres" .game:Player  rdf:type rdfs:Class ;  rdfs:comment "The class of all game players" .game:hasAgeRating  rdf:type rdf:Property ;  rdfs:domain game:Game ;  rdfs:range game:AgeRating ;  rdfs:comment "The age rating of the game" .game:AgeRating  rdf:type rdfs:Class ;  rdfs:comment "The class of all age ratings for games" .game:isSinglePlayer  rdf:type rdf:Property ;  rdfs:domain game:Game ;  rdfs:range xsd:boolean ;  rdfs:comment "Indicates whether the game is single player" .# You can also define individuals and instances of classes like so:game:Action  rdf:type game:Genre ;  rdfs:label "Action" .game:Minecraft  rdf:type game:Game ;  game:hasName "Minecraft" ;  game:hasGenre game:Action ;  game:isSinglePlayer "true"^^xsd:boolean .`  In this ontology, we define the concept of a 'Game', with properties such as 'hasName', 'hasGenre', 'hasPlayer', 'hasAgeRating' and 'isSinglePlayer'. The classes 'Genre', 'Player' and 'AgeRating' are defined as well. We have also instantiated a few examples with the 'Action' genre and a game called 'Minecraft'.


@prefix : <http://example.org/game#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:Game a rdfs:Class ;  rdfs:label "Game" .:hasTitle a rdfs:Property ;  rdfs:label "hasTitle" ;  rdfs:domain :Game ;  rdfs:range xsd:string .:hasGenre a rdfs:Property ;  rdfs:label "hasGenre" ;  rdfs:domain :Game ;  rdfs:range :GameGenre .:hasRating a rdfs:Property ;  rdfs:label "hasRating" ;  rdfs:domain :Game ;  rdfs:range xsd:float .:GameGenre a rdfs:Class ;  rdfs:label "Game Genre" .:Action a :GameGenre ;  rdfs:label "Action" .:Adventure a :GameGenre ;  rdfs:label "Adventure" .:RPG a :GameGenre ;  rdfs:label "RPG" .:Strategy a :GameGenre ;  rdfs:label "Strategy" .:exampleGame a :Game ;  :hasTitle "Example Game Title" ;  :hasGenre :Action ;  :hasRating "8.5"^^xsd:float .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://www.example.com/game#> .game:Game rdf:type owl:Class ;    rdfs:comment "A class representing games" ;    rdfs:label "Game" .game:hasTitle rdf:type owl:DatatypeProperty ;    rdfs:range xsd:string ;    rdfs:domain game:Game ;    rdfs:comment "The title of the game" ;    rdfs:label "hasTitle" .game:hasDeveloper rdf:type owl:ObjectProperty ;    rdfs:range game:Developer ;    rdfs:domain game:Game ;    rdfs:comment "The developer of the game" ;    rdfs:label "hasDeveloper" .game:Developer rdf:type owl:Class ;    rdfs:comment "A class representing game developers" ;    rdfs:label "Developer" .game:hasGenre rdf:type owl:ObjectProperty ;    rdfs:range game:Genre ;    rdfs:domain game:Game ;    rdfs:comment "The genre of the game" ;    rdfs:label "hasGenre" .game:Genre rdf:type owl:Class ;    rdfs:comment "A class representing game genres" ;    rdfs:label "Genre" .game:hasPlatform rdf:type owl:ObjectProperty ;    rdfs:range game:Platform ;    rdfs:domain game:Game ;    rdfs:comment "The platform for which the game is developed" ;    rdfs:label "hasPlatform" .game:Platform rdf:type owl:Class ;    rdfs:comment "A class representing game platforms" ;    rdfs:label "Platform" .game:hasReleaseDate rdf:type owl:DatatypeProperty ;    rdfs:range xsd:date ;    rdfs:domain game:Game ;    rdfs:comment "The release date of the game" ;    rdfs:label "hasReleaseDate" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix game: <http://www.example.com/game#> .game:Game rdf:type rdfs:Class .game:name rdf:type rdf:Property ;          rdfs:domain game:Game ;          rdfs:range xsd:string .game:Player rdf:type rdfs:Class .game:playsGame rdf:type rdf:Property ;              rdfs:domain game:Player ;              rdfs:range game:Game .game:GamePlatform rdf:type rdfs:Class .game:isAvailableOn rdf:type rdf:Property ;                  rdfs:domain game:Game ;                  rdfs:range game:GamePlatform .                  game:GameDeveloper rdf:type rdfs:Class .game:developedBy rdf:type rdf:Property ;                rdfs:domain game:Game ;                rdfs:range game:GameDeveloper .game:releaseDate rdf:type rdf:Property ;                rdfs:domain game:Game ;                rdfs:range xsd:date .game:genre rdf:type rdf:Property ;          rdfs:domain game:Game ;          rdfs:range xsd:string .game:hasRating rdf:type rdf:Property ;              rdfs:domain game:Game ;              rdfs:range xsd:float .game:GameReview rdf:type rdfs:Class .game:hasReview rdf:type rdf:Property ;              rdfs:domain game:Game ;              rdfs:range game:GameReview .game:reviewText rdf:type rdf:Property ;               rdfs:domain game:GameReview ;               rdfs:range xsd:string .game:reviewScore rdf:type rdf:Property ;                rdfs:domain game:GameReview ;                rdfs:range xsd:float .



