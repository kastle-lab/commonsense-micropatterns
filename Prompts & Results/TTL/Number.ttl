@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix number: <http://example.org/number#> .number:    rdf:type rdfs:Class ;    rdfs:comment "The class of numbers." .number:Natural    rdf:type rdfs:Class ;    rdfs:subClassOf number: ;    rdfs:comment "The class of natural numbers." .number:Integer    rdf:type rdfs:Class ;    rdfs:subClassOf number: ;    rdfs:comment "The class of integer numbers." .number:Real    rdf:type rdfs:Class ;    rdfs:subClassOf number: ;    rdfs:comment "The class of real numbers." .number:Complex    rdf:type rdfs:Class ;    rdfs:subClassOf number: ;    rdfs:comment "The class of complex numbers." .number:value    rdf:type rdf:Property ;    rdfs:domain number: ;    rdfs:range xsd:decimal ;    rdfs:comment "The value of a number item." .number:isPositive    rdf:type rdf:Property ;    rdfs:domain number:Integer ;    rdfs:range xsd:boolean ;    rdfs:comment "The property indicating whether an integer number item is positive or not." .number:isEven    rdf:type rdf:Property ;    rdfs:domain number:Integer ;    rdfs:range xsd:boolean ;    rdfs:comment "The property indicating whether an integer number item is even or not." .number:isIrrational    rdf:type rdf:Property ;    rdfs:domain number:Real ;    rdfs:range xsd:boolean ;    rdfs:comment "The property indicating whether a real number item is irrational or not." .number:isImaginary    rdf:type rdf:Property ;    rdfs:domain number:Complex ;    rdfs:range xsd:boolean ;    rdfs:comment "The property indicating whether a complex number item is imaginary or not." .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix number: <http://example.org/ontology/number#> .number:Number   rdf:type owl:Class ;  rdfs:label "Number"@en ;  rdfs:comment "Represents a number."@en .number:RealNumber  rdf:type owl:Class ;  rdfs:label "Real number"@en ;  rdfs:subClassOf number:Number ;  rdfs:comment "Represents a real number, can be both rational and irrational."@en .number:ComplexNumber  rdf:type owl:Class ;  rdfs:label "Complex number"@en ;  rdfs:subClassOf number:Number ;  rdfs:comment "Represents a complex number, a number that can be expressed in the form a + bi."@en .number:isPartOf  rdf:type owl:ObjectProperty ;  rdfs:label "is part of"@en ;  rdfs:domain number:Number ;  rdfs:range number:Number ;  rdfs:comment "Indicates that a number is part of another number, in segments or fractions for example."@en .number:hasValue  rdf:type owl:DatatypeProperty ;  rdfs:label "has value"@en ;  rdfs:domain number:Number ;  rdfs:range rdfs:Literal ;  rdfs:comment "Represents the actual numerical value of a number."@en .number:isGreaterThan  rdf:type owl:TransitiveProperty ;  rdfs:label "is greater than"@en ;  rdfs:domain number:Number ;  rdfs:range number:Number ;  rdfs:comment "Indicates a number is greater than another number."@en .number:isLessThan  rdf:type owl:TransitiveProperty ;  rdfs:label "is less than"@en ;  rdfs:domain number:Number ;  rdfs:range number:Number ;  rdfs:comment "Indicates a number is less than another number."@en .  number:isEqualTo  rdf:type owl:SymmetricProperty ;  rdfs:label "is equal to"@en ;  rdfs:domain number:Number ;  rdfs:range number:Number ;  rdfs:comment "Indicates a number is equal to another number."@en .



