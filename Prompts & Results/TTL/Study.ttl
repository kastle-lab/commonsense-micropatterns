

@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix study: <http://www.example.com/study#> .<http://www.example.com/study>  rdf:type owl:Ontology ;  rdfs:comment "An ontology focused on the concept of Study" ;  rdfs:label "StudyOntology" .study:Study  rdf:type owl:Class ;  rdfs:label "Study" ;  rdfs:comment "The concept of study or learning activity" .study:Subject  rdf:type owl:Class ;  rdfs:label "Subject" ;  rdfs:comment "The subject or topic associated with a study" .study:Student  rdf:type owl:Class ;  rdfs:label "Student" ;  rdfs:comment "The individual who engage in the activity of study" .study:reads  rdf:type owl:ObjectProperty ;  rdfs:domain study:Student ;  rdfs:range study:Study ;  rdfs:label "reads" ;  rdfs:comment "The action of studying performed by a student" .study:studies  rdf:type owl:ObjectProperty ;  rdfs:domain study:Student ;  rdfs:range study:Subject ;  rdfs:label "studies" ;  rdfs:comment "The action by which a student study a specific subject" .study:hasStudy  rdf:type owl:ObjectProperty ;  rdfs:domain study:Subject ;  rdfs:range study:Study ;    rdfs:label "hasStudy" ;  rdfs:comment "The relationship between a subject and a study, indicating that a study is about a specific subject" . study:duration  rdf:type owl:DatatypeProperty ;  rdfs:domain study:Study ;  rdfs:range rdf:integer ;  rdfs:label "duration" ;  rdfs:comment "The duration of a study in hours" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://www.example.org/study#> .@base <http://www.example.org/study> .<http://www.example.org/study> rdf:type owl:Ontology ;                             rdfs:comment "An ontology of scientific study" ;                             owl:versionInfo "1.0" .study:Study rdf:type owl:Class ;            rdfs:comment "A scientific or academic investigation or experiment" ;            rdfs:subClassOf study:Research .study:Research rdf:type owl:Class ;              rdfs:comment "Systematic investigation to establish facts or principles" .study:hasSubject rdf:type owl:ObjectProperty ;                rdfs:domain study:Study ;                rdfs:range study:Subject ;                rdfs:comment "The subject or topic of the study" .study:Subject rdf:type owl:Class ;              rdfs:comment "The topic or subject matter of a study" .study:hasResearcher rdf:type owl:ObjectProperty ;                   rdfs:domain study:Study ;                   rdfs:range study:Researcher ;                   rdfs:comment "The researcher conducting the study" .study:Researcher rdf:type owl:Class ;                rdfs:comment "A person who conducts academic or scientific research" .study:hasPublication rdf:type owl:ObjectProperty ;                    rdfs:domain study:Study ;                    rdfs:range study:Publication ;                    rdfs:comment "The publication in which the study is published" .study:Publication rdf:type owl:Class ;                 rdfs:comment "A published document reporting a study's findings" .study:hasMethod rdf:type owl:ObjectProperty ;               rdfs:domain study:Study ;               rdfs:range study:Method ;               rdfs:comment "The method or methods used in the study" .study:Method rdf:type owl:Class ;             rdfs:comment "The techniques or procedures used to conduct a study" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://example.org/study#> .study:Study rdf:type rdfs:Class ;  rdfs:label "Study" ;  rdfs:comment "The activity of devoting time and attention to acquire knowledge" .study:Student rdf:type rdfs:Class ;  rdfs:label "Student" ;  rdfs:comment "An individual who is participating in a study" ;  rdfs:subClassOf study:Study .study:Subject rdf:type rdfs:Class ;  rdfs:label "Subject" ;  rdfs:comment "The topic on which the study is focused" ;  rdfs:subClassOf study:Study .study:Course rdf:type rdfs:Class ;  rdfs:label "Course" ;  rdfs:comment "Organized study of a subject over a period of time" ;  rdfs:subClassOf study:Study .study:LearningMaterial rdf:type rdfs:Class ;  rdfs:label "LearningMaterial" ;  rdfs:comment "Resources used in the study to facilitate the understanding of a subject" ;  rdfs:subClassOf study:Study .study:isStudying rdf:type rdf:Property ;  rdfs:domain study:Student ;  rdfs:range study:Subject ;  rdfs:label "is studying" .study:isCourseOf rdf:type rdf:Property ;  rdfs:domain study:Subject ;  rdfs:range study:Course ;  rdfs:label "is a course of" .study:uses rdf:type rdf:Property ;  rdfs:domain study:Study ;  rdfs:range study:LearningMaterial ;  rdfs:label "uses" .study:StudiesSubject rdf:type owl:ObjectProperty ;  rdfs:label "studiesSubject" ;  owl:inverseOf study:isStudying .study:CourseSubject rdf:type owl:ObjectProperty ;  rdfs:label "courseSubject" ;  owl:inverseOf study:isCourseOf.


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix study: <http://www.example.org/study#> .study:Study rdf:type rdfs:Class .study:hasTopic rdf:type rdf:Property ;              rdfs:domain study:Study ;              rdfs:range study:Topic .study:Topic rdf:type rdfs:Class .study:studentStudy rdf:type rdf:Property ;                  rdfs:domain study:Student ;                  rdfs:range study:Study .study:Student rdf:type rdfs:Class .study:courses rdf:type rdf:Property ;             rdfs:domain study:Study ;             rdfs:range study:Course .study:Course rdf:type rdfs:Class .study:hasTeachingAssistant rdf:type rdf:Property ;                          rdfs:domain study:Course ;                          rdfs:range study:TeachingAssistant .study:TeachingAssistant rdf:type rdfs:Class ;study:Study a rdfs:Class ;             rdfs:label "Study" ;             rdfs:comment "The act, process, or experience of studying" .study:hasDuration rdf:type rdf:Property ;                 rdf:domain study:Study ;                 rdf:range xsd:duration .study:StudyFromHome rdf:type rdfs:Class ;                    rdfs:subClassOf study:Study .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://example.org/study#> .study:Study a rdfs:Class ;    rdfs:comment "Class representing a study or research" ;    rdfs:label "Study" .study:Course a rdfs:Class ;    rdfs:comment "Class representing a course in a study" ;    rdfs:label "Course" ;    rdfs:subClassOf study:Study .    study:hasCourse a rdf:Property ;    rdfs:comment "Property linking a study with its courses" ;    rdfs:label "hasCourse" ;    rdfs:domain study:Study ;    rdfs:range study:Course .study:Topic a rdfs:Class ;    rdfs:comment "Class representing a topic in a course" ;    rdfs:label "Topic" ;    rdfs:subClassOf study:Course .study:hasTopic a rdf:Property ;    rdfs:comment "Property linking a course with its topics" ;    rdfs:label "hasTopic" ;    rdfs:domain study:Course ;    rdfs:range study:Topic .study:Institution a rdfs:Class ;    rdfs:comment "Class representing the institution where the study is taking place" ;    rdfs:label "Institution" .study:isInInstitution a rdf:Property ;    rdfs:comment "Property linking the study with the institution" ;    rdfs:label "isInInstitution" ;    rdfs:domain study:Study ;    rdfs:range study:Institution .study:Student a rdfs:Class ;    rdfs:comment "Class representing a student studying" ;    rdfs:label "Student" .study:isStudyBy a rdf:Property ;    rdfs:comment "Property linking the study with the student" ;    rdfs:label "isStudyBy" ;    rdfs:domain study:Study ;    rdfs:range study:Student .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://www.example.org/study#> .:Study a rdfs:Class ;  rdfs:label "Study" ;  rdfs:comment "Acquisition of knowledge by means of researching, being taught, self-teaching, or from learning from experience" .:Subject a rdfs:Class ;  rdfs:label "Subject" ;  rdfs:comment "Specific area of knowledge that forms part of a curriculum or individual study" .:Student a rdfs:Class ;  rdfs:label "Student" ;  rdfs:comment "Individual who is engaged in learning, typically within an educational institution" .:Teacher a rdfs:Class ;  rdfs:label "Teacher" ;  rdfs:comment "Individual who instructs others in a specific area of knowledge" .:EducationalInstitution a rdfs:Class ;  rdfs:label "Educational Institution" ;  rdfs:comment "Organization that provides education or training" .:studiesIn a rdfs:Property ;  rdfs:domain :Student ;  rdfs:range :EducationalInstitution ;  rdfs:label "studies in" ;  rdfs:comment "Denotes the educational institution where a student is studying" .:studiesSubject a rdfs:Property ;  rdfs:domain :Student ;  rdfs:range :Subject ;  rdfs:label "studies subject" ;  rdfs:comment "Denotes the subject a student is studying" .:teachesSubject a rdfs:Property ;  rdfs:domain :Teacher ;  rdfs:range :Subject ;  rdfs:label "teaches subject" ;  rdfs:comment "Denotes the subject a teacher instructs" .:teachesIn a rdfs:Property ;  rdfs:domain :Teacher ;  rdfs:range :EducationalInstitution ;  rdfs:label "teaches in" ;  rdfs:comment "Denotes the educational institution where a teacher instructs students" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://example.org/study#> .study:Study rdf:type rdfs:Class .study:student rdf:type rdfs:Class ;              rdfs:subClassOf study:Study .study:course rdf:type rdfs:Class ;             rdfs:subClassOf study:Study .study:teacher rdf:type rdfs:Class ;              rdfs:subClassOf study:Study .study:hasName rdf:type rdf:Property ;              rdfs:domain study:Study ;              rdfs:range xsd:string .study:hasID rdf:type rdf:Property ;            rdfs:domain study:Study ;            rdfs:range xsd:string .study:hasLocation rdf:type rdf:Property ;                 rdfs:domain study:course ;                 rdfs:range xsd:string .study:isEnrolledIn rdf:type rdf:Property ;                  rdfs:domain study:student ;                  rdfs:range study:course .study:isTaughtBy rdf:type rdf:Property ;                rdfs:domain study:course ;                rdfs:range study:teacher .study:isStudiedBy rdf:type rdf:Property ;                 rdfs:domain study:course ;                 rdfs:range study:student .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/study#> .ex:Study rdf:type rdfs:Class .ex:hasSubject rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range rdfs:Literal .ex:hasLevel rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range rdfs:Literal .ex:partOfCurriculum rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range rdfs:Resource .ex:taughtBy rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range ex:Teacher .ex:Teacher rdf:type rdfs:Class .ex:Student rdf:type rdfs:Class .ex:attends rdf:type rdf:Property ;    rdfs:domain ex:Student ;    rdfs:range ex:Study .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://example.org/study#> .study:Study a rdfs:Class ;  rdfs:comment "A period of concentrated learning or research" ;  rdfs:label "Study" .study:subject a rdf:Property ;  rdfs:comment "The subject that the study is about" ;  rdfs:label "subject" ;  rdfs:domain study:Study ;  rdfs:range xsd:string .study:duration a rdf:Property ;  rdfs:comment "The duration of the study" ;  rdfs:label "duration" ;  rdfs:domain study:Study ;  rdfs:range xsd:duration .study:student a rdf:Property ;  rdfs:comment "The student carrying out the study" ;  rdfs:label "student" ;  rdfs:domain study:Study ;  rdfs:range study:Student .study:Student a rdfs:Class ;  rdfs:comment "A person who is studying" ;  rdfs:label "Student" .study:name a rdf:Property ;  rdfs:comment "The name of the student" ;  rdfs:label "name" ;  rdfs:domain study:Student ;  rdfs:range xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://www.example.org/study#> .# defining classes study:Study rdf:type rdfs:Class ;study:Subject rdf:type rdfs:Class ;study:Student rdf:type rdfs:Class ;study:Instructor rdf:type rdfs:Class ;study:Book rdf:type rdfs:Class ;study:Course rdf:type rdfs:Class .# defining propertiesstudy:studySubject rdf:type rdf:Property ;    rdfs:domain study:Study ;    rdfs:range study:Subject .study:studyAt rdf:type rdf:Property ;    rdfs:domain study:Student ;    rdfs:range study:Course .study:teach rdf:type rdf:Property ;    rdfs:domain study:Instructor ;    rdfs:range study:Course .study:studyFrom rdf:type rdf:Property ;    rdfs:domain study:Study ;    rdfs:range study:Book .study:studyBy rdf:type rdf:Property ;    rdfs:domain study:Course ;    rdfs:range study:Student .study:enrolls rdf:type rdf:Property ;    rdfs:domain study:Course ;    rdfs:range study:Student .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://www.study.com/ontology#> .study:Study a rdfs:Class ;rdfs:label "Study" ;rdfs:comment "A comprehensive operation to gather knowledge or learn about something" .study:subjectOfStudy a rdfs:Property ;rdfs:label "subject of study" ;rdfs:comment "The specific topic or area being studied" ;rdfs:domain study:Study ;rdfs:range rdfs:Resource .study:durationOfStudy a rdfs:Property ;rdfs:label "duration of study" ;rdfs:comment "The length of time a study takes place" ;rdfs:domain study:Study ;rdfs:range xsd:duration .study:methodOfStudy a rdfs:Property ;rdfs:label "method of study" ;rdfs:comment "The method or approach used in a study" ;rdfs:domain study:Study ;rdfs:range rdfs:Resource .study:Study rdf:type owl:Class ;owl:equivalentClass [  a owl:Restriction ;  owl:onProperty study:subjectOfStudy ;  owl:someValuesFrom rdfs:Resource ] ;owl:equivalentClass [  a owl:Restriction ;  owl:onProperty study:durationOfStudy ;  owl:someValuesFrom xsd:duration ] ;owl:equivalentClass [  a owl:Restriction ;  owl:onProperty study:methodOfStudy ;  owl:someValuesFrom rdfs:Resource ] .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://www.example.com/study#> .#Class Declarationsstudy:Study rdfs:subClassOf owl:Thing .study:Subject rdfs:subClassOf owl:Thing .study:Student rdfs:subClassOf owl:Thing .study:Tutor rdfs:subClassOf owl:Thing .study:SourceMaterial rdfs:subClassOf owl:Thing .study:Location rdfs:subClassOf owl:Thing .#Property Declarationsstudy:hasSubject rdf:type owl:ObjectProperty .study:hasStudent rdf:type owl:ObjectProperty .study:isTutoredBy rdf:type owl:ObjectProperty .study:usesSourceMaterial rdf:type owl:ObjectProperty .study:takesPlaceAt rdf:type owl:ObjectProperty .#Inverse Property Declarationsstudy:isSubjectOf owl:inverseOf study:hasSubject .study:isStudiedBy owl:inverseOf study:hasStudent .study:tutorsStudy owl:inverseOf study:isTutoredBy .study:isUsedInStudy owl:inverseOf study:usesSourceMaterial .study:isLocationOfStudy owl:inverseOf study:takesPlaceAt .#Property Domain and Range Declarationsstudy:hasSubject rdfs:domain study:Study .study:hasSubject rdfs:range study:Subject .study:hasStudent rdfs:domain study:Study .study:hasStudent rdfs:range study:Student .study:isTutoredBy rdfs:domain study:Study .study:isTutoredBy rdfs:range study:Tutor .study:usesSourceMaterial rdfs:domain study:Study .study:usesSourceMaterial rdfs:range study:SourceMaterial .study:takesPlaceAt rdfs:domain study:Study .study:takesPlaceAt rdfs:range study:Location .





@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://example.org/study#> .study:Study rdf:type rdfs:Class .study:Subject rdf:type rdfs:Class .study:hasSubject rdf:type rdf:Property;    rdfs:domain study:Study;    rdfs:range study:Subject .study:Course rdf:type rdfs:Class .study:isPartOf rdf:type rdf:Property;    rdfs:domain study:Subject;    rdfs:range study:Course.study:Reference rdf:type rdfs:Class .study:hasReference rdf:type rdf:Property;    rdfs:domain study:Study;    rdfs:range study:Reference .study:Professor rdf:type rdfs:Class.study:lecturedBy rdf:type rdf:Property;    rdfs:domain study:Course;    rdfs:range study:Professor.study:Student rdf:type rdfs:Class.study:studiedBy rdf:type rdf:Property;    rdfs:domain study:Course;    rdfs:range study:Student.


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://example.com/study#> .study:Study a rdfs:Class ;    rdfs:label "Study" ;    rdfs:comment "A detailed investigation and analysis of a subject or situation" .study:Subject a rdfs:Class ;    rdfs:label "Subject" ;    rdfs:comment "The object or topic being studied" .study:Method a rdfs:Class ;    rdfs:label "Method" ;    rdfs:comment "The procedure or process used in the study" .study:Participant a rdfs:Class ;    rdfs:label "Participant" ;    rdfs:comment "An individual who partakes in the study" .study:studyHasSubject rdf:Property ;    rdfs:label "study has subject" ;    rdfs:domain study:Study ;    rdfs:range study:Subject .study:studyUsesMethod rdf:Property ;    rdfs:label "study uses method" ;    rdfs:domain study:Study ;    rdfs:range study:Method .    study:studyInvolvesParticipant rdf:Property ;    rdfs:label "study involves participant" ;    rdfs:domain study:Study ;    rdfs:range study:Participant .study:participantInvolvesIn rdf:Property ;    rdfs:label "participant involves in" ;    rdfs:domain study:Participant ;    rdfs:range study:Study .




@prefix : <http://www.example.org/study#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:Study a rdfs:Class .:title a rdf:Property ;       rdfs:domain :Study ;       rdfs:range xsd:string .       :researcher a rdf:Property ;            rdfs:domain :Study ;            rdfs:range :Person .:focusArea a rdf:Property ;          rdfs:domain :Study ;          rdfs:range :SubjectArea .:duration a rdf:Property ;          rdfs:domain :Study ;          rdfs:range xsd:integer .# Defining additional classes necessary for the ontology:Person a rdfs:Class .:SubjectArea a rdfs:Class .



