@base <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/study#> .ex:Study rdf:type rdfs:Class ;    rdfs:comment "An academic or research study" ;    rdfs:label "Study" .ex:hasResearcher rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range ex:Researcher ;    rdfs:comment "The researcher conducting the study." ;    rdfs:label "has researcher" .ex:Researcher rdf:type rdfs:Class ;    rdfs:comment "An individual conducting a study." ;    rdfs:label "Researcher" .ex:hasTopic rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range ex:Topic ;    rdfs:comment "The specific focus or subject of the study." ;    rdfs:label "has topic" .ex:Topic rdf:type rdfs:Class ;    rdfs:comment "The subject or focus of a study." ;    rdfs:label "Topic" .ex:hasResult rdf:type rdf:Property ;    rdfs:domain ex:Study ;    rdfs:range ex:Result ;    rdfs:comment "The findings or outcomes of the study." ;    rdfs:label "has result" .ex:Result rdf:type rdfs:Class ;    rdfs:comment "The findings or outcomes of a study." ;    rdfs:label "Result" .

@prefix : <http://example.org/study#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Study a rdfs:Class ;   rdfs:label "Study" ;   rdfs:comment "A scientific study or research" .:hasTitle a rdf:Property ;   rdfs:label "has title" ;   rdfs:domain :Study ;   rdfs:range rdfs:Literal ;   rdfs:comment "The title of the study" .:hasAuthor a rdf:Property ;   rdfs:label "has author" ;   rdfs:domain :Study ;   rdfs:range :Person ;   rdfs:comment "Person who authored the study" .:Person a rdfs:Class ;   rdfs:label "Person" ;   rdfs:comment "A person." .:study1 a :Study ;   :hasTitle "Study on climate change" ;   :hasAuthor :author1 .:author1 a :Person ;   rdfs:label "John Doe" .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix study: <http://example.org/study#> .study:Study rdf:type owl:Class ;    rdfs:label "Study"@en ;    rdfs:comment "A Study represents a structured attempt to gain knowledge through research."@en ;    .study:hasTitle rdf:type owl:DatatypeProperty ;    rdfs:label "has title"@en ;    rdfs:domain study:Study ;    rdfs:range xsd:string ;    .study:hasStudyArea rdf:type owl:ObjectProperty ;    rdfs:label "has study area"@en ;    rdfs:domain study:Study ;    rdfs:range study:StudyArea ;    .study:StudyArea rdf:type owl:Class ;    rdfs:label "Study Area"@en ;    rdfs:comment "Subject or field in which a study is conducted."@en ;    .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix study: <http://www.example.org/study#> .study:Study    a rdf:Class ;    rdfs:label "Study" ;    rdfs:comment "Represents a study" .study:hasStudyID    a rdf:Property ;    rdfs:label "has study ID" ;    rdfs:comment "The unique ID of a study" ;    rdfs:domain study:Study ;    rdfs:range xsd:string .study:hasStudyName    a rdf:Property ;    rdfs:label "has study name" ;    rdfs:comment "The name of a study" ;    rdfs:domain study:Study ;    rdfs:range xsd:string .study:hasStudyTopic    a rdf:Property ;    rdfs:label "has study topic" ;    rdfs:comment "The topic of a study" ;    rdfs:domain study:Study ;    rdfs:range xsd:string .study:hasStudyDate    a rdf:Property ;    rdfs:label "has study date" ;    rdfs:comment "The date of a study" ;    rdfs:domain study:Study ;    rdfs:range xsd:date .

