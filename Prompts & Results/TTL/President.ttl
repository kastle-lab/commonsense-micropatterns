@prefix : <http://www.example.com#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . :President     rdf:type owl:Class ;    rdfs:subClassOf :Person .:hasBeenPresident     rdf:type owl:ObjectProperty ;    rdfs:domain :Person ;    rdfs:range :President ;    rdfs:label "has been president" ;    rdfs:comment "The property that defines if a person has been a president." .:startOfPresidency    rdf:type owl:DatatypeProperty ;    rdfs:domain :President ;    rdfs:range xsd:date ;    rdfs:label "start of presidency" ;    rdfs:comment "The property that details the start date of a person's presidency." .:endOfPresidency    rdf:type owl:DatatypeProperty ;    rdfs:domain :President ;    rdfs:range xsd:date ;    rdfs:label "end of presidency" ;    rdfs:comment "The property that details the end date of a person's presidency." .:Person    rdf:type owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:label "Person" .

@prefix : <http://example.org/#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .      :President     rdf:type rdfs:Class ;    rdfs:label "President"@en ;    rdfs:comment "The head of a republic, typically representative of the people and elected by them"@en .:hasName     rdf:type rdf:Property ;    rdfs:domain :President ;    rdfs:range xsd:string ;    rdfs:label "has name"@en ;    rdfs:comment "The name of the president"@en .:hasCountry     rdf:type rdf:Property ;    rdfs:domain :President ;    rdfs:range xsd:string ;    rdfs:label "has country"@en ;    rdfs:comment "The country of the president"@en .:hasTerm     rdf:type rdf:Property ;    rdfs:domain :President ;    rdfs:range xsd:string ;    rdfs:label "has term"@en ;    rdfs:comment "The term of the President"@en .:bornOn     rdf:type rdf:Property ;    rdfs:domain :President ;    rdfs:range xsd:date ;    rdfs:label "Born on"@en ;    rdfs:comment "The birth date of the president"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .# Define 'President' as a class:President    rdf:type owl:Class ;    rdfs:label "President"@en ;    rdfs:comment "A person who holds the office of head of state in a country"@en .# Define 'servesAsPresident' property that can link a person to a country:servesAsPresident    rdf:type owl:ObjectProperty ;    rdfs:label "serves as President"@en ;    rdfs:domain :Person ;    rdfs:range :Country ;    rdfs:comment "Property that connects a person to a nation where the person serves as the president"@en .  # Define 'termStart' property to represent term start date of a President:termStart    rdf:type owl:DatatypeProperty ;    rdfs:label "start of term"@en ;    rdfs:range xsd:dateTime ;    rdfs:domain :President ;    rdfs:comment "The date that the president's term starts"@en .# Define 'termEnd' property to represent term end date of a President:termEnd    rdf:type owl:DatatypeProperty ;    rdfs:label "end of term"@en ;    rdfs:range xsd:dateTime ;    rdfs:domain :President ;    rdfs:comment "The date that the president's term ends"@en .

