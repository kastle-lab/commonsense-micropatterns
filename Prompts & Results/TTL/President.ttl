
@prefix : <http://www.example.com/president#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:president a rdfs:Class ;    rdfs:label "President" ;    rdfs:comment "The chief executive in a democratic government" .:hasName a rdfs:Property ;    rdfs:label "has name" ;    rdfs:domain :president ;    rdfs:range rdfs:Literal .:presidesOver a rdfs:Property ;    rdfs:label "presides over" ;    rdfs:domain :president ;    rdfs:range :country .:country a rdfs:Class ;    rdfs:label "Country" ;    rdfs:comment "A nation with its own government, occupying a particular territory" .:servedTerm a rdfs:Property ;    rdfs:label "served term" ;    rdfs:domain :president ;    rdfs:range :term .:term a rdfs:Class ;    rdfs:label "Term" ;    rdfs:comment "The period of time during which a person serves as president" .:startOfTerm a rdfs:Property ;    rdfs:label "start of term" ;    rdfs:domain :term ;    rdfs:range rdfs:Literal .:endOfTerm a rdfs:Property ;    rdfs:label "end of term" ;    rdfs:domain :term ;    rdfs:range rdfs:Literal .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix pres: <http://example.org/pres/1.0/> .pres:President rdf:type owl:Class ;  rdfs:comment "The class of all individuals who hold or have held the office of president in a political context." ;  rdfs:subClassOf pres:PoliticalLeader .pres:PoliticalLeader rdf:type owl:Class ;  rdfs:comment "The class of all individuals who hold or have held a leadership position in a political context." .pres:hasFirstName rdf:type owl:DatatypeProperty ;  rdfs:comment "The first name of the president." ;  rdfs:domain pres:President ;  rdfs:range xsd:string .pres:hasLastName rdf:type owl:DatatypeProperty ;  rdfs:comment "The last name of the president." ;  rdfs:domain pres:President ;  rdfs:range xsd:string .pres:termStart rdf:type owl:DatatypeProperty ;  rdfs:comment "The start date of the president's term." ;  rdfs:domain pres:President ;  rdfs:range xsd:date .pres:termEnd rdf:type owl:DatatypeProperty ;  rdfs:comment "The end date of the president's term." ;  rdfs:domain pres:President ;  rdfs:range xsd:date .pres:presidesOver rdf:type owl:ObjectProperty ;  rdfs:comment "The country or organization that the president presides over." ;  rdfs:domain pres:President ;  rdfs:range pres:PoliticalEntity .pres:PoliticalEntity rdf:type owl:Class ;  rdfs:comment "The class of all political countries or organizations." .pres:hasBefore rdf:type owl:ObjectProperty ;  rdfs:comment "The president who served before the current one." ;  rdfs:domain pres:President ;  rdfs:range pres:President .pres:hasAfter rdf:type owl:ObjectProperty ;  rdfs:comment "The president who served after the current one." ;  rdfs:domain pres:President ;  rdfs:range pres:President .


