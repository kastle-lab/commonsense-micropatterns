turt@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.com/> .ex:Question a rdfs:Class ;    rdfs:label "Question" ;    rdfs:comment "The class of all questions" .ex:hasQuestionText a rdf:Property ;    rdfs:label "has question text" ;    rdfs:comment "The property that relates a question to its text" ;    rdfs:domain ex:Question ;    rdfs:range rdfs:Literal .ex:hasAnswer a rdf:Property ;    rdfs:label "has answer" ;    rdfs:comment "The property that relates a question to its answer" ;    rdfs:domain ex:Question ;    rdfs:range rdfs:Literal .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .# Define "Question" as a classex:Question rdf:type rdfs:Class .# Define properties for the Question classex:askedBy rdf:type rdf:Property ;  rdfs:label "asked by" ;  rdfs:domain ex:Question ;  rdfs:range xsd:string .ex:askedOn rdf:type rdf:Property ;  rdfs:label "asked on" ;  rdfs:domain ex:Question ;  rdfs:range xsd:date .ex:hasCategory rdf:type rdf:Property ;  rdfs:label "has category" ;  rdfs:domain ex:Question ;  rdfs:range xsd:string .ex:hasText rdf:type rdf:Property ;  rdfs:label "has text" ;  rdfs:domain ex:Question ;  rdfs:range xsd:string .# Define an instance of a Questionex:Question1 rdf:type ex:Question ;  ex:askedBy "John Doe" ;  ex:askedOn "2022-01-01"^^xsd:date ;  ex:hasCategory "General" ;  ex:hasText "What is the capital of France?" .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix ex: <http://example.com/> .ex:Question a rdfs:Class ;    rdfs:comment "A class representing a question."@en .ex:hasQuestion a rdf:Property ;    rdfs:domain ex:Person ;    rdfs:range ex:Question ;    rdfs:comment "A property representing that a person has a question."@en .ex:person1 ex:hasQuestion ex:question1 .ex:question1 a ex:Question ;    rdfs:label "What is the capital of France?"@en ;    ex:hasAnswer "Paris"@en .ex:hasAnswer a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range xsd:string ;    rdfs:comment "A property representing the answer to a question."@en .

@prefix ex: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .ex:Question a rdfs:Class ;    rdfs:label "Question" ;    rdfs:comment "A class denoting a question" .ex:askedIn a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range rdfs:Resource;    rdfs:label "asked in";    rdfs:comment "A property showing the resource where the question was asked".
