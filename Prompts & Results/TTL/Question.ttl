@prefix : <http://example.org/book/> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .:book a rdfs:Class ;      rdfs:label "Book"@en ;      rdfs:comment "The class of all books."@en .:title a rdfs:DatatypeProperty ;       rdfs:label "has title"@en ;       rdfs:comment "The title of a book."@en ;       rdfs:domain :book ;       rdfs:range xsd:string .:author a rdfs:ObjectProperty ;        rdfs:label "has author"@en ;        rdfs:comment "The author of a book."@en ;        rdfs:domain :book ;        rdfs:range :person .:person a rdfs:Class ;        rdfs:label "Person"@en ;        rdfs:comment "The class of all persons."@en .:name a rdfs:DatatypeProperty ;      rdfs:label "has name"@en ;      rdfs:comment "The name of a person."@en ;      rdfs:domain :person ;      rdfs:range xsd:string .

