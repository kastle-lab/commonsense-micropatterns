
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix dq: <http://www.example.org/questionOntology#> .dq:Question a rdfs:Class ;    rdfs:comment "The class of all questions."@en ;    rdfs:label "Question"@en .dq:hasType a rdf:Property ;    rdfs:comment "The type of the question e.g. multiple choice, open-ended, yes/no."@en ;    rdfs:label "has type"@en ;    rdfs:range rdfs:Literal ;    rdfs:domain dq:Question .dq:hasDifficultyLevel a rdf:Property ;    rdfs:comment "The difficulty level of the question e.g. easy, medium, hard."@en ;    rdfs:label "has difficulty level"@en ;    rdfs:range rdfs:Literal ;    rdfs:domain dq:Question .dq:isRelatedToTopic a rdf:Property ;    rdfs:comment "The area or topic to which the question belongs."@en ;    rdfs:label "is related to"@en ;    rdfs:range rdfs:Literal ;    rdfs:domain dq:Question .dq:hasAnswer a rdf:Property ;    rdfs:comment "The proposed answer(s) for the question."@en ;    rdfs:label "has answer"@en ;    rdfs:range rdfs:Literal ;    rdfs:domain dq:Question .dq:drawsFromSource a rdf:Property ;    rdfs:comment "The source or reference from which the question is derived."@en ;    rdfs:label "draws from source"@en ;    rdfs:range rdfs:Literal ;    rdfs:domain dq:Question .dq:Answer a rdfs:Class ;    rdfs:label "Answer"@en ;    rdfs:comment "The class of all answers."@en .dq:hasAnswerText a rdf:Property ;    rdfs:comment "The text of the answer."@en ;    rdfs:label "has answer text"@en ;    rdfs:range rdfs:Literal ;    rdfs:domain dq:Answer .dq:isCorrect a rdf:Property ;    rdfs:comment "The property that indicates if the answer is correct or not."@en ;    rdfs:label "is correct"@en ;    rdfs:range xsd:boolean ;    rdfs:domain dq:Answer .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ques: <http://example.org/question#> .ques:Question rdfs:subClassOf rdfs:Resource ;  rdfs:comment "Represents a question"^^xsd:string ;  rdfs:label "Question" .ques:QuestionType rdfs:subClassOf rdfs:Resource ;  rdfs:comment "Represents the type of a question"^^xsd:string ;  rdfs:label "Question Type" .ques:askedBy rdfs:subPropertyOf rdf:Property ;  rdfs:comment "Represents who asked the question"^^xsd:string ;  rdfs:label "Asked By" ;  rdfs:domain ques:Question ;  rdfs:range rdfs:Resource .ques:hasType rdfs:subPropertyOf rdf:Property ;  rdfs:comment "Represents the type of question"^^xsd:string ;  rdfs:label "Has type" ;  rdfs:domain ques:Question ;  rdfs:range ques:QuestionType .ques:hasAnswer rdfs:subPropertyOf rdf:Property ;  rdfs:comment "Represents the answer of a question"^^xsd:string ;  rdfs:label "Has answer" ;  rdfs:domain ques:Question ;  rdfs:range rdfs:Resource . ques:MCQ rdfs:subClassOf ques:QuestionType ;  rdfs:comment "Represents a multiple choice question"^^xsd:string ;  rdfs:label "MCQ" . ques:OpenEnded rdfs:subClassOf ques:QuestionType ;  rdfs:comment "Represents an open-ended question"^^xsd:string ;  rdfs:label "Open Ended" . ques:ClosedEnded rdfs:subClassOf ques:QuestionType ;  rdfs:comment "Represents a close-ended question"^^xsd:string ;  rdfs:label "Closed Ended" . 



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix question: <http://example.com/question#> .question:QuestionEntity rdf:type owl:Class ;  rdfs:comment "A class representing a question."@en ;  rdfs:label "Question"@en .question:hasText rdf:type owl:DatatypeProperty ;  rdfs:comment "The text of the question"@en ;  rdfs:label "hasText"@en ;  rdfs:domain question:QuestionEntity ;  rdfs:range xsd:string .question:hasAnswer rdf:type owl:ObjectProperty ;  rdfs:comment "The answer to the question"@en ;  rdfs:label "hasAnswer"@en ;  rdfs:domain question:QuestionEntity ;  rdfs:range question:AnswerEntity .question:AnswerEntity rdf:type owl:Class ;  rdfs:comment "A class representing an answer."@en ;  rdfs:label "Answer"@en .question:hasAnswerText rdf:type owl:DatatypeProperty ;  rdfs:comment "The text of the answer"@en ;  rdfs:label "hasAnswerText"@en ;  rdfs:domain question:AnswerEntity ;  rdfs:range xsd:string .question:QuestionCategory rdf:type owl:Class ;  rdfs:comment "A class representing a category of questions."@en ;  rdfs:label "QuestionCategory"@en .question:hasCategory rdf:type owl:ObjectProperty ;  rdfs:comment "The category to which the question belongs"@en ;  rdfs:label "hasCategory"@en ;  rdfs:domain question:QuestionEntity ;  rdfs:range question:QuestionCategory .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix qa: <http://www.semanticweb.org/questionnaire#> .qa:Question rdf:type rdfs:Class .qa:hasTitle rdf:type rdf:Property ;            rdfs:domain qa:Question ;            rdfs:range xsd:string .qa:hasContent rdf:type rdf:Property ;              rdfs:domain qa:Question ;              rdfs:range xsd:string .qa:hasSpecificDate rdf:type rdf:Property ;                   rdfs:domain qa:Question ;                   rdfs:range xsd:date .qa:relatedTo rdf:type rdf:Property ;             rdfs:domain qa:Question .             qa:Answer rdf:type rdfs:Class ;          rdfs:subClassOf qa:Question .qa:hasAnswer rdf:type rdf:Property ;             rdfs:domain qa:Question ;             rdfs:range qa:Answer . qa:SingleChoiceAnswer rdf:type rdfs:Class ;                      rdfs:subClassOf qa:Answer .qa:MultipleChoiceAnswer rdf:type rdfs:Class ;                        rdfs:subClassOf qa:Answer .qa:hasChoice rdf:type rdf:Property ;             rdfs:domain qa:Question ;             rdfs:range qa:Answer .qa:User rdf:type rdfs:Class .qa:asked rdf:type rdf:Property ;          rdfs:domain qa:User ;          rdfs:range qa:Question .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix qaont: <http://example.org/ontology/question#> . qaont:Question rdf:type owl:Class ;    rdfs:label "Question"@en ;    rdfs:comment "A sentence or phrase that seeks to find out information."@en ;    rdfs:subClassOf rdfs:Resource .qaont:MultipleChoiceQuestion rdf:type owl:Class ;    rdfs:label "Multiple Choice Question"@en ;    rdfs:comment "A question that allows selection of one option from several choices."@en ;    rdfs:subClassOf qaont:Question .qaont:OpenEndedQuestion rdf:type owl:Class ;    rdfs:label "Open Ended Question"@en ;    rdfs:comment "A question that requires a full, meaningful answer."@en ;    rdfs:subClassOf qaont:Question .qaont:QuestionTopic rdf:type owl:Class ;    rdfs:label "Question Topic"@en ;    rdfs:comment "The subject that the question is about."@en ;    rdfs:subClassOf rdfs:Resource .qaont:TopicOf rdf:type owl:ObjectProperty ;    rdfs:label "is topic of"@en ;    rdfs:comment "Connects a question to its topic"@en ;    rdfs:domain qaont:QuestionTopic ;    rdfs:range qaont:Question .qaont:Answer rdf:type owl:Class ;    rdfs:label "Answer"@en ;    rdfs:comment "A solution or explanation for a question."@en ;    rdfs:subClassOf rdfs:Resource .qaont:HasAnswer rdf:type owl:ObjectProperty ;    rdfs:label "has answer"@en ;    rdfs:comment "Connects a question to its answer"@en ;    rdfs:domain qaont:Question ;    rdfs:range qaont:Answer .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix question: <http://example.org/question#> .question:     a owl:Ontology;    rdfs:comment "An ontology for describing questions".question:Question    a owl:Class ;    rdfs:label "Question" ;    rdfs:comment "A class representing a question".question:hasSubject    a owl:ObjectProperty ;    rdfs:label "has subject" ;    rdfs:comment "The subject area of a question" ;    rdfs:domain question:Question ;    owl:inverseOf question:isSubjectOf.question:hasDifficultyLevel    a owl:DatatypeProperty ;    rdfs:label "has difficulty level" ;    rdfs:comment "The difficulty level of a question" ;    rdfs:domain question:Question ;    rdfs:range xsd:string .question:isSubjectOf    a owl:ObjectProperty ;    rdfs:label "is subject of" ;    rdfs:comment "Inverse property, the question a subject belongs to" ;    rdfs:domain question:Subject ;    owl:inverseOf question:hasSubject .question:Subject     a owl:Class;    rdfs:label "Subject";    rdfs:comment "A class representing subject of a question";    rdfs:subClassOf question:Question.  question:Difficulty_Level     a owl:Class;    rdfs:label "Difficulty Level";    rdfs:comment "A class representing difficulty of a question";    rdfs:subClassOf question:Question.




@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix question: <http://www.example.org/question#> .question:Question a rdfs:Class ;    rdfs:label "Question" ;    rdfs:comment "A class representing questions in general." .question:text a rdf:Property ;    rdfs:label "text" ;    rdfs:comment "The text of the question." ;    rdfs:domain question:Question ;    rdfs:range xsd:string .question:type a rdf:Property ;    rdfs:label "type" ;    rdfs:comment "The type of the question." ;    rdfs:domain question:Question ;    rdfs:range question:QuestionType .question:QuestionType a rdfs:Class ;    rdfs:label "Question Type" ;    rdfs:comment "The class of different types of questions." .question:multipleChoice a rdf:Property ;    rdfs:subClassOf question:QuestionType ;    rdfs:label "Multiple Choice" ;    rdfs:comment "A type of question where the answer is selected from multiple choices." .question:openEnded a rdf:Property ;    rdfs:subClassOf question:QuestionType ;    rdfs:label "Open Ended" ;    rdfs:comment "A type of question requiring a freely formulated answer." . question:weight a rdf:Property ;    rdfs:label "weight" ;    rdfs:comment "The weight or importance of the question." ;    rdfs:domain question:Question ;    rdfs:range xsd:int .question:possibleAnswer a rdf:Property ;    rdfs:label "possible answer" ;    rdfs:comment "A possible answer to the question." ;    rdfs:domain question:Question ;    rdfs:range question:Answer .question:Answer a rdfs:Class ;    rdfs:label "Answer" ;    rdfs:comment "A class representing possible answers to questions." .question:answerText a rdf:Property ;    rdfs:label "answer text" ;    rdfs:comment "The text of the answer." ;    rdfs:domain question:Answer ;    rdfs:range xsd:string . question:isCorrect a rdf:Property ;    rdfs:label "is correct" ;    rdfs:comment "Indicates whether the answer is correct or not." ;    rdfs:domain question:Answer ;    rdfs:range xsd:boolean .


@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix question: <http://example.org/question/> .question: a rdfs:Class ;












@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Question a rdfs:Class .ex:hasBody a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range xsd:string .ex:hasAnswer a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range xsd:string .ex:hasDifficulty a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range xsd:integer .ex:hasCategory a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range xsd:string .ex:hasAuthor a rdf:Property ;    rdfs:domain ex:Question ;    rdfs:range xsd:string . ex:question1 a ex:Question ;    ex:hasBody "What is the capital of France?" ;    ex:hasAnswer "Paris" ;    ex:hasDifficulty 2 ;    ex:hasCategory "Geography" ;    ex:hasAuthor "John Doe" . ex:question2 a ex:Question ;    ex:hasBody "Who wrote '1984'?" ;    ex:hasAnswer "George Orwell" ;    ex:hasDifficulty 3 ;    ex:hasCategory "Literature" ;    ex:hasAuthor "Jane Doe" .



@prefix : <http://example.org/question#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Question rdf:type rdfs:Class .:hasQuestion rdf:type rdf:Property ;    rdfs:domain :Person ;    rdfs:range :Question .:hasAnswer rdf:type rdf:Property ;    rdfs:domain :Question ;    rdfs:range :Answer .:Answer rdf:type rdfs:Class .:Person rdf:type rdfs:Class .

@prefix : <http://example.org/question_ontology#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Question a rdfs:Class ;    rdfs:label "Question" ;    rdfs:comment "A query or prompt that seeks for information or input from a user" .:hasQuestion a rdf:Property ;    rdfs:label "has question" ;    rdfs:comment "Link to a question asked by an entity" ;    rdfs:domain :Entity ;    rdfs:range :Question .:Entity a rdfs:Class ;    rdfs:label "Entity" ;    rdfs:comment "An abstract or physical object that can ask a question" .:Response a rdfs:Class ;    rdfs:label "Response" ;    rdfs:comment "An answer or reaction to a question" .:hasResponse a rdf:Property ;    rdfs:label "has response" ;    rdfs:comment "Link to a response given by an entity" ;    rdfs:domain :Entity ;    rdfs:range :Response .:isResponseTo a rdf:Property ;    rdfs:label "is response to" ;    rdfs:comment "Link from a response to the question it answers" ;    rdfs:domain :Response ;    rdfs:range :Question .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/#> .ex:Question rdf:type rdfs:Class .ex:hasQuestionText rdf:type rdf:Property ;    rdfs:range xsd:string ;    rdfs:domain ex:Question .ex:askedBy rdf:type rdf:Property ;    rdfs:range ex:User ;    rdfs:domain ex:Question .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .ex:Question  rdf:type rdfs:Class ;  rdfs:label "Question"@en ;  rdfs:comment "Entity that represents a query or examination prompt"@en .ex:hasQuestionText  rdf:type rdf:Property ;  rdfs:domain ex:Question ;  rdfs:range xsd:string ;  rdfs:label "has question text"@en ;  rdfs:comment "Text that shows exact query or examination prompt"@en .ex:hasAnswer  rdf:type rdf:Property ;  rdfs:domain ex:Question ;  rdfs:range ex:Answer ;  rdfs:label "has answer"@en ;  rdfs:comment "Answer to the specific question"@en .ex:Answer  rdf:type rdfs:Class ;  rdfs:label "Answer"@en ;  rdfs:comment "Response or reply to a question"@en .ex:hasAnswerText  rdf:type rdf:Property ;  rdfs:domain ex:Answer ;  rdfs:range xsd:string ;  rdfs:label "has answer text"@en ;  rdfs:comment "The actual response or reply text to a question"@en .

@prefix : <http://www.example.org#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Question rdf:type rdfs:Class .:hasTitle rdf:type rdf:Property ;          rdfs:domain :Question ;          rdfs:range rdfs:Literal .:hasBody rdf:type rdf:Property ;         rdfs:domain :Question ;         rdfs:range rdfs:Literal .:question1 rdf:type :Question ;           :hasTitle "What is the Turtle format?" ;           :hasBody "I'd like to get more details on the Turtle format." .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix qa: <http://example.org/ontology/questionAnswering#> .qa:Question a rdfs:Class ;    rdfs:comment "A class representing a question."@en ;    rdfs:label "Question"@en .qa:hasQuestionText a rdf:Property ;    rdfs:comment "Representing the text of the question."@en ;    rdfs:label "has question text"@en ;    rdfs:domain qa:Question ;    rdfs:range xsd:string .qa:hasQuestionType a rdf:Property ;    rdfs:comment "Representing the type of the question."@en ;    rdfs:label "has question type"@en ;    rdfs:domain qa:Question ;    rdfs:subPropertyOf rdf:type .qa:QuestionType a rdfs:Class ;    rdfs:comment "A class representing a type of question."@en ;    rdfs:label "Question Type"@en .qa:QuestionType rdfs:subClassOf owl:Thing ;    owl:oneOf (qa:FactoidQuestion qa:YesNoQuestion qa:ListQuestion). qa:FactoidQuestion a qa:QuestionType ;    rdfs:label "Factoid question"@en .qa:YesNoQuestion a qa:QuestionType ;    rdfs:label "Yes/no question"@en .qa:ListQuestion a qa:QuestionType ;    rdfs:label "List question"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.com/> .ex:Question a rdfs:Class ;    rdfs:label "Question" ;    rdfs:comment "Representation of Question in ontology" .ex:hasQuestion a rdf:Property ;    rdfs:domain ex:User ;    rdfs:range ex:Question ;    rdfs:label "has question" ;    rdfs:comment "Property linking User with a Question they asked." .ex:User a rdfs:Class ;    rdfs:label "User" ;    rdfs:comment "Representation of User in ontology" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.com/> .ex:Question a rdfs:Class ;  rdfs:label "Question" ;  rdfs:comment "Class representing a question.It has properties such as question text and answer."ex:hasQuestionText a rdf:Property ;  rdfs:label "hasQuestionText" ;  rdfs:comment "Property representing the text of the question. It is a datatype property as it links a question instance to a literal (text)." ;  rdfs:domain ex:Question ;  rdfs:range rdfs:Literal .ex:hasAnswer a rdf:Property ;  rdfs:label "hasAnswer" ;  rdfs:comment "Property represents the answer to the question. It could either be a datatype property (textual answer) or an object property (links to answer entity)" ;  rdfs:domain ex:Question ;  rdfs:range rdfs:Resource .# Instance of a Questionex:Question1 a ex:Question ;  ex:hasQuestionText "What's the capital of France?" ;  ex:hasAnswer "Paris" .


@prefix : <http://www.someurl.com#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .:Book rdf:type owl:Class .:title rdf:type owl:DatatypeProperty ;      rdfs:domain :Book ;      rdfs:range xsd:string .:author rdf:type owl:ObjectProperty ;       rdfs:domain :Book ;       rdfs:range :Person .:Person rdf:type owl:Class .

