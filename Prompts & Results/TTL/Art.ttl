@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Artwork a rdfs:Class ;  rdfs:label "Artwork" ;  rdfs:comment "Class representing an artwork" .ex:Creator a rdfs:Class ;  rdfs:label "Creator" ;  rdfs:comment "Class representing the creator of an artwork" .ex:createdBy a rdf:Property ;  rdfs:domain ex:Artwork ;  rdfs:range ex:Creator ;  rdfs:label "created by" ;  rdfs:comment "Property linking an artwork to its creator" .ex:Artwork1 a ex:Artwork ;  rdfs:label "The Starry Night" ;  ex:createdBy ex:Creator1 .ex:Creator1 a ex:Creator ;  rdfs:label "Vincent Van Gogh" .

