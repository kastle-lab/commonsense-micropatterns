@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix art: <http://example.org/art#> .art:Artwork a rdfs:Class ;    rdfs:label "Artwork"@en .art:Artist a rdfs:Class ;    rdfs:label "Artist"@en .art:Painting a rdfs:Class ;    rdfs:subClassOf art:Artwork ;    rdfs:label "Painting"@en .art:Sculpture a rdfs:Class ;    rdfs:subClassOf art:Artwork ;    rdfs:label "Sculpture"@en .art:createdBy a rdf:Property ;    rdfs:domain art:Artwork ;    rdfs:range art:Artist ;    rdfs:label "created by"@en .art:creationDate a rdf:Property ;    rdfs:domain art:Artwork ;    rdfs:range xsd:date ;    rdfs:label "creation date"@en .art:ArtGenre a rdfs:Class ;    rdfs:label "Art Genre"@en .art:artGenre a rdf:Property ;    rdfs:domain art:Artwork ;    rdfs:range art:ArtGenre ;    rdfs:label "art genre"@en .art:title a rdf:Property ;    rdfs:domain art:Artwork ;    rdfs:range xsd:string ;    rdfs:label "title"@en .art:medium a rdf:Property ;    rdfs:domain art:Painting ;    rdfs:range xsd:string ;    rdfs:label "medium"@en .art:mass a rdf:Property ;    rdfs:domain art:Sculpture ;    rdfs:range xsd:decimal ;    rdfs:label "mass"@en .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix art: <http://example.org/art#> .art:Art    rdf:type owl:Class ;    rdfs:label "Art"@en ;    rdfs:comment "The concept of Art which represents human creativity and imagination."@en ;.art:Artist    rdf:type owl:Class ;    rdfs:label "Artist"@en ;    rdfs:comment "The concept of Artist, who is the creator of art."@en ;    rdfs:subClassOf art:Person ;.art:Person    rdf:type owl:Class ;    rdfs:label "Person"@en ;    rdfs:comment "The concept of person, an individual human."@en ;.art:painting    rdf:type owl:Class ;    rdfs:label "Painting"@en ;    rdfs:comment "The concept of Painting, a form of art created using colors on a surface."@en ;    rdfs:subClassOf art:Art ;.art:sculpture    rdf:type owl:Class ;    rdfs:label "Sculpture"@en ;    rdfs:comment "The concept of Sculpture, a three-dimensional form of art."@en ;    rdfs:subClassOf art:Art ;.art:createdBy    rdf:type owl:ObjectProperty ;    rdfs:label "created by"@en ;    rdfs:comment "A relationship between an art piece and the artist who created it."@en ;    rdfs:domain art:Art ;    rdfs:range art:Artist ;.art:creationDate    rdf:type owl:DatatypeProperty ;    rdfs:label "creation date"@en ;    rdfs:comment "The date when a piece of art was created."@en ;    rdfs:domain art:Art ;    rdfs:range xsd:date ;.



@prefix ex: <http://example.org/art#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .<http://example.org/art> a owl:Ontology ;  rdfs:comment "A basic ontology for the domain of art."@en .ex:Art a owl:Class ;  rdfs:comment "The class of all works of art."@en .ex:Artist a owl:Class ;  rdfs:comment "The class of all artists."@en .ex:hasArtist a owl:ObjectProperty ;  rdfs:domain ex:Art ;  rdfs:range ex:Artist ;  rdfs:comment "The artist who created a work of art."@en .ex:hasStyle a owl:DatatypeProperty ;  rdfs:domain ex:Art ;  rdfs:range xsd:string ;  rdfs:comment "The style of a work of art."@en .ex:hasPeriod a owl:DatatypeProperty ;  rdfs:domain ex:Art ;  rdfs:range xsd:string ;  rdfs:comment "The period during which a work of art was created."@en .ex:Museum a owl:Class ;  rdfs:comment "The class of all art museums."@en .ex:housesArt a owl:ObjectProperty ;  rdfs:domain ex:Museum ;  rdfs:range ex:Art ;  rdfs:comment "The relation between an art museum and the works of art it houses."@en .ex:ArtGenre a owl:Class ;  rdfs:comment "The class of all genres of art."@en .ex:belongsGenre a owl:ObjectProperty ;  rdfs:domain ex:Art ;  rdfs:range ex:ArtGenre ;  rdfs:comment "The genre to which a work of art belongs."@en .


@prefix : <http://example.org/art#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .:Art a rdfs:Class ;    rdfs:comment "The class representing all types of art." .:Painting a rdfs:Class ;    rdfs:subClassOf :Art ;    rdfs:comment "The class representing all paintings." .:Sculpture a rdfs:Class ;    rdfs:subClassOf :Art ;    rdfs:comment "The class representing all sculptures." .:DigitalArt a rdfs:Class ;    rdfs:subClassOf :Art ;    rdfs:comment "The class representing all digital art works." .    :Artist a rdfs:Class ;    rdfs:comment "The class representing all artists." .:hasCreator a rdf:Property ;    rdfs:domain :Art ;    rdfs:range :Artist ;    rdfs:comment "The property representing the artist who created a piece of art." .:hasTitle a rdf:Property ;    rdfs:domain :Art ;    rdfs:range rdfs:Literal ;    rdfs:comment "The property representing the title of a piece of art." .:hasYear a rdf:Property ;    rdfs:domain :Art ;    rdfs:range rdfs:Literal ;    rdfs:comment "The property representing the year of creation for a piece of art." .:hasMedium a rdf:Property ;    rdfs:domain :Art ;    rdfs:range rdfs:Literal ;    rdfs:comment "The property representing the medium of a piece of art." .  :exhibitedIn a rdf:Property ;    rdfs:domain :Art ;    rdfs:range :Museum ;    rdfs:comment "The property representing the museum where a piece of art is exhibited." .:Museum a rdfs:Class ;    rdfs:comment "The class representing all museums." .

