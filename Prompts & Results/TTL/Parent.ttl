//The main class for Parentclass Parent{  //Properties of the Parent  String name;  int age;  List<Child> children = new ArrayList<Child>();  //Constructor for the Parent class  Parent(String name, int age){    this.name = name;    this.age = age;  }  //Add child to the Parent's children list.  void addChild(Child child){    this.children.add(child);  }  //getters for Parent's details  String getName(){ return this.name; }  int getAge(){ return this.age; }  List<Child> getChildren(){ return this.children; }}//The main class for Childclass Child{  //Properties of the Child  String name;  int age;  //Constructor for the Child class  Child(String name, int age){    this.name = name;    this.age = age;  }  //getters for Child's details  String getName(){ return this.name; }  int getAge(){ return this.age; }}
