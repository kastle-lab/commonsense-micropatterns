

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix place: <http://example.org/place#> .place:   rdf:type owl:Ontology ;  rdfs:comment "An ontology about places" .place:Place   rdf:type owl:Class ;  rdfs:comment "A geographic location" .place:City   rdf:type owl:Class ;  rdfs:comment "A large human settlement" ;  rdfs:subClassOf place:Place .place:Country   rdf:type owl:Class ;  rdfs:comment "A region that is identified as a distinct entity in political geography" ;  rdfs:subClassOf place:Place .place:Continent   rdf:type owl:Class ;  rdfs:comment "One of the world's main continuous expanses of land" ;  rdfs:subClassOf place:Place .place:Building   rdf:type owl:Class ;  rdfs:comment "A structure with a roof and walls standing more or less permanently in one place" ;  rdfs:subClassOf place:Place .place:landmark   rdf:type owl:Class ;  rdfs:comment "An object or feature of a landscape or town that is easily seen and recognized from a distance, especially one that marks a location" ;  rdfs:subClassOf place:Place .place:isLocatedIn  rdf:type owl:ObjectProperty ;  rdfs:comment "Indicates the place where a certain location is situated" ;  rdfs:domain place:Place ;  rdfs:range place:Place .place:hasName  rdf:type owl:DatatypeProperty ;  rdfs:comment "Represent the name of a place" ;  rdfs:domain place:Place ;  rdfs:range xsd:string .




@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix place: <http://www.example.org/place#> .place:Place    rdf:type owl:Class ;   rdfs:label "Place" ;   rdfs:comment "A location, area or space." .place:hasName   rdf:type owl:DatatypeProperty ;   rdfs:label "has name" ;   rdfs:domain place:Place ;   rdfs:range xsd:string .place:Country    rdf:type owl:Class ;   rdfs:label "Country" ;   rdfs:subClassOf place:Place ;   rdfs:comment "A political entity usually comprising numerous states or provinces" . place:City   rdf:type owl:Class ;   rdfs:label "City" ;   rdfs:subClassOf place:Place ;   rdfs:comment "A large human settlement, can be part of a state or province." .place:isLocatedIn    rdf:type owl:ObjectProperty ;   rdfs:label "is located in" ;   rdfs:domain place:City ;   rdfs:range place:Country .place:Park   rdf:type owl:Class ;   rdfs:label "Park" ;   rdfs:subClassOf place:Place ;   rdfs:comment "A public area of land for enjoyment of nature." .place:isPartOf    rdf:type owl:ObjectProperty ;   rdfs:label "is part of" ;   rdfs:domain place:Park ;   rdfs:range place:City .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix place: <http://www.example.org/place#> .place:    a rdfs:Class ;    rdfs:comment "The class of all places" ;    rdfs:label "Place" .place:name    a rdf:Property ;    rdfs:comment "The name of the place" ;    rdfs:label "Name" ;    rdfs:domain place: ;    rdfs:range xsd:string .place:hasLocation    a rdf:Property ;    rdfs:comment "The specific location of the place" ;    rdfs:label "Has Location" ;    rdfs:domain place: ;    rdfs:range xsd:string .place:hasType    a rdf:Property ;    rdfs:comment "The type of the place (e.g., city, country, etc.)" ;    rdfs:label "Has Type" ;    rdfs:domain place: ;    rdfs:range place:Type .place:Type    a rdfs:Class ;    rdfs:comment "The class of all types of places" ;    rdfs:label "Type" .place:Country    a place:Type ;    rdfs:comment "The type of place that is a country" ;    rdfs:label "Country" .place:City    a place:Type ;    rdfs:comment "The type of place that is a city" ;    rdfs:label "City" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix place: <http://example.org/place#> .place:Place rdf:type owl:Class .place:hasName rdf:type owl:DatatypeProperty ;    rdfs:domain place:Place ;    rdfs:range xsd:string .place:hasAddress rdf:type owl:DatatypeProperty ;    rdfs:domain place:Place ;    rdfs:range xsd:string .place:locatedIn rdf:type owl:ObjectProperty ;    rdfs:domain place:Place ;    rdfs:range place:Place .place:City rdf:type owl:Class ;    rdfs:subClassOf place:Place .place:Country rdf:type owl:Class ;    rdfs:subClassOf place:Place .place:Continent rdf:type owl:Class ;    rdfs:subClassOf place:Place .place:hasLatitude rdf:type owl:DatatypeProperty ;    rdfs:domain place:Place ;    rdfs:range xsd:float .place:hasLongitude rdf:type owl:DatatypeProperty ;    rdfs:domain place:Place ;    rdfs:range xsd:float .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix place: <http://www.example.org/place#> .place:Place rdf:type owl:Class ;    rdfs:label "Place"@en ;    rdfs:comment "A location, which may be physical or virtual"@en .place:City rdf:type owl:Class ;    rdfs:subClassOf place:Place ;    rdfs:label "City"@en ;    rdfs:comment "A large and permanent human settlement"@en .place:Country rdf:type owl:Class ;    rdfs:subClassOf place:Place ;    rdfs:label "Country"@en ;    rdfs:comment "A region that is identified as a distinct entity in political geography"@en .place:hasLocation rdf:type owl:ObjectProperty ;    rdfs:label "has location"@en ;    rdfs:comment "Relates a Place to its specific geographical coordinates"@en ;    rdfs:domain place:Place ;    rdfs:range place:GeographicalCoordinates.place:isLocatedIn rdf:type owl:ObjectProperty ;    rdfs:label "is located in"@en ;    rdfs:comment "Relates a Place to the larger Place that contains it"@en ;    rdfs:domain place:Place ;    rdfs:range place:Place.place:GeographicalCoordinates rdf:type owl:Class ;    rdfs:label "Geographical coordinates"@en ;    rdfs:comment "A point's geographical coordinates expressed in latitude and longitude"@en .place:latitude rdf:type owl:DatatypeProperty ;    rdfs:label "latitude"@en ;    rdfs:comment "The latitude of a geographical coordinates"@en ;    rdfs:domain place:GeographicalCoordinates ;    rdfs:range xsd:decimal.place:longitude rdf:type owl:DatatypeProperty ;    rdfs:label "longitude"@en ;    rdfs:comment "The longitude of a geographical coordinates"@en ;    rdfs:domain place:GeographicalCoordinates ;    rdfs:range xsd:decimal.


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix place: <http://example.org/place#> .place:Place rdf:type rdfs:Class ;  rdfs:comment "A location or area."@en .place:name rdf:type rdf:Property ;  rdfs:comment "The name of a place."@en ;  rdfs:domain place:Place ;  rdfs:range xsd:string .place:latitude rdf:type rdf:Property ;  rdfs:comment "The latitude of a place."@en ;  rdfs:domain place:Place ;  rdfs:range xsd:decimal .place:longitude rdf:type rdf:Property ;  rdfs:comment "The longitude of a place."@en ;  rdfs:domain place:Place ;  rdfs:range xsd:decimal .place:Country rdf:type rdfs:Class ;  rdfs:subClassOf place:Place ;  rdfs:comment "A country, a distinct region in geography."@en .place:city rdf:type rdfs:Class ;  rdfs:subClassOf place:Place ;  rdfs:comment "A city, a large and permanent human settlement."@en .place:countryOf rdf:type rdf:Property ;  rdfs:domain place:city ;  rdfs:range place:Country ;  rdfs:comment "The country in which a city is located."@en .place:population rdf:type rdf:Property ;  rdfs:domain place:Place ;  rdfs:range xsd:integer ;  rdfs:comment "The population of a place."@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix geo: <http://www.geonames.org/ontology#> .geo:Place rdf:type rdfs:Class .geo:city rdf:type geo:Place ;    rdfs:label "City"@en .geo:country rdf:type geo:Place ;    rdfs:label "Country"@en .geo:continent rdf:type geo:Place ;    rdfs:label "Continent"@en .geo:region rdf:type geo:Place ;    rdfs:label "Region"@en .geo:hasPopulation rdf:type rdf:Property ;    rdfs:domain geo:Place ;    rdfs:range xsd:integer ;    rdfs:label "has population"@en .geo:hasArea rdf:type rdf:Property ;    rdfs:domain geo:Place ;    rdfs:range xsd:float ;    rdfs:label "has area"@en .geo:isLocatedIn rdf:type rdf:Property ;    rdfs:domain geo:Place ;    rdfs:range geo:Place ;    rdfs:label "is located in"@en .geo:hasName rdf:type rdf:Property ;    rdfs:domain geo:Place ;    rdfs:range xsd:string ;    rdfs:label "has name"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.com/> .#Defining "Place" as a Classex:Place rdf:type rdfs:Class .#Properties related to "Place"ex:name rdf:type rdf:Property ;    rdfs:domain ex:Place ;    rdfs:range rdfs:Literal .    ex:hasPostalAddress rdf:type rdf:Property ;    rdfs:domain ex:Place ;    rdfs:range ex:PostalAddress .    ex:containedInPlace rdf:type rdf:Property ;    rdfs:domain ex:Place ;    rdfs:range ex:Place .#Defining "PostalAddress" as a Class    ex:PostalAddress rdf:type rdfs:Class .#Properties related to "PostalAddress"ex:streetAddress rdf:type rdf:Property ;    rdfs:domain ex:PostalAddress ;    rdfs:range rdfs:Literal .ex:postCode rdf:type rdf:Property ;    rdfs:domain ex:PostalAddress ;    rdfs:range rdfs:Literal .ex:addressLocality rdf:type rdf:Property ;    rdfs:domain ex:PostalAddress ;    rdfs:range rdfs:Literal .ex:addressCountry rdf:type rdf:Property ;    rdfs:domain ex:PostalAddress ;    rdfs:range rdfs:Literal .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix place: <http://example.com/place#> .# Class definitionsplace:Place rdf:type rdfs:Class .place:Country rdf:type rdfs:Class .place:City rdf:type rdfs:Class .place:Region rdf:type rdfs:Class .# Sub-class declarationsplace:Country rdfs:subClassOf place:Place .place:City rdfs:subClassOf place:Place .place:Region rdfs:subClassOf place:Place .# Property definitionsplace:hasName rdf:type rdf:Property ;    rdfs:range xsd:string ;    rdfs:domain place:Place .    place:locatedIn rdf:type rdf:Property ;    rdfs:range place:Place ;    rdfs:domain place:Place . place:hasPopulation rdf:type rdf:Property ;    rdfs:range xsd:integer ;    rdfs:domain place:Place . place:hasArea rdf:type rdf:Property ;    rdfs:range xsd:decimal ;    rdfs:domain place:Place . # Example instancesplace:UnitedStates rdf:type place:Country ;    place:hasName "United States"^^xsd:string ;    place:hasPopulation 328200000^^xsd:integer ;    place:hasArea 9629091.0^^xsd:decimal .    place:California rdf:type place:Region ;    place:hasName "California"^^xsd:string ;    place:hasPopulation 39538223^^xsd:integer ;    place:hasArea 423967.0^^xsd:decimal ;    place:locatedIn place:UnitedStates .place:LosAngeles rdf:type place:City ;    place:hasName "Los Angeles"^^xsd:string ;    place:hasPopulation 3977686^^xsd:integer ;    place:hasArea 503.0^^xsd:decimal ;    place:locatedIn place:California .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix my: <http://www.example.org/place#> .my:Place a owl:Class ;    rdfs:label "Place"@en ;    rdfs:comment "A location or area"@en ;    rdfs:subClassOf owl:Thing .my:Country a owl:Class ;    rdfs:label "Country"@en ;    rdfs:comment "A nation with its own government, occupying a particular territory"@en ;    rdfs:subClassOf my:Place .my:City a owl:Class ;    rdfs:label "City"@en ;    rdfs:comment "A large town, in particular."@en ;    rdfs:subClassOf my:Place .my:Street a owl:Class ;    rdfs:label "Street"@en ;    rdfs:comment "A public road in a city or town, typically with houses and buildings on one or both sides"@en ;    rdfs:subClassOf my:Place .my:name a owl:DatatypeProperty ;    rdfs:label "name"@en ;    rdfs:comment "The name of a place"@en ;    rdfs:domain my:Place ;    rdfs:range xsd:string .my:hasLocation a owl:ObjectProperty ;    rdfs:label "hasLocation"@en ;    rdfs:comment "Indicates the Place (city, country, etc.) where this Place is located"@en ;    rdfs:domain my:Place ;    rdfs:range my:Place .my:population a owl:DatatypeProperty ;    rdfs:label "population"@en ;    rdfs:comment "The number of people living in a particular place"@en ;    rdfs:domain my:Place ;    rdfs:range xsd:integer .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix place: <http://www.example.org/place#> .# Class Placeplace:Place rdf:type rdfs:Class ;      rdfs:label "Place" ;      rdfs:comment "Concept of a Place." .# Sub Class Cityplace:City rdf:type rdfs:Class ;      rdfs:label "City" ;      rdfs:subClassOf place:Place ;      rdfs:comment "Concept of a City, a subtype of Place." .# Sub Class Countryplace:Country rdf:type rdfs:Class ;      rdfs:label "Country" ;      rdfs:subClassOf place:Place ;      rdfs:comment "Concept of a Country, a subtype of Place." .# Property locatedInplace:locatedIn rdf:type rdf:Property ;        rdfs:label "located in" ;        rdfs:domain place:Place ;        rdfs:range place:Place ;        rdfs:comment "Expresses that a place is located in another place." .# Property hasPopulationplace:hasPopulation rdf:type rdf:Property ;        rdfs:label "has population" ;        rdfs:domain place:Place ;        rdfs:range xsd:integer ;        rdfs:comment "Expresses the population of a place." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix place: <http://www.example.org/place#> .place:Place rdf:type rdfs:Class .place:Country rdf:type rdfs:Class ;    rdfs:subClassOf place:Place .place:State rdf:type rdfs:Class ;    rdfs:subClassOf place:Place .place:City rdf:type rdfs:Class ;    rdfs:subClassOf place:Place .place:Town rdf:type rdfs:Class ;    rdfs:subClassOf place:Place .place:Road rdf:type rdfs:Class ;    rdfs:subClassOf place:Place .place:isPartOf rdf:type rdf:Property ;    rdfs:domain place:Place ;    rdfs:range place:Place .place:hasName rdf:type rdf:Property ;    rdfs:domain place:Place ;    rdfs:range xsd:string .place:hasPopulation rdf:type rdf:Property ;    rdfs:domain place:Place ;    rdfs:range xsd:integer .place:hasArea rdf:type rdf:Property ;    rdfs:domain place:Place ;    rdfs:range xsd:float .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com/> .# Defining the class 'Place'ex:Place rdf:type rdfs:Class ;    # Defining the label 'Place'    rdfs:label "Place"@en ;      # Defining the comment for 'Place'    rdfs:comment "A location. This can include countries, cities, towns, villages, streets, buildings, etc."@en .# Defining the property 'hasName'ex:hasName rdf:type rdf:Property ;          # Defining the label 'hasName'    rdfs:label "has name"@en ;    # Defining the comment for 'hasName'    rdfs:comment "The name of the place"@en ;    # Defining the domain as 'Place'    rdfs:domain ex:Place ;    # Defining the range as string    rdfs:range xsd:string .# Defining the property 'locatedIn'ex:locatedIn rdf:type rdf:Property ;      # Defining the label 'locatedIn'    rdfs:label "located in"@en ;    # Defining the comment for 'locatedIn'    rdfs:comment "The larger geographic entity which contains the place"@en ;    # Defining the domain as 'Place'    rdfs:domain ex:Place ;    # Defining the range as 'Place'    rdfs:range ex:Place .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix place: <http://www.example.org/place#> .place:Place rdf:type rdfs:Class ;    rdfs:comment "The class of all places"^^xsd:string ;    rdfs:label "Place"^^xsd:string .place:name rdf:type rdf:Property ;    rdfs:domain place:Place ;    rdfs:range xsd:string ;    rdfs:comment "The name of a place"^^xsd:string ;    rdfs:label "name"^^xsd:string .place:location rdf:type rdf:Property ;    rdfs:domain place:Place ;    rdfs:range xsd:string ;    rdfs:comment "The physical location of a place"^^xsd:string ;    rdfs:label "location"^^xsd:string .place:Country rdf:type rdfs:Class ;    rdfs:subClassOf place:Place ;    rdfs:comment "The class of all countries"^^xsd:string ;    rdfs:label "Country"^^xsd:string .place:City rdf:type rdfs:Class ;    rdfs:subClassOf place:Place ;    rdfs:comment "The class of all cities"^^xsd:string ;    rdfs:label "City"^^xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix my: <http://www.example.org/myontology#> .my:Place rdf:type rdfs:Class .my:name rdf:type rdf:Property ;        rdfs:domain my:Place ;        rdfs:range xsd:string .my:latitude rdf:type rdf:Property ;           rdfs:domain my:Place ;           rdfs:range xsd:decimal .my:longitude rdf:type rdf:Property ;            rdfs:domain my:Place ;            rdfs:range xsd:decimal .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix geo: <http://www.opengis.net/ont/geosparql#> .@prefix dct: <http://purl.org/dc/terms/> .# Defining a Place Class:Place rdf:type rdfs:Class ;    rdfs:comment "A location, area, or site"  ;    rdfs:subClassOf geo:Feature .# Defining basic attributes of Place:name rdf:type rdf:Property ;      rdf:domain :Place ;      rdf:range xsd:string .:hasExactLocation rdf:type rdf:Property ;      rdf:domain :Place ;      rdf:range geo:Point .# Defining the date a Place was established:dateEstablished rdf:type rdf:Property ;      rdf:domain :Place ;      rdf:range dct:date .# Defining the population of a Place:population rdf:type rdf:Property ;      rdf:domain :Place ;      rdf:range xsd:integer . # Defining the area of a Place:area rdf:type rdf:Property ;      rdf:domain :Place ;      rdf:range xsd:float . # Defining a point of interest within a Place:hasPointOfInterest rdf:type rdf:Property ;      rdf:domain :Place ;      rdf:range :PointOfInterest . # Defining a PointOfInterest Class:PointOfInterest rdf:type rdfs:Class ;    rdfs:comment "A place of interest within a location, area, or site" ;    rdfs:subClassOf geo:Feature .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix geo: <http://www.opengis.net/ont/geosparql#> .@prefix ex: <http://www.example.org/> .ex:Place rdf:type rdfs:Class ;  rdfs:label "Place"@en ;  rdfs:comment "A location, i.e., spatial area or site"@en ;  rdfs:subClassOf geo:Feature .ex:hasName rdf:type rdf:Property ;  rdfs:label "has name"@en ;  rdfs:comment "Relates a place to its name"@en ;  rdfs:domain ex:Place ;  rdfs:range xsd:string .ex:hasCoordinate rdf:type rdf:Property ;  rdfs:label "has coordinate"@en ;  rdfs:comment "Relates a place to its geographic coordinate"@en ;  rdfs:domain ex:Place ;  rdfs:range geo:Point .# Usage exampleex:Place1 a ex:Place ;  ex:hasName "Test Place" ;  ex:hasCoordinate "POINT (0 0)"^^geo:wktLiteral .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://www.example.com#> .:Place     rdf:type rdfs:Class ;    rdfs:label "Place" ;    rdfs:comment "A class representing a physical location or geographical entity" .:hasName     rdf:type owl:DatatypeProperty ;    rdfs:label "has name" ;    rdfs:comment "The name of the place" ;    rdfs:domain :Place ;    rdfs:range xsd:string .:hasLocation     rdf:type owl:ObjectProperty ;    rdfs:label "has location" ;    rdfs:comment "The specific geographical coordinate of a place" ;    rdfs:domain :Place ;    rdfs:range :Coordinate .:Coordinate     rdf:type rdfs:Class ;    rdfs:label "Coordinate" ;    rdfs:comment "A class representing a geographic coordinate system, typically including latitude and longitude" .:hasLatitude     rdf:type owl:DatatypeProperty ;    rdfs:label "has latitude" ;    rdfs:comment "The latitude of a coordinate" ;    rdfs:domain :Coordinate ;    rdfs:range xsd:decimal .:hasLongitude     rdf:type owl:DatatypeProperty ;    rdfs:label "has longitude" ;    rdfs:comment "The longitude of a coordinate" ;    rdfs:domain :Coordinate ;    rdfs:range xsd:decimal .
shellClass: Place   SubClassOf:       Entity      ObjectProperties:      hasLocation,      hasName      DataProperties:      latitude,      longitude,      name   AnnotationProperties:      comment

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com/> .ex:Place rdf:type rdfs:Class .ex:name rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Place ;    rdfs:range xsd:string .ex:latitude rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Place ;    rdfs:range xsd:decimal .ex:longitude rdf:type owl:DatatypeProperty ;    rdfs:domain ex:Place ;    rdfs:range xsd:decimal .ex:locatedIn rdf:type owl:ObjectProperty ;    rdfs:domain ex:Place ;    rdfs:range ex:Place .

@prefix ex: <http://example.org/> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .ex:Place a rdfs:Class ;  rdfs:comment "A location or space" ;  rdfs:subClassOf owl:Thing .  ex:name a owl:DatatypeProperty ;  rdfs:domain ex:Place ;  rdfs:range xsd:string ;  rdfs:comment "The name of the place" .    ex:address a owl:DatatypeProperty ;  rdfs:domain ex:Place ;  rdfs:range xsd:string ;  rdfs:comment "The address of the place" .    ex:Place ex:hasPart ex:Place;  rdfs:comment "The place may include other places, e.g., a country includes cities" .ex:Place ex:isLocatedIn ex:Place;  rdfs:comment "The place is located within another place, e.g., a city is located in a country" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix place: <http://example.com/place/> .place:Place rdf:type rdfs:Class ;    rdfs:label "Place"@en ;    rdfs:comment "A location or area."@en .place:name rdf:type rdf:Property ;    rdfs:label "name"@en ;    rdfs:comment "The name of the place."@en ;    rdfs:domain place:Place ;    rdfs:range xsd:string .place:location rdf:type rdf:Property ;    rdfs:label "Location"@en ;    rdfs:comment "The geographical coordinates of the place."@en ;    rdfs:domain place:Place ;    rdfs:range xsd:string .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ex: <http://example.com/> .# Place Classex:Place a owl:Class ;    rdfs:comment "A class representing Places" ;    rdfs:label "Place" .# Property : hasNameex:hasName a owl:DatatypeProperty ;    rdfs:comment "Name of the place" ;    rdfs:label "hasName" ;    rdfs:domain ex:Place ;    rdfs:range rdf:langString .# Property : isLocatedInex:isLocatedIn a owl:ObjectProperty ;    rdfs:comment "Location of the place within another place" ;    rdfs:label "isLocatedIn" ;    rdfs:domain ex:Place ;    rdfs:range ex:Place .# Individual place : New Yorkex:NewYork a ex:Place ;    ex:hasName "New York" ;    ex:isLocatedIn ex:USA .    # Individual place : USAex:USA a ex:Place ;    ex:hasName "United States of America" .

@prefix ex: <http://example.org#> .@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .ex:Place_123 a ex:Place ;    ex:name "Example Place" ;    geo:lat "50.85045" ;    geo:long "4.34878" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix ex: <http://example.com/> .ex:Place a rdfs:Class ;    rdfs:label "Place" ;    rdfs:comment "A location or area" .ex:hasAddress a rdf:Property ;    rdfs:domain ex:Place ;    rdfs:range ex:Address .ex:Address a rdfs:Class ;    rdfs:label "Address" ;    rdfs:comment "The specific location details for a place" .ex:StreetAddress a rdf:Property ;    rdfs:domain ex:Address ;    rdfs:range rdf:string .ex:City a rdf:Property ;    rdfs:domain ex:Address ;    rdfs:range rdf:string .ex:Country a rdf:Property ;    rdfs:domain ex:Address ;    rdfs:range rdf:string .

