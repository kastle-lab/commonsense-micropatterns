@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix part: <http://example.com/part#> .part:Part    rdf:type owl:Class ;    rdfs:comment "Representation of a part within an ontology."@en ;    rdfs:label "Part"@en .part:hasPartNumber    rdf:type owl:DatatypeProperty ;    rdfs:domain part:Part ;    rdfs:range xsd:string ;    rdfs:comment "The unique identifier of a part."@en ;    rdfs:label "hasPartNumber"@en .part:belongsToAssembly    rdf:type owl:ObjectProperty ;    rdfs:domain part:Part ;    rdfs:range part:Assembly ;    rdfs:comment "The assembly the part belongs to."@en ;    rdfs:label "belongsToAssembly"@en .part:Assembly    rdf:type owl:Class ;    rdfs:comment "An assembly, which is a group of parts."@en ;    rdfs:label "Assembly"@en .
