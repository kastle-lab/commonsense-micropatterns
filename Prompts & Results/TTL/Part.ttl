@prefix ex: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .ex:Part a rdfs:Class ;    rdfs:label "Part"@en ;    rdfs:comment "Representing a part of something."@en ;    rdfs:subClassOf owl:Thing .# assuming a part has a name and it is related to some product (source)ex:name a rdf:Property;    rdfs:domain ex:Part ;    rdfs:range rdfs:Literal .ex:source a rdf:Property;    rdfs:domain ex:Part ;    rdfs:range ex:Product .# Sample Part instanceex:part1 a ex:Part ;    ex:name "Part A" ;    ex:source ex:product1 .# Sample Product instance ex:product1 a ex:Product ;    rdfs:label "Product 1" .

