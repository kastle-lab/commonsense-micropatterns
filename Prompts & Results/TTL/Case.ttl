@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix case: <http://www.example.org/case#> .case:     rdf:type owl:Ontology ;    rdfs:comment "Ontology of the conceptual class Case" .case:Case    rdf:type owl:Class ;    rdfs:label "Case" ;    rdfs:comment "Conceptual class of Case" .case:hasID    rdf:type owl:DatatypeProperty ;    rdfs:domain case:Case ;    rdfs:range xsd:string ;    rdfs:label "has ID" ;    rdfs:comment "The ID number of the case" .case:hasTitle    rdf:type owl:DatatypeProperty ;    rdfs:domain case:Case ;    rdfs:range xsd:string ;    rdfs:label "has title" ;    rdfs:comment "The title or description of the case" .case:hasStatus    rdf:type owl:DatatypeProperty ;    rdfs:domain case:Case ;    rdfs:range xsd:string ;    rdfs:label "has status" ;    rdfs:comment "Current status of the case" .case:belongsTo    rdf:type owl:ObjectProperty ;    rdfs:domain case:Case ;    rdfs:range case:Entity ;    rdfs:label "belongs to" ;    rdfs:comment "The entity that the case belongs to" .case:Entity    rdf:type owl:Class ;    rdfs:label "Entity" ;    rdfs:comment "An entity that can have cases associated with it" .

