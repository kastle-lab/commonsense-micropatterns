
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix com: <http://example.com/community#> .com:Member     rdf:type rdfs:Class .com:Community     rdf:type rdfs:Class ;    rdfs:subClassOf rdfs:Resource ;    rdfs:comment "A social group of any size whose members reside in a specific locality, share government, and often have a common cultural and historical heritage."@en .com:hasMember     rdf:type rdf:Property ;    rdfs:domain com:Community ;    rdfs:range com:Member .com:Role     rdf:type rdfs:Class ;    rdfs:comment "Roles that members can assume in a community."@en .com:hasRole     rdf:type rdf:Property ;    rdfs:domain com:Member ;    rdfs:range com:Role .com:Organization     rdf:type rdfs:Class ;    rdfs:subClassOf com:Member ;    rdfs:comment "Organizations can be members of a community."@en .com:Individual     rdf:type rdfs:Class ;    rdfs:subClassOf com:Member ;    rdfs:comment "Individual persons can be members of a community."@en .com:Event     rdf:type rdfs:Class ;    rdfs:comment "Events that occur within a community."@en .com:hasEvent     rdf:type rdf:Property ;    rdfs:domain com:Community ;    rdfs:range com:Event .com:Cultural_Identity     rdf:type rdfs:Class ;    rdfs:comment "Cultural identity unique to a community."@en .com:hasCultural_Identity     rdf:type rdf:Property ;    rdfs:domain com:Community ;    rdfs:range com:Cultural_Identity .com:Community_Service     rdf:type rdfs:Class ;    rdfs:comment "Services provided within a community."@en .com:providesService     rdf:type rdf:Property ;    rdfs:domain com:Community ;    rdfs:range com:Community_Service .




@prefix ex: <http://example.com/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .ex:Community    rdf:type owl:Class ;    rdfs:label "Community" ;    rdfs:comment "A group of individuals that interact within a specific location or share common interests" ;.ex:hasMember    rdf:type owl:ObjectProperty ;    rdfs:label "has member" ;    rdfs:comment "Relates a community to its members" ;    rdfs:domain ex:Community ;    rdfs:range ex:Person ;.ex:Person    rdf:type owl:Class ;    rdfs:label "Person" ;    rdfs:comment "An individual human being" ;.ex:isLocatedIn    rdf:type owl:ObjectProperty ;    rdfs:label "is located in" ;    rdfs:comment "Indicates the location of a community" ;    rdfs:domain ex:Community ;    rdfs:range ex:Location ;.ex:Location    rdf:type owl:Class ;    rdfs:label "Location" ;    rdfs:comment "A geographical location" ;.ex:hasInterest    rdf:type owl:ObjectProperty ;    rdfs:label "has interest" ;    rdfs:comment "Indicates an interest shared by members of a community" ;    rdfs:domain ex:Community ;    rdfs:range ex:Interest ;.ex:Interest    rdf:type owl:Class ;    rdfs:label "Interest" ;    rdfs:comment "A shared interest or activity within a community" ;.

