@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix com: <http://www.example.com/communities#> .com:Community     rdf:type owl:Class ;    rdfs:label "Community"@en ;    rdfs:comment "A group of people living in the same place or having a particular characteristic in common"@en .com:name    rdf:type owl:DatatypeProperty ;    rdfs:label "name"@en ;    rdfs:domain com:Community ;    rdfs:range xsd:string .com:hasMember     rdf:type owl:ObjectProperty ;    rdfs:label "has member"@en ;    rdfs:domain com:Community ;    rdfs:range com:Member .com:Member     rdf:type owl:Class ;    rdfs:label "Member"@en ;    rdfs:comment "An individual belongs to a community"@en .com:locatedIn    rdf:type owl:ObjectProperty ;    rdfs:label "located in"@en ;    rdfs:domain com:Community ;    rdfs:range com:Location .com:Location     rdf:type owl:Class ;    rdfs:label "Location"@en ;    rdfs:comment "The geographical area where a community is based"@en .    com:population      rdf:type owl:DatatypeProperty ;    rdfs:label "population"@en ;    rdfs:domain com:Community ;    rdfs:range xsd:integer .  



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix comm: <http://example.org/ontology/community#> .comm:Community rdf:type owl:Class ;               rdfs:comment "A group of people living in the same place or having a particular characteristic in common." ;                rdfs:label "Community" ;               rdfs:subClassOf owl:Thing .comm:hasMember rdf:type owl:ObjectProperty ;               rdfs:label "has member" ;               rdfs:comment "A relation between a community and an individual who is a member of that community." ;               rdfs:domain comm:Community ;                rdfs:range comm:CommunityMember .comm:CommunityMember rdf:type owl:Class ;                    rdfs:comment "An individual who is a member of a community." ;                    rdfs:label "Community Member" ;                    rdfs:subClassOf owl:Thing .comm:hasCommunityRole rdf:type owl:ObjectProperty ;                     rdfs:label "has community role" ;                     rdfs:comment "A relation between a member of a community and their role within that community." ;                     rdfs:domain comm:CommunityMember ;                     rdfs:range comm:CommunityRole .comm:CommunityRole rdf:type owl:Class ;                  rdfs:comment "A position or function that a member can have within a community." ;                  rdfs:label "Community Role" ;                  rdfs:subClassOf owl:Thing .comm:isLocatedIn rdf:type owl:ObjectProperty ;                rdfs:label "is located in" ;                rdfs:comment "A relation between a community and the geographical location where it exists." ;                rdfs:domain comm:Community ;                rdfs:range comm:GeographicalLocation .comm:GeographicalLocation rdf:type owl:Class ;                          rdfs:comment "A specific place defined by geographical coordinates or other landmark." ;                          rdfs:label "Geographical Location" ;                          rdfs:subClassOf owl:Thing .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix com: <http://example.com/vocab#> .com:Community rdf:type rdfs:Class ;    rdfs:comment "A group of people who live in the same area or have a common interest or goal" .com:name rdf:type rdf:Property ;    rdfs:label "name" ;    rdfs:domain com:Community ;    rdfs:range xsd:string ;    rdfs:comment "The name of the community" .com:location rdf:type rdf:Property ;    rdfs:label "location" ;    rdfs:domain com:Community ;    rdfs:range xsd:string ;    rdfs:comment "The location of the community" .com:member rdf:type rdf:Property ;    rdfs:label "member" ;    rdfs:domain com:Community ;    rdfs:range xsd:string ;    rdfs:comment "A member of the community" .com:goal rdf:type rdf:Property ;    rdfs:label "goal" ;    rdfs:domain com:Community ;    rdfs:range xsd:string ;    rdfs:comment "The common goal of the community".

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix comm: <http://example.com/community#> .comm:Community a rdfs:Class ;    rdfs:comment "A group of people living in the same place or having a particular characteristic in common" ;    rdfs:subClassOf rdfs:Resource .comm:hasMember a rdf:Property ;    rdfs:comment "The relationship between a community and its member" ;    rdfs:domain comm:Community ;    rdfs:range comm:Member .comm:Member a rdfs:Class ;    rdfs:comment "An individual who is part of a community" ;    rdfs:subClassOf rdfs:Resource .comm:hasRole a rdf:Property ;    rdfs:comment "The relationship between a member and their role within the community" ;    rdfs:domain comm:Member ;    rdfs:range comm:Role .comm:Role a rdfs:Class ;    rdfs:comment "A function assumed or part played by a member in a particular situation in a community" ;    rdfs:subClassOf rdfs:Resource .comm:hasEvent a rdf:Property ;    rdfs:comment "A relationship between a community and an event taking place in the community" ;    rdfs:domain comm:Community ;    rdfs:range comm:Event .comm:Event a rdfs:Class ;    rdfs:comment "An occurrence happening at a determinable time and place, with or without the participation of members" ;    rdfs:subClassOf rdfs:Resource .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix community: <http://example.org/community#> .community:   rdf:type owl:Ontology ;  rdfs:comment "An ontology representing communities" .community:Community   rdf:type owl:Class ;  rdfs:comment "A class representing a generic community" ;  rdfs:subClassOf owl:Thing .community:member   rdf:type owl:ObjectProperty ;  rdfs:domain community:Community ;  rdfs:range community:Member ;  rdfs:comment "A property linking Community with its Members" .community:Member   rdf:type owl:Class ;  rdfs:comment "A class representing a member of a community" ;  rdfs:subClassOf owl:Thing .community:hasName   rdf:type owl:DatatypeProperty ;  rdfs:domain community:Member ;  rdfs:range xsd:string ;  rdfs:comment "A property representing the name of a member" .community:hasAge   rdf:type owl:DatatypeProperty ;  rdfs:domain community:Member ;  rdfs:range xsd:integer ;  rdfs:comment "A property representing the age of a member" .community:hasGender   rdf:type owl:DatatypeProperty ;  rdfs:domain community:Member ;  rdfs:range xsd:string ;   rdfs:comment "A property representing the gender of a member" . community:LocatedIn   rdf:type owl:Class ;  rdfs:comment "A class representing the location of a community" ;   rdfs:subClassOf owl:Thing .community:hasLocation   rdf:type owl:ObjectProperty ;  rdfs:domain community:Community ;  rdfs:range community:LocatedIn ;   rdfs:comment "A property linking a Community to its Location" .


