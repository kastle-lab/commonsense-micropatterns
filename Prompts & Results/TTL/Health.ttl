@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix health: <http://example.com/health#> .health:Patient a rdfs:Class ;    rdfs:label "Patient".health:Doctor a rdfs:Class ;    rdfs:label "Doctor".health:hasDoctor a rdf:Property ;    rdfs:domain health:Patient ;    rdfs:range health:Doctor ;    rdfs:label "has doctor" .health:hasPatient a rdf:Property ;    rdfs:domain health:Doctor ;    rdfs:range health:Patient ;    rdfs:label "has patient" .

@en ;              rdfs:comment "This represents the health status of an individual"@en .health:disease rdf:type rdf:Property ;               rdfs:domain health:Health ;               rdfs:range xsd:string ;               rdfs:label "disease"@en ;               rdfs:comment "The specific disease that the individual has, if any"@en .health:hasMedication rdf:type rdf:Property ;                     rdfs:domain health:Health ;                     rdfs:range health:Medication ;                     rdfs:label "has medication"@en ;                     rdfs:comment "The medication that the individual is currently taking, if any"@en .health:Medication rdf:type rdfs:Class ;                  rdfs:label "Medication"@en ;                  rdfs:comment "This represents a specific medication, which could be prescribed to an individual"@en .health:medicationName rdf:type rdf:Property ;                      rdfs:domain health:Medication ;                      rdfs:range xsd:string ;                      rdfs:label "medication name"@en ;                      rdfs:comment "The name of the medication"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix health: <http://www.example.com/health#> .health:Disease rdf:type rdfs:Class ;    rdfs:label "Disease" ;    rdfs:comment "Disease is an abnormal condition affecting the body of an organism" .    health:Symptom rdf:type rdfs:Class ;    rdfs:label "Symptom" ;    rdfs:comment "Symptoms are signs or signals of disease or disorder".health:hasSymptom rdf:type rdf:Property;    rdfs:label "has symptom";    rdfs:domain health:Disease;    rdfs:range health:Symptom;    rdfs:comment "Relates a disease to its symptoms".health:Treatment rdf:type rdfs:Class;    rdfs:label "Treatment";    rdfs:comment "Medical treatment is a procedure used to cure, manage or prevent a disease".health:hasTreatment rdf:type rdf:Property;    rdfs:label "has treatment";    rdfs:domain health:Disease;    rdfs:range health:Treatment;    rdfs:comment "Relates a disease to its treatment".    health:HealthCareProfessional rdf:type rdfs:Class;    rdfs:label "HealthCareProfessional";    rdfs:comment "Healthcare professional is a person who is qualified and licensed to provide health care services".


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://example.com/ontology/health#> .:PhysicalHealth rdf:type rdfs:Class ;    rdfs:label "Physical Health" ;    rdfs:comment "Represents the physical state of a person's health" .:MentalHealth rdf:type rdfs:Class ;    rdfs:label "Mental Health" ;    rdfs:comment "Represents the mental/emotional state of a person's health" .:Person rdf:type rdfs:Class ;    rdfs:comment "Represents an individual person" .:hasPhysicalHealth rdf:type rdf:Property ;    rdfs:domain :Person ;    rdfs:range :PhysicalHealth ;    rdfs:label "has physical health" ;    rdfs:comment "A property that indicates a person's physical health" .:hasMentalHealth rdf:type rdf:Property ;    rdfs:domain :Person ;    rdfs:range :MentalHealth ;    rdfs:label "has mental health" ;    rdfs:comment "A property that indicates a person's mental health" .
