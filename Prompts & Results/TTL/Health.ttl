@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://example.org/health#> .:Health rdf:type owl:Class .:Patient rdf:type owl:Class .:Doctor rdf:type owl:Class .:Treatment rdf:type owl:Class .:hasHealth rdf:type owl:ObjectProperty ;  rdfs:domain :Patient ;  rdfs:range :Health .:hasDoctor rdf:type owl:ObjectProperty ;  rdfs:domain :Patient ;  rdfs:range :Doctor .:hasPatient rdf:type owl:ObjectProperty ;  rdfs:domain :Doctor ;  rdfs:range :Patient .:prescribes rdf:type owl:ObjectProperty ;  rdfs:domain :Doctor ;  rdfs:range :Treatment .:receives rdf:type owl:ObjectProperty ;  rdfs:domain :Patient ;  rdfs:range :Treatment .:Healthy rdf:type owl:NamedIndividual, :Health .:Sick rdf:type owl:NamedIndividual, :Health .:John rdf:type owl:NamedIndividual, :Patient ;  :hasHealth :Sick ;  :hasDoctor :DrSmith ;  :receives :Antibiotics .:DrSmith rdf:type owl:NamedIndividual, :Doctor ;  :hasPatient :John ;  :prescribes :Antibiotics .:Antibiotics rdf:type owl:NamedIndividual, :Treatment .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix healthSystem: <http://example.com/healthSystem#> .# Health ProviderhealthSystem:HealthProvider rdf:type owl:Class ;rdfs:comment "A class representing health provider entities in the health care system" .# Health Provider properties.# Define general properties that can be used by instances of the HealthProvider class.# NamehealthSystem:healthProviderName rdf:type owl:DatatypeProperty ; rdfs:domain healthSystem:HealthProvider ; rdfs:range xsd:string ; rdfs:comment "The name of the health provider." .# IDhealthSystem:healthProviderId rdf:type owl:DatatypeProperty ; rdfs:domain healthSystem:HealthProvider ; rdfs:range xsd:integer ; rdfs:comment "Unique identifier of a health provider." .# AddresshealthSystem:healthProviderAddress rdf:type owl:DatatypeProperty ; rdfs:domain healthSystem:HealthProvider ; rdfs:range xsd:string ; rdfs:comment "Address of a health provider." .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.com/> .ex:Health rdf:type rdfs:Class .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix health: <http://example.com/health#> .health:Patient a rdfs:Class .health:hasName a rdf:Property ;    rdfs:domain health:Patient ;    rdfs:range xsd:string .health:hasAge a rdf:Property ;    rdfs:domain health:Patient ;    rdfs:range xsd:int .health:hasHealthCondition a rdf:Property ;    rdfs:domain health:Patient ;    rdfs:range health:HealthCondition .health:HealthCondition a rdfs:Class .health:Diabetes a health:HealthCondition ;    rdfs:subClassOf health:HealthCondition .health:Hypertension a health:HealthCondition;    rdfs:subClassOf health:HealthCondition .health:JohnDoe a health:Patient ;    health:hasName "John Doe"^^xsd:string ;    health:hasAge 45^^xsd:int ;    health:hasHealthCondition health:Diabetes.

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .# Define a class "Person"ex:Person a rdfs:Class .# Define a class "Health"ex:Health a rdfs:Class .# Define a property "hasHealthStatus"ex:hasHealthStatus a rdf:Property ;    rdfs:domain ex:Person ;    rdfs:range ex:Health .# Define a health status instance "Good"ex:GoodHealth a ex:Health ;    rdfs:label "Good" .# Define an instance of class "person" and assign a health statusex:John a ex:Person ;    ex:hasHealthStatus ex:GoodHealth .
