@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix health: <http://www.example.com/health#> .health:Patient a rdfs:Class ;    rdfs:label "Patient" ;    rdfs:comment "A person who is receiving or has received treatment." .health:Doctor a rdfs:Class ;    rdfs:label "Doctor" ;    rdfs:comment "A qualified medical practitioner." .health:Disease a rdfs:Class ;    rdfs:label "Disease" ;    rdfs:comment "A particular abnormal, pathological condition that affects part or all of an organism." .health:Medication a rdfs:Class ;    rdfs:label "Medication" ;    rdfs:comment "A substance used for medical treatment." .health:treats a rdf:Property ;    rdfs:domain health:Doctor ;    rdfs:range health:Patient ;    rdfs:label "treats" ;    rdfs:comment "A doctor treats a patient." .health:prescribes a rdf:Property ;    rdfs:domain health:Doctor ;    rdfs:range health:Medication ;    rdfs:label "prescribes" ;    rdfs:comment "A doctor prescribes a medication." .health:hasDisease a rdf:Property ;    rdfs:domain health:Patient ;    rdfs:range health:Disease ;    rdfs:label "has disease" ;    rdfs:comment "A patient has a disease." .health:Treatment a rdfs:Class ;    rdfs:label "Treatment" ;    rdfs:comment "The management and care of a patient." .health:receivesTreatment a rdf:Property ;    rdfs:domain health:Patient ;    rdfs:range health:Treatment ;    rdfs:label "receives treatment" ;    rdfs:comment "A patient receives a treatment." .health:administersTreatment a rdf:Property ;    rdfs:domain health:Doctor ;    rdfs:range health:Treatment ;    rdfs:label "administers treatment" ;    rdfs:comment "A doctor administers a treatment." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix health: <http://example.com/health#> .health:     rdf:type owl:Ontology ;    rdfs:comment "An ontology for health domain" .health:Patient    rdf:type owl:Class ;    rdfs:comment "Represents the health patient" .health:Doctor    rdf:type owl:Class ;    rdfs:comment "Represents the doctor handling health patients" .health:Disease    rdf:type owl:Class ;    rdfs:comment "Represent various health diseases" .health:Medication    rdf:type owl:Class ;    rdfs:comment "Represent various health medications" .health:Treatment    rdf:type owl:Class ;    rdfs:comment "Represent various health treatments" .health:hasDisease    rdf:type owl:ObjectProperty ;    rdfs:domain health:Patient ;    rdfs:range health:Disease ;    rdfs:comment "Relates a patient to a disease he/she has" .health:prescribesMedication    rdf:type owl:ObjectProperty ;    rdfs:domain health:Doctor ;    rdfs:range health:Medication ;    rdfs:comment "Relates a doctor to a medication he/she prescribes" .health:receivesTreatment    rdf:type owl:ObjectProperty ;    rdfs:domain health:Patient ;    rdfs:range health:Treatment ;    rdfs:comment "Relates a patient to a treatment he/she receives" .health:administersTreatment    rdf:type owl:ObjectProperty ;    rdfs:domain health:Doctor ;    rdfs:range health:Treatment ;    rdfs:comment "Relates a doctor to a treatment he/she administers" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . @prefix health: <http://www.example.org/health#> .health:Health     a owl:Class ;    rdfs:comment "Overall state of an organism at a given time" ;    rdfs:label "Health" .health:Disease     a owl:Class ;    rdfs:subClassOf health:Health ;    rdfs:comment "Specific health condition characterized by abnormal functioning of the body or mind" ;    rdfs:label "Disease" .health:Symptom    a owl:Class ;    rdfs:subClassOf health:Health ;    rdfs:comment "Physical or mental feature that indicates a condition of disease" ;    rdfs:label "Symptom" .health:Treatment     a owl:Class ;    rdfs:subClassOf health:Health ;    rdfs:comment "Medical care given to a patient for an illness or injury" ;    rdfs:label "Treatment" .health:hasSymptom    a owl:ObjectProperty ;    rdfs:domain health:Disease ;    rdfs:range health:Symptom ;    rdfs:comment "Relation between a health condition and the symptoms it causes" ;    rdfs:label "has symptom" .health:hasTreatment    a owl:ObjectProperty ;    rdfs:domain health:Disease;    rdfs:range health:Treatment ;    rdfs:comment "Relates a health condition with its possible treatments" ;    rdfs:label "has treatment" .health:Patient    a owl:Class ;    rdfs:comment "An individual who receives medical care" ;    rdfs:label "Patient" .health:isDiagnosedWith    a owl:ObjectProperty ;    rdfs:domain health:Patient ;    rdfs:range health:Disease ;    rdfs:comment "Relates a patient with the disease(s) they have been diagnosed with" ;    rdfs:label "is diagnosed with" .health:isFollowingTreatment    a owl:ObjectProperty ;    rdfs:domain health:Patient ;    rdfs:range health:Treatment ;    rdfs:comment "Relates a patient with the treatment(s) they are following" ;    rdfs:label "is following treatment" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix health: <http://www.example.org/ontology/health#> .health:MedicalCondition rdf:type owl:Class .health:hasSymptom rdf:type owl:ObjectProperty .health:isDiagnosedBy rdf:type owl:ObjectProperty .health:Disease rdf:type owl:Class ;    rdfs:subClassOf health:MedicalCondition .health:ChronicDisease rdf:type owl:Class ;    rdfs:subClassOf health:Disease .health:AcuteDisease rdf:type owl:Class ;    rdfs:subClassOf health:Disease .health:MedicalTest rdf:type owl:Class .health:MedicalProcedure rdf:type owl:Class .health:Patient rdf:type owl:Class .health:hasMedicalCondition rdf:type owl:ObjectProperty .health:hasMedicalHistory rdf:type owl:ObjectProperty .health:Diabetes rdf:type owl:Class ;    rdfs:subClassOf health:ChronicDisease ;    health:hasSymptom health:FrequentUrination,                    health:IncreasedThirst,                    health:UnexplainedWeightLoss;    health:isDiagnosedBy health:BloodSugarTest .health:FrequentUrination rdf:type owl:Class ;    rdfs:subClassOf health:Symptom .health:IncreasedThirst rdf:type owl:Class ;    rdfs:subClassOf health:Symptom .health:UnexplainedWeightLoss rdf:type owl:Class ;    rdfs:subClassOf health:Symptom .health:BloodSugarTest rdf:type owl:Class ;    rdfs:subClassOf health:MedicalTest .

@prefix : <http://www.example.org/health#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .# Classes:Person a rdfs:Class ;  rdfs:comment "A human being"@en .:HealthCondition a rdfs:Class ;  rdfs:comment "Any health condition a person can have"@en .# Subclasses:Disease a rdfs:Class ;  rdfs:subClassOf :HealthCondition ;  rdfs:comment "A type of health condition that negatively affects how a person feels, thinks, or behaves"@en .:PhysicalHealth a rdfs:Class ;  rdfs:subClassOf :HealthCondition ;  rdfs:comment "The state of physical well-being or lack thereof"@en .:MentalHealth a rdfs:Class ;  rdfs:subClassOf :HealthCondition ;  rdfs:comment "The state of mental well-being or lack thereof"@en .# Properties:hasHealthCondition a rdf:Property ;  rdfs:domain :Person ;  rdfs:range :HealthCondition ;  rdfs:comment "Indicates a health condition a person has"@en .:hasDisease a rdf:Property ;  rdfs:subPropertyOf :hasHealthCondition ;  rdfs:domain :Person ;  rdfs:range :Disease ;  rdfs:comment "Indicates a disease a person has"@en .:hasPhysicalHealthStatus a rdf:Property ;  rdfs:subPropertyOf :hasHealthCondition ;  rdfs:domain :Person ;  rdfs:range :PhysicalHealth ;  rdfs:comment "Indicates the physical health status of a person"@en .:hasMentalHealthStatus a rdf:Property ;  rdfs:subPropertyOf :hasHealthCondition ;  rdfs:domain :Person ;  rdfs:range :MentalHealth ;  rdfs:comment "Indicates the mental health status of a person"@en .


