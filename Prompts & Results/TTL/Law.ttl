
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix law: <http://example.com/law/> .law:Law a rdfs:Class ;    rdfs:label "Law" .law:LegalEntity a rdfs:Class ;    rdfs:label "LegalEntity" ;    rdfs:subClassOf law:Law .    law:CriminalLaw a rdfs:Class ;    rdfs:label "Criminal Law" ;    rdfs:subClassOf law:Law .law:CivilLaw a rdfs:Class ;    rdfs:label "Civil Law" ;    rdfs:subClassOf law:Law .law:Lawyer a rdfs:Class ;    rdfs:label "Lawyer" ;    rdfs:subClassOf law:LegalEntity .law:Client a rdfs:Class ;    rdfs:label "Client" ;    rdfs:subClassOf law:LegalEntity .law:Judge a rdfs:Class ;    rdfs:label "Judge" ;    rdfs:subClassOf law:LegalEntity .law:Court a rdfs:Class ;    rdfs:label "Court" ;    rdfs:subClassOf law:LegalEntity .law:Legislation a rdfs:Class ;    rdfs:label "Legislation" ;    rdfs:subClassOf law:Law .law:Represents a rdf:Property ;    rdfs:domain law:Lawyer ;    rdfs:range law:Client ;    rdfs:label "represents" .law:Decides a rdf:Property ;    rdfs:domain law:Judge ;    rdfs:range law:LegalEntity ;    rdfs:label "decides" .law:Applies a rdf:Property ;    rdfs:domain law:Court ;    rdfs:range law:Legislation ;    rdfs:label "applies" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law: <http://www.example.org/law#> .law:Law rdfs:subClassOf rdf:Property ;  rdfs:label "Law"@en ;  rdfs:comment "Legislation passed by a competent authority establishing some course of conduct"@en .law:CriminalLaw rdfs:subClassOf law:Law ;  rdfs:label "Criminal Law"@en ;  rdfs:comment "The body of law that relates to crime"@en .law:CivilLaw rdfs:subClassOf law:Law ;  rdfs:label "Civil Law"@en ;  rdfs:comment "The body of law that relates to private relations between members of a community"@en ;law:ConstitutionalLaw rdfs:subClassOf law:Law ;  rdfs:label "Constitutional Law"@en ;  rdfs:comment "The body of law that defines the relationship between different entities within a nation, most commonly the judiciary, the executive and the legislature bodies"@en ;law:Statute rdfs:subClassOf rdf:Property ;  rdfs:label "Statute"@en ;  rdfs:comment "A written law passed by a legislative body"@en .law:Case rdfs:subClassOf rdf:Property ;  rdfs:label "Case"@en ;  rdfs:comment "A legal action, cause, suit, or controversy, at law or in equity"@en .law:Lawyer rdfs:subClassOf rdf:Property ;  rdfs:label "Lawyer"@en ;  rdfs:comment "A person who practices or studies law; an attorney or a counselor"@en .law:Judge rdfs:subClassOf rdf:Property ;  rdfs:label "Judge"@en ;  rdfs:comment "A public official appointed to decide cases in a court of law"@en .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix lc: <http://www.example.com/lawConcept#> .lc:Law rdf:type owl:Class ;     rdfs:label "Law"@en ;    rdfs:comment "A system of rules adopted by a government to regulate behavior"@en .lc:hasLawNumber rdf:type owl:DatatypeProperty ;     rdfs:label "has law number"@en ;    rdfs:comment "An identifier of the law"@en ;    rdfs:domain lc:Law ;     rdfs:range xsd:string .lc:hasTitle rdf:type owl:DatatypeProperty ;     rdfs:label "has title"@en ;    rdfs:comment "Title of the law"@en ;    rdfs:domain lc:Law ;    rdfs:range xsd:string .lc:isRatifiedBy rdf:type owl:ObjectProperty ;     rdfs:label "is ratified by"@en ;    rdfs:comment "The entity which ratified the law"@en ;    rdfs:domain lc:Law .lc:GovernmentEntity rdf:type owl:Class ;    rdfs:label "Government Entity"@en ;    rdfs:comment "A government institution or group"@en .lc:isRatifiedBy rdfs:range lc:GovernmentEntity .lc:hasJurisdiction rdf:type owl:DatatypeProperty ;    rdfs:label "has jurisdiction"@en ;    rdfs:comment "Jurisdiction where the law is applicable"@en ;    rdfs:domain lc:Law ;    rdfs:range xsd:string .lc:isEnforcedBy rdf:type owl:ObjectProperty ;    rdfs:label "is enforced by"@en ;    rdfs:comment "Entity which enforces the law"@en ;    rdfs:domain lc:Law ;    rdfs:range lc:GovernmentEntity .lc:hasEffectiveDate rdf:type owl:DatatypeProperty ;    rdfs:label "has effective date"@en ;    rdfs:comment "Date when the law comes into force"@en ;    rdfs:domain lc:Law ;    rdfs:range xsd:date .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law: <http://www.example.org/ontology/law#> .law:Law rdf:type owl:Class .law:Regulation rdf:type owl:Class ;    rdfs:subClassOf law:Law .law:Statute rdf:type owl:Class ;    rdfs:subClassOf law:Law .law:CaseLaw rdf:type owl:Class ;    rdfs:subClassOf law:Law .law:hasCitation rdf:type owl:DatatypeProperty ;    rdfs:domain law:Law ;    rdfs:range xsd:string .law:hasJurisdiction rdf:type owl:ObjectProperty ;    rdfs:domain law:Law ;    rdfs:range law:Jurisdiction .law:Jurisdiction rdf:type owl:Class .law:hasEffectiveDate rdf:type owl:DatatypeProperty ;    rdfs:domain law:Law ;    rdfs:range xsd:dateTime .law:Lawyer rdf:type owl:Class .law:isSpecializedIn rdf:type owl:ObjectProperty ;    rdfs:domain law:Lawyer ;    rdfs:range law:Law .law:hasRepresented rdf:type owl:ObjectProperty ;    rdfs:domain law:Lawyer ;    rdfs:range law:Client .law:Client rdf:type owl:Class .law:hasLegalIssue rdf:type owl:ObjectProperty ;    rdfs:domain law:Client ;    rdfs:range law:LegalIssue .law:LegalIssue rdf:type owl:Class .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law: <http://www.example.org/law#> .law:Law    a rdfs:Class ;    rdfs:comment "The abstraction encompassing all legal principles and rules."@en ;    rdfs:label "Law"@en .    law:Case    a rdfs:Class ;    rdfs:subClassOf law:Law ;    rdfs:comment "A specific application of the law, usually involving a dispute between parties."@en ;    rdfs:label "Case"@en .law:Statute    a rdfs:Class ;    rdfs:subClassOf law:Law ;    rdfs:comment "A written law passed by a legislative body."@en ;    rdfs:label "Statute"@en .law:Constitution    a rdfs:Class ;    rdfs:subClassOf law:Law ;    rdfs:comment "The system of fundamental principles according to which a nation, state, corporation, or the like, is governed."@en ;    rdfs:label "Constitution"@en .law:hasCase    a rdf:Property ;    rdfs:domain law:Law ;    rdfs:range law:Case ;    rdfs:comment "Relates a legal domain to its specific case."@en ;    rdfs:label "has case"@en .law:hasStatute    a rdf:Property ;    rdfs:domain law:Law ;    rdfs:range law:Statute ;    rdfs:comment "Relates a legal domain to its specific statute."@en ;    rdfs:label "has statute"@en .law:hasConstitution    a rdf:Property ;    rdfs:domain law:Law ;    rdfs:range law:Constitution ;    rdfs:comment "Relates a legal domain to its specific constitution."@en ;    rdfs:label "has constitution"@en .law:correspondsTo    a rdf:Property ;    rdfs:domain law:Case ;    rdfs:range law:Statute ;    rdfs:comment "Relates a case to the statute it corresponds to."@en ;    rdfs:label "corresponds to"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix law: <http://example.org/law#> .law:Law rdfs:subClassOf rdfs:Class .law:CriminalLaw rdfs:subClassOf law:Law .law:CivilLaw rdfs:subClassOf law:Law .law:Lawyer rdfs:subClassOf rdfs:Class .law:Prosecutor rdfs:subClassOf law:Lawyer .law:DefenceAttorney rdfs:subClassOf law:Lawyer .law:Case rdfs:subClassOf rdfs:Class .law:CriminalCase rdfs:subClassOf law:Case .law:CivilCase rdfs:subClassOf law:Case .law:Rule rdfs:subClassOf rdfs:Class .law:Regulation rdfs:subClassOf law:Rule .law:Precedent rdfs:subClassOf law:Rule .law:handles rdf:type rdf:Property ;            rdfs:domain law:Lawyer ;            rdfs:range law:Case .law:involves rdf:type rdf:Property ;             rdfs:domain law:Case ;             rdfs:range law:Law .law:follows rdf:type rdf:Property ;            rdfs:domain law:Law ;            rdfs:range law:Rule .law:enforces rdf:type rdf:Property ;             rdfs:domain law:Prosecutor ;             rdfs:range law:Rule .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law: <http://example.org/law/> .# Class Assertionslaw:LegalPerson rdf:type rdfs:Class .law:NationalLaw rdf:type rdfs:Class .law:InternationalLaw rdf:type rdfs:Class .law:Treaty rdf:type rdfs:Class .law:Legislation rdf:type rdfs:Class .law:ConstitutionalLaw rdf:type rdfs:Class .law:CriminalLaw rdf:type rdfs:Class .law:CivilLaw rdf:type rdfs:Class .law:CaseLaw rdf:type rdfs:Class .law:Regulation rdf:type rdfs:Class .law:Lawyer rdf:type rdfs:Class .law:Judge rdf:type rdfs:Class .law:LegalProcess rdf:type rdfs:Class .law:Court rdf:type rdfs:Class .law:Appeal rdf:type rdfs:Class .# Subclass Assertionslaw:NationalLaw rdfs:subClassOf law:LegalProcess .law:InternationalLaw rdfs:subClassOf law:LegalProcess .law:Treaty rdfs:subClassOf law:InternationalLaw .law:ConstitutionalLaw rdfs:subClassOf law:NationalLaw .law:CriminalLaw rdfs:subClassOf law:NationalLaw .law:CivilLaw rdfs:subClassOf law:NationalLaw .law:CaseLaw rdfs:subClassOf law:LegalProcess .law:Regulation rdfs:subClassOf law:NationalLaw .# Data Propertieslaw:hasName rdf:type rdf:Property ;  rdfs:domain law:LegalPerson ;  rdfs:range xsd:string .# Object Propertieslaw:representedBy rdf:type rdf:Property ;  rdfs:domain law:LegalPerson ;  rdfs:range law:Lawyer .law:madeJudgement rdf:type rdf:Property ;  rdfs:domain law:Judge ;  rdfs:range law:LegalProcess .law:appealed rdf:type rdf:Property ;  rdfs:domain law:LegalProcess ;  rdfs:range law:Appeal .law:heldIn rdf:type rdf:Property ;  rdfs:domain law:LegalProcess ;  rdfs:range law:Court .


@prefix law: <http://example.org/lawpro#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .law:Law     rdf:type owl:Class ;    rdfs:comment "Represents a body of rules established by an authority." .law:Jurisdiction     rdf:type owl:Class ;    rdfs:comment "Jurisdiction refers to the legal authority to administer justice." .law:Statute     rdf:type owl:Class ;    rdfs:subClassOf law:Law ;    rdfs:comment "A specific, formal written enactment by the legislative branch." .law:CaseLaw     rdf:type owl:Class ;    rdfs:subClassOf law:Law ;    rdfs:comment "Law established by the outcome of former cases." .law:authoritativeEntity     rdf:type owl:ObjectProperty ;    rdfs:domain law:Law ;    rdfs:range law:Jurisdiction ;    rdfs:comment "The authoritative entity that enforces the law." .law:relatedTo     rdf:type owl:ObjectProperty ;    rdfs:domain law:Law ;    rdfs:range law:Law ;    rdfs:comment "Indicates another law that is related to this law." .law:enactedDate     rdf:type owl:DatatypeProperty ;    rdfs:domain law:Statute ;    rdfs:range xsd:date;    rdfs:comment "The date when the law was formally enacted." .

@prefix law: <http://www.example.com/law#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .#Classeslaw:Law rdf:type owl:Class .law:Criminal_Law rdf:type owl:Class ;               rdfs:subClassOf law:Law .law:Civil_Law rdf:type owl:Class ;             rdfs:subClassOf law:Law .law:Constitutional_Law rdf:type owl:Class ;                      rdfs:subClassOf law:Law .law:Case rdf:type owl:Class .law:Criminal_Case rdf:type owl:Class ;                 rdfs:subClassOf law:Case .law:Civil_Case rdf:type owl:Class ;              rdfs:subClassOf law:Case .law:Jurisdiction rdf:type owl:Class .law:Judge rdf:type owl:Class .#Object Propertieslaw:under rdf:type owl:ObjectProperty ;          rdfs:domain law:Case ;          rdfs:range law:Law .law:in rdf:type owl:ObjectProperty ;       rdfs:domain law:Case ;       rdfs:range law:Jurisdiction .law:assigned rdf:type owl:ObjectProperty ;            rdfs:domain law:Case ;            rdfs:range law:Judge .#Data Propertylaw:name rdf:type owl:DatatypeProperty ;         rdfs:domain law:Case ;          rdfs:range xsd:string .# Example caselaw:Case_123 rdf:type law:Criminal_Case ;             law:under law:Criminal_Law ;             law:in law:California_Jurisdiction ;             law:assigned law:Judge_John ;             law:name "The People v. John Doe" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix law: <http://example.org/law#> .law:Law rdfs:subClassOf rdfs:Resource ;    rdfs:label "Law" ;    rdfs:comment "Represents a legal system or a specific law." .law:Case rdfs:subClassOf rdfs:Resource ;    rdfs:label "Legal Case" ;    rdfs:comment "Represents a specific legal case." .law:Jurisdiction rdfs:subClassOf rdfs:Resource ;    rdfs:label "Legal Jurisdiction" ;    rdfs:comment "Represents a jurisdiction in which a certain law or set of laws applies." .law:Statute rdfs:subClassOf law:Law ;    rdfs:label "Statute" ;    rdfs:comment "Represents a specific enacted law." .law:CommonLaw rdfs:subClassOf law:Law ;    rdfs:label "Common Law" ;    rdfs:comment "Represents a system of law based on precedent." .law:CriminalLaw rdfs:subClassOf law:Law ;    rdfs:label "Criminal Law" ;    rdfs:comment "Represents a system of law related to crime." .law:CivilLaw rdfs:subClassOf law:Law ;    rdfs:label "Civil Law" ;    rdfs:comment "Represents a system of law related to civil matters." .law:Judge rdfs:subClassOf rdfs:Resource ;    rdfs:label "Judge" ;    rdfs:comment "A person who presides over court proceedings." .law:codes rdfs:domain law:Law ;    rdfs:range rdfs:Literal ;    rdfs:label "codes" ;    rdfs:comment "Legal codes related to a Law." .law:established rdfs:domain law:Law ;    rdfs:range rdfs:Literal ;    rdfs:label "established" ;    rdfs:comment "Year a Law was established." .law:relatedCase rdfs:domain law:Law ;    rdfs:range law:Case ;    rdfs:label "related case" ;    rdfs:comment "Legal case that is related to a specific Law." .law:governs rdfs:domain law:Law ;    rdfs:range law:Jurisdiction ;    rdfs:label "governs" ;    rdfs:comment "Jurisdiction governed by a specific Law." .law:presidedBy rdfs:domain law:Case ;    rdfs:range law:Judge ;    rdfs:label "presided by" ;    rdfs:comment "Judge who presided a Legal Case." .


@prefix : <http://www.semanticweb.org/ontology#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix dbr: <http://dbpedia.org/resource/> .:Law rdf:type owl:Class .:ContractLaw rdf:type owl:Class ; rdfs:subClassOf :Law .:CriminalLaw rdf:type owl:Class ; rdfs:subClassOf :Law .:ConstitutionalLaw rdf:type owl:Class ; rdfs:subClassOf :Law .:judge rdf:type owl:Class .:lawyer rdf:type owl:Class .:juridiction rdf:type owl:ObjectProperty ; rdfs:domain :Law ; rdfs:range :judge . :practice rdf:type owl:ObjectProperty ; rdfs:domain :Law ; rdfs:range :lawyer .:dbr:Judge_Judy rdf:type :judge .:dbr:Barack_Obama rdf:type :lawyer .:dbr:US_Constitution rdf:type :ConstitutionalLaw ; :juridiction :dbr:Judge_Judy ; :practice :dbr:Barack_Obama .


@prefix : <http://www.example.com/lawOntology#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .:Law rdf:type owl:Class .:CriminalLaw rdf:type owl:Class ;    rdfs:subClassOf :Law .:CivilLaw rdf:type owl:Class ;    rdfs:subClassOf :Law .:Statute rdf:type owl:Class .:CaseLaw rdf:type owl:Class .:court rdfs:range :Court ;    rdf:type owl:ObjectProperty .:lawyer rdfs:range :Lawyer ;    rdf:type owl:ObjectProperty .:Lawyer rdf:type owl:Class .:Court rdf:type owl:Class .:LawFirm rdf:type owl:Class .:client rdfs:range :Client ;    rdf:type owl:ObjectProperty .:Client rdf:type owl:Class .:contract rdf:type owl:Class ;    rdfs:subClassOf :CivilLaw .:tort rdf:type owl:Class ;    rdfs:subClassOf :CivilLaw .:felony rdf:type owl:Class ;    rdfs:subClassOf :CriminalLaw .:misdemeanor rdf:type owl:Class ;    rdfs:subClassOf :CriminalLaw .

@prefix : <http://www.example.com/law#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .:Law    rdf:type owl:Class ;    rdfs:label "Law" ;    rdfs:comment "Class representing all laws" .:Statute    rdf:type owl:Class ;    rdfs:label "Statute" ;    rdfs:comment "Class representing a written law passed by a legislative body" ;    rdfs:subClassOf :Law .:Regulation    rdf:type owl:Class ;    rdfs:label "Regulation" ;    rdfs:comment "Class representing a rule or directive made and maintained by an authority" ;    rdfs:subClassOf :Law .:CaseLaw    rdf:type owl:Class ;    rdfs:label "Case Law" ;    rdfs:comment "Class representing laws as established by the outcome of court cases" ;    rdfs:subClassOf :Law .:Jurisdiction    rdf:type owl:Class ;    rdfs:label "Jurisdiction" ;    rdfs:comment "Class representing a geographic area under a certain legal authority" .:hasJurisdiction    rdf:type owl:ObjectProperty ;    rdfs:label "has jurisdiction" ;    rdfs:comment "Indicates the jurisdiction where a law is applicable" ;    rdfs:domain :Law ;    rdfs:range :Jurisdiction .:title    rdf:type owl:DatatypeProperty ;    rdfs:label "title" ;    rdfs:comment "Indicates the title of a law" ;    rdfs:domain :Law ;    rdfs:range rdfs:Literal .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law: <http://example.com/law#> .law:   rdf:type owl:Ontology ;  rdfs:comment "A basic ontology covering various concepts related to law."@en .law:Law  rdf:type owl:Class ;  rdfs:comment "The class of all laws."@en ;  rdfs:label "Law"@en .law:CriminalLaw  rdf:type owl:Class ;  rdfs:subClassOf law:Law ;  rdfs:comment "The class of all criminal laws."@en ;  rdfs:label "Criminal Law"@en .law:CivilLaw  rdf:type owl:Class ;  rdfs:subClassOf law:Law ;  rdfs:comment "The class of all civil laws."@en ;  rdfs:label "Civil Law"@en .law:case_law   rdf:type owl:Class ;  rdfs:subClassOf law:Law ;  rdfs:comment "The class of all case laws."@en ;   rdfs:label "Case Law"@en .law:statutory_law   rdf:type owl:Class ;  rdfs:subClassOf law:Law ;  rdfs:comment "The class of all statutory laws."@en ;   rdfs:label "Statutory Law"@en .law:constitutional_law   rdf:type owl:Class ;  rdfs:subClassOf law:Law ;  rdfs:comment "The class of all constitutional laws."@en ;   rdfs:label "Constitutional Law"@en .law:jurisdiction   rdf:type owl:Class ;  rdfs:comment "The class of all jurisdictions."@en ;   rdfs:label "Jurisdiction"@en .law:Lawyer  rdf:type owl:Class ;  rdfs:comment "The class of all lawyers."@en ;  rdfs:label "Lawyer"@en .law:practicesIn   rdf:type owl:ObjectProperty ;  rdfs:domain law:Lawyer ;  rdfs:range law:Law ;  rdfs:comment "The property that links a lawyer to the law they practice."@en ;  rdfs:label "practicesIn"@en .law:appliesIn   rdf:type owl:ObjectProperty ;  rdfs:domain law:Law ;  rdfs:range law:jurisdiction ;  rdfs:comment "The property that links a law to its jurisdiction."@en ;  rdfs:label "appliesIn"@en .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>@prefix owl: <http://www.w3.org/2002/07/owl#>@prefix xsd: <http://www.w3.org/2001/XMLSchema#>@prefix law: <http://example.org/law/># This Ontology will represent the concept of a 'Law', including the 'Lawmaker', 'Country' and 'Year of publication'law:Law rdf:type owl:Class .law:Law rdfs:comment "A legal rule established by an authoritative figure."@en .law:Lawmaker rdf:type owl:Class .law:Lawmaker rdfs:comment "An individual, body, or organisation responsible for making a law."@en .law:Country rdf:type owl:Class .law:Country rdfs:comment "The country where a law is official or enforced."@en .law:YearOfPublication rdf:type owl:DatatypeProperty .law:YearOfPublication rdfs:domain law:Law .law:YearOfPublication rdfs:range xsd:date .law:YearOfPublication rdfs:comment "The date when a law was officially enacted or published."@en .law:madeBy rdf:type owl:ObjectProperty .law:madeBy rdfs:domain law:Law .law:madeBy rdfs:range law:Lawmaker .law:madeBy rdfs:comment "The lawmaker that made or enacted a specific law."@en .law:validIn rdf:type owl:ObjectProperty .law:validIn rdfs:domain law:Law .law:validIn rdfs:range law:Country .law:validIn rdfs:comment "The country where the law is valid or enforced."@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://www.example.com/law#> .:Law rdf:type rdfs:Class .:CriminalLaw rdf:type rdfs:Class ;    rdfs:subClassOf :Law .:CivilLaw rdf:type rdfs:Class ;    rdfs:subClassOf :Law .:Lawyer rdf:type rdfs:Class .:Judge rdf:type rdfs:Class .:Case rdf:type rdfs:Class .:lawSpeciality rdf:type rdf:Property ;    rdfs:domain :Lawyer ;    rdfs:range :Law .:defendedIn rdf:type rdf:Property ;    rdfs:domain :Lawyer ;    rdfs:range :Case .:judgedIn rdf:type rdf:Property ;    rdfs:domain :Judge ;    rdfs:range :Case .:relatedTo rdf:type rdf:Property ;    rdfs:domain :Case ;    rdfs:range :Law .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law: <http://example.org/law#> .@prefix dbo: <http://dbpedia.org/ontology/> .law:Law a rdfs:Class ;    rdfs:comment "A class representing a legal rule or body of law" ;    rdfs:label "Law" .law:hasCode a rdf:Property ;    rdfs:comment "A property linking a law to its code or identifier" ;    rdfs:label "hasCode" ;    rdfs:domain law:Law ;    rdfs:range xsd:string .law:hasTitle a rdf:Property ;    rdfs:comment "A property linking a law to its title" ;    rdfs:label "hasTitle" ;    rdfs:domain law:Law ;    rdfs:range xsd:string .law:hasJurisdiction a rdf:Property ;    rdfs:comment "A property linking a law to its jurisdiction" ;    rdfs:label "hasJurisdiction" ;    rdfs:domain law:Law ;    rdfs:range dbo:Place .law:hasEffectiveDate a rdf:Property ;    rdfs:comment "A property linking a law to its effective date" ;    rdfs:label "hasEffectiveDate" ;    rdfs:domain law:Law ;    rdfs:range xsd:date .law:hasRepealDate a rdf:Property ;    rdfs:comment "A property linking a law to its repeal date, if any" ;    rdfs:label "hasRepealDate" ;    rdfs:domain law:Law ;    rdfs:range xsd:date .

