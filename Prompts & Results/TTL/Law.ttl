@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix law-ontology: <http://www.example.com/law-ontology#> .law-ontology:Law rdf:type rdfs:Class;                 rdfs:label "Law"@en ;                 rdfs:comment "A system of rules which a particular country or community recognizes as regulating the actions of its members."@en . law-ontology:hasTitle rdf:type rdf:Property;                     rdfs:domain law-ontology:Law;                     rdfs:range xsd:string;                     rdfs:label "has title"@en;                     rdfs:comment "The title of the law."@en .law-ontology:hasYearImplemented rdf:type rdf:Property;                                rdfs:domain law-ontology:Law;                                rdfs:range xsd:gYear;                                rdfs:label "has year implemented"@en;                                rdfs:comment "The year the law was implemented."@en .law-ontology:appliesTo rdf:type rdf:Property;                       rdfs:domain law-ontology:Law;                       rdfs:range law-ontology:Jurisdiction;                       rdfs:label "applies to"@en;                       rdfs:comment "The jurisdiction to which the law applies."@en .law-ontology:Jurisdiction rdf:type rdfs:Class;                          rdfs:label "Jurisdiction"@en ;                          rdfs:comment "A region or range of governmental or administrative control."@en . 

