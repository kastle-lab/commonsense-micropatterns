@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix body: <http://www.example.org/ontology/body#> .body:Body a owl:Class ;    rdfs:label "Body" ;    rdfs:comment "A physical structure of an organism" .body:hasPart a owl:ObjectProperty ;    rdfs:label "has part" ;    rdfs:domain body:Body ;    rdfs:range body:BodyPart .body:BodyPart a owl:Class ;    rdfs:label "Body Part" ;    rdfs:comment "A distinct anatomical structure that is part of a body" .body:Head a owl:Class ;    rdfs:label "Head" ;    rdfs:subClassOf body:BodyPart .body:Torso a owl:Class ;    rdfs:label "Torso" ;    rdfs:subClassOf body:BodyPart .body:isPartOf a owl:ObjectProperty ;    rdfs:label "is part of" ;    rdfs:domain body:BodyPart ;    rdfs:range body:Body .body:Body rdfs:subClassOf [    a owl:Restriction ;    owl:onProperty body:hasPart ;    owl:someValuesFrom body:BodyPart] .body:BodyPart rdfs:subClassOf [    a owl:Restriction ;    owl:onProperty body:isPartOf ;    owl:someValuesFrom body:Body] .

