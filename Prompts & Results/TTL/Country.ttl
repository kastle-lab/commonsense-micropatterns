@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/schema#> .ex:Country rdf:type rdfs:Class .ex:name rdf:type rdf:Property ;    rdfs:domain ex:Country ;    rdfs:range xsd:string .ex:population rdf:type rdf:Property ;    rdfs:domain ex:Country ;    rdfs:range xsd:integer .ex:area rdf:type rdf:Property ;    rdfs:domain ex:Country ;    rdfs:range xsd:decimal .ex:capital rdf:type rdf:Property ;    rdfs:domain ex:Country ;    rdfs:range ex:City .

@prefix ex: <http://example.org/> .ex:Country a rdfs:Class ;  rdfs:label "Country"@en ;  rdfs:comment "A nation with its own government, occupying a particular territory."@en ;  rdfs:subClassOf ex:GeographicalEntity .ex:hasName a rdf:Property ;  rdfs:label "has name"@en ;  rdfs:comment "The official or commonly recognized name of the country."@en ;  rdfs:domain ex:Country ;  rdfs:range xsd:string .ex:hasPopulation a rdf:Property ;  rdfs:label "has population"@en ;  rdfs:comment "The number of people living in the country."@en ;  rdfs:domain ex:Country ;  rdfs:range xsd:integer .ex:hasArea a rdf:Property ;  rdfs:label "has area"@en ;  rdfs:comment "The total surface area of the country, usually measured in square kilometers."@en ;  rdfs:domain ex:Country ;  rdfs:range xsd:decimal .ex:hasCapital a rdf:Property ;    rdfs:label "has capital"@en ;    rdfs:comment "The city that serves as the seat of government of a country."@en ;    rdfs:domain ex:Country ;    rdfs:range ex:City .
:Country rdf:type owl:Class .:hasName rdf:type owl:DatatypeProperty ;                rdfs:domain :Country ;                rdfs:range xsd:string .:hasPopulation rdf:type owl:DatatypeProperty ;                rdfs:domain :Country ;                rdfs:range xsd:integer .:isLocatedIn rdf:type owl:ObjectProperty ;             rdfs:domain :Country ;             rdfs:range :Continent .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/> .ex:Country a rdfs:Class ;    rdfs:label "Country" ;    rdfs:comment "A nation with its own government, occupying a particular territory." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/exampleOntology#> .ex:Country a rdfs:Class ;    rdfs:label "Country"@en ;    rdfs:comment "A class representing a Country."@en .ex:hasName a rdf:Property ;    rdfs:label "hasName"@en ;    rdfs:comment "A property representing the name of the Country."@en ;    rdfs:domain ex:Country ;    rdfs:range xsd:string .ex:hasPopulation a rdf:Property ;    rdfs:label "hasPopulation"@en ;    rdfs:comment "A property representing the population of the Country."@en ;    rdfs:domain ex:Country ;    rdfs:range xsd:integer .

