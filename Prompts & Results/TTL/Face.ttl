@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix face: <http://www.example.com/ontologies/face#> .face:Face rdf:type owl:Class ;          rdfs:label "Face" ;          rdfs:comment "Concept representing a face of a human or animal" .face:hasEye rdf:type owl:ObjectProperty ;            rdfs:label "hasEye" ;            rdfs:comment "The face has eyes as a part of its structure" ;            rdfs:domain face:Face ;            rdfs:range face:Eye .face:Eye rdf:type owl:Class ;         rdfs:label "Eye" ;         rdfs:comment "Eye is a part of the face structure" .face:hasNose rdf:type owl:ObjectProperty ;             rdfs:label "hasNose" ;             rdfs:comment "The face has a nose as a part of its structure" ;             rdfs:domain face:Face ;             rdfs:range face:Nose .face:Nose rdf:type owl:Class ;          rdfs:label "Nose" ;          rdfs:comment "Nose is a part of the face structure" .face:hasMouth rdf:type owl:ObjectProperty ;              rdfs:label "hasMouth" ;              rdfs:comment "The face has a mouth as a part of its structure" ;              rdfs:domain face:Face ;              rdfs:range face:Mouth .face:Mouth rdf:type owl:Class ;           rdfs:label "Mouth" ;           rdfs:comment "Mouth is a part of the face structure" .      face:hasCheek rdf:type owl:ObjectProperty ;             rdfs:label "hasCheek" ;             rdfs:comment "The face has cheek as a part of its structure" ;             rdfs:domain face:Face ;             rdfs:range face:Cheek .face:Cheek rdf:type owl:Class ;           rdfs:label "Cheek" ;           rdfs:comment "Cheek is a part of the face structure" .



@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix face: <http://example.com/ontology/face#> .face:     a owl:Ontology ;    rdfs:comment "Ontology for Face specific domain" .face:Face     a owl:Class ;    rdfs:label "Face" ;    rdfs:comment "An individual human face" .face:hasPart    a owl:ObjectProperty ;    rdfs:domain face:Face ;    rdfs:range face:FacePart ;    rdfs:label "has part" ;    rdfs:comment "The face has a Face Part as its constituent" .face:FacePart     a owl:Class ;    rdfs:label "Face Part" ;    rdfs:comment "A part of the face. E.g. Nose, Eyes, Mouth etc." .face:Nose    a owl:Class ;    rdfs:subClassOf face:FacePart ;    rdfs:label "Nose" ;    rdfs:comment "The nose part of the face" .face:Eyes    a owl:Class ;    rdfs:subClassOf face:FacePart ;    rdfs:label "Eyes" ;    rdfs:comment "The eyes part of the face" .face:Mouth    a owl:Class ;    rdfs:subClassOf face:FacePart ;    rdfs:label "Mouth" ;    rdfs:comment "The mouth part of the face" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix face: <http://example.com/ontology/face#> .face:Face a rdfs:Class ;    rdfs:label "Face" ;    rdfs:comment "The front part of a person's head, from the forehead to the chin." .face:Eye a rdfs:Class ;    rdfs:label "Eye" ;    rdfs:comment "An organ of vision." ;    rdfs:subClassOf face:Face .face:Nose a rdfs:Class ;    rdfs:label "Nose";    rdfs:comment "The organ of smell and entrance to the respiratory tract." ;    rdfs:subClassOf face:Face .face:Mouth a rdfs:Class ;    rdfs:label "Mouth" ;    rdfs:comment "The opening in the face used for eating, speaking, etc." ;    rdfs:subClassOf face:Face .face:hasEye a rdf:Property ;    rdfs:label "hasEye" ;    rdfs:comment "Relates a Face to its Eye." ;    rdfs:domain face:Face ;    rdfs:range face:Eye .face:hasNose a rdf:Property ;    rdfs:label "hasNose" ;    rdfs:comment "Relates a Face to its Nose." ;    rdfs:domain face:Face ;    rdfs:range face:Nose .face:hasMouth a rdf:Property ;    rdfs:label "hasMouth" ;    rdfs:comment "Relates a Face to its Mouth." ;    rdfs:domain face:Face ;    rdfs:range face:Mouth . face:color a rdf:Property ;    rdfs:label "color" ;    rdfs:comment "The color of the face." ;    rdfs:domain face:Face ;    rdfs:range xsd:string .face:shape a rdf:Property ;    rdfs:label "shape" ;    rdfs:comment "The shape or contour of the face." ;    rdfs:domain face:Face ;    rdfs:range xsd:string .


