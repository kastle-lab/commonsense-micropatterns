@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl:  <http://www.w3.org/2002/07/owl#> .@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .@prefix face: <http://www.example.com/face#> .face:       a owl:Ontology ;      rdfs:comment "An ontology dedicated to faces."@en ;      rdfs:label "Face ontology"@en .face:Face      a rdfs:Class ;      rdfs:label "A facial detail"@en ;      rdfs:comment "A class representing facial details."@en ;      rdfs:subClassOf owl:Thing .face:Eye      a rdfs:Class ;      rdfs:label "Eye"@en ;      rdfs:comment "A class representing the eye component of the face."@en ;      rdfs:subClassOf face:Face .face:Nose      a rdfs:Class ;      rdfs:label "Nose"@en ;      rdfs:comment "A class representing the nose component of the face."@en ;      rdfs:subClassOf face:Face .face:Mouth      a rdfs:Class ;      rdfs:label "Mouth"@en ;      rdfs:comment "A class representing the mouth component of the face."@en ;      rdfs:subClassOf face:Face .face:hasEye      a owl:ObjectProperty ;      rdfs:label "has eye"@en ;      rdfs:comment "A property representing the relationship between a face and its eyes."@en ;      rdfs:domain face:Face ;      rdfs:range face:Eye .      face:hasNose      a owl:ObjectProperty ;      rdfs:label "has nose"@en ;      rdfs:comment "A property representing the relationship between a face and its nose."@en ;      rdfs:domain face:Face ;      rdfs:range face:Nose .face:hasMouth      a owl:ObjectProperty ;      rdfs:label "has mouth"@en ;      rdfs:comment "A property representing the relationship between a face and its mouth."@en ;      rdfs:domain face:Face ;      rdfs:range face:Mouth .




