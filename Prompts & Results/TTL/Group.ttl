@prefix ex: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .# Defining the base concept of Groupex:Group a rdfs:Class ;    rdfs:comment "A collection of individuals who interact with one another regularly and conceive of themselves as a group."@en ;    rdfs:label "Group"@en .# Defining properties of Group  # The property 'hasMember' of a groupex:hasMember a rdf:Property ;    rdfs:domain ex:Group ;    rdfs:range rdfs:Resource ;    rdfs:comment "A member of a group."@en ;    rdfs:label "hasMember"@en .# The property 'groupName' of a groupex:groupName a rdf:Property ;    rdfs:domain ex:Group ;    rdfs:range xsd:string ;    rdfs:comment "The name of the group."@en ;    rdfs:label "groupName"@en .# The property 'groupSize' of a groupex:groupSize a rdf:Property ;    rdfs:domain ex:Group ;    rdfs:range xsd:int ;    rdfs:comment "The size of the group."@en ;    rdfs:label "groupSize"@en .# The property 'groupType' of a groupex:groupType a rdf:Property ;    rdfs:domain ex:Group ;    rdfs:range ex:GroupType ;    rdfs:comment "The type of the group."@en ;    rdfs:label "groupType"@en .# Defining subclasses of Group# The subclass 'WorkGroup' of Groupex:WorkGroup a rdfs:Class ;    rdfs:subClassOf ex:Group ;    rdfs:comment "A group of individuals who collaborate on a particular task or project."@en ;    rdfs:label "WorkGroup"@en .# The subclass 'SocialGroup' of Groupex:SocialGroup a rdfs:Class ;    rdfs:subClassOf ex:Group ;    rdfs:comment "A group of individuals who engage in social activities together."@en ;    rdfs:label "SocialGroup"@en .# Defining instances of Group# The instance 'SampleGroup' of Groupex:SampleGroup a ex:Group ;    ex:groupName "Sample Group"^^xsd:string ;    ex:groupSize 20^^xsd:int ;    ex:groupType ex:SocialGroup ;    ex:hasMember ex:JohnDoe .# The instance 'JohnDoe' of Resourceex:JohnDoe a rdfs:Resource ;    rdfs:label "John Doe"@en .

