@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix furniture: <http://example.org/furniture#> .furniture:Chair rdf:type owl:Class ;    rdfs:label ""Chair"" ;    rdfs:comment ""A piece of furniture designed to seat one person, typically consisting of a back, a seat, and four legs.""@en ;    rdfs:subClassOf furniture:Furniture .    furniture:hasLegs rdf:type owl:DatatypeProperty ;    rdfs:label ""has legs"" ;    rdfs:comment ""The number of legs the piece of furniture has.""@en ;    rdfs:domain furniture:Furniture ;    rdfs:range xsd:integer .furniture:hasBack rdf:type owl:ObjectProperty ;    rdfs:label ""has back"" ;    rdfs:comment ""Indicates if the chair has a back""@en ;    rdfs:domain furniture:Chair ;    rdfs:range xsd:boolean .furniture:hasArmrests rdf:type owl:ObjectProperty ;    rdfs:label ""has armrests"" ;    rdfs:comment ""Indicates if the chair has armrests""@en ;    rdfs:domain furniture:Chair ;    rdfs:range xsd:boolean  .furniture:material rdf:type owl:ObjectProperty ;    rdfs:label ""made of"" ;    rdfs:comment ""Indicates the material the chair is made of""@en ;    rdfs:domain furniture:Chair ;    rdfs:range furniture:Material .furniture:Material rdf:type owl:Class ;    rdfs:label ""Material"" ;    rdfs:comment ""The type of material a piece of furniture is made from.""@en ;furniture:Wood rdf:type furniture:Material ;    rdfs:label ""Wood"" ;    rdfs:comment ""A type of hard material derived from trees.""@en .furniture:Metal rdf:type furniture:Material ;    rdfs:label ""Metal"" ;    rdfs:comment ""A type of hard material derived from mining.""@en .furniture:Plastic rdf:type furniture:Material ;    rdfs:label ""Plastic"" ;    rdfs:comment ""A type of synthetic or semi-synthetic materials.""@en ."

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ont: <http://example.org/chair-ontology#> .ont:Chair rdf:type rdfs:Class ;rdfs:label ""Chair""@en ;rdfs:comment ""A seat typically having four legs and a back for one person""@en .ont:hasMaterial rdf:type rdf:Property ;rdfs:label ""has material""@en ;rdfs:comment ""The material that the chair is made from""@en ;rdfs:domain ont:Chair ;rdfs:range rdfs:Resource .ont:hasStyle rdf:type rdf:Property ;rdfs:label ""has style""@en ;rdfs:comment ""The style of the chair""@en ;rdfs:domain ont:Chair ;rdfs:range rdfs:Resource .ont:hasColor rdf:type rdf:Property ;rdfs:label ""has color""@en ;rdfs:comment ""The color of the chair""@en ;rdfs:domain ont:Chair ;rdfs:range xsd:string .ont:hasSize rdf:type rdf:Property ;rdfs:label ""has size""@en ;rdfs:comment ""The size of the chair""@en ;rdfs:domain ont:Chair ;rdfs:range xsd:string .ont:isFor rdf:type rdf:Property ;rdfs:label ""is for""@en ;rdfs:comment ""The intended user of the chair""@en ;rdfs:domain ont:Chair ;rdfs:range rdfs:Resource ."

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix ont: <http://www.example.org/ontology#> .# Classesont:Chair rdf:type rdfs:Class ;          rdfs:label ""Chair""@en ;          rdfs:comment ""A piece of furniture designed to accommodate one sitting or reclining person, providing support for the back and often the arms""@en .ont:Material rdf:type rdfs:Class ;             rdfs:label ""Material""@en ;             rdfs:comment ""The substance from which a thing is or can be made""@en .ont:Style rdf:type rdfs:Class ;          rdfs:label ""Style""@en ;          rdfs:comment ""The design or look of a particular object""@en .# Propertiesont:hasMaterial rdf:type rdf:Property ;                rdfs:label ""has material""@en ;                rdfs:domain ont:Chair ;                rdfs:range ont:Material .ont:hasStyle rdf:type rdf:Property ;             rdfs:label ""has style""@en ;             rdfs:domain ont:Chair ;             rdfs:range ont:Style .ont:hasColor rdf:type rdf:Property ;             rdfs:label ""has color""@en ;             rdfs:domain ont:Chair ;             rdfs:range xsd:string .ont:hasWeight rdf:type rdf:Property ;              rdfs:label ""has weight""@en ;              rdfs:domain ont:Chair ;              rdfs:range xsd:decimal .# Individualsont:Wood rdf:type ont:Material ;         rdfs:label ""Wood""@en .ont:Plastic rdf:type ont:Material ;            rdfs:label ""Plastic""@en .ont:ModernStyle rdf:type ont:Style ;                rdfs:label ""Modern""@en .ont:TraditionalStyle rdf:type ont:Style ;                    rdfs:label ""Traditional""@en .ont:Chair1 rdf:type ont:Chair ;           ont:hasColor ""Red""@en ;           ont:hasWeight ""7.5""^^xsd:decimal ;           ont:hasMaterial ont:Wood ;           ont:hasStyle ont:ModernStyle .ont:Chair2 rdf:type ont:Chair ;           ont:hasColor ""B