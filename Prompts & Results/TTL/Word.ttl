@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@base <http://example.org/ontology/Word> .<#Word> rdf:type owl:Class ;  rdfs:label "Word" ;  rdfs:comment "A unit of human language." .<#Spelling> rdf:type owl:DatatypeProperty ;  rdfs:label "spelling" ;  rdfs:comment "The letters that make up a word in a written language." ;  rdfs:domain <#Word> ;  rdfs:range xsd:string .<#Length> rdf:type owl:DatatypeProperty ;  rdfs:label "length" ;  rdfs:comment "The number of letters in a word." ;  rdfs:domain <#Word> ;  rdfs:range xsd:integer .<#PartOfSpeech> rdf:type owl:ObjectProperty ;  rdfs:label "part of speech" ;  rdfs:comment "The function of a word in a sentence." ;  rdfs:domain <#Word> ;  rdfs:range <#PartOfSpeechClass> .<#PartOfSpeechClass> rdf:type owl:Class ;  rdfs:label "Part of Speech" ;  rdfs:comment "The grammatical categories into which words can be put." .<#PartOfSpeechClass/Noun> rdf:type <#PartOfSpeechClass> ;  rdfs:label "Noun" ;  rdfs:comment "A word that represents a person, place, thing, or idea." .<#PartOfSpeechClass/Verb> rdf:type <#PartOfSpeechClass> ;  rdfs:label "Verb" ;  rdfs:comment "A word that represents an action or a state of being." .

@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix word: <http://example.com/word#> .word:Word a owl:Class ;    rdfs:comment "A representation of a word" ;    rdfs:label "Word" .word:hasSpelling a owl:DatatypeProperty ;    rdfs:range xsd:string ;    rdfs:domain word:Word ;    rdfs:comment "The spelling of the word" ;    rdfs:label "has spelling" .word:hasMeaning a owl:DatatypeProperty ;    rdfs:range xsd:string ;    rdfs:domain word:Word ;    rdfs:comment "The meaning of the word" ;    rdfs:label "has meaning" .word:partOfSpeech a owl:ObjectProperty ;    rdfs:range word:PartOfSpeech ;    rdfs:domain word:Word ;    rdfs:comment "The part of speech of the word" ;    rdfs:label "is part of speech" .word:PartOfSpeech a owl:Class ;    rdfs:comment "A grammatical term that deals with the roles words play when used in context" ;    rdfs:label "Part of Speech" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix word: <http://www.example.org/word#> .word:     rdf:type owl:Ontology ;    rdfs:comment "Ontology that pertains to words" .word:Word    rdf:type owl:Class ;     rdfs:comment "Class representing a word" .word:hasSpelling     rdf:type owl:DatatypeProperty ;    rdfs:domain word:Word ;    rdfs:range xsd:string ;     rdfs:comment "Property representing the spelling of a word" .word:hasLength     rdf:type owl:DatatypeProperty ;    rdfs:domain word:Word ;    rdfs:range xsd:integer ;     rdfs:comment "Property indicating how many letters are in a word" .word:isPartOfSpeech     rdf:type owl:ObjectProperty ;    rdfs:domain word:Word ;    rdfs:range word:PartOfSpeech ;    rdfs:comment "Property indicating in what part of speech a word is used" .word:PartOfSpeech     rdf:type owl:Class ;     rdfs:comment "Class representing a part of speech" .word:noun     rdf:type word:PartOfSpeech ;     rdfs:comment "Instance representing the noun part of speech" .word:verb     rdf:type word:PartOfSpeech ;     rdfs:comment "Instance representing the verb part of speech" .word:adjective     rdf:type word:PartOfSpeech ;     rdfs:comment "Instance representing the adjective part of speech" .


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix word: <http://example.org/word/> .@prefix word-ontology: <http://example.org/word-ontology#> .word-ontology:    rdf:type owl:Ontology ;    rdfs:comment "An ontology for representing words" .word-ontology:Word    rdf:type owl:Class ;    rdfs:label "Word" ;    rdfs:comment "A basic unit to express meaning in a language." .word-ontology:hasSpelling    rdf:type owl:DatatypeProperty ;    rdfs:domain word-ontology:Word ;    rdfs:range xsd:string ;    rdfs:label "hasSpelling" ;    rdfs:comment "The spelling of the word." .word-ontology:hasPartOfSpeech    rdf:type owl:ObjectProperty ;    rdfs:domain word-ontology:Word ;    rdfs:range word-ontology:PartOfSpeech ;    rdfs:label "hasPartOfSpeech" ;    rdfs:comment "The grammatical role of word." .word-ontology:PartOfSpeech    rdf:type owl:Class ;    rdfs:label "PartOfSpeech" ;    rdfs:comment "The grammatical category of words." .   word:word1    rdf:type word-ontology:Word ;    word-ontology:hasSpelling "Hello"@en ;    word-ontology:hasPartOfSpeech word-ontology:Noun .word:word2    rdf:type word-ontology:Word ;    word-ontology:hasSpelling "Run"@en ;    word-ontology:hasPartOfSpeech word-ontology:Verb .word-ontology:Noun    rdf:type word-ontology:PartOfSpeech ;    rdfs:label "Noun" .word-ontology:Verb    rdf:type word-ontology:PartOfSpeech ;    rdfs:label "Verb" .

