@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.org/> .#Define "Word" as a classex:Word a owl:Class ;  rdfs:label "Word" ;  rdfs:comment "Represents a single word in a language" ;  rdfs:subClassOf owl:Thing .#Define the property "hasSpelling"ex:hasSpelling a owl:DatatypeProperty ;  rdfs:label "hasSpelling" ;  rdfs:comment "The spelling of the word" ;  rdfs:domain ex:Word ;  rdfs:range xsd:string .#Define the property "hasLength"ex:hasLength a owl:DatatypeProperty ;  rdfs:label "hasLength" ;  rdfs:comment "The length of the word" ;  rdfs:domain ex:Word ;  rdfs:range xsd:integer .#Define the property "partOfSpeech"ex:partOfSpeech a owl:ObjectProperty ;  rdfs:label "partOfSpeech" ;  rdfs:comment "Part of speech the word belongs to" ;  rdfs:domain ex:Word ;  rdfs:range ex:PartOfSpeech .#Define "PartOfSpeech" as a classex:PartOfSpeech a owl:Class ;  rdfs:label "Part of Speech" ;  rdfs:comment "Represents a grammatical category of a word" ;  rdfs:subClassOf owl:Thing .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix onto: <http://example.com/ontology/> .# Define Word classonto:Word     a rdfs:Class ;    rdfs:comment "A unit of language that native speakers can identify."@en .# Define hasSpelling propertyonto:hasSpelling     a rdf:Property ;    rdfs:domain onto:Word ;    rdfs:range rdfs:Literal ;    rdfs:comment "The spelling of a word."@en .# Define hasMeaning propertyonto:hasMeaning     a rdf:Property ;    rdfs:domain onto:Word ;    rdfs:range rdfs:Literal ;    rdfs:comment "The meaning of a word."@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix dbo: <http://dbpedia.org/ontology/> .dbo:Word rdf:type owl:Class ;         rdfs:subClassOf dbo:Entity ;         rdfs:comment "A Word is a single distinct meaningful element of speech or writing, used with others (or sometimes alone) to form a sentence and typically shown with space on either side when written or printed."@en ;         rdfs:label "Word"@en .  dbo:Word rdfs:subClassOf dbo:LanguageObject ;         owl:disjointWith dbo:Phrase, dbo:Sentence .dbo:WordProperty rdf:type owl:Class ;                 rdfs:subClassOf rdf:Property ;                 rdfs:domain dbo:Word ;                 rdfs:range rdf:Literal ;                 rdfs:comment "A property that a Word can have. It could be length of the word, syllables, part of speech etc."@en ;                 rdfs:label "Word Property"@en .dbo:hasWordProperty rdf:type owl:ObjectProperty ;                    rdfs:domain dbo:Word ;                    rdfs:range dbo:WordProperty ;                    rdfs:comment "The property that a Word possesses."@en ;                    rdfs:label "has Word Property"@en .


@base <http://example.com/> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://example.com/ontology#> .:Word a rdfs:Class ;    rdfs:label "Word"@en ;    rdfs:comment "Represents words in a language"@en ;    rdfs:subClassOf :LinguisticElement ;    _:hasSpelling a rdfs:DatatypeProperty ;        rdfs:label "word spelling"@en ;        rdfs:range xsd:string ;        rdfs:domain :Word ;    _:hasLength a rdfs:DatatypeProperty ;        rdfs:label "word length"@en ;        rdfs:range xsd:int ;        rdfs:domain :Word ;    :isPartOfSentence a rdfs:ObjectProperty ;        rdfs:label "is part of sentence"@en ;        rdfs:domain :Word ;        rdfs:range :Sentence ;    :hasSpeechPart a rdfs:ObjectProperty ;        rdfs:label "has speech part"@en ;        rdfs:domain :Word ;        rdfs:range :SpeechPart .# Example usage::Word1 a :Word ;    _:hasSpelling "hello" ;    _:hasLength 5 ;    :isPartOfSentence :Sentence1 ;    :hasSpeechPart :Noun .

