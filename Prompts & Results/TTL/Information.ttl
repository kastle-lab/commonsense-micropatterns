
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.com/ontology/information/> .info:Information a rdfs:Class ;  rdfs:label "Information" ;  rdfs:comment "The entity for representing information or data" .info:hasTitle a rdf:Property ;  rdfs:label "has title" ;  rdfs:comment "The title of the information" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:hasSource a rdf:Property ;  rdfs:label "has source" ;  rdfs:comment "The source from where the information is derived" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:hasAuthor a rdf:Property ;  rdfs:label "has author" ;  rdfs:comment "The author who generated the information" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:hasPublicationDate a rdf:Property ;  rdfs:label "has publication date" ;  rdfs:comment "The date when the information was published" ;  rdfs:domain info:Information ;  rdfs:range xsd:date .info:hasContent a rdf:Property ;  rdfs:label "has content" ;  rdfs:comment "The actual content of the information" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:isConfidential a rdf:Property ;  rdfs:label "is confidential" ;  rdfs:comment "Indicates if the information is confidential" ;  rdfs:domain info:Information ;  rdfs:range xsd:boolean .




@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix info: <http://example.com/info#> .info:Information a rdfs:Class ;    rdfs:label "Information"@en ;    rdfs:comment "Data processed in such a way to add value"@en .info:hasAuthor a rdf:Property ;    rdfs:label "has author"@en ;    rdfs:comment "Author of the information"@en ;    rdfs:domain info:Information ;    rdfs:range info:Author .info:hasSource a rdf:Property ;    rdfs:label "has source"@en ;    rdfs:comment "Source where the information was acquired"@en ;    rdfs:domain info:Information ;    rdfs:range info:Source .info:hasDate a rdf:Property ;    rdfs:label "has date"@en ;    rdfs:comment "Date when the information was created or updated"@en ;    rdfs:domain info:Information ;    rdfs:range info:Date .info:Author a rdfs:Class ;    rdfs:label "Author"@en ;    rdfs:comment "Person who created or contributed to create the information"@en .info:Source a rdfs:Class ;    rdfs:label "Source"@en ;    rdfs:comment "Place, Medium or Person from where the information was acquired"@en .info:Date a rdfs:Class ;    rdfs:label "Date"@en ;    rdfs:comment "Point or period of time associated with an event in the lifecycle of the information"@en .  info:hasFormat a rdf:Property ;    rdfs:label "has format"@en ;    rdfs:comment "File format or physical format of the information"@en ;    rdfs:domain info:Information ;    rdfs:range info:Format .info:Format a rdfs:Class ;    rdfs:label "Format"@en ;    rdfs:comment "File format or physical format in which the information is presented"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://www.example.com/info#> .info:Information rdf:type rdfs:Class ;    rdfs:comment "The class of all information entities."@en ;    rdfs:label "Information"@en .info:Document rdf:type rdfs:Class ;    rdfs:subClassOf info:Information ;    rdfs:comment "A tangible or digital document containing information."@en ;    rdfs:label "Document"@en .info:hasInformation rdf:type rdf:Property ;    rdfs:domain info:Document ;    rdfs:range info:Information ;    rdfs:comment "Property linking documents to the information they contain."@en ;    rdfs:label "hasInformation"@en .info:DataType rdf:type rdfs:Class ;    rdfs:subClassOf info:Information ;    rdfs:comment "Describes the type or form of information."@en ;    rdfs:label "Data Type"@en .info:dataType rdf:type rdf:Property ;    rdfs:domain info:Information ;    rdfs:range info:DataType ;    rdfs:comment "Property linking information to its data type."@en ;    rdfs:label "hasDataType"@en .info:source rdf:type rdf:Property ;    rdfs:domain info:Information ;    rdfs:range xsd:string ;    rdfs:comment "The source from where the information was obtained."@en ;    rdfs:label "source"@en .info:creationDate rdf:type rdf:Property ;    rdfs:domain info:Information ;    rdfs:range xsd:dateTime ;    rdfs:comment "The date and time when the information was created."@en ;    rdfs:label "creation date"@en .info:author rdf:type rdf:Property ;    rdfs:domain info:Information ;    rdfs:range xsd:string ;    rdfs:comment "The author or creator of the information."@en ;    rdfs:label "author"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://www.example.org/info#> .info:     a owl:Ontology ;    rdfs:comment "An ontology about Information" .info:Information    a rdfs:Class ;    rdfs:label "Information" ;    rdfs:comment "Concept that entails data, knowledge or any type of valued content." .info:Data    a rdfs:Class ;     rdfs:subClassOf info:Information ;    rdfs:comment "Representation of information in a formalized manner suitable for communication, interpretation, or processing." .info:Knowledge    a rdfs:Class ;     rdfs:subClassOf info:Information ;    rdfs:comment "Awareness or understanding of facts, information, descriptions, or skills." .info:hasSource    a rdf:Property ;    rdfs:domain info:Information ;    rdfs:range info:Source ;    rdfs:label "has Source" ;    rdfs:comment "Source or origin of Information." .info:Source    a rdfs:Class ;    rdfs:label "Source" ;    rdfs:comment "Place, person, or thing from where the Information originates, can be a book, website, etc." .info:hasFormat    a rdf:Property ;    rdfs:domain info:Information ;    rdfs:range info:Format ;    rdfs:label "has format" ;    rdfs:comment "the Medium through which Information is present. can be a book, digital file, etc." .info:Format    a rdfs:Class ;    rdfs:label "Format" ;    rdfs:comment "The medium in which information is stored or represented." .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:Information rdfs:label "Information" .info:Information rdf:type owl:Class .info:hasAuthor rdf:type owl:ObjectProperty ;  rdfs:domain info:Information ;  rdfs:range info:Author .info:Author rdf:type owl:Class ;  rdfs:label "Author" .info:title rdf:type owl:DatatypeProperty ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:dateCreated rdf:type owl:DatatypeProperty ;  rdfs:domain info:Information ;  rdfs:range xsd:date .info:content rdf:type owl:DatatypeProperty ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:source rdf:type owl:DatatypeProperty ;  rdfs:domain info:Information ;  rdfs:range xsd:anyURI .info:InformationRepresentation rdf:type owl:Class ;  rdfs:subClassOf info:Information ;  rdfs:label "Information Representation" .info:encodingMethod rdf:type owl:DatatypeProperty ;  rdfs:domain info:InformationRepresentation ;  rdfs:range xsd:string .info:InformationSecurity rdf:type owl:Class ;  rdfs:subClassOf info:Information ;  rdfs:label "Information Security" .info:encryptionMethod rdf:type owl:DatatypeProperty ;  rdfs:domain info:InformationSecurity ;  rdfs:range xsd:string .info:hasConfidentiality rdf:type owl:DatatypeProperty ;  rdfs:domain info:InformationSecurity ;  rdfs:range xsd:boolean .info:hasIntegrity rdf:type owl:ObjectProperty ;  rdfs:domain info:InformationSecurity ;  rdfs:range xsd:boolean .info:hasAvailability rdf:type owl:ObjectProperty ;  rdfs:domain info:InformationSecurity ;  rdfs:range xsd:boolean .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:   rdf:type owl:Ontology ;  rdfs:comment "Ontology for modelling information" .info:Information  rdf:type owl:Class ;  rdfs:comment "Class representing units of information" ;  rdfs:label "Information" .info:hasSource   rdf:type owl:ObjectProperty ;  rdfs:comment "Relates an information unit to its source" ;  rdfs:label "has source" ;  rdfs:domain info:Information .info:hasDate   rdf:type owl:DatatypeProperty ;  rdfs:comment "Indicates the date when the information was created or obtained" ;  rdfs:label "has date" ;  rdfs:domain info:Information ;  rdfs:range xsd:date .info:hasTitle   rdf:type owl:DatatypeProperty ;  rdfs:comment "Indicates the title of the information unit" ;  rdfs:label "has title" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:hasContent   rdf:type owl:DatatypeProperty ;  rdfs:comment "Indicates the content body of the information unit" ;  rdfs:label "has content" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:Source   rdf:type owl:Class ;  rdfs:comment "Class representing source from which information emanates" ;  rdfs:label "Source" .



@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:    a rdfs:Class ;    rdfs:label "Information"^^xsd:string ;    rdfs:comment "Class representing the concept of information."^^xsd:string .info:hasSource     a rdf:Property ;    rdfs:label "has source"^^xsd:string ;     rdfs:comment "Property showing source of the information."^^xsd:string ;    rdfs:domain info: ;    rdfs:range xsd:string .info:hasDate     a rdf:Property ;    rdfs:label "has date"^^xsd:string ;     rdfs:comment "Property showing the date when the information was created or collected."^^xsd:string ;    rdfs:domain info: ;    rdfs:range xsd:date .info:hasFormat    a rdf:Property ;    rdfs:label "has format"^^xsd:string ;    rdfs:comment "Property showing the format of the information (for example, text, image, video, etc.)."^^xsd:string ;    rdfs:domain info: ;    rdfs:range xsd:string .info:hasAuthor    a rdf:Property ;    rdfs:label "has author"^^xsd:string ;    rdfs:comment "Property showing the author of the information."^^xsd:string ;    rdfs:domain info: ;    rdfs:range xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.com/info#> .info:     a rdfs:Class ;    rdfs:label "Information" .info:Data     a rdfs:Class ;    rdfs:label "Data" ;    rdfs:subClassOf info: .info:Knowledge     a rdfs:Class ;    rdfs:label "Knowledge" ;    rdfs:subClassOf info: .info:Fact     a rdfs:Class ;    rdfs:label "Fact" ;    rdfs:subClassOf info: .info:Communication     a rdfs:Class ;    rdfs:label "Communication" ;    rdfs:subClassOf info: .info:InformationProcessing     a rdfs:Class ;    rdfs:label "InformationProcessing" ;    rdfs:subClassOf info: .info:contains     a rdf:Property ;    rdfs:label "contains" ;    rdfs:domain info: ;    rdfs:range info:Data .info:provides     a rdf:Property ;    rdfs:label "provides" ;    rdfs:domain info:Fact ;    rdfs:range info:Knowledge .info:engagesIn     a rdf:Property ;    rdfs:label "engagesIn" ;    rdfs:domain info:Communication ;    rdfs:range info:InformationProcessing .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:Information rdf:type rdfs:Class ;                 rdfs:comment "A collection of data or knowledge" .info:isBasedOn rdf:type rdf:Property ;               rdfs:range info:Source ;               rdfs:domain info:Information ;               rdfs:comment "The source where the information came from" .info:Source rdf:type rdfs:Class ;            rdfs:comment "The origin for pieces of information" .info:hasFormat rdf:type rdf:Property ;               rdfs:range info:Format ;               rdfs:domain info:Information ;               rdfs:comment "The format in which the information is presented" .info:Format rdf:type rdfs:Class ;            rdfs:comment "Different ways that information can be organised or presented" .info:hasAuthor rdf:type rdf:Property ;               rdfs:range info:Author ;               rdfs:domain info:Information ;               rdfs:comment "Individual or entity who created the information" .info:Author rdf:type rdfs:Class ;            rdfs:comment "Creator of the piece of information" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix inf: <http://example.org/info#> .inf:Information a rdfs:Class ;  rdfs:label "Information" ;  rdfs:comment "A collection of data or facts from which conclusions may be drawn or decisions made." .inf:hasSource a rdf:Property ;  rdfs:domain inf:Information ;  rdfs:range inf:Source ;  rdfs:comment "Property linking information to its source." .inf:isAbout a rdf:Property ;  rdfs:domain inf:Information ;  rdfs:range inf:Topic ;  rdfs:comment "Property linking information to the topic it pertains to." .inf:Topic a rdfs:Class ;  rdfs:label "Topic" ;  rdfs:comment "Subject matter that information is about." .inf:Source a rdfs:Class ;  rdfs:label "Source" ;  rdfs:comment "Origin from which information is obtained." .inf:hasTimestamp a rdf:Property ;  rdfs:domain inf:Information ;  rdfs:range xsd:dateTime ;  rdfs:comment "Property indicating the time at which the information was created or obtained." .inf:hasAuthor a rdf:Property ;  rdfs:domain inf:Information ;  rdfs:range inf:Person ;  rdfs:comment "Property linking information to its author." .inf:Person a rdfs:Class ;  rdfs:label "Person" ;  rdfs:comment "Individual who authored the information." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix info: <http://example.org/info#> .info:     a rdfs:Class ;    rdfs:label "Information" ;    rdfs:comment "This class represents any type of information." .info:textualInformation     a rdfs:Class ;    rdfs:subClassOf info: ;    rdfs:label "Textual Information" ;    rdfs:comment "This class represents information expressed in textual form." .info:numericInformation     a rdfs:Class ;    rdfs:subClassOf info: ;    rdfs:label "Numeric Information" ;    rdfs:comment "This class represents information expressed in numerical form." .info:graphicInformation     a rdfs:Class ;    rdfs:subClassOf info: ;    rdfs:label "Graphic Information" ;    rdfs:comment "This class represents information expressed in graphical form." .info:soundInformation     a rdfs:Class ;    rdfs:subClassOf info: ;    rdfs:label "Sound Information" ;    rdfs:comment "This class represents information expressed in sound form." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:    a owl:Ontology ;    rdfs:comment "An information ontology"@en .info:Information    a owl:Class ;    rdfs:comment "A class representing generic information"@en ;    rdfs:label "Information"@en .info:hasSource    a owl:ObjectProperty ;    rdfs:domain info:Information ;    rdfs:range info:Source ;    rdfs:comment "Relates information to its source"@en ;    rdfs:label "has source"@en .info:Source    a owl:Class ;    rdfs:comment "A class representing source of information"@en ;    rdfs:label "Source"@en .info:hasType    a owl:DatatypeProperty ;    rdfs:domain info:Information ;    rdfs:range xsd:string ;    rdfs:comment "The type of the information"@en ;    rdfs:label "has type"@en .info:hasDate    a owl:DatatypeProperty ;    rdfs:domain info:Information ;    rdfs:range xsd:dateTime ;    rdfs:comment "The date when the information was created or updated"@en ;    rdfs:label "has date"@en .info:hasAuthor    a owl:ObjectProperty ;    rdfs:domain info:Information ;    rdfs:range info:Author ;    rdfs:comment "Relates information to its author"@en ;    rdfs:label "has author"@en .info:Author    a owl:Class ;    rdfs:comment "A class representing the author of the information"@en ;    rdfs:label "Author"@en .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:Information a rdfs:Class ;  rdfs:label "Information" ;  rdfs:comment "The class of all pieces of Information" .info:hasSource a rdf:Property ;  rdfs:label "has source" ;  rdfs:comment "The source from where the Information is derived" ;  rdfs:domain info:Information ;  rdfs:range rdfs:Resource .info:hasContent a rdf:Property ;  rdfs:label "has content" ;  rdfs:comment "The content of the Information" ;  rdfs:domain info:Information ;  rdfs:range xsd:string .info:hasDate a rdf:Property ;  rdfs:label "has date" ;  rdfs:comment "The date when the Information was created/published" ;  rdfs:domain info:Information ;  rdfs:range xsd:date .info:isVerified a rdf:Property ;  rdfs:label "is verified" ;  rdfs:comment "Indicator if the Information is verified or not" ;  rdfs:domain info:Information ;  rdfs:range xsd:boolean .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:Information    rdf:type rdfs:Class ;    rdfs:label "Information" ;    rdfs:comment "A collection of data or knowledge" .info:hasDataType    rdf:type rdf:Property ;    rdfs:label "has data type" ;    rdfs:comment "The type of data represented by the information" ;    rdfs:domain info:Information ;    rdfs:range rdfs:Class .info:hasSource    rdf:type rdf:Property ;    rdfs:label "has source" ;    rdfs:comment "The origin or initial provider of the information" ;    rdfs:domain info:Information ;    rdfs:range rdfs:Resource .info:hasTimestamp    rdf:type rdf:Property ;    rdfs:label "has timestamp" ;    rdfs:comment "The time when the information was generated or collected" ;    rdfs:domain info:Information ;    rdfs:range xsd:dateTime .info:hasValue    rdf:type rdf:Property ;    rdfs:label "has value" ;    rdfs:comment "The actual data or knowledge contained in the information" ;    rdfs:domain info:Information ;    rdfs:range rdfs:Literal .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix info: <http://example.org/info#> .info:   rdf:type rdfs:Class ;  rdfs:label "Information" ;  rdfs:comment "This class represents all types of information." .info:WrittenInformation  rdf:type rdfs:Class ;  rdfs:subClassOf info: ;  rdfs:label "Written Information" ;  rdfs:comment "This class represents information in written form." .info:OralInformation  rdf:type rdfs:Class ;  rdfs:subClassOf info: ;  rdfs:label "Oral Information" ;  rdfs:comment "This class represents information conveyed by speech." .info:DigitalInformation  rdf:type rdfs:Class ;  rdfs:subClassOf info: ;  rdfs:label "Digital Information" ;  rdfs:comment "This class represents information that is in digital format." .info:VisualInformation  rdf:type rdfs:Class ;  rdfs:subClassOf info: ;  rdfs:label "Visual Information" ;  rdfs:comment "This class represents information that is visually conveyed." .info:hasSource   rdf:type rdf:Property ;  rdfs:label "has source" ;  rdfs:comment "The property that links an information to its source." ;  rdfs:domain info: ;  rdfs:range xsd:string .info:hasDate   rdf:type rdf:Property ;  rdfs:label "has date" ;  rdfs:comment "The property that links an information to its creation or publication date." ;  rdfs:domain info: ;  rdfs:range xsd:date .


