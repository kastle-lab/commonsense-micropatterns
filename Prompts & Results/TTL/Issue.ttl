@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://www.example.com#> .ex:Issue rdf:type rdfs:Class .ex:hasIssue rdf:type rdf:Property ;           rdfs:domain ex:Person ;           rdfs:range ex:Issue .          ex:hasDescription rdf:type rdf:Property ;                  rdfs:domain ex:Issue ;                  rdfs:range rdf:langString .     ex:Person rdf:type rdfs:Class .ex:issueSeverity rdf:type rdf:Property ;                 rdfs:domain ex:Issue ;                 rdfs:range ex:IssueSeverity .                 ex:IssueSeverity rdf:type rdfs:Class .ex:hasResolutionStatus rdf:type rdf:Property ;                       rdfs:domain ex:Issue ;                       rdfs:range ex:Status .ex:Status rdf:type rdfs:Class ;          rdfs:subClassOf ex:Issue .ex:hasPriority rdf:type rdf:Property ;               rdfs:domain ex:Issue ;               rdfs:range ex:Priority .ex:Priority rdf:type rdfs:Class ;            rdfs:subClassOf ex:Issue .ex:Moderate rdf:type ex:IssueSeverity .ex:Severe rdf:type ex:IssueSeverity .ex:Low rdf:type ex:IssueSeverity .  ex:Resolved rdf:type ex:Status .ex:Unresolved rdf:type ex:Status .ex:High rdf:type ex:Priority .ex:Medium rdf:type ex:Priority .ex:Low rdf:type ex:Priority .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix foaf: <http://xmlns.com/foaf/0.1/> .@prefix dc: <http://purl.org/dc/elements/1.1/> .@prefix issue: <http://example.com/issue#> .issue: a rdfs:Class ;  rdfs:label "Issue" ;  rdfs:comment "A topic in question or under discussion."@en .issue:id a rdfs:Datatype ;  rdfs:subClassOf xsd:integer ;  rdfs:label "Issue ID" ;  rdfs:comment "The unique identifier for an issue."@en .issue:title a rdfs:Datatype ;  rdfs:subClassOf xsd:string ;  rdfs:label "Issue Title" ;  rdfs:comment "The title or brief description of the issue."@en .issue:description a rdfs:Datatype ;  rdfs:subClassOf xsd:string ;  rdfs:label "Issue Description" ;  rdfs:comment "A detailed description of the issue."@en .issue:status a rdfs:Class ;  rdfs:label "Status" ;  rdfs:comment "Current state of the issue (open, closed, in progress, etc.)."@en .issue:category a rdfs:Class ;  rdfs:label "Category" ;  rdfs:comment "Category or type of the issue (bug, enhancement, task, etc.)."@en .issue:createdBy a owl:ObjectProperty ;  rdfs:domain issue: ;  rdfs:range foaf:Agent ;  rdfs:label "created by" ;  rdfs:comment "The agent (person, bot, etc.) who created the issue."@en ;issue:assignedTo a owl:ObjectProperty ;  rdfs:domain issue: ;  rdfs:range foaf:Agent ;  rdfs:label "assigned to" ;  rdfs:comment "The agent (person, bot, etc.) who is responsible for resolving the issue."@en ;  issue:createdOn a owl:DatatypeProperty ;  rdfs:domain issue: ;  rdfs:range xsd:dateTime ;  rdfs:label "created on" ;  rdfs:comment "The date and time when the issue was created."@en .issue:resolvedOn a owl:DatatypeProperty ;  rdfs:domain issue: ;  rdfs:range xsd:dateTime ;  rdfs:label "resolved on" ;  rdfs:comment "The date and time when the issue was resolved."@en.


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix : <http://example.com/ontology/issue#> .:Issue rdf:type rdfs:Class ;       rdfs:label "Issue" ;       rdfs:comment "A topic or problem that needs attention or discussion" .:IssueID rdf:type rdf:Property ;         rdfs:range xsd:string ;         rdfs:domain :Issue ;         rdfs:label "has issue identifier" ;         rdfs:comment "A unique identifier for the issue" .:IssueTitle rdf:type rdf:Property ;            rdfs:range xsd:string ;            rdfs:domain :Issue ;            rdfs:label "has issue title" ;            rdfs:comment "The title or main topic of the issue" .:IssueDescription rdf:type rdf:Property ;                  rdfs:range xsd:string ;                  rdfs:domain :Issue ;                  rdfs:label "has issue description" ;                  rdfs:comment "Detailed information about the issue" .:IssueDate rdf:type rdf:Property ;           rdfs:range xsd:date ;           rdfs:domain :Issue ;           rdfs:label "has issue date" ;           rdfs:comment "Date when the issue was raised or recorded" .:IssueStatus rdf:type rdf:Property ;             rdfs:range :IssueStatusEnum ;             rdfs:domain :Issue ;             rdfs:label "has issue status" ;             rdfs:comment "The current status of the issue" .:IssueStatusEnum rdf:type rdfs:Class ;                 rdfs:label "Status" ;                 rdfs:subClassOf rdfs:Resource ;                 rdfs:comment "An enumeration of possible issue statuses" .:Open rdf:type :IssueStatusEnum ;      rdfs:label "Open" ;      rdfs:comment "The issue is currently open and needs attention" .:InProgress rdf:type :IssueStatusEnum ;            rdfs:label "In Progress" ;            rdfs:comment "Work has started on the issue" .:Resolved rdf:type :IssueStatusEnum ;          rdfs:label "Resolved" ;          rdfs:comment "The issue has been resolved" .:Closed rdf:type :IssueStatusEnum ;        rdfs:label "Closed" ;        rdfs:comment "The issue is closed and no further action is necessary" .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/ontology/issue#> .ex:Issue rdf:type owl:Class ;  rdfs:label "Issue"^^xsd:string ;  rdfs:comment "Representation of an event or subject of any type that requires attention or action"^^xsd:string ;  rdfs:subClassOf ex:Entity .ex:hasId rdf:type owl:DatatypeProperty ;  rdfs:domain ex:Issue ;  rdfs:range xsd:integer ;  rdfs:label "hasId"^^xsd:string ;  rdfs:comment "Unique identifier for issue"^^xsd:string .ex:hasTitle rdf:type owl:DatatypeProperty ;  rdfs:domain ex:Issue ;  rdfs:range xsd:string ;  rdfs:label "hasTitle"^^xsd:string ;  rdfs:comment "Title of the issue"^^xsd:string .ex:hasDescription rdf:type owl:DatatypeProperty ;  rdfs:domain ex:Issue ;  rdfs:range xsd:string ;  rdfs:label "hasDescription"^^xsd:string ;  rdfs:comment "Detailed description about the issue"^^xsd:string .ex:hasStatus rdf:type owl:ObjectProperty ;  rdfs:domain ex:Issue ;  rdfs:range ex:Status ;  rdfs:label "hasStatus"^^xsd:string ;  rdfs:comment "Current status of the issue"^^xsd:string .ex:Status rdf:type owl:Class ;  rdfs:label "Status"^^xsd:string ;  rdfs:comment "Representation of the status of an issue"^^xsd:string ;  rdfs:subClassOf ex:Entity .ex:assignedTo rdf:type owl:ObjectProperty ;  rdfs:domain ex:Issue ;  rdfs:range ex:Person ;  rdfs:label "assignedTo"^^xsd:string ;  rdfs:comment "The person the issue is currently assigned to"^^xsd:string .ex:Person rdf:type owl:Class ;  rdfs:label "Person"^^xsd:string ;  rdfs:comment "An individual person"^^xsd:string;  rdfs:subClassOf ex:Entity .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix issue: <http://www.example.org/ontology/issue#> .issue:Issue  rdf:type owl:Class ;  rdfs:comment "Class representing the concept of an issue" ;  rdfs:label "Issue" ;  rdfs:subClassOf owl:Thing .issue:hasID  rdf:type owl:DatatypeProperty ;  rdfs:domain issue:Issue ;  rdfs:range xsd:string ;  rdfs:comment "Property representing the unique identifier of an issue" ;  rdfs:label "has ID" .issue:hasDescription  rdf:type owl:DatatypeProperty ;  rdfs:domain issue:Issue ;  rdfs:range xsd:string ;  rdfs:comment "Property representing the description of an issue" ;  rdfs:label "has description" .issue:hasStatus  rdf:type owl:ObjectProperty ;  rdfs:domain issue:Issue ;  rdfs:range issue:Status ;  rdfs:comment "Property representing the status of an issue" ;  rdfs:label "has status" .issue:Status  rdf:type owl:Class ;  rdfs:comment "Class representing the status of an issue" ;  rdfs:label "Status" ;  rdfs:subClassOf owl:Thing .issue:statusOpen  rdf:type issue:Status ;  rdfs:label "Open" ;  rdfs:comment "Status indicating that an issue is open" .issue:statusClosed  rdf:type issue:Status ;  rdfs:label "Closed" ;  rdfs:comment "Status indicating that an issue is closed" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix issue: <http://www.example.org/ontology/issue#> .issue:Issue a owl:Class ;    rdfs:comment "An item of concern or importance that needs to be resolved or dealt with" ;    rdfs:subClassOf owl:Thing .issue:hasIssue a rdf:Property ;    rdfs:domain owl:Thing ;    rdfs:range issue:Issue ;    rdfs:comment "Relates an entity to an issue associated with it" .issue:hasDescription a rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range rdfs:Literal ;    rdfs:comment "Relates an issue to its textual description" .issue:hasPriority a rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range issue:Priority ;    rdfs:comment "Relates an issue to its level of urgency or importance" .issue:Priority a owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:comment "Level of urgency or importance of an issue" .issue:Low a issue:Priority ;    rdfs:comment "Low level of urgency or importance" .issue:Medium a issue:Priority ;    rdfs:comment "Medium level of urgency or importance" .issue:High a issue:Priority ;    rdfs:comment "High level of urgency or importance" .issue:hasStatus a rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range issue:Status ;    rdfs:comment "Relates an issue to its current status" .issue:Status a owl:Class ;    rdfs:subClassOf owl:Thing ;    rdfs:comment "Current status of an issue" .issue:Open a issue:Status ;    rdfs:comment "The issue is currently open" .issue:Closed a issue:Status ;    rdfs:comment "The issue is currently closed" .issue:In_Progress a issue:Status ;    rdfs:comment "Work is currently being done on the issue" .




@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix : <http://example.org/issue#> .:Issue rdf:type rdfs:Class ;    rdfs:label "Issue"@en ;    rdfs:comment "A matter or problem regarded as requiring discussion or settlement"@en .:IssueIdentifier rdf:type rdfs:Datatype ;    rdfs:label "Issue Identifier"@en ;    rdfs:comment "Unique ID assigned to an issue for identification"@en ;    rdfs:range xsd:string .:IssueTitle rdf:type rdfs:Datatype ;    rdfs:label "Issue Title"@en ;    rdfs:comment "Title or headline of the issue"@en ;    rdfs:range xsd:string .:IssueDescription rdf:type rdfs:Datatype ;    rdfs:label "Issue Description"@en ;    rdfs:comment "Detailed depiction of the issue"@en ;    rdfs:range xsd:string .    :IssueStatus rdf:type rdfs:Datatype ;    rdfs:label "Issue Status"@en ;    rdfs:comment "Current state or condition of the issue"@en ;    rdfs:range xsd:string .:IssueDateCreated rdf:type rdfs:Datatype ;    rdfs:label "Issue Date Created"@en ;    rdfs:comment "Date when the issue was firstly created"@en ;    rdfs:range xsd:dateTime .    :hasIssueIdentifier rdf:type rdf:Property ;    rdfs:domain :Issue ;    rdfs:range :IssueIdentifier .:hasIssueTitle rdf:type rdf:Property ;    rdfs:domain :Issue ;    rdfs:range :IssueTitle .:hasIssueDescription rdf:type rdf:Property ;    rdfs:domain :Issue ;    rdfs:range :IssueDescription .:hasIssueStatus rdf:type rdf:Property ;    rdfs:domain :Issue ;    rdfs:range :IssueStatus .:hasIssueDateCreated rdf:type rdf:Property ;    rdfs:domain :Issue ;    rdfs:range :IssueDateCreated .




