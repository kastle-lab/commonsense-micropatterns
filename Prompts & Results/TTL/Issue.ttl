@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix issue: <http://example.com/issue#> .issue:Issue rdf:type rdfs:Class ;    rdfs:comment "Class representing an issue"^^xsd:string .issue:id rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:string ;    rdfs:comment "Unique identifier of the issue"^^xsd:string . issue:title rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:string ;    rdfs:comment "Title of the issue"^^xsd:string . issue:description rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:string ;    rdfs:comment "Description of the issue"^^xsd:string . issue:status rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:string ;    rdfs:comment "Status of the issue (e.g., open, closed, in progress)"^^xsd:string .issue:reporter rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:string ;    rdfs:comment "Person who reported the issue"^^xsd:string . issue:assignee rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:string ;    rdfs:comment "Person assigned to solve the issue"^^xsd:string . issue:createdDate rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:dateTime ;    rdfs:comment "Date when the issue was created"^^xsd:string .issue:closedDate rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:dateTime ;    rdfs:comment "Date when the issue was closed"^^xsd:string . issue:resolvedDate rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:dateTime ;    rdfs:comment "Date when the issue was resolved"^^xsd:string . issue:dueDate rdf:type rdf:Property ;    rdfs:domain issue:Issue ;    rdfs:range xsd:dateTime ;    rdfs:comment "Due date for the issue to be resolved"^^xsd:string . 


@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix ex: <http://example.com/> .ex:Issue a rdfs:Class .ex:hasIssue a rdf:Property ;    rdfs:domain ex:Entity ;    rdfs:range ex:Issue .ex:solveIssue a rdf:Property ;    rdfs:domain ex:Entity ;    rdfs:range ex:Issue .ex:Issue     rdfs:label "Issue"@en ;    rdfs:comment "An issue or problem that needs to be resolved"@en .ex:hasIssue    rdfs:label "has issue"@en ;    rdfs:comment "Link an entity with an issue it has"@en .ex:solveIssue    rdfs:label "solve issue"@en ;    rdfs:comment "Link an entity with an issue it solves"@en .

