@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix ex: <http://example.org/> .ex:Change a rdf:Class ;      rdfs:label "Change"@en ;      rdfs:comment "A class representing change or modification"@en ;      rdfs:subClassOf ex:Event .ex:hasChange a rdf:Property ;      rdfs:label "has change"@en ;      rdfs:domain ex:Object ;      rdfs:range ex:Change .ex:ChangeStartDate a rdf:Property ;      rdfs:label "Change start date"@en ;      rdfs:domain ex:Change ;      rdfs:range xsd:dateTime .ex:ChangeEndDate a rdf:Property ;      rdfs:label "Change end date"@en ;      rdfs:domain ex:Change ;      rdfs:range xsd:dateTime .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix : <http://example.org/> .:Change a rdfs:Class ;  rdfs:label "Change" ;  rdfs:comment "Represents the concept of change or alteration in any context or domain" ;  rdfs:subClassOf rdfs:Resource .:hasInitiator a rdf:Property ;  rdfs:label "has initiator" ;  rdfs:comment "The property representing what initiates or triggers the change" ;  rdfs:domain :Change ;  rdfs:range rdfs:Resource .:hasResult a rdf:Property ;  rdfs:label "has result" ;  rdfs:comment "The property representing the result or outcome of the change" ;  rdfs:domain :Change ;  rdfs:range rdfs:Resource .:hasContext a rdf:Property ;  rdfs:label "has context" ;  rdfs:comment "The property representing the context in which the change occurs" ;  rdfs:domain :Change ;  rdfs:range rdfs:Resource .:occursAt a rdf:Property ;  rdfs:label "occurs at" ;  rdfs:comment "The property pointing to when or where the change occurs" ;  rdfs:domain :Change ;  rdfs:range rdfs:Resource .


@prefix base: <http://example.org/> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .base:Change a rdfs:Class ;             rdfs:comment "A change or transition from one state to another" .base:fromState a rdf:Property ;               rdfs:domain base:Change ;               rdfs:range rdfs:Literal ;               rdfs:comment "A property representing the initial state before the transition" .base:toState a rdf:Property ;             rdfs:domain base:Change ;             rdfs:range rdfs:Literal ;             rdfs:comment "A property representing the final state after the transition" .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix : <http://www.example.com/ontology#> .:Change a rdf:Class ;    rdfs:label "Change" ;    rdfs:comment "Changes or modifications to entities" .:Event a rdf:Class ;    rdfs:label "Event" ;    rdfs:comment "Events that constitute a change" .:hasEvent a rdf:Property ;    rdf:domain :Change ;    rdf:range :Event ;    rdfs:label "has event" ;    rdfs:comment "Change has event or series of events" .:occursAt a rdf:Property ;    rdf:domain :Event ;    rdf:range xsd:dateTime ;    rdfs:label "occurs at" ;    rdfs:comment "Time at which event occurs" .:performedBy a rdf:Property ;    rdf:domain :Event ;    rdf:range :Agent ;    rdfs:label "performed by" ;    rdfs:comment "Agent performing the event" .
