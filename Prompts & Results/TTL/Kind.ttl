@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix kind: <http://example.com/kind#> .kind:Kind  rdf:type rdfs:Class ;  rdfs:label "Kind"@en ;  rdfs:comment "Class of all kinds or types, classes, sorts, genres or forms of things"@en .kind:name  rdf:type rdf:Property ;  rdfs:label "Name"@en ;  rdfs:comment "The name of a kind."@en ;  rdfs:domain kind:Kind ;  rdfs:range xsd:string .kind:hasSubKind  rdf:type rdf:Property ;  rdfs:label "has SubKind"@en ;  rdfs:comment "A relation between a kind and its subkind."@en ;  rdfs:domain kind:Kind ;  rdfs:range kind:Kind .kind:hasSuperKind  rdf:type rdf:Property ;  rdfs:label "has SuperKind"@en ;  rdfs:comment "A relation from a kind to the kind above it in the hierarchy."@en ;  rdfs:domain kind:Kind ;  rdfs:range kind:Kind .kind:isKindOf  rdf:type rdf:Property ;  rdfs:label "is Kind of"@en ;  rdfs:comment "An object property to indicate that one kind is a kind of another kind."@en ;  rdfs:domain kind:Kind ;  rdfs:range kind:Kind . kind:hasInstances  rdf:type rdf:Property ;  rdfs:label "has instances"@en ;  rdfs:comment "An object property to indicate that a particular kind has certain instances."@en ;  rdfs:domain kind:Kind ;  rdfs:range xsd:string .kind:example  rdf:type rdf:Property ;  rdfs:label "Example"@en ;  rdfs:comment "An example of the kind."@en ;  rdfs:domain kind:Kind ;  rdfs:range xsd:string .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix kind: <http://example.org/kind#> .kind:Kind rdf:type rdfs:Class ;  rdfs:label "Kind"@en ;  rdfs:comment "A class for representing kinds or types of things or beings"@en .kind:name rdf:type rdf:Property ;  rdfs:domain kind:Kind ;  rdfs:range xsd:string ;  rdfs:label "name"@en ;  rdfs:comment "The name of the kind or type"@en .kind:description rdf:type rdf:Property ;  rdfs:domain kind:Kind ;  rdfs:range xsd:string ;  rdfs:label "description"@en ;  rdfs:comment "Description about that certain kind"@en .kind:hasSubKind rdf:type rdf:Property ;  rdfs:domain kind:Kind ;  rdfs:range kind:Kind ;  rdfs:label "has sub kind"@en ;  rdfs:comment "Relation between a kind and its sub kinds"@en .kind:Animal rdf:type kind:Kind ;  kind:name "Animal"@en ;  kind:description "Animals are multicellular eukaryotic organisms that form the biological kingdom Animalia"@en .kind:Mammal rdf:type kind:Kind ;  kind:name "Mammal"@en ;  kind:description "Mammals include humans and are the vertebrates within the class Mammalia"@en ;  kind:hasSubKind kind:Animal . kind:Birds rdf:type kind:Kind ;  kind:name "Birds"@en ;  kind:description "Birds are a group of warm-blooded vertebrates constituting the class Aves"@en ;  kind:hasSubKind kind:Animal .


