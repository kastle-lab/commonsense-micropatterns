@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix week: <http://example.org/week#> .week:Week rdf:type rdfs:Class ;    rdfs:comment "A week in a calendar" .week:hasDay rdf:type rdf:Property;    rdfs:range week:Day ;    rdfs:domain week:Week ;    rdfs:comment "Relates a week to its days" .week:Day rdf:type rdfs:Class ;    rdfs:comment "The days in a week" .week:Monday rdf:type week:Day ;    rdfs:comment "Monday" .  week:Tuesday rdf:type week:Day ;    rdfs:comment "Tuesday" .week:Wednesday rdf:type week:Day ;    rdfs:comment "Wednesday" .week:Thursday rdf:type week:Day ;    rdfs:comment "Thursday" .week:Friday rdf:type week:Day ;    rdfs:comment "Friday" .week:Saturday rdf:type week:Day ;    rdfs:comment "Saturday" .week:Sunday rdf:type week:Day ;    rdfs:comment "Sunday" .week:hasSequence rdf:type rdf:Property;    rdfs:domain week:Day ;    rdfs:range xsd:integer ;    rdfs:comment "Defines the sequence of the day in a week" .week:Monday week:hasSequence "1"^^xsd:integer .week:Tuesday week:hasSequence "2"^^xsd:integer .week:Wednesday week:hasSequence "3"^^xsd:integer .week:Thursday week:hasSequence "4"^^xsd:integer .week:Friday week:hasSequence "5"^^xsd:integer .week:Saturday week:hasSequence "6"^^xsd:integer .week:Sunday week:hasSequence "7"^^xsd:integer .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix dbo: <http://dbpedia.org/ontology/> .@prefix time: <http://www.w3.org/2006/time#> .@prefix week: <http://example.org/week#> .week: a owl:Ontology ;  rdfs:label "Ontology of Week" ;  rdfs:comment "An ontology that describes the concept of a week and its components." .week:Week a owl:Class ;  rdfs:label "Week" ;  rdfs:subClassOf time:TemporalUnit ;  rdfs:comment "A class representing a week, a temporal unit of seven days." .week:Day a owl:Class ;  rdfs:label "Day" ;  rdfs:subClassOf time:TemporalUnit ;  rdfs:comment "A class representing a day, a unit of time defined as 24 hours." .week:hasDay a owl:ObjectProperty ;  rdfs:label "has day" ;  rdfs:domain week:Week ;  rdfs:range week:Day ;  rdfs:comment "The property that connects a week with its days." .week:dayName a owl:DatatypeProperty ;  rdfs:label "day name" ;  rdfs:domain week:Day ;  rdfs:range xsd:string ;  rdfs:comment "The property that gives a day its name (e.g., Monday, Tuesday, etc.)." .  week:dayNumber a owl:DatatypeProperty ;  rdfs:label "day number" ;  rdfs:domain week:Day ;  rdfs:range xsd:integer ;  rdfs:comment "The property that assigns a number to a day within the week (e.g., 1 for Monday, 2 for Tuesday, etc.)." .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix week: <http://www.example.org/week#> .week:Week a rdfs:Class ;  rdfs:comment "Represents a Week."@en ;  rdfs:label "Week"@en .week:hasDay a rdf:Property ;  rdfs:comment "The relation between a week and its days."@en ;  rdfs:label "hasDay"@en ;  rdfs:domain week:Week ;  rdfs:range week:Day .week:Day a rdfs:Class ;  rdfs:comment "Represents a Day within a Week."@en ;  rdfs:label "Day"@en .week:Monday a week:Day ;  rdfs:label "Monday"@en .week:Tuesday a week:Day ;  rdfs:label "Tuesday"@en .week:Wednesday a week:Day ;  rdfs:label "Wednesday"@en .week:Thursday a week:Day ;  rdfs:label "Thursday"@en .week:Friday a week:Day ;  rdfs:label "Friday"@en .week:Saturday a week:Day ;  rdfs:label "Saturday"@en .week:Sunday a week:Day ;  rdfs:label "Sunday"@en .





@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix week: <http://example.org/week#> .week:Week rdf:type rdfs:Class ;      rdfs:label "Week"@en ;      rdfs:comment "A period of seven days"@en .week:Day rdf:type rdfs:Class ;      rdfs:subClassOf week:Week ;      rdfs:label "Day"@en ;      rdfs:comment "A period of 24 hours"@en .week:hasDay rdf:type rdf:Property ;      rdfs:domain week:Week ;      rdfs:range week:Day ;      rdfs:label "has day"@en ;      rdfs:comment "A week has seven days"@en .week:isDayOf rdf:type rdf:Property ;      rdfs:domain week:Day ;      rdfs:range week:Week ;      rdfs:label "is day of"@en ;      rdfs:comment "A day is part of a week"@en .week:Monday rdf:type week:Day ;      rdfs:label "Monday"@en ;      rdfs:comment "The second day of the week"@en .week:Tuesday rdf:type week:Day ;      rdfs:label "Tuesday"@en ;      rdfs:comment "The third day of the week"@en .week:Wednesday rdf:type week:Day ;      rdfs:label "Wednesday"@en ;      rdfs:comment "The fourth day of the week"@en .week:Thursday rdf:type week:Day ;      rdfs:label "Thursday"@en ;      rdfs:comment "The fifth day of the week"@en .week:Friday rdf:type week:Day ;      rdfs:label "Friday"@en ;      rdfs:comment "The sixth day of the week"@en .week:Saturday rdf:type week:Day ;      rdfs:label "Saturday"@en ;      rdfs:comment "The seventh day of the week"@en .week:Sunday rdf:type week:Day ;      rdfs:label "Sunday"@en ;      rdfs:comment "The first day of the week"@en .


@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix owl: <http://www.w3.org/2002/07/owl#> .@prefix time: <http://www.w3.org/2006/time#> .@prefix ex: <http://example.org/week#> .ex:Week rdf:type owl:Class ;  rdfs:subClassOf time:TemporalUnit ;  rdfs:label "Week"@en ;  rdfs:comment "A unit of time consisting of seven consecutive 24-hour days."@en .ex:consistsOf rdf:type owl:ObjectProperty ;  rdfs:domain ex:Week ;  rdfs:range ex:Day ;  rdfs:label "consists of"@en ;  rdfs:comment "A week consists of seven days."@en ;  owl:propertyChainAxiom ( ex:consistsOf ex:Day ) ;  owl:cardinality "7"^^xsd:nonNegativeInteger .ex:Day rdf:type owl:Class ;  rdfs:subClassOf time:TemporalUnit ;  rdfs:label "Day"@en ;  rdfs:comment "A unit of time representing a complete turn of the Earth on its axis, approximately 24 hours."@en .ex:Monday rdf:type ex:Day ;  rdfs:label "Monday"@en .ex:Tuesday rdf:type ex:Day ;  rdfs:label "Tuesday"@en .ex:Wednesday rdf:type ex:Day ;  rdfs:label "Wednesday"@en .ex:Thursday rdf:type ex:Day ;  rdfs:label "Thursday"@en .ex:Friday rdf:type ex:Day ;  rdfs:label "Friday"@en .ex:Saturday rdf:type ex:Day ;  rdfs:label "Saturday"@en .ex:Sunday rdf:type ex:Day ;  rdfs:label "Sunday"@en .


