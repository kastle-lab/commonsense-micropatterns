@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .@prefix my: <http://example.org/MyOntology#> .my:Week rdf:type rdfs:Class ;        rdfs:label "Week" ;        rdfs:comment "Representing a week" .my:hasDay rdf:type rdf:Property ;          rdfs:label "has day" ;          rdfs:comment "A property representing a day of the week" ;          rdfs:domain my:Week ;          rdfs:range my:Day .my:Day rdf:type rdfs:Class ;       rdfs:label "Day" ;       rdfs:comment "Representing a day" ;       rdfs:subClassOf my:Week .my:Monday rdf:type my:Day ;           rdfs:label "Monday" .my:Tuesday rdf:type my:Day ;            rdfs:label "Tuesday" .my:Wednesday rdf:type my:Day ;              rdfs:label "Wednesday" .my:Thursday rdf:type my:Day ;             rdfs:label "Thursday" .my:Friday rdf:type my:Day ;           rdfs:label "Friday" .my:Saturday rdf:type my:Day ;             rdfs:label "Saturday" .my:Sunday rdf:type my:Day ;           rdfs:label "Sunday" .my:Week my:hasDay my:Monday, my:Tuesday, my:Wednesday, my:Thursday, my:Friday, my:Saturday, my:Sunday .

