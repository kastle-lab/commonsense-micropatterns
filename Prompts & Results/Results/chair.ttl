@prefix ex: <http://example.com/chairs#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:Chair <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Class ;
	rdfs:comment "A class representing all chairs" .

ex:comfortScore <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "A subjective measure of how comfortable a chair is" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:hasArmrest <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "Whether a chair has an armrest or not" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:hasColor <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "The color of a chair" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:hasHeight <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "The height of a chair" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:hasMaterial <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "The material from which a chair is made" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:hasStyle <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "The style of a chair" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:hasUserCapacity <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "The maximum number of people a chair can accommodate" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

ex:swivelCapability <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> rdfs:Property ;
	rdfs:comment "Whether a chair can swivel or not" ;
	rdfs:domain ex:Chair ;
	rdfs:range rdfs:Literal .

