@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix kastle: <https://kastle.cs.wright.edu/csmodl#> .
@prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle:Machine a rdfs:Class .

kastle:MachiningService a rdfs:Class .

kastle:Material a rdfs:Class .

<https://archive.org/services/purl/domain/modular_ontology_design_library/precisionmachiningservice> a owl:Ontology ;
    opla-core:hasPatternName "precisionmachiningservice Pattern"^^xsd:string ;
    dc:contributor "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string ;
    dc:creator "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string .

kastle:precisionLevel a rdf:Property ;
    rdfs:domain kastle:MachiningService ;
    rdfs:range xsd:int .

kastle:processMaterial a rdf:Property ;
    rdfs:domain kastle:MachiningService ;
    rdfs:range kastle:Material .

kastle:usesMachine a rdf:Property ;
    rdfs:domain kastle:MachiningService ;
    rdfs:range kastle:Machine .

