@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix concept: <http://www.example.com/concept#> .
concept:     rdf:type owl:Ontology ;
    rdfs:comment ""An ontology of concepts""^^xsd:string ;
    rdfs:label ""Concept Ontology""^^xsd:string .
concept:Concept     rdf:type owl:Class ;
    rdfs:comment ""The class of all concepts""^^xsd:string ;
    rdfs:label ""Concept""^^xsd:string .
concept:hasDescription     rdf:type owl:DatatypeProperty ;
    rdfs:domain concept:Concept ;
    rdfs:range xsd:string ;
    rdfs:comment ""The property that represents a description of a concept""^^xsd:string ;
    rdfs:label ""has description""^^xsd:string .
concept:hasExample     rdf:type owl:DatatypeProperty ;
    rdfs:domain concept:Concept ;
    rdfs:range xsd:string ;
    rdfs:comment ""The property that represents an example of a concept""^^xsd:string ;
    rdfs:label ""has example""^^xsd:string .
concept:isRelatedTo     rdf:type owl:ObjectProperty ;
    rdfs:domain concept:Concept ;
    rdfs:range concept:Concept ;
    rdfs:comment ""The property that represents a relationship between two concepts""^^xsd:string ;
    rdfs:label ""is related to""^^xsd:string .
