"@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix spt: <http://www.speciality.com/> .
spt:speciality rdf:type rdfs:Class .
spt:medicine_speciality rdf:type rdfs:Class ;
  rdfs:subClassOf spt:speciality .
spt:engineering_speciality rdf:type rdfs:Class ;
  rdfs:subClassOf spt:speciality .
spt:cardiology rdf:type spt:medicine_speciality .
spt:neurology rdf:type spt:medicine_speciality .
spt:civil_engineering rdf:type spt:engineering_speciality .
spt:mechanical_engineering rdf:type spt:engineering_speciality .
spt:hasSpeciality rdf:type rdf:Property ;
  rdfs:domain spt:Professional ;
  rdfs:range spt:speciality .
spt:Professional a rdfs:Class ;
  rdfs:label ""Professional""^^xsd:string .
spt:doctor rdf:type spt:Professional ;
  spt:hasSpeciality spt:cardiology .
spt:engineer rdf:type spt:Professional ;
  spt:hasSpeciality spt:civil_engineering .
spt:doctor rdf:type spt:Professional ;
  spt:hasSpeciality spt:neurology .
spt:engineer rdf:type spt:Professional ;
  spt:hasSpeciality spt:mechanical_engineering .
"