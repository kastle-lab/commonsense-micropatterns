@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix kastle: <https://kastle.cs.wright.edu/csmodl#> .
@prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle:Parent a rdfs:Class ;
    rdfs:subClassOf kastle:Adult .

kastle:Son a rdfs:Class ;
    rdfs:subClassOf kastle:Child .

<https://archive.org/services/purl/domain/modular_ontology_design_library/son> a owl:Ontology ;
    opla-core:hasPatternName "son Pattern"^^xsd:string ;
    dc:contributor "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string ;
    dc:creator "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string .

kastle:hasSon a rdf:Property ;
    rdfs:domain kastle:Parent ;
    rdfs:range kastle:Son .

