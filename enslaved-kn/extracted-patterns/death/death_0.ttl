"@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix death: <http://www.example.org/death#> .
death:Death  rdf:type rdfs:Class ;
  rdfs:label ""Death"" ;
  rdfs:comment ""The cessation of life and all associated processes""@en .
death:CauseOfDeath  rdf:type rdfs:Class ;
  rdfs:label ""Cause of death"" ;
  rdfs:comment ""The specific cause that leads to the occurrence of death""@en .
death:TimeOfDeath  rdf:type rdfs:Datatype ;
  rdfs:label ""Time of death"" ;
  rdfs:comment ""The specific time when death occurred""@en ;
  rdfs:subClassOf xsd:dateTime .
death:PlaceOfDeath  rdf:type rdfs:Class ;
  rdfs:label ""Place of death"" ;
  rdfs:comment ""The specific location where death occurred""@en .
death:isCauseOf  rdf:type rdf:Property ;
  rdfs:label ""is cause of"" ;
  rdfs:comment ""The property that links a cause to the death it results in"" ;
  rdfs:domain death:CauseOfDeath ;
  rdfs:range death:Death .
death:occursAt  rdf:type rdf:Property ;
  rdfs:label ""occurs at"" ;
  rdfs:comment ""The property that links the death to its location""@en ;
  rdfs:domain death:Death ;
  rdfs:range death:PlaceOfDeath .
death:happensAt  rdf:type rdf:Property ;
  rdfs:label ""happens at"" ;
  rdfs:comment ""The property that links the death to its time of occurrence""@en ;
  rdfs:domain death:Death ;
  rdfs:range death:TimeOfDeath .
"