@prefix ex: <http://example.com/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
ex:Surname  rdf:type rdfs:Class ;
  rdfs:comment ""The surname of a person""^^xsd:string ;
  rdfs:label ""Surname""^^xsd:string .
ex:hasSurname  rdf:type rdf:Property ;
  rdfs:comment ""The property that links a person to their surname""^^xsd:string ;
  rdfs:label ""has surname""^^xsd:string ;
  rdfs:domain ex:Person ;
  rdfs:range ex:Surname .
  ex:Person   rdf:type rdfs:Class ;
  rdfs:comment ""An individual human being""^^xsd:string ;
  rdfs:label ""Person""^^xsd:string .
ex:JohnSmith  rdf:type ex:Person ;
  ex:hasSurname ex:Smith .
ex:Smith   rdf:type ex:Surname ;
  rdfs:label ""Smith""^^xsd:string .
