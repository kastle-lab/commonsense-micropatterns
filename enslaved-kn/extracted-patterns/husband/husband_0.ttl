"@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix relationship: <http://www.relationship.com/> .
@prefix person: <http://www.person.com/> .
relationship:hasHusband a rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range person:Person ;
    rdfs:label ""has husband""@en ;
    rdfs:comment ""The property that links to a person who is a husband of someone.""@en .
person:Man a rdfs:Class ;
    rdfs:label ""Man""@en ;
    rdfs:comment ""A man is an adult human male.""@en .
person:Woman a rdfs:Class ;
    rdfs:label ""Woman""@en ;
    rdfs:comment ""A woman is an adult human female.""@en .
person:Person a rdfs:Class;
    rdfs:subClassOf person:Man , person:Woman ;
    rdfs:label ""Person""@en ;
    rdfs:comment ""A person can be a man or a woman.""@en .
person:John a person:Man ;
    rdfs:label ""John""@en .
 person:Jane a person:Woman ;
    rdfs:label ""Jane""@en ;
    relationship:hasHusband person:John .
person:John relationship:isHusbandOf person:Jane .
"