@prefix ex: <http://example.org/ontology#> .
ex:Name a rdf:Class .
ex:hasFirstName a rdf:Property ;
    rdfs:domain ex:Name ;
    rdfs:range xsd:string .
ex:hasLastName a rdf:Property ;
    rdfs:domain ex:Name ;
    rdfs:range xsd:string .
ex:hasMiddleName a rdf:Property ;
    rdfs:domain ex:Name ;
    rdfs:range xsd:string .
ex:isAliasOf a rdf:Property ;
    rdfs:domain ex:Name ;
    rdfs:range ex:Name .
