@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix place: <http://example.org/place#> .
place:     a owl:Ontology ;
    rdfs:comment ""An ontology focusing on places."" .
place:Place     a owl:Class ;
    rdfs:comment ""A class representing any type of place."" ;
    rdfs:label ""Place"" .
place:Country    a owl:Class ;
    rdfs:subClassOf place:Place ;
    rdfs:comment ""A class representing a country."" ;
    rdfs:label ""Country"" .
place:City    a owl:Class ;
    rdfs:subClassOf place:Place ;
    rdfs:comment ""A class representing a city."" ;
    rdfs:label ""City"".
place:adress    a owl:DatatypeProperty ;
    rdfs:domain place:Place ;
    rdfs:range xsd:string ;
    rdfs:comment ""A property representing the address of a place."" ;
    rdfs:label ""address"".
place:CountryOf    a owl:ObjectProperty ;
    rdfs:domain place:City ;
    rdfs:range place:Country ;
    rdfs:comment ""A property linking a city to its containing country."" ;
    rdfs:label ""is in country"" .
