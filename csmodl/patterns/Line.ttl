kastle:hasEndPoint a rdf:Property ;
    rdfs:domain kastle:,
        kastle:Line ;
    rdfs:range kastle:Point .

kastle:hasLength a rdf:Property ;
    rdfs:domain kastle:,
        kastle:Line ;
    rdfs:range kastle:decimal,
        kastle:double,
        kastle:float .

kastle:hasPoint a rdf:Property ;
    rdfs:domain kastle:Line ;
    rdfs:range kastle:Point .

kastle:hasStart a rdf:Property ;
