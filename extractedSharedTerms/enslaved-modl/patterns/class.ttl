@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix kastle: <https://kastle.cs.wright.edu/enslaved-modl#> .
@prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle:Class a rdfs:Class .

kastle:Datetime a rdfs:Class .

kastle:Student a rdfs:Class .

kastle:Teacher a rdfs:Class .

<https://archive.org/services/purl/domain/modular_ontology_design_library/class> a owl:Ontology ;
    opla-core:hasPatternName "class Pattern"^^xsd:string ;
    dc:contributor "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string ;
    dc:creator "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string .

kastle:endTime a rdf:Property ;
    rdfs:domain kastle:Class ;
    rdfs:range kastle:Datetime .

kastle:hasStudent a rdf:Property ;
    rdfs:domain kastle:Class ;
    rdfs:range kastle:Student .

kastle:hasSubject a rdf:Property ;
    rdfs:domain kastle:Class ;
    rdfs:range xsd:string .

kastle:hasTeacher a rdf:Property ;
    rdfs:domain kastle:Class ;
    rdfs:range kastle:Teacher .

kastle:startTime a rdf:Property ;
    rdfs:domain kastle:Class ;
    rdfs:range kastle:Datetime .

