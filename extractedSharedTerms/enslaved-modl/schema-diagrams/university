digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Department [label="kastle:Department"]
	University [label="kastle:University"]
	Department -> University [label="kastle:belongsTo"]
	Student [label="kastle:Student"]
	Course [label="kastle:Course"]
	Student -> Course [label="kastle:enrolledIn"]
	University [label="kastle:University"]
	University -> Department [label="kastle:hasDepartment"]
	Faculty [label="kastle:Faculty"]
	Department -> Faculty [label="kastle:hasFaculty"]
	Course [label="kastle:Course"]
	Department -> Course [label="kastle:offeredBy"]
	Course [label="kastle:Course"]
	Student -> Course [label="kastle:studies"]
	Professor [label="kastle:Professor"]
	Course [label="kastle:Course"]
	Professor -> Course [label="kastle:teaches"]
	Course [label="kastle:Course"]
	Professor -> Course [label="kastle:teachesCourse"]
}
