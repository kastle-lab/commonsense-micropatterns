digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Book [label="kastle:Book"]
	Author [label="kastle:Author"]
	Book -> Author [label="kastle:authoredBy"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Book -> string [label="kastle:bookTitle"]
	Person [label="kastle:Person"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:hasName"]
	Publication [label="kastle:Publication"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Publication -> date [label="kastle:hasPublicationDate"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Publication -> string [label="kastle:hasTitle"]
	Author [label="kastle:Author"]
	Author -> Publication [label="kastle:hasWritten"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Book -> date [label="kastle:publishedIn"]
}
