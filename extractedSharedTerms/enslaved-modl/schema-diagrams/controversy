	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Controversy [label="kastle:Controversy"]
	Subject [label="kastle:Subject"]
	Controversy -> Subject [label="kastle:concerns"]
	Actor [label="kastle:Actor"]
	Controversy -> Actor [label="kastle:hasActor"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Controversy -> date [label="kastle:hasEndDate"]
	Resource [label="kastle:Resource"]
	Controversy -> Resource [label="kastle:hasStakeholder"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Controversy -> date [label="kastle:hasStartDate"]
	Resource [label="kastle:Resource"]
	Subject [label="kastle:Subject"]
	Controversy -> Subject [label="kastle:hasSubject"]
	Actor [label="kastle:Actor"]
	Actor -> Controversy [label="kastle:participatesIn"]
}
