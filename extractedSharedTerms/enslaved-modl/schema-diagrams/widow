digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Person [label="kastle:Person"]
	Person -> Person [label="kastle:hasSpouse"]
	Widow [label="kastle:Widow"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Widow -> date [label="kastle:hasSpouseDeathDate"]
	Man [label="kastle:Man"]
	Widow -> Man [label="kastle:wasMarried"]
}
