digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Person [label="kastle:Person"]
	School [label="kastle:School"]
	Person -> School [label="kastle:attends"]
	Family [label="kastle:Family"]
	Person -> Family [label="kastle:belongsTo"]
	Society [label="kastle:Society"]
	Institution [label="kastle:Institution"]
	Society -> Institution [label="kastle:consistsOf"]
	CulturalNorm [label="kastle:CulturalNorm"]
	Society -> CulturalNorm [label="kastle:hasCulturalNorm"]
	Institution [label="kastle:Institution"]
	Society -> Institution [label="kastle:hasInstitution"]
	Individual [label="kastle:Individual"]
	Society -> Person [label="kastle:hasMember"]
	Individual [label="kastle:Individual"]
	Role [label="kastle:Role"]
	Individual -> Role [label="kastle:hasRole"]
}
