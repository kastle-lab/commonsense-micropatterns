@prefix ex: <http://example.com/>@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>#Define "Chair" as a subclass of "Furniture"ex:Chair rdf:type rdfs:Class ;
  rdfs:subClassOf ex:Furniture .
#Define properties of "Chair"ex:hasColor rdf:type rdf:Property ;
  rdfs:domain ex:Chair ;
  rdfs:range rdfs:Literal .
ex:hasMaterial rdf:type rdf:Property ;
  rdfs:domain ex:Chair ;
  rdfs:range rdfs:Literal .
#Chair instanceex:chair1 rdf:type ex:Chair ;
  ex:hasColor "Black" ;
  ex:hasMaterial "Wood" .
