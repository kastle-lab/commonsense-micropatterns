@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .
ex:World     rdfs:subClassOf ex:Entity ;
ex:Continent    rdfs:subClassOf ex:Entity ;
ex:Country    rdfs:subClassOf ex:Entity ;
ex:State    rdfs:subClassOf ex:Entity ;
ex:City    rdfs:subClassOf ex:Entity .
ex:Earth     rdf:type ex:World .
ex:Asia     rdf:type ex:Continent .
ex:India     rdf:type ex:Country .
ex:Karnataka     rdf:type ex:State .
ex:Bangalore     rdf:type ex:City .
ex:Earth ex:hasContinent ex:Asia .
ex:Asia ex:hasCountry ex:India .
ex:India ex:hasState ex:Karnataka .
ex:Karnataka ex:hasCity ex:Bangalore .
