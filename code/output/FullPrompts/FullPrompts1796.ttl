@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix power: <http://www.example.com/power#> .
# Power ontologypower: a rdfs:Class;
    rdfs:label "Power".
power:electrical a rdfs:Class;
    rdfs:subClassOf power:;
    rdfs:label "Electrical power".
power:thermal a rdfs:Class;
    rdfs:subClassOf power:;
    rdfs:label "Thermal power".
power:mechanical a rdfs:Class;
    rdfs:subClassOf power:;
    rdfs:label "Mechanical power".
power:solar a rdfs:Class;
    rdfs:subClassOf power:;
    rdfs:label "Solar power".
power:wind a rdfs:Class;
    rdfs:subClassOf power:;
    rdfs:label "Wind power".
power:isGeneratedBy a rdf:Property;
    rdfs:domain power:;
    rdfs:range rdfs:Resource;
    rdfs:label "is generated by".
power:isTransmittedBy a rdf:Property;
    rdfs:domain power:;
    rdfs:range rdfs:Resource;
    rdfs:label "is transmitted by".
power:isUsedBy a rdf:Property;
    rdfs:domain power:;
    rdfs:range rdfs:Resource;
    rdfs:label "is used by".
# Instancespower:Tesla a power:electrical;
    power:isGeneratedBy "Solar panels";
    power:isTransmittedBy "Power grid";
    power:isUsedBy "Electric cars".
power:Chevron a power:thermal;
    power:isGeneratedBy "Fossil fuels";
    power:isTransmittedBy "Pipelines";
    power:isUsedBy "Industrial plants".
