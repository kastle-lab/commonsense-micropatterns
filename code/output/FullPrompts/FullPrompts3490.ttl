@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lvl: <http://example.com/levels#> .
lvl:Level a rdfs:Class ;
    rdfs:label "Level"@en ;
    rdfs:comment "A class to represent varying levels of hierarchy or difficulty in a system"@en .
lvl:Beginner a lvl:Level ;
    rdfs:label "Beginner Level"@en ;
    rdfs:comment "A level indicating the beginning stage of a system"@en .
lvl:Intermediate a lvl:Level ;
    rdfs:label "Intermediate Level"@en ;
    rdfs:comment "A level indicating the middle stage of a system"@en .
lvl:Expert a lvl:Level ;
    rdfs:label "Expert Level"@en ;
    rdfs:comment "A level indicating the advanced stage of a system"@en .
