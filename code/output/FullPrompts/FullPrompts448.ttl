@prefix ruga: <http://www.semanticweb.org/handOntology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
ruga:UserInstruction a rdfs:Class ;
                      rdfs:label "Instructions of usage"^^xsd:string ;
                      rdfs:comment "This class represents the instructions of usage for a hand."^^xsd:string .
ruga:BiologicalHand a rdfs:Class ;
                     rdfs:label "Biological hand"^^xsd:string ;
                     rdfs:comment "This class represents a biological hand."^^xsd:string .
ruga:RoboticHand a rdfs:Class ;
                 rdfs:label "Robotic hand"^^xsd:string ;
                 rdfs:comment "This class represents a robotic hand."^^xsd:string .
ruga:hasFiveFingers a rdf:Property ;
                     rdfs:label "has five fingers"^^xsd:string ;
                     rdfs:domain ruga:Hand ;
                     rdfs:range ruga:Finger ;
                     rdfs:comment "This property links a hand to its fingers."^^xsd:string .
ruga:Finger a rdfs:Class ;
             rdfs:label "Finger"^^xsd:string ;
             rdfs:comment "This class represents a finger."^^xsd:string .
ruga:hasJoint a rdf:Property ;
               rdfs:label "has joint"^^xsd:string ;
               rdfs:domain ruga:Finger ;
               rdfs:range ruga:Joint ;
               rdfs:comment "This property links a finger to its joint."^^xsd:string .
ruga:Joint a rdfs:Class ;
            rdfs:label "Joint"^^xsd:string ;
            rdfs:comment "This class represents a joint in a finger."^^xsd:string .
ruga:hasNail a rdf:Property ;
              rdfs:label "has nail"^^xsd:string ;
              rdfs:domain ruga:Finger ;
              rdfs:range ruga:Nail ;
              rdfs:comment "This property links a finger to its nail."^^xsd:string .
ruga:Nail a rdfs:Class ;
          rdfs:label "Nail"^^xsd:string ;
          rdfs:comment "This class represents a nail on a finger."^^xsd:string .
