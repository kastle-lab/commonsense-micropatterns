@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .
ex:Hand rdf:type rdfs:Class .
ex:hasFiveFingers rdf:type rdf:Property ;
   rdfs:domain ex:Hand ;
   rdfs:range xsd:boolean .
ex:hasThumb rdf:type rdf:Property ;
   rdfs:domain ex:Hand ;
   rdfs:range xsd:boolean .
ex:canGrasp rdf:type rdf:Property ;
   rdfs:domain ex:Hand ;
   rdfs:range xsd:boolean .
ex:leftHand rdf:type ex:Hand ;
   ex:hasFiveFingers "true"^^xsd:boolean ;
   ex:hasThumb "true"^^xsd:boolean ;
   ex:canGrasp "true"^^xsd:boolean .
ex:rightHand rdf:type ex:Hand ;
   ex:hasFiveFingers "true"^^xsd:boolean ;
   ex:hasThumb "true"^^xsd:boolean ;
   ex:canGrasp "true"^^xsd:boolean .
