@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix car: <http://example.org/car#> .
car:Car rdf:type rdfs:Class ;
car:manufacturer rdf:type rdf:Property ;
                  rdfs:domain car:Car ;
                  rdfs:range xsd:string .
car:model rdf:type rdf:Property ;
           rdfs:domain car:Car ;
           rdfs:range xsd:string .
car:year rdf:type rdf:Property ;
          rdfs:domain car:Car ;
          rdfs:range xsd:integer .
car:color rdf:type rdf:Property ;
           rdfs:domain car:Car ;
           rdfs:range xsd:string .
car:engineType rdf:type rdf:Property ;
                rdfs:domain car:Car ;
                rdfs:range xsd:string .
car:bodyStyle rdf:type rdf:Property ;
               rdfs:domain car:Car ;
               rdfs:range xsd:string .
car:fuelType rdf:type rdf:Property ;
              rdfs:domain car:Car ;
              rdfs:range xsd:string .
car:transmissionType rdf:type rdf:Property ;
                      rdfs:domain car:Car ;
                      rdfs:range xsd:string .
