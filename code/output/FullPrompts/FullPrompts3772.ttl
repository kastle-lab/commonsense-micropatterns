@prefix : <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Morning a rdfs:Class ;
         rdfs:label "Morning" ;
         rdfs:subClassOf :DayTime .
:hasMorningTime a rdfs:Property ;
             rdfs:domain :Person ;
             rdfs:range :Morning ;
             rdfs:label "has morning time" .
             :Person a rdfs:Class ;
         rdfs:label "Person" .
