@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix hand: <http://example.org/hand#> .
hand:   rdf:type owl:Ontology ;
  rdfs:comment "Ontology about the Human Hand"^^xsd:string .
hand:Hand   rdf:type owl:Class ;
  rdfs:comment "Human hand, part of the arm"^^xsd:string ;
  rdfs:label "Hand"^^xsd:string .
hand:hasFingers   rdf:type owl:ObjectProperty ;
  rdfs:comment "The hand has fingers."^^xsd:string ;
  rdfs:label "has fingers"^^xsd:string ;
  rdfs:range hand:Finger ;
  rdfs:domain hand:Hand .
hand:hasThumb   rdf:type owl:ObjectProperty ;
  rdfs:comment "The hand has a thumb."^^xsd:string ;
  rdfs:label "has thumb"^^xsd:string ;
  rdfs:range hand:Thumb ;
  rdfs:domain hand:Hand .
hand:Finger   rdf:type owl:Class ;
  rdfs:comment "A finger is part of the hand."^^xsd:string ;
  rdfs:label "Finger"^^xsd:string .
hand:Thumb   rdf:type owl:Class ;
  rdfs:comment "The thumb is a digit, but is not typically referred to as a finger. It is part of the hand."^^xsd:string ;
  rdfs:label "Thumb"^^xsd:string .
