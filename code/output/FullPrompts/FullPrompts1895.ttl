@prefix : <http://example.org/> .
:Law a rdfs:Class ;
    rdfs:label "Law" ;
    rdfs:comment "This class represents all the laws" .
:hasLaw a rdf:Property ;
    rdfs:domain :Country ;
    rdfs:range :Law ;
    rdfs:label "has law" ;
    rdfs:comment "This property links a country to its laws." .
:Country a rdfs:Class ;
    rdfs:label "Country" ;
    rdfs:comment "This class represents all the countries" .
