@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.org/hand#> .
ex:Hand rdf:type owl:Class ;
    rdfs:label "Hand" ;
    rdfs:subClassOf ex:HumanBodyPart;
    rdfs:comment "A hand is a prehensile, multi-fingered appendage located at the end of the forearm or forelimb of primates.".
ex:Finger rdf:type owl:Class;
    rdfs:label "Finger" ;
    rdfs:subClassOf ex:Hand;
    rdfs:comment "Finger is any of the long, thin, separate parts of the hand, especially those that are not thumbs.".
ex:Thumb rdf:type owl:Class;
    rdfs:label "Thumb" ;
    rdfs:subClassOf ex:Hand;
    rdfs:comment "Thumb is the short, thick first digit of the human hand.".
ex:Palm rdf:type owl:Class;
    rdfs:label "Palm" ;
    rdfs:subClassOf ex:Hand;
    rdfs:comment "Palm is the inner aspect of the hand between the wrist and fingers.".
ex:HumanBodyPart rdf:type owl:Class;
    rdfs:label "Human Body Part";
    rdfs:comment "A part of a human body.".
ex:hasHand rdf:type owl:ObjectProperty ;
    rdfs:label "has hand" ;
    rdfs:domain ex:Human ;
    rdfs:range ex:Hand ;
    rdfs:comment "Property to denote that a human has hand." .
ex:hasFinger rdf:type owl:ObjectProperty ;
    rdfs:label "has finger" ;
    rdfs:domain ex:Hand ;
    rdfs:range ex:Finger ;
    rdfs:comment "Property to denote that a hand has fingers." .
ex:hasThumb rdf:type owl:ObjectProperty ;
    rdfs:label "has thumb" ;
    rdfs:domain ex:Hand ;
    rdfs:range ex:Thumb ;
    rdfs:comment "Property to denote that a hand has thumb." .
ex:hasPalm rdf:type owl:ObjectProperty ;
    rdfs:label "has palm" ;
    rdfs:domain ex:Hand ;
    rdfs:range ex:Palm ;
    rdfs:comment "Property to denote that a hand has palm." .
