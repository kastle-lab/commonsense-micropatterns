@prefix : <http://example.org/house#>@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>@prefix owl: <http://www.w3.org/2002/07/owl#>:House a rdfs:Class ;
 :hasAddress a rdf:Property ;
   rdfs:domain :House ;
   rdfs:range rdfs:Literal ;
   :hasOwner a rdf:Property ;
   rdfs:domain :House ;
   rdfs:range :Person ;
:Person a rdfs:Class .
:House1 a :House ;
   :hasAddress "123 Main St" ;
   :hasOwner :Person1 .
:Person1 a :Person ;
   rdfs:label "John Doe" .
