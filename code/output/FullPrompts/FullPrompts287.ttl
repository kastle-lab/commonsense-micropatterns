@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Thing rdf:type rdfs:Class .
:Animal rdf:type rdfs:Class ;
  rdfs:subClassOf :Thing .
:Plant rdf:type rdfs:Class ;
  rdfs:subClassOf :Thing .
:Mineral rdf:type rdfs:Class ;
  rdfs:subClassOf :Thing .
:Dog rdf:type rdfs:Class ;
  rdfs:subClassOf :Animal .
:Cat rdf:type rdfs:Class ;
  rdfs:subClassOf :Animal .
:Tree rdf:type rdfs:Class ;
  rdfs:subClassOf :Plant .
:Rose rdf:type rdfs:Class ;
  rdfs:subClassOf :Plant .
:Gold rdf:type rdfs:Class ;
  rdfs:subClassOf :Mineral .
:Silver rdf:type rdfs:Class ;
  rdfs:subClassOf :Mineral .
