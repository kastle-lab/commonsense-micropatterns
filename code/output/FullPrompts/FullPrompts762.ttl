@prefix : <http://www.example.org/#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Case a rdfs:Class ;
    rdfs:label "Case" ;
    rdfs:comment "This class represents a generic case or instance" .
:hasID a rdfs:Property ;
    rdfs:domain :Case ;
    rdfs:range rdfs:Literal ;
    rdfs:label "hasID" ;
    rdfs:comment "The property that provides a unique ID to a case" .
:hasDescription a rdfs:Property ;
    rdfs:domain :Case ;
    rdfs:range rdfs:Literal ;
    rdfs:label "hasDescription" ;
    rdfs:comment "The property that provides a textual description of a case" .
:hasDate a rdfs:Property ;
    rdfs:domain :Case ;
    rdfs:range rdfs:Literal ;
    rdfs:label "hasDate" ;
    rdfs:comment "The property representing the date associated with a case" .
