@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix week: <http://www.example.org/week#> .
week:Week rdf:type owl:Class .
week:hasDay rdf:type owl:ObjectProperty ;
week:Monday rdf:type week:Day .
week:Tuesday rdf:type week:Day .
week:Wednesday rdf:type week:Day .
week:Thursday rdf:type week:Day .
week:Friday rdf:type week:Day .
week:Saturday rdf:type week:Day .
week:Sunday rdf:type week:Day .
week:Week-01 rdf:type week:Week ;
    week:hasDay week:Monday ;
    week:hasDay week:Tuesday ;
    week:hasDay week:Wednesday ;
    week:hasDay week:Thursday ;
    week:hasDay week:Friday ;
    week:hasDay week:Saturday ;
    week:hasDay week:Sunday .
