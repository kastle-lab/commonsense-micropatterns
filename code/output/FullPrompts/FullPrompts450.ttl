@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix hand: <http://example.org/hand#> .
hand:Hand rdf:type owl:Class .
hand:Finger rdf:type owl:Class ;
  rdfs:subClassOf hand:HandPart .
hand:Palm rdf:type owl:Class ;
  rdfs:subClassOf hand:HandPart .
hand:Thumb rdf:type owl:Class ;
  rdfs:subClassOf hand:HandPart .
hand:BackOfHand rdf:type owl:Class ;
  rdfs:subClassOf hand:HandPart .
hand:HandPart rdf:type owl:Class ;
  rdfs:subClassOf hand:Hand .
hand:hasPart rdf:type owl:ObjectProperty ;
  rdfs:domain hand:Hand ;
  rdfs:range hand:HandPart .
hand:hasFinger rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf hand:hasPart ;
  rdfs:domain hand:Hand ;
  rdfs:range hand:Finger .
hand:hasThumb rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf hand:hasPart ;
  rdfs:domain hand:Hand ;
  rdfs:range hand:Thumb .
hand:hasPalm rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf hand:hasPart ;
  rdfs:domain hand:Hand ;
  rdfs:range hand:Palm .
hand:hasBackOfHand rdf:type owl:ObjectProperty ;
  rdfs:subPropertyOf hand:hasPart ;
  rdfs:domain hand:Hand ;
  rdfs:range hand:BackOfHand .
