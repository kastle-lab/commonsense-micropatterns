@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org/> .
ex:Loveseat rdf:type rdfs:Class .
ex:hasMaterial rdfs:domain ex:Loveseat ;
        rdfs:range ex:Material .
ex:hasColor rdfs:domain ex:Loveseat ;
        rdfs:range ex:Color .
ex:hasStyle rdfs:domain ex:Loveseat ;
        rdfs:range ex:Style .
ex:hasManufacturer rdfs:domain ex:Loveseat ;
        rdfs:range ex:Manufacturer .
