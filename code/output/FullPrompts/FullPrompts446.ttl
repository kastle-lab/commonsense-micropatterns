@prefix hand: <http://example.com/hand#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
hand:Hand a rdfs:Class ;
    rdfs:label "Hand"@en ;
    rdfs:comment "Class representing the body part hand"@en .
hand:fingers a rdfs:Property ;
    rdfs:label "has fingers"@en ;
    rdfs:comment "Property linking hand to fingers"@en ;
    rdfs:domain hand:Hand ;
    rdfs:range xsd:integer .
hand:thumb a rdfs:Class ;
    rdfs:label "Thumb"@en ;
    rdfs:comment "Class representing the thumb"@en ;
    rdfs:subClassOf hand:Hand .
hand:Finger a rdfs:Class ;
    rdfs:label "Finger"@en ;
    rdfs:comment "Class representing the other fingers"@en ;
    rdfs:subClassOf hand:Hand .
hand:right a rdfs:Class ;
    rdfs:label "Right hand"@en ;
    rdfs:comment "Class representing the right hand"@en ;
    rdfs:subClassOf hand:Hand .
hand:left a rdfs:Class ;
    rdfs:label "Left hand"@en ;
    rdfs:comment "Class representing the left hand"@en ;
    rdfs:subClassOf hand:Hand .
