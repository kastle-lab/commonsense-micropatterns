@prefix foaf: <http://xmlns.com/foaf/0.1/> .
<http://example.com/kid1> rdf:type foaf:Person ;
    foaf:firstName "John" ;
    foaf:lastName "Doe" ;
    foaf:age "10"^^<http://www.w3.org/2001/XMLSchema#integer> ;
    foaf:based_near <http://example.com/school1> ;
    foaf:status "Student" .
<http://example.com/kid2> rdf:type foaf:Person ;
    foaf:firstName "Jane" ;
    foaf:lastName "Doe" ;
    foaf:age "12"^^<http://www.w3.org/2001/XMLSchema#integer> ;
    foaf:based_near <http://example.com/school2> ;
    foaf:status "Student" .
<http://example.com/school1> rdf:type foaf:Organization ;
    foaf:name "Happy School" .
<http://example.com/school2> rdf:type foaf:Organization ;
    foaf:name "Bright School" .
