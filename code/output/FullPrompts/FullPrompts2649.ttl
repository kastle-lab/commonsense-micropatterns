@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com/> .
ex:Kind    rdf:type owl:Class ;
    rdfs:label "Kind" ;
    rdfs:comment "This represents a generic kind or type."@en .
ex:specificKind    rdf:type ex:Kind ;
    rdfs:label "Specific Kind" ;
    rdfs:comment "This represents a specific kind or type within the generic kind or type."@en .
