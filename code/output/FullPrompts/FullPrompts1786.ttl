@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www3.org/2000/01/rdf-schema#> .
@prefix power: <http://example.org/power#> .
power:     rdf:type rdfs:Class ;
    rdfs:comment "Class representing the concept of power" .
power:MechanicalPower      rdf:type rdfs:Class ;
    rdfs:subClassOf power: ;
    rdfs:comment "Power generated by mechanical means" .
power:ElectricalPower     rdf:type rdfs:Class ;
    rdfs:subClassOf power: ;
    rdfs:comment "Power generated by electricity" .
power:NuclearPower     rdf:type rdfs:Class ;
    rdfs:subClassOf power: ;
    rdfs:comment "Power generated by nuclear energy" .
power:SolarPower     rdf:type rdfs:Class ;
    rdfs:subClassOf power: ;
    rdfs:comment "Power generated by solar energy" .
power:WindPower     rdf:type rdfs:Class ;
    rdfs:subClassOf power: ;
    rdfs:comment "Power generated by wind energy" .
power:HydroPower     rdf:type rdfs:Class ;
    rdfs:subClassOf power: ;
    rdfs:comment "Power generated by water energy" .
power:Magnitude     rdf:type rdf:Property ;
    rdfs:domain power: ;
    rdfs:range rdfs:Literal ;
    rdfs:comment "Property representing the magnitude of power" .
power:Unit     rdf:type rdf:Property ;
    rdfs:domain power: ;
    rdfs:range rdfs:Literal ;
    rdfs:comment "Property representing the unit of power measurement" .
power:GeneratedBy     rdf:type rdf:Property ;
    rdfs:domain power: ;
    rdfs:range rdfs:Resource ;
    rdfs:comment "Property representing the source generating the power" .
