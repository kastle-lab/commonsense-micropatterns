@prefix ex: <http://example.com/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
ex:Sofa rdf:type rdfs:Class ;
    rdfs:label "Sofa" ;
    rdfs:comment "A piece of furniture made for sitting" .
ex:hasColor rdf:type rdf:Property ;
    rdfs:label "has color" ;
    rdfs:domain ex:Sofa ;
    rdfs:range rdfs:Literal .
ex:hasMaterial rdf:type rdf:Property ;
    rdfs:label "has material" ;
    rdfs:domain ex:Sofa ;
    rdfs:range rdfs:Literal .
ex:hasBrand rdf:type rdf:Property ;
    rdfs:label "has brand" ;
    rdfs:domain ex:Sofa ;
    rdfs:range rdfs:Literal .
ex:hasDimension rdf:type rdf:Property ;
    rdfs:label "has dimension" ;
    rdfs:domain ex:Sofa ;
    rdfs:range rdfs:Literal .
