@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/month#> .
ex:Month a rdfs:Class ;
    rdfs:label "Month"@en ;
    rdfs:subClassOf xsd:string ;
    rdfs:comment "Class representing a month in a year."@en .
ex:January a ex:Month ;
    rdfs:label "January"@en .
ex:February a ex:Month ;
    rdfs:label "February"@en .
ex:March a ex:Month ;
    rdfs:label "March"@en .
ex:April a ex:Month ;
    rdfs:label "April"@en .
ex:May a ex:Month ;
    rdfs:label "May"@en .
ex:June a ex:Month ;
    rdfs:label "June"@en .
ex:July a ex:Month ;
    rdfs:label "July"@en .
ex:August a ex:Month ;
    rdfs:label "August"@en .
ex:September a ex:Month ;
    rdfs:label "September"@en .
ex:October a ex:Month ;
    rdfs:label "October"@en .
ex:November a ex:Month ;
    rdfs:label "November"@en .
ex:December a ex:Month ;
    rdfs:label "December"@en .
