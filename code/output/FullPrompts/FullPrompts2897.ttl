@prefix : <http://www.example.org/gameontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
:Game rdf:type rdf:Class .
:hasTitle rdf:type rdf:Property ;
  rdf:domain :Game ;
  rdf:range rdf:Literal .
:hasGenre rdf:type rdf:Property ;
  rdf:domain :Game ;
  rdf:range :Genre .
:hasDeveloper rdf:type rdf:Property ;
  rdf:domain :Game ;
  rdf:range :Developer .
:hasPlatform rdf:type rdf:Property ;
  rdf:domain :Game ;
  rdf:range :Platform .
:hasReleaseDate rdf:type rdf:Property ;
  rdf:domain :Game ;
  rdf:range rdf:Literal .
:Genre rdf:type rdf:Class .
:Developer rdf:type rdf:Class .
:Platform rdf:type rdf:Class .
