@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/man#> .
:Man a rdfs:Class .
:age a rdf:Property ;
    rdfs:domain :Man ;
    rdfs:range rdf:Literal .
:hasName a rdf:Property ;
    rdfs:domain :Man ;
    rdfs:range rdf:Literal .
:isMarried a rdf:Property ;
    rdfs:domain :Man ;
    rdfs:range rdf:Literal .
:hasChildren a rdf:Property ;
    rdfs:domain :Man ;
    rdfs:range rdf:Literal .
   :hasOccupation a rdf:Property ;
    rdfs:domain :Man ;
    rdfs:range rdf:Literal .
