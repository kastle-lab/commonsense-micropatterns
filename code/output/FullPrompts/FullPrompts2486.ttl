@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com#> .
ex:word1 a owl:Thing ;
    rdfs:label "Example Word 1"@en;
    rdfs:comment "This is a sample word used for illustrating an ontology"@en ;
ex:word2 a owl:Thing ;
    rdfs:label "Example Word 2"@en;
    rdfs:comment "This is another sample word used for illustrating an ontology"@en .
ex:word1 rdf:type ex:word2 .
