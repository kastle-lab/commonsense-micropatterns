@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.org/> .
ex:War rdf:type owl:Class .
ex:War rdfs:subClassOf ex:Conflict .
ex:hasBelligerent rdf:type owl:ObjectProperty .
ex:hasBelligerent rdfs:range ex:Entity .
ex:hasBelligerent rdfs:domain ex:War .
ex:hasCause rdf:type owl:ObjectProperty .
ex:hasCause rdfs:range ex:Cause .
ex:hasCause rdfs:domain ex:War .
ex:hasTimeframe rdf:type owl:ObjectProperty .
ex:hasTimeframe rdfs:range ex:TimePeriod .
ex:hasTimeframe rdfs:domain ex:War .
ex:hasLocation rdf:type owl:ObjectProperty .
ex:hasLocation rdfs:range ex:Location .
ex:hasLocation rdfs:domain ex:War .
