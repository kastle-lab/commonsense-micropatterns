@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ns: <http://www.example.org/ns#> .
ns:Number a rdfs:Class ;
    rdfs:comment "A class that represents numerical values." .
ns:integer a ns:Number ;
    rdfs:comment "A type of number that has no decimal part." .
ns:decimal a ns:Number ;
    rdfs:comment "A type of number that has a decimal part." .
