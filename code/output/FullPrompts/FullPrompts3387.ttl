@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://www.example.org/ontology#> .
:Parent a rdfs:Class ;
       rdfs:label "Parent" .
:hasChild a rdfs:Property ;
      rdfs:label "has child" ;
      rdfs:domain :Parent ;
       rdfs:range :Person .
:Person a rdfs:Class ;
      rdfs:label "Person" .
