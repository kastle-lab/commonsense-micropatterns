@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/ontology#> .
:Case rdf:type rdfs:Class .
:hasCaseId rdf:type rdf:Property ;
    rdfs:domain :Case ;
    rdfs:range :Identifier .
:hasCaseTitle rdf:type rdf:Property ;
    rdfs:domain :Case ;
    rdfs:range :Title .
:hasCaseDescription rdf:type rdf:Property ;
    rdfs:domain :Case ;
    rdfs:range :Description .
:hasCaseDate rdf:type rdf:Property ;
    rdfs:domain :Case ;
    rdfs:range :Date .
:Identifier rdf:type rdfs:Datatype .
:Title rdf:type rdfs:Datatype .
:Description rdf:type rdfs:Datatype .
:Date rdf:type rdfs:Datatype .
