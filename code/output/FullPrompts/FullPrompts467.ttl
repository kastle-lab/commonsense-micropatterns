@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
dbo:Hand rdf:type rdfs:Class ;
    rdfs:label "Hand"@en ;
    rdfs:subClassOf dbo:BodyPart .
dbo:Finger rdf:type rdfs:Class ;
    rdfs:subClassOf dbo:Hand ;
    rdfs:label "Finger"@en .
dbo:Thumb rdf:type rdfs:Class ;
    rdfs:subClassOf dbo:Hand ;
    rdfs:label "Thumb"@en .
dbo:Palm rdf:type rdfs:Class ;
    rdfs:subClassOf dbo:Hand ;
    rdfs:label "Palm"@en .
dbo:Wrist rdf:type rdfs:Class ;
    rdfs:subClassOf dbo:Hand ;
    rdfs:label "Wrist"@en .
