@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.com/book#> .
:Book a rdfs:Class .
:title a rdf:Property ;
    rdfs:range xsd:string ;
    rdfs:domain :Book .
:author a rdf:Property ;
    rdfs:range xsd:string ;
    rdfs:domain :Book .
:publisher a rdf:Property ;
    rdfs:range xsd:string ;
    rdfs:domain :Book .
:publicationYear a rdf:Property ;
    rdfs:range xsd:integer ;
    rdfs:domain :Book .
:isbn a rdf:Property ;
    rdfs:range xsd:string ;
    rdfs:domain :Book .
:pageCount a rdf:Property ;
    rdfs:range xsd:integer ;
    rdfs:domain :Book .
