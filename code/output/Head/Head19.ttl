@prefix : <http://example.org/head#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Head a rdfs:Class .
:hasFace a rdf:Property ;
    rdfs:domain :Head ;
    rdfs:range :Face .
:hasHair a rdf:Property ;
    rdfs:domain :Head ;
    rdfs:range :Hair .
:hasEars a rdf:Property ;
    rdfs:domain :Head ;
    rdfs:range :Ears .
:Ears a rdfs:Class .
:Face a rdfs:Class .
:Hair a rdfs:Class .
:hasEyes a rdf:Property ;
    rdfs:domain :Face ;
    rdfs:range :Eyes .
:Eyes a rdfs:Class .
:hasNose a rdf:Property ;
    rdfs:domain :Face ;
    rdfs:range :Nose .
:Nose a rdfs:Class .
:hasMouth a rdf:Property ;
    rdfs:domain :Face ;
    rdfs:range :Mouth .
:Mouth a rdfs:Class .
