@prefix dsys: <http://example.org/ontologies/digitalSystem#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
dsys:ComputerSystem a rdf:Class .
dsys:hasCPU a rdf:Property ;
    rdfs:domain dsys:ComputerSystem ;
    rdfs:range dsys:CPU .
dsys:hasRAM a rdf:Property ;
    rdfs:domain dsys:ComputerSystem ;
    rdfs:range dsys:RAM .
dsys:hasStorage a rdf:Property ;
    rdfs:domain dsys:ComputerSystem ;
    rdfs:range dsys:Storage .
