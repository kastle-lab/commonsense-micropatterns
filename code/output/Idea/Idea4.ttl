@prefix : <http://example.org/>.@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.:Idea a rdfs:Class.:id a :Idea;
    rdfs:label "Idea ID"@en;
    rdfs:comment "Unique identifier for the idea"@en.:name a :Idea;
    rdfs:label "Idea Name"@en;
    rdfs:comment "Name given to the idea"@en.:description a :Idea;
    rdfs:label "Idea Description"@en;
    rdfs:comment "Description of what the idea is about"@en.:creationDate a :Idea;
    rdfs:label "Creation Date"@en;
    rdfs:comment "Date when the idea was generated"@en;
    rdfs:range xsd:date.:createdBy a :Idea;
    rdfs:label "Created By"@en;
    rdfs:comment "Name of the person who created the idea"@en.:revolvesAround a :Idea;
    rdfs:label "Revolves Around"@en;
    rdfs:comment "Main theme that the idea is centered around"@en.:isDiscussed a :Idea;
    rdfs:label "Is Discussed"@en;
    rdfs:comment "Indicates whether the idea has been discussed or not"@en;
    rdfs:range xsd:boolean.:isImplemented a :Idea;
    rdfs:label "Is Implemented"@en;
    rdfs:comment "Indicates whether the idea has been implemented or not"@en;
    rdfs:range xsd:boolean.
