@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org/> .
ex:Fact rdf:type rdf:Class .
ex:hasSubject rdf:type rdf:Property ;
    rdf:domain ex:Fact ;
    rdf:range rdf:Resource .
ex:hasPredicate rdf:type rdf:Property ;
    rdf:domain ex:Fact ;
    rdf:range rdf:Resource .
ex:hasObject rdf:type rdf:Property ;
    rdf:domain ex:Fact ;
    rdf:range rdf:Resource .
