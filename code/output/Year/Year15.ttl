@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
<http://example.org/#year>    rdf:type rdfs:Class ;
    rdfs:comment "Representing years" ;
    rdfs:label "Year" .
<http://example.org/#hasYear>    rdf:type rdf:Property ;
    rdfs:domain <http://example.org/#Entity> ;
    rdfs:range <http://example.org/#year> ;
    rdfs:comment "Property linking an entity with a specific year" ;
    rdfs:label "hasYear" .
<http://example.org/#year2019>    rdf:type <http://example.org/#year> ;
    rdfs:comment "Representing the year 2019" ;
    rdfs:label "2019" .
<http://example.org/#year2020>    rdf:type <http://example.org/#year> ;
    rdfs:comment "Representing the year 2020" ;
    rdfs:label "2020" .
