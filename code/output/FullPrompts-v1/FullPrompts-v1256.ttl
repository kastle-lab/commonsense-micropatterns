@prefix : <http://example.com#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:World a owl:Class .
:Continent a owl:Class ;
    rdfs:subClassOf :World .
:Country a owl:Class ;
    rdfs:subClassOf :Continent .
:City a owl:Class ;
    rdfs:subClassOf :Country .
:Ocean a owl:Class ;
    rdfs:subClassOf :World .
:Mountain a owl:Class ;
    rdfs:subClassOf :Country .
:River a owl:Class ;
    rdfs:subClassOf :Country .
:hasPopulation a owl:ObjectProperty ;
    rdfs:domain :City ;
    rdfs:range xsd:integer .
:hasArea a owl:ObjectProperty ;
    rdfs:domain :Country ;
    rdfs:range xsd:float .
:isLocatedIn a owl:ObjectProperty ;
    rdfs:domain [ owl:unionOf ( :City :Country :Continent ) ] ;
    rdfs:range :World .
:hasHighestPoint a owl:ObjectProperty ;
    rdfs:domain :Mountain ;
    rdfs:range xsd:float .
