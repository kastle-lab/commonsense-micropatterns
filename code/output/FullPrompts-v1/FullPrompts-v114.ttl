@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix person: <http://example.org/person#> .
person:Person rdf:type rdfs:Class .
person:name rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:age rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:integer .
person:birthDate rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:date .
person:gender rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range person:Gender .
person:nationality rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range person:Country .
person:Gender rdf:type rdfs:Class .
person:Male rdf:type person:Gender .
person:Female rdf:type person:Gender .
person:Country rdf:type rdfs:Class .
person:USA rdf:type person:Country .
person:UK rdf:type person:Country .
