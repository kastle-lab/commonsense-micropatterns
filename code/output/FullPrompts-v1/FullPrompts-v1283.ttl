@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://www.example.org/life#> .
life:LivingThing    rdf:type owl:Class ;
    rdfs:comment "A living thing" .
life:Plant    rdf:type owl:Class ;
    rdfs:subClassOf life:LivingThing ;
    rdfs:comment "A plant is a living thing." .
life:Animal     rdf:type owl:Class ;
    rdfs:subClassOf life:LivingThing ;
    rdfs:comment "An animal is a living thing." .
life:Human    rdf:type owl:Class ;
    rdfs:subClassOf life:Animal ;
    rdfs:comment "Human is a type of animal." .
life:hasLifeSpan    rdf:type owl:DatatypeProperty ;
    rdfs:domain life:LivingThing ;
    rdfs:range xsd:int ;
    rdfs:comment "The lifespan of a living thing in years." .
life:hasName    rdf:type owl:DatatypeProperty ;
    rdfs:domain life:LivingThing ;
    rdfs:range xsd:string ;
    rdfs:comment "The name of a living thing." .
life:eats    rdf:type owl:ObjectProperty ;
    rdfs:domain life:Animal ;
    rdfs:range life:LivingThing ;
    rdfs:comment "An animal eats a living thing." .
life:isEatenBy    rdf:type owl:ObjectProperty ;
    rdfs:domain life:LivingThing ;
    rdfs:range life:Animal ;
    rdfs:comment "A living thing is eaten by an animal.".
 life:photosynthesis    rdf:type owl:ObjectProperty ;
    rdfs:domain life:Plant ;
    rdfs:range life:Plant ;
    rdfs:comment "Plants perform photosynthesis." .
