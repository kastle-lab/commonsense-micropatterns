@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix life: <http://example.org/life#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
life:   rdf:type owl:Ontology ;
  rdfs:label "Life"@en ;
  rdfs:comment "An ontology describing life"@en .
    life:Life  rdf:type owl:Class ;
  rdfs:label "Life"@en ;
  rdfs:comment "This class describes living entities."@en .
life:Biological  rdf:type owl:Class ;
  rdfs:label "Biological"@en ;
  rdfs:subclassOf life:Life ;
  rdfs:comment "These are entities which are biological by nature"@en .
   life:NonBiological  rdf:type owl:Class ;
  rdfs:label "Non-biological"@en ;
  rdfs:subclassOf life:Life ;
  rdfs:comment "These are entities that are non-biological but still part of life such as ideas, emotions etc."@en .
      life:HumanLife  rdf:type owl:Class ;
  rdfs:label "Human Life"@en ;
  rdfs:subclassOf life:Biological ;
  rdfs:comment "Humans"@en .
life:AnimalLife  rdf:type owl:Class ;
  rdfs:label "Animal life"@en ;
  rdfs:subclassOf life:Biological ;
  rdfs:comment "Animal kingdom"@en .
  life:PlantLife  rdf:type owl:Class ;
  rdfs:label "Plant life"@en ;
  rdfs:subclassOf life:Biological ;
  rdfs:comment "Plant kingdom"@en .
  life:Foraminifera  rdf:type owl:Class ;
  rdfs:label "Foraminifera"@en ;
  rdfs:subclassOf life:Biological ;
  rdfs:comment "Microscopic marine plankton that form beautiful shells"@en .
  life:MentalLife  rdf:type owl:Class ;
  rdfs:label "Mental life"@en ;
  rdfs:subclassOf life:NonBiological ;
  rdfs:comment "Thoughts, ideas, emotions"@en .
life:Thought  rdf:type owl:Class ;
  rdfs:label "Thought"@en ;
  rdfs:subclassOf life:MentalLife ;
  rdfs:comment "Thought process in human mind"@en .
  life:Emotions  rdf:type owl:Class ;
  rdfs:label "Emotions"@en ;
  rdfs:subclassOf life:MentalLife ;
  rdfs:comment "Emotional aspects of living beings"@en .
