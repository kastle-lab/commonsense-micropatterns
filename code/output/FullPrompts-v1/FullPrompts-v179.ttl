@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .
ex:Year rdf:type rdfs:Class ;
    rdfs:label "Year"@en ;
    rdfs:comment "Represents a calendar year"@en .
ex:hasYear rdf:type rdf:Property ;
    rdf:domain ex:Entity ;
    rdf:range ex:Year ;
    rdfs:label "has year"@en ;
    rdfs:comment "Denotes the year an entity pertains to."@en .
# Example of usageex:Entity1 ex:hasYear "2022"^^xsd:integer .
