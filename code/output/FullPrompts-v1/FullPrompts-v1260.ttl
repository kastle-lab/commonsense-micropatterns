@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix world: <http://www.example.org/world#> .
world:    rdf:type owl:Ontology ;
    rdfs:comment "Ontology of the World"^^xsd:string ;
    rdfs:label "World Ontology"^^xsd:string .
world:Continents    rdf:type owl:Class ;
    rdfs:label "Continents"^^xsd:string ;
    rdfs:comment "The Continents in the world"^^xsd:string .
world:Oceans    rdf:type owl:Class ;
    rdfs:label "Oceans"^^xsd:string ;
    rdfs:comment "The Oceans in the world"^^xsd:string ;
world:Mountains    rdf:type owl:Class ;
    rdfs:label "Mountains"^^xsd:string ;
    rdfs:comment "The Mountains in the world"^^xsd:string .
world:Rivers    rdf:type owl:Class ;
    rdfs:label "Rivers"^^xsd:string ;
    rdfs:comment "The Rivers in the world"^^xsd:string .
world:Deserts    rdf:type owl:Class ;
    rdfs:label "Deserts"^^xsd:string ;
    rdfs:comment "The Deserts in the world"^^xsd:string .
world:Towns    rdf:type owl:Class ;
    rdfs:label "Towns"^^xsd:string ;
    rdfs:comment "The Towns in the world"^^xsd:string .
world:Countries    rdf:type owl:Class ;
    rdfs:label "Countries"^^xsd:string ;
    rdfs:comment "The Countries in the world"^^xsd:string .
world:Inhabitants    rdf:type owl:Class ;
    rdfs:label "Inhabitants"^^xsd:string ;
    rdfs:comment "The Inhabitants in the world"^^xsd:string .
