@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .
#Class Definitionex:World a rdfs:Class ;
    rdfs:comment "Represents the world as a physical entity" ;
    rdfs:label "World" .
#Property Definitionsex:hasContinent a rdf:Property ;
    rdfs:domain ex:World ;
    rdfs:range ex:Continent ;
    rdfs:label "has continent" ;
    rdfs:comment "Link between the world and its continents" .
ex:hasOcean a rdf:Property ;
    rdfs:domain ex:World ;
    rdfs:range ex:Ocean ;
    rdfs:label "has ocean" ;
    rdfs:comment "Link between the world and its oceans" .
#Example usageex:Earth a ex:World ;
    ex:hasContinent ex:Africa, ex:Asia ;
    ex:hasOcean ex:Atlantic, ex:Pacific .
ex:Africa a ex:Continent .
ex:Asia a ex:Continent .
ex:Atlantic a ex:Ocean .
ex:Pacific a ex:Ocean .
