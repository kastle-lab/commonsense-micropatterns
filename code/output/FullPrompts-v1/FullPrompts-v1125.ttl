@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix day: <http://example.org/day#> .
day:Day a owl:Class ;
  rdfs:comment "A class representing a day"^^xsd:string ;
  rdfs:label "Day"^^xsd:string .
day:hasName a owl:DatatypeProperty ;
  rdfs:comment "The name of the day in a week"^^xsd:string ;
  rdfs:label "has name"^^xsd:string ;
  rdfs:domain day:Day ;
  rdfs:range xsd:string .
day:hasNumber a owl:DatatypeProperty ;
  rdfs:comment "The number of the day in a month"^^xsd:string ;
  rdfs:label "has number"^^xsd:string ;
  rdfs:domain day:Day ;
  rdfs:range xsd:integer .
day:belongsToMonth a owl:ObjectProperty ;
  rdfs:comment "The month to which the day belongs"^^xsd:string ;
  rdfs:label "belongs to month"^^xsd:string ;
  rdfs:domain day:Day ;
  rdfs:range day:Month .
day:Month a owl:Class ;
  rdfs:comment "A class representing a month"^^xsd:string ;
  rdfs:label "Month"^^xsd:string .
day:hasMonthName a owl:DatatypeProperty ;
  rdfs:comment "The name of the month"^^xsd:string ;
  rdfs:label "has month name"^^xsd:string ;
  rdfs:domain day:Month ;
  rdfs:range xsd:string .
