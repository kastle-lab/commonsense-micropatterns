@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix man: <http://example.org/man#> .
man:Man a rdfs:Class ;
    rdfs:label "Man"@en ;
    rdfs:comment "A human adult male"@en .
man:hasName a rdf:Property ;
    rdfs:label "has name"@en ;
    rdfs:domain man:Man ;
    rdfs:range xsd:string ;
    rdfs:comment "The name of a man"@en .
man:hasAge a rdf:Property ;
    rdfs:label "has age"@en ;
    rdfs:domain man:Man ;
    rdfs:range xsd:integer ;
    rdfs:comment "The age of a man"@en .
man:hasNationality a rdf:Property ;
    rdfs:label "has nationality"@en ;
    rdfs:domain man:Man ;
    rdfs:range xsd:string ;
    rdfs:comment "The nationality of a man"@en .
man:hasOccupation a rdf:Property ;
    rdfs:label "has occupation"@en ;
    rdfs:domain man:Man ;
    rdfs:range xsd:string ;
    rdfs:comment "The occupation of a man"@en .
