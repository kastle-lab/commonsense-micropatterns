@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.com/day#> .
:Day rdf:type rdfs:Class .
:hasDate rdf:type rdf:Property ;
    rdfs:domain :Day ;
    rdfs:range xsd:date .
:hasName rdf:type rdf:Property ;
    rdfs:domain :Day ;
    rdfs:range xsd:string .
:hasDayOfWeek rdf:type rdf:Property ;
    rdfs:domain :Day ;
    rdfs:range :DayOfWeek .
:DayOfWeek rdf:type rdfs:Class .
:sunday rdf:type :DayOfWeek .
:monday rdf:type :DayOfWeek .
:tuesday rdf:type :DayOfWeek .
:wednesday rdf:type :DayOfWeek .
:thursday rdf:type :DayOfWeek .
:friday rdf:type :DayOfWeek .
:saturday rdf:type :DayOfWeek .
