@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://www.example.org/> .
ex:World    rdf:type rdfs:Class ;
    rdfs:label "World"@en ;
    rdfs:comment "The class of all worlds"@en .
    ex:hasContinent    rdf:type rdf:Property ;
    rdfs:label "has continent"@en ;
    rdfs:domain ex:World ;
    rdfs:range ex:Continent .
ex:hasOcean    rdf:type rdf:Property ;
    rdfs:label "has ocean"@en ;
    rdfs:domain ex:World ;
    rdfs:range ex:Ocean .
ex:Continent     rdf:type rdfs:Class ;
    rdfs:label "Continent"@en ;
    rdfs:comment "The class of all continents"@en .
ex:Ocean     rdf:type rdfs:Class ;
    rdfs:label "Ocean"@en ;
    rdfs:comment "The class of all oceans"@en .
