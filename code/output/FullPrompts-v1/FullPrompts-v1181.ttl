@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com/ex#> .
ex:Thing rdf:type owl:Class .
ex:PhysicalObject rdf:type owl:Class ;
                 rdfs:subClassOf ex:Thing .
ex:AbstractObject rdf:type owl:Class ;
                 rdfs:subClassOf ex:Thing .
ex:hasColor rdf:type owl:ObjectProperty ;
           rdfs:domain ex:Thing ;
           rdfs:range xsd:string .
ex:hasSize rdf:type owl:ObjectProperty ;
           rdfs:domain ex:Thing ;
           rdfs:range xsd:string .
ex:hasWeight rdf:type owl:ObjectProperty ;
             rdfs:domain ex:Thing ;
             rdfs:range xsd:string .
