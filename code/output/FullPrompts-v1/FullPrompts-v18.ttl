@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
 @prefix : <http://example.org/person#> .
:Person rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ;
          rdfs:label "Person" ;
          rdfs:comment "An individual human being"@en .
:hasFirstName rdf:type owl:DatatypeProperty ;
               rdfs:domain :Person ;
               rdfs:range xsd:string ;
               rdfs:label "has first name" ;
               rdfs:comment "The first name of the person"@en .
:hasLastName rdf:type owl:DatatypeProperty ;
              rdfs:domain :Person ;
              rdfs:range xsd:string ;
              rdfs:label "has last name" ;
              rdfs:comment "The last name of the person"@en .
:hasAge rdf:type owl:DatatypeProperty ;
         rdfs:domain :Person ;
         rdfs:range xsd:integer ;
         rdfs:label "has age" ;
         rdfs:comment "The age of the person"@en .
:hasEmailAddress rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Person ;
                  rdfs:range xsd:string ;
                  rdfs:label "has email address" ;
                  rdfs:comment "The email address of the person"@en .
                  :bornIn rdf:type owl:ObjectProperty ;
         rdfs:domain :Person ;
         rdfs:range :Country ;
         rdfs:label "born in" ;
         rdfs:comment "The country where the person was born"@en .
:Country rdf:type owl:Class ;
          rdfs:subClassOf owl:Thing ;
          rdfs:label "Country" ;
          rdfs:comment "A nation with its own government, occupying a particular territory."@en .
          :knows rdf:type owl:ObjectProperty ;
        rdfs:domain :Person ;
        rdfs:range :Person ;
        rdfs:label "knows" ;
        rdfs:comment "Another person known by this person"@en .
