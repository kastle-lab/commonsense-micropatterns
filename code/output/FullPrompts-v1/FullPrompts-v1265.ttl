@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .
ex:Earth rdf:type ex:Planet .
ex:Earth rdfs:label "Earth" .
ex:Asia rdf:type ex:Continent .
ex:Asia rdfs:label "Asia" .
ex:Asia ex:partOf ex:Earth .
ex:Europe rdf:type ex:Continent .
ex:Europe rdfs:label "Europe" .
ex:Europe ex:partOf ex:Earth .
ex:Germany rdf:type ex:Country .
ex:Germany rdfs:label "Germany" .
ex:Germany ex:partOf ex:Europe .
ex:Berlin rdf:type ex:City .
ex:Berlin rdfs:label "Berlin" .
ex:Berlin ex:partOf ex:Germany .
