@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dayOnt: <http://www.example.org/dayOntology#> .
dayOnt:Day rdf:type owl:Class .
dayOnt:DayOfWeek rdf:type owl:Class ;
         rdfs:subClassOf dayOnt:Day .
dayOnt:DayOfMonth rdf:type owl:Class ;
           rdfs:subClassOf dayOnt:Day .
dayOnt:DayOfYear rdf:type owl:Class ;
           rdfs:subClassOf dayOnt:Day .
dayOnt:dayName rdf:type owl:DatatypeProperty ;
           rdfs:domain dayOnt:DayOfWeek ;
           rdfs:range xsd:string .
dayOnt:dayNumber rdf:type owl:DatatypeProperty ;
            rdfs:domain dayOnt:DayOfMonth ;
            rdfs:range xsd:int .
dayOnt:dayOfYearNumber rdf:type owl:DatatypeProperty ;
               rdfs:domain dayOnt:DayOfYear ;
               rdfs:range xsd:int .
dayOnt:Monday rdf:type dayOnt:DayOfWeek ;
         dayOnt:dayName "Monday"^^xsd:string .
dayOnt:Tuesday rdf:type dayOnt:DayOfWeek ;
          dayOnt:dayName "Tuesday"^^xsd:string .
dayOnt:First rdf:type dayOnt:DayOfMonth ;
        dayOnt:dayNumber "1"^^xsd:int .
dayOnt:Second rdf:type dayOnt:DayOfMonth ;
         dayOnt:dayNumber "2"^^xsd:int .
dayOnt:JanuaryFirst rdf:type dayOnt:DayOfYear ;
            dayOnt:dayOfYearNumber "1"^^xsd:int .
dayOnt:JanuarySecond rdf:type dayOnt:DayOfYear ;
             dayOnt:dayOfYearNumber "2"^^xsd:int .
