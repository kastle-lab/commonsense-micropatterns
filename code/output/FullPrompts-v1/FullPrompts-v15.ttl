@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix person: <http://example.com/person#> .
person:Person rdf:type owl:Class .
person:hasName rdf:type owl:DatatypeProperty ;
              rdfs:domain person:Person ;
              rdfs:range xsd:string .
person:hasAge rdf:type owl:DatatypeProperty ;
              rdfs:domain person:Person ;
              rdfs:range xsd:integer .
person:hasGender rdf:type owl:DatatypeProperty ;
                 rdfs:domain person:Person ;
                 rdfs:range xsd:string .
person:isMarriedTo rdf:type owl:ObjectProperty ;
                   rdfs:domain person:Person ;
                   rdfs:range person:Person .
person:livesIn rdf:type owl:ObjectProperty ;
               rdfs:domain person:Person ;
               rdfs:range person:Location .
person:Location rdf:type owl:Class .
     person:isBornIn rdf:type owl:ObjectProperty ;
                rdfs:domain person:Person ;
                rdfs:range person:Location .
                person:hasNationality rdf:type owl:DatatypeProperty ;
                      rdfs:domain person:Person ;
                      rdfs:range xsd:string .
