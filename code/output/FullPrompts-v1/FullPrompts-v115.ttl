@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
# Class Definitionsex:Person rdf:type rdfs:Class .
ex:Male rdf:type rdfs:Class ;
    rdfs:subClassOf ex:Person .
ex:Female rdf:type rdfs:Class ;
    rdfs:subClassOf ex:Person .
# Property Definitionsex:hasName rdf:type rdf:Property ;
    rdfs:domain ex:Person ;
    rdfs:range xsd:string .
ex:hasAge rdf:type rdf:Property ;
    rdfs:domain ex:Person ;
    rdfs:range xsd:integer .
    ex:isBirthDayOf rdf:type rdf:Property ;
    rdfs:domain xsd:date;
    rdfs:range ex:Person.ex:isParentOf rdf:type rdf:Property ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Person .
ex:isSiblingOf rdf:type rdf:Property ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Person .
ex:isSpouseOf rdf:type rdf:Property ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Person .
