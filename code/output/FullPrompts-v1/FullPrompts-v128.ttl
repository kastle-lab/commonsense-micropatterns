@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
ex:Person a rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "An individual human being" ;
    rdfs:subClassOf foaf:Person .
ex:hasName a rdf:Property ;
    rdfs:label "has name" ;
    rdfs:comment "Relates a person to their name" ;
    rdfs:domain ex:Person ;
    rdfs:range rdfs:Literal .
ex:hasBirthDate a rdf:Property ;
    rdfs:label "has birth date" ;
    rdfs:comment "Relates a person to their date of birth" ;
    rdfs:domain ex:Person ;
    rdfs:range rdfs:Literal .
ex:hasGender a rdf:Property ;
    rdfs:label "has gender" ;
    rdfs:comment "Relates a person to their gender" ;
    rdfs:domain ex:Person ;
    rdfs:range rdfs:Literal .
ex:isMarriedTo a rdf:Property ;
    rdfs:label "is married to" ;
    rdfs:comment "Relates a person to their spouse" ;
    rdfs:domain ex:Person ;
    rdfs:range ex:Person .
