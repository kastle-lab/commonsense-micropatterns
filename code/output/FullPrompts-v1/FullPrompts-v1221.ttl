@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com#> .
ex:Man    rdf:type owl:Class ;
    rdfs:subClassOf ex:Human ;
    rdfs:label "Man" ;
    rdfs:comment "Male human being" ;
.ex:Human    rdf:type owl:Class ;
    rdfs:label "Human" ;
    rdfs:comment "Homo Sapiens, the human species" ;
.ex:hasAge    rdf:type owl:ObjectProperty ;
    rdfs:domain ex:Man ;
    rdfs:range ex:Age ;
    rdfs:label "has age" ;
    rdfs:comment "Specifies the age of a man" ;
.ex:Age    rdf:type owl:Class ;
    rdfs:label "Age" ;
    rdfs:comment "Numerical value denoting the length of time a man has been alive" ;
.
