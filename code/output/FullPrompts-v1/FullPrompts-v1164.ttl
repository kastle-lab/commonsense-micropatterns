@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .
ex:Thing a rdfs:Class ;
  rdfs:label "Thing"@en ;
  rdfs:comment "The base class for all things."@en ;
  rdfs:subClassOf rdfs:Resource .
ex:PhysicalThing a rdfs:Class ;
  rdfs:label "Physical Thing"@en ;
  rdfs:comment "A physical object or substance."@en ;
  rdfs:subClassOf ex:Thing .
ex:AbstractThing a rdfs:Class ;
  rdfs:label "Abstract Thing"@en ;
  rdfs:comment "An idea or concept that does not have a physical presence."@en ;
  rdfs:subClassOf ex:Thing .
ex:ThingProperty a rdfs:Class ;
  rdfs:label "Thing Property"@en ;
  rdfs:comment "Properties which characterize a Thing."@en ;
  rdfs:subClassOf rdfs:Resource .
ex:hasProperty a rdf:Property ;
  rdfs:label "has property"@en ;
  rdfs:comment "An abstract relation denoting that a Thing has a specific Property."@en ;
  rdfs:domain ex:Thing ;
  rdfs:range ex:ThingProperty .
