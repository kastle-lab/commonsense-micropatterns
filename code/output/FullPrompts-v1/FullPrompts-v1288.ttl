@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://example.org/life#> .
# Basic Life classeslife:Life rdf:type owl:Class ;
    rdfs:label "Life"^^xsd:string ;
    rdfs:comment "The class that encompasses all forms of life."^^xsd:string .
life:Human rdf:type owl:Class ;
    rdfs:subClassOf life:Life ;
    rdfs:label "Human"^^xsd:string ;
    rdfs:comment "The class that comprehends all human beings."^^xsd:string .
life:Animal rdf:type owl:Class ;
    rdfs:subClassOf life:Life ;
    rdfs:label "Animal"^^xsd:string ;
    rdfs:comment "The class that comprehends all animal beings."^^xsd:string .
life:Plant rdf:type owl:Class ;
    rdfs:subClassOf life:Life ;
    rdfs:label "Plant"^^xsd:string ;
    rdfs:comment "The class that encompasses all plants."^^xsd:string .
# Life propertieslife:isLivingOn rdf:type owl:ObjectProperty ;
    rdfs:label "is living on"^^xsd:string ;
    rdfs:comment "The property that links living beings to their habitats."^^xsd:string ;
    rdfs:domain life:Life ;
    rdfs:range life:Habitat .
life:Habitat rdf:type owl:Class ;
    rdfs:label "Habitat"^^xsd:string ;
    rdfs:comment "The class that identifies habitats where living beings can live."^^xsd:string .
# Human propertieslife:isHumanOf rdf:type owl:ObjectProperty ;
    rdfs:label "is human of"^^xsd:string ;
    rdfs:comment "The property that links a human to his/her properties."^^xsd:string ;
    rdfs:domain life:Human ;
    rdfs:range life:HumanProperties .
life:HumanProperties rdf:type owl:Class ;
    rdfs:label "Human Properties"^^xsd:string ;
    rdfs:comment "The class that identifies human properties."^^xsd:string .
