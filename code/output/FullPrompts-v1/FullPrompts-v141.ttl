@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/#> .
:Person   rdf:type rdfs:Class ;
  rdfs:comment "A class representing a person."@en ;
  rdfs:label "Person"@en .
:hasBirthdate  rdf:type rdf:Property ;
  rdfs:domain :Person ;
  rdfs:range xsd:date ;
  rdfs:comment "The birthdate of a person."@en ;
  rdfs:label "has birthdate"@en .
:hasName  rdf:type rdf:Property ;
  rdfs:domain :Person ;
  rdfs:range xsd:string ;
  rdfs:comment "The name of a person."@en ;
  rdfs:label "has name"@en .
