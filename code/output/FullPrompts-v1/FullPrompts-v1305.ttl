@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://www.example.org/life#> .
life:Life rdf:type owl:Class .
life:Organism rdf:type owl:Class ;
    rdfs:subClassOf life:Life .
life:Animal rdf:type owl:Class ;
    rdfs:subClassOf life:Organism .
life:Plant rdf:type owl:Class ;
    rdfs:subClassOf life:Organism .
life:hasLifeAction rdf:type owl:ObjectProperty ;
    rdfs:domain life:Organism ;
    rdfs:range life:LifeAction .
life:LifeAction rdf:type owl:Class .
life:Reproduction rdf:type owl:Class ;
    rdfs:subClassOf life:LifeAction .
life:Eating rdf:type owl:Class ;
    rdfs:subClassOf life:LifeAction .
life:Sleeping rdf:type owl:Class ;
    rdfs:subClassOf life:LifeAction .
