@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix h: <http://www.example.com/humanOntology#> .
# Declare Man as a Classh:Man a owl:Class ;
    rdfs:label "Man"@en ;
    rdfs:comment "The class of all men"@en ;
    rdfs:subClassOf h:Human ;
    .
# Declare properties applicable to Man h:hasAge a owl:DatatypeProperty ;
    rdfs:label "has age"@en ;
    rdfs:comment "The age of a man"@en ;
    rdfs:domain h:Man ;
    rdfs:range xsd:nonNegativeInteger ;
    .
h:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en ;
    rdfs:comment "The name of a man"@en ;
    rdfs:domain h:Man ;
    rdfs:range xsd:string ;
    .
# Declare John as an instance of Manh:John a h:Man ;
    h:hasName "John"^^xsd:string ;
    h:hasAge "30"^^xsd:nonNegativeInteger ;
    .
 # Declare Tom as another instance of Man h:Tom a h:Man ;
    h:hasName "Tom"^^xsd:string ;
    h:hasAge "25"^^xsd:nonNegativeInteger ;
    .
