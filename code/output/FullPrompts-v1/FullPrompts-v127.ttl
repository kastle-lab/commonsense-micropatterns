@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
# declaration of the class Personfoaf:Person a rdfs:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "A person."@en .
# declaration of the properties for the class Personfoaf:name a rdf:Property ;
    rdfs:label "name"@en ;
    rdfs:comment "The name of a person."@en ;
    rdfs:domain foaf:Person ;
    rdfs:range rdfs:Literal .
foaf:age a rdf:Property ;
    rdfs:label "age"@en ;
    rdfs:comment "The age of a person."@en ;
    rdfs:domain foaf:Person ;
    rdfs:range rdfs:Literal .
