@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://www.example.org/#> .
ex:World rdf:type rdfs:Class .
ex:Continent rdf:type rdfs:Class .
ex:isPartOf rdf:type rdf:Property ;
    rdf:domain ex:Continent ;
    rdf:range ex:World .
ex:hasContinent rdf:type rdf:Property ;
    rdf:domain ex:World ;
    rdf:range ex:Continent .
ex:Europe rdf:type ex:Continent ;
    ex:isPartOf ex:World .
ex:World ex:hasContinent ex:Europe .
ex:Country rdf:type rdfs:Class .
ex:isLocatedIn rdf:type rdf:Property ;
    rdf:domain ex:Country ;
    rdf:range ex:Continent .
ex:hasCountry rdf:type rdf:Property ;
    rdf:domain ex:Continent ;
    rdf:range ex:Country .
ex:France rdf:type ex:Country ;
    ex:isLocatedIn ex:Europe .
ex:Europe ex:hasCountry ex:France .
