@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix man: <http://example.org/man#> .
man: a owl:Class ;
    rdfs:label "Man"@en .
man:hasName a owl:DatatypeProperty ;
    rdfs:label "name"@en ;
    rdfs:domain man: ;
    rdfs:range xsd:string .
man:hasAge a owl:DatatypeProperty ;
    rdfs:label "age"@en ;
    rdfs:domain man: ;
    rdfs:range xsd:integer .
man:isMarriedTo a owl:ObjectProperty ;
    rdfs:label "is married to"@en ;
    rdfs:domain man: ;
    rdfs:range man: .
man:hasChild a owl:ObjectProperty ;
    rdfs:label "has child"@en ;
    rdfs:domain man: ;
    rdfs:range man: .
man:hasJob a owl:ObjectProperty ;
    rdfs:label "has job"@en ;
    rdfs:domain man: ;
    rdfs:range man:Job .
man:Job a owl:Class ;
    rdfs:label "Job"@en .
