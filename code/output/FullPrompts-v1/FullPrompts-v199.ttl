@prefix ex: <http://example.com/> .
ex:Pathway a rdfs:Class .
ex:Road a rdfs:Class ;
 rdfs:subClassOf ex:Pathway .
ex:Highway a rdfs:Class ;
 rdfs:subClassOf ex:Road .
ex:Bridge a rdfs:Class ;
 rdfs:subClassOf ex:Pathway .
ex:Tunnel a rdfs:Class ;
 rdfs:subClassOf ex:Pathway .
ex:PedestrianWay a rdfs:Class ;
     rdfs:subClassOf ex:Pathway ;
    rdfs:comment "Pathway designed for pedestrians".
ex:BicycleWay a rdfs:Class ;
     rdfs:subClassOf ex:Pathway ;
    rdfs:comment "Pathway designed for cyclists".
ex:Direction a rdfs:Class .
ex:Distance a rdfs:Class .
ex:hasDirection a rdf:Property ;
    rdfs:domain ex:Pathway ;
    rdfs:range ex:Direction ;
    rdfs:comment "Property indicating the direction of a pathway".
ex:hasDistance a rdf:Property ;
    rdfs:domain ex:Pathway ;
    rdfs:range ex:Distance ;
    rdfs:comment "Property indicating the distance of a pathway".
