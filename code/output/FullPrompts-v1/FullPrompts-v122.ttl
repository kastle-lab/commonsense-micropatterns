@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/> .
:Person a rdfs:Class ;
    rdfs:comment "A person or human being" ;
    rdfs:subClassOf rdfs:Resource .
:hasName a rdf:Property ;
    rdfs:comment "The name of a person" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .
:hasAge a rdf:Property ;
    rdfs:comment "The age of a person" ;
    rdfs:domain :Person ;
    rdfs:range xsd:integer .
:hasGender a rdf:Property ;
    rdfs:comment "The gender of a person" ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .
:John a :Person ;
    :hasName "John" ;
    :hasAge 25 ;
    :hasGender "Male" .
