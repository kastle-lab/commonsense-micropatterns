@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix man: <http://www.example.com/ontology/man#> .
man:Man rdf:type rdfs:Class .
man:name rdf:type rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range xsd:string .
man:age rdf:type rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range xsd:integer .
man:isMarried rdf:type rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range xsd:boolean .
man:hasChild rdf:type rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range man:Man .
man:worksAt rdf:type rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range man:Organization .
man:Organization rdf:type rdfs:Class .
man:orgName rdf:type rdf:Property ;
  rdfs:domain man:Organization ;
  rdfs:range xsd:string .
    man:orgAddress rdf:type rdf:Property ;
  rdfs:domain man:Organization ;
  rdfs:range xsd:string .
man:orgType rdf:type rdf:Property ;
  rdfs:domain man:Organization ;
  rdfs:range xsd:string .
