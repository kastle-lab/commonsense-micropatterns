@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix person: <http://example.org/Person#> .
person:Person rdf:type rdfs:Class .
person:name rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:age rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:integer .
person:bornIn rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:gender rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:hasParent rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range person:Person .
person:hasChild rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range person:Person .
person:hasSpouse rdf:type rdf:Property ;
    rdfs:domain person:Person ;
    rdfs:range person:Person .
