@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix way: <http://example.com/way#> .
# The class of all Waysway:Way rdf:type owl:Class ;
    rdfs:label "Way" ;
    rdfs:comment "The class of all Ways, which could be a road or a method to do something".
    # A Way has a Nameway:name rdf:type owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain way:Way ;
    rdfs:range xsd:string .
# A Way has a Purposeway:purpose rdf:type owl:ObjectProperty ;
    rdfs:label "purpose" ;
    rdfs:domain way:Way ;
    rdfs:range rdf:Resource .
# A Way may have a Starting Pointway:startingPoint rdf:type owl:ObjectProperty ;
    rdfs:comment "The starting point of a Way.";
    rdfs:label "starting point" ;
    rdfs:domain way:Way ;
    rdfs:range rdf:Resource .
# A Way may have an Ending Pointway:endingPoint rdf:type owl:ObjectProperty ;
    rdfs:comment "The ending point of a Way.";
    rdfs:label "ending point" ;
    rdfs:domain way:Way ;
    rdfs:range rdf:Resource .
