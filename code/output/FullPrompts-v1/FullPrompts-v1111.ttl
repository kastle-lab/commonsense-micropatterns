@prefix : <http://example.org/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Way rdf:type rdfs:Class ;
     rdfs:comment "A path or route to reach a specific location or goal through." .
:hasStart rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range :Location ;
    rdfs:comment "Indicates the starting point of the way." .
:hasEnd rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range :Location ;
    rdfs:comment "Indicates the end or destination point of the way." .
:hasLength rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range rdf:integer ;
    rdfs:comment "Indicates the length of the way." .
:hasDirection rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range :Direction ;
    rdfs:comment "Indicates the direction of the way." .
