@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://example.org/life#> .
life:Life a rdfs:Class .
life:hasLifeform a rdf:Property ;
    rdfs:domain life:Life ;
    rdfs:range life:Lifeform .
life:Lifeform a rdfs:Class .
life:Animal a rdfs:Class ;
    rdfs:subClassOf life:Lifeform .
life:Plant a rdfs:Class ;
    rdfs:subClassOf life:Lifeform .
life:hasName a rdf:Property ;
    rdfs:domain life:Lifeform ;
    rdfs:range xsd:string .
life:hasAge a rdf:Property ;
    rdfs:domain life:Lifeform ;
    rdfs:range xsd:decimal .
