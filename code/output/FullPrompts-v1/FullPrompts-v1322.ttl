@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://www.example.org/life#> .
:Life rdf:type rdfs:Class .
:Plant rdf:type rdfs:Class ;
  rdfs:subClassOf :Life .
:Animal rdf:type rdfs:Class ;
  rdfs:subClassOf :Life .
:Dog rdf:type rdfs:Class ;
  rdfs:subClassOf :Animal .
:Cat rdf:type rdfs:Class ;
  rdfs:subClassOf :Animal .
:Rose rdf:type rdfs:Class ;
  rdfs:subClassOf :Plant .
:Tulip rdf:type rdfs:Class ;
  rdfs:subClassOf :Plant .
