@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix way: <http://www.example.org/way#> .
way:Way a rdfs:Class ;
    rdfs:comment "A path or route that can be traversed or used to get from one place to another." .
way:Street a rdfs:Class ;
    rdfs:subClassOf way:Way ;
    rdfs:comment "A public thoroughfare in a built environment." .
    way:Highway a rdfs:Class ;
    rdfs:subClassOf way:Way ;
    rdfs:comment "A major road for any form of motor transport." .
way:footpath a rdfs:Class ;
    rdfs:subClassOf way:Way ;
    rdfs:comment "A type of road or path that is designed for people to walk along." .
    way:startPoint a rdf:Property ;
    rdfs:domain way:Way ;
    rdfs:range xsd:string ;
    rdfs:comment "The starting point of a Way entity." .
way:endPoint a rdf:Property ;
    rdfs:domain way:Way ;
    rdfs:range xsd:string ;
    rdfs:comment "The ending point of a Way entity." .
way:length a rdf:Property ;
    rdfs:domain way:Way ;
    rdfs:range xsd:float ;
    rdfs:comment "The length of a Way entity." .
way:connects a rdf:Property ;
    rdfs:domain way:Way ;
    rdfs:range way:Way ;
    rdfs:comment "The connection between two Way entities." .
