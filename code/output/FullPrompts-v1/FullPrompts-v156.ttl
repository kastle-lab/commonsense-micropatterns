@prefix : <http://example.org/year#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:Year a rdfs:Class ;
  rdfs:label "Year"@en ;
  rdfs:comment "Class representing a calendar year"@en ;
  rdfs:subClassOf xsd:gYear .
:hasLeapYear a rdf:Property ;
  rdfs:label "hasLeapYear"@en ;
  rdfs:comment "Indicates if the year is a leap year"@en ;
  rdfs:domain :Year ;
  rdfs:range xsd:boolean .
:hasYearNumber a rdf:Property ;
  rdfs:label "hasYearNumber"@en ;
  rdfs:comment "The numerical representation of a year"@en ;
  rdfs:domain :Year ;
  rdfs:range xsd:int .
