@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix day: <http://example.org/ontology/day#> .
day: Day    rdf:type rdfs:Class ;
    rdfs:label "Day" ;
    rdfs:comment "A concept representing a day. Includes every day of the week and the concept of today." .
    day: Monday    rdf:type day:Day ;
    rdfs:label "Monday" ;
    rdfs:comment "The day of the week following Sunday and preceding Tuesday." .
day: Today     rdf:type day:Day ;
     rdfs:label "Today" ;
     rdfs:comment "The day now occurring or in process." .
