@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://www.example.com/ontology#> .
ex:Way rdf:type rdf:Class .
ex:wayId rdf:type rdf:Property ;
    rdf:domain ex:Way ;
    rdf:range xsd:string .
ex:startLocation rdf:type rdf:Property ;
    rdf:domain ex:Way ;
    rdf:range ex:Location .
ex:endLocation rdf:type rdf:Property ;
    rdf:domain ex:Way ;
    rdf:range ex:Location .
ex:distance rdf:type rdf:Property ;
    rdf:domain ex:Way ;
    rdf:range xsd:decimal .
ex:Location rdf:type rdf:Class .
ex:locationId rdf:type rdf:Property ;
    rdf:domain ex:Location ;
    rdf:range xsd:string .
ex:locationName rdf:type rdf:Property ;
    rdf:domain ex:Location ;
    rdf:range xsd:string .
ex:locationLatitude rdf:type rdf:Property ;
    rdf:domain ex:Location ;
    rdf:range xsd:decimal .
ex:locationLongitude rdf:type rdf:Property ;
    rdf:domain ex:Location ;
    rdf:range xsd:decimal .
