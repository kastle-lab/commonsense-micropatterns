@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix man: <http://example.com/man#> .
man:Man a rdfs:Class ;
rdfs:label "Man"@en .
man:hasName a rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range xsd:string .
man:hasAge a rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range xsd:integer .
man:hasGender a rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range man:Gender .
man:Male a man:Gender ;
  rdfs:label "Male"@en .
man:Female a man:Gender ;
  rdfs:label "Female"@en .
man:hasNationality a rdf:Property ;
  rdfs:domain man:Man ;
  rdfs:range man:Country .
man:Country a rdfs:Class ;
  rdfs:label "Country"@en .
