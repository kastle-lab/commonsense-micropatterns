@prefix : <http://www.example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
:Day a rdfs:Class ;
     rdfs:comment "A class to represent a Day" .
:hasDate a owl:ObjectProperty ;
     rdfs:domain :Day ;
     rdfs:range xsd:date ;
     rdfs:comment "Property to link a Day with a specific date" .
:hasDayOfWeek a owl:ObjectProperty ;
     rdfs:domain :Day ;
     rdfs:range :DayOfWeek ;
     rdfs:comment "Property to link a Day with a specific Day of Week" .
:DayOfWeek a rdfs:Class ;
     rdfs:comment "A class to represent a Day of the Week." .
:Monday a :DayOfWeek ;
     rdfs:label "Monday"@en .
:Tuesday a :DayOfWeek ;
     rdfs:label "Tuesday"@en .
