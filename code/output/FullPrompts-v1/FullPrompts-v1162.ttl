@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix thing: <http://www.example.org/ontology/thing#> .
thing: id rdf:type owl:Class ;
    rdfs:comment "A unique identifier for the Thing" ;
    rdfs:label "Thing ID" .
thing:Name id rdf:type owl:DatatypeProperty ;
    rdfs:comment "The name of the Thing" ;
    rdfs:label "Thing Name" ;
    rdfs:domain thing: ;
    rdfs:range xsd:string .
thing:Description rdf:type owl:DatatypeProperty ;
    rdfs:comment "A detailed description of the Thing" ;
    rdfs:label "Thing Description" ;
    rdfs:domain thing: ;
    rdfs:range xsd:string .
thing:Category rdf:type owl:ObjectProperty ;
    rdfs:comment "Category of the Thing"   ;
    rdfs:label "Thing Category" ;
    rdfs:domain thing: ;
    rdfs:range thing:Category .
thing:Thing rdf:type owl:Class ;
    rdfs:comment "A class for things" ;
    rdfs:label "Thing" ;
    owl:equivalentClass thing: ;
    owl:disjointWith thing:Category .
