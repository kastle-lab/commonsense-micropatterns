@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix person: <http://example.com/person#> .
person:Person a rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "An individual human being" .
person:firstName a rdf:Property ;
    rdfs:label 'First Name' ;
    rdfs:comment 'First name of a person.' ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:lastName a rdf:Property ;
    rdfs:label 'Last Name' ;
    rdfs:comment 'Last name of a person.' ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:age a rdf:Property ;
    rdfs:label 'Age' ;
    rdfs:comment 'Age of a person.' ;
    rdfs:domain person:Person ;
    rdfs:range xsd:integer .
person:birthdate a rdf:Property ;
    rdfs:label 'Birth Date' ;
    rdfs:comment 'The birth date of the person.' ;
    rdfs:domain person:Person ;
    rdfs:range xsd:date .
person:gender a rdf:Property ;
    rdfs:label 'Gender' ;
    rdfs:comment 'The gender of the person.' ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
