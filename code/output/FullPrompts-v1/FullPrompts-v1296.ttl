@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://example.org/life#> .
life:Organism a rdfs:Class ;
    rdfs:comment "A living individual or entity"@en .
life:Species a rdfs:Class ;
    rdfs:comment "Group of similar organisms capable of interbreeding"@en .
life:Lifeform a rdfs:Class ;
    rdfs:comment "Distinct type of life, such as animal, plant, fungi, etc."@en .
life:LifeProcess a rdfs:Class ;
    rdfs:comment "Biochemical or physical events involved in the functioning of a living organism"@en .
life:birthDate a rdf:Property ;
    rdfs:domain life:Organism ;
    rdfs:range xsd:dateTime .
life:belongs_to_species a rdf:Property ;
    rdfs:domain life:Organism ;
    rdfs:range life:Species .
life:represents_lifeform a rdf:Property ;
    rdfs:domain life:Species ;
    rdfs:range life:Lifeform .
life:undergoes_life_process a rdf:Property ;
    rdfs:domain life:Organism ;
    rdfs:range life:LifeProcess .
