@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix world: <http://www.example.org/world#> .
world:World a rdfs:Class ;
    rdfs:comment "The specified World" .
world:hasContinent a rdf:Property ;
    rdfs:domain world:World ;
    rdfs:range world:Continent ;
    rdfs:comment "Relates World to its Continents" .
world:hasClimate a rdf:Property ;
    rdfs:domain world:World ;
    rdfs:range world:Climate ;
    rdfs:comment "Relates World to its Climate" .
world:Continent a rdfs:Class ;
    rdfs:comment "Continents in the World" .
world:Climate a rdfs:Class ;
    rdfs:comment "Climate found in the World" .
world:Earth a world:World ;
    world:hasContinent world:Africa , world:Asia , world:Europe ;
    world:hasClimate world:Tropical , world:Temperate.    world:Africa a world:Continent.world:Asia a world:Continent.world:Europe a world:Continent.world:Tropical a world:Climate.world:Temperate a world:Climate.
