@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix man: <http://example.org/man#> .
man:     rdf:type owl:Ontology ;
    rdfs:comment "An ontology that covers Man"@en ;
    rdfs:label "Man Ontology"@en .
man:man     rdf:type owl:Class ;
    rdfs:label "Man"@en ;
    rdfs:comment "The class of all men"@en .
man:hasAge     rdf:type owl:DatatypeProperty ;
    rdfs:domain man:man ;
    rdfs:range xsd:integer ;
    rdfs:label "has age"@en ;
    rdfs:comment "The age of a man"@en .
man:hasName     rdf:type owl:DatatypeProperty ;
    rdfs:domain man:man ;
    rdfs:range xsd:string ;
    rdfs:label "has name"@en ;
    rdfs:comment "The name of a man"@en .
man:isMarriedTo     rdf:type owl:ObjectProperty ;
    rdfs:domain man:man ;
    rdfs:range man:man ;
    rdfs:label "is married to"@en ;
    rdfs:comment "The man a man is married to"@en .
man:hasChild     rdf:type owl:ObjectProperty ;
    rdfs:domain man:man ;
    rdfs:range man:man ;
    rdfs:label "has child"@en ;
    rdfs:comment "The child of a man"@en .
