@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix world: <http://www.example.org/world#> .
world:     rdf:type owl:Ontology ;
    rdfs:comment "Ontology of the World" ;
    rdfs:label "World" .
world:Continent    rdf:type owl:Class ;
    rdfs:comment "A large, continuous area of land on Earth" ;
    rdfs:label "Continent" .
world:Country    rdf:type owl:Class ;
    rdfs:comment "A nation with its own government, occupying a particular territory" ;
    rdfs:label "Country" ;
    rdfs:subClassOf world:Continent .
world:City    rdf:type owl:Class ;
    rdfs:comment "A large town or a small city" ;
    rdfs:label "City" ;
    rdfs:subClassOf world:Country .
world:River    rdf:type owl:Class ;
    rdfs:comment "A large natural stream of water flowing towards an ocean or sea" ;
    rdfs:label "River" .
world:Mountain    rdf:type owl:Class ;
    rdfs:comment "A large natural elevation of the earth's surface rising abruptly from the surrounding level" ;
    rdfs:label "Mountain" .
world:lake    rdf:type owl:Class ;
    rdfs:comment "A large body of water surrounded by land" ;
    rdfs:label "Lake" .
world:Ocean    rdf:type owl:Class ;
    rdfs:comment "A vast expanse of salt water" ;
    rdfs:label "Ocean" .
