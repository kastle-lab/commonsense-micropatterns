@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com/> .
ex:Way rdf:type rdfs:Class .
ex:Street rdf:type rdfs:Class ;
    rdfs:subClassOf ex:Way .
ex:City rdf:type rdfs:Class .
ex:Country rdf:type rdfs:Class .
ex:locatedIn rdf:type rdf:Property ;
    rdfs:domain ex:Way ;
    rdfs:range ex:City .
ex:cityIn rdf:type rdf:Property ;
    rdfs:domain ex:City ;
    rdfs:range ex:Country .
ex:MainStreet rdf:type ex:Street ;
    ex:locatedIn ex:NewYork .
ex:NewYork rdf:type ex:City ;
    ex:cityIn ex:USA .
ex:USA rdf:type ex:Country .
