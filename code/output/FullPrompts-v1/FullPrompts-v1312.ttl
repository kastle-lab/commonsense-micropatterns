@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .
ex:Life rdf:type rdfs:Class .
ex:Animal rdf:type rdfs:Class ;
          rdfs:subClassOf ex:Life .
ex:Plant rdf:type rdfs:Class ;
         rdfs:subClassOf ex:Life .
ex:Human rdf:type rdfs:Class ;
         rdfs:subClassOf ex:Animal .
         ex:Dog rdf:type rdfs:Class ;
       rdfs:subClassOf ex:Animal .
ex:Rose rdf:type rdfs:Class ;
        rdfs:subClassOf ex:Plant .
ex:john rdf:type ex:Human .
ex:fido rdf:type ex:Dog .
ex:rose1 rdf:type ex:Rose .
