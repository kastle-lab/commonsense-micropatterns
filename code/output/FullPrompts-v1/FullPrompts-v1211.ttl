@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix man: <http://www.example.org/man#> .
man:Man rdf:type rdfs:Class .
man:hasAge rdf:type rdf:Property ;
     rdfs:domain man:Man ;
     rdfs:range xsd:integer .
man:hasName rdf:type rdf:Property ;
     rdfs:domain man:Man ;
     rdfs:range xsd:string .
man:isMarriedTo rdf:type rdf:Property ;
     rdfs:domain man:Man ;
     rdfs:range man:Man .
man:John rdf:type man:Man ;
    man:hasAge 30 ;
    man:hasName "John" .
man:Jane rdf:type man:Man ;
    man:hasAge 28 ;
    man:hasName "Jane" ;
    man:isMarriedTo man:John .
