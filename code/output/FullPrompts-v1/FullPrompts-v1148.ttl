@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
ex:Day a owl:Class ;
    rdfs:label "Day"@en ;
    rdfs:comment "A day in the context of a calendar."@en ;
    rdfs:subClassOf ex:TimeMeasurement .
ex:hasDay a owl:ObjectProperty ;
    rdfs:domain ex:Month ;
    rdfs:range ex:Day ;
    rdfs:label "has day"@en ;
    rdfs:comment "Property linking a month to its days."@en .
ex:DayInMonth a owl:DatatypeProperty ;
    rdfs:domain ex:Day ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf ex:hasQuantity ;
    rdfs:label "day in month"@en ;
    rdfs:comment "Property indicating the position of a day within a month."@en .
ex:hasQuantity a owl:DatatypeProperty ;
    rdfs:range xsd:integer ;
    rdfs:comment "Base property for quantities."@en .
ex:TimeMeasurement a owl:Class ;
    rdfs:comment "A general class for time measurements."@en .
