@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dayOnt: <http://www.example.org/dayOntology#> .
dayOnt:Day rdf:type owl:Class .
 dayOnt:Weekday rdf:type owl:Class ;
  rdfs:subClassOf dayOnt:Day ;
  rdfs:label "week day"@en  .
dayOnt:Moment rdf:type owl:Class ;
  rdfs:label "moment"@en  .
dayOnt:timeOfDay rdf:type owl:ObjectProperty ;
  rdfs:domain dayOnt:Day ;
   rdfs:range dayOnt:Moment ;
   rdfs:label "time of day"@en  .
dayOnt:Monday rdf:type owl:NamedIndividual, dayOnt:Weekday ;
  rdfs:label "Monday"@en  .
dayOnt:Tuesday rdf:type owl:NamedIndividual, dayOnt:Weekday ;
  rdfs:label "Tuesday"@en  .
dayOnt:Wednesday rdf:type owl:NamedIndividual, dayOnt:Weekday ;
  rdfs:label "Wednesday"@en  .
dayOnt:Thursday rdf:type owl:NamedIndividual, dayOnt:Weekday ;
  rdfs:label "Thursday"@en  .
dayOnt:Friday rdf:type owl:NamedIndividual, dayOnt:Weekday ;
  rdfs:label "Friday"@en  .
dayOnt:Saturday rdf:type owl:NamedIndividual, dayOnt:Day ;
  rdfs:label "Saturday"@en  .
dayOnt:Sunday rdf:type owl:NamedIndividual, dayOnt:Day ;
  rdfs:label "Sunday"@en  .
dayOnt:Morning rdf:type owl:NamedIndividual, dayOnt:Moment ;
  rdfs:label "Morning"@en  .
dayOnt:Afternoon rdf:type owl:NamedIndividual, dayOnt:Moment ;
  rdfs:label "Afternoon"@en  .
dayOnt:Evening rdf:type owl:NamedIndividual, dayOnt:Moment ;
  rdfs:label "Evening"@en  .
dayOnt:Night rdf:type owl:NamedIndividual, dayOnt:Moment ;
  rdfs:label "Night"@en  .
 dayOnt:Monday dayOnt:timeOfDay dayOnt:Morning .
dayOnt:Tuesday dayOnt:timeOfDay dayOnt:Afternoon .
dayOnt:Wednesday dayOnt:timeOfDay dayOnt:Evening .
dayOnt:Thursday dayOnt:timeOfDay dayOnt:Night .
dayOnt:Friday dayOnt:timeOfDay dayOnt:Morning .
dayOnt:Saturday dayOnt:timeOfDay dayOnt:Afternoon .
dayOnt:Sunday dayOnt:timeOfDay dayOnt:Evening .
