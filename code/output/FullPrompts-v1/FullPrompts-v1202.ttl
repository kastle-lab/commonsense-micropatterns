@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix man: <http://www.example.org/ontology/man#> .
man:Man a owl:Class ;
    rdfs:label "Man" ;
    rdfs:comment "A class representing human male." .
man:hasName a owl:DatatypeProperty ;
    rdfs:label "has name" ;
    rdfs:comment "The name of the man." ;
    rdfs:domain man:Man ;
    rdfs:range xsd:string .
man:hasAge a owl:DatatypeProperty ;
    rdfs:label "has age" ;
    rdfs:comment "The age of the man." ;
    rdfs:domain man:Man ;
    rdfs:range xsd:integer .
man:isMarried a owl:DatatypeProperty ;
    rdfs:label "is married" ;
    rdfs:comment "The marital status of the man." ;
    rdfs:domain man:Man ;
    rdfs:range xsd:boolean .
man:hasJob a owl:ObjectProperty ;
    rdfs:label "has job" ;
    rdfs:comment "The job of the man." ;
    rdfs:domain man:Man ;
    rdfs:range man:Job .
man:Job a owl:Class ;
    rdfs:label "Job" ;
    rdfs:comment "A class representing a job." .
man:isFatherOf a owl:ObjectProperty ;
    rdfs:label "is father of" ;
    rdfs:comment "The child(ren) of the man." ;
    rdfs:domain man:Man ;
    rdfs:range man:Man .
man:isMarriedTo a owl:ObjectProperty ;
    rdfs:label "is married to" ;
    rdfs:comment "The spouse of the man." ;
    rdfs:domain man:Man ;
    rdfs:range man:Woman .
man:Woman a owl:Class ;
    rdfs:label "Woman" ;
    rdfs:comment "A class representing human female." .
