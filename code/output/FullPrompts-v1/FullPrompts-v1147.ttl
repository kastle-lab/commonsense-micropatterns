@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .
ex:Day a rdfs:Class ;
    rdfs:comment "Represents a day in an ontology."@en .
ex:hasDate a rdf:Property ;
    rdfs:domain ex:Day ;
    rdfs:range xsd:date ;
    rdfs:comment "Property to represent the date of the day."@en .
ex:day1 a ex:Day ;
    ex:hasDate "2022-01-01"^^xsd:date .
