@prefix : <http://www.example.com/world#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:Planet rdf:type rdfs:Class .
:Earth rdf:type :Planet ;
  rdfs:label "Earth"@en .
  :Continent rdf:type rdfs:Class .
:Africa rdf:type :Continent ;
  rdfs:label "Africa"@en .
 :Country rdf:type rdfs:Class .
:SouthAfrica rdf:type :Country ;
  rdfs:label "South Africa"@en .
:City rdf:type rdfs:Class .
:CapeTown rdf:type :City ;
  rdfs:label "Cape Town"@en ;
:locatedIn rdf:type rdf:Property ;
  rdfs:domain :City ;
  rdfs:range :Country .
  :CapeTown :locatedIn :SouthAfrica .
:partOf rdf:type rdf:Property ;
  rdfs:domain :Country ;
  rdfs:range :Continent .
  :SouthAfrica :partOf :Africa .
:locatedOn rdf:type rdf:Property ;
  rdfs:domain :Continent ;
  rdfs:range :Planet .
:Africa :locatedOn :Earth .
