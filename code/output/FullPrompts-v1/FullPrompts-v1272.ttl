@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix geo: <http://example.com/geography/> .
geo:World a rdfs:Class ;
    rdfs:subClassOf rdfs:Resource ;
    rdfs:label "World"^^xsd:string .
geo:Continent a rdfs:Class ;
    rdfs:subClassOf geo:World ;
    rdfs:label "Continent"^^xsd:string .
geo:Country a rdfs:Class ;
    rdfs:subClassOf geo:Continent ;
    rdfs:label "Country"^^xsd:string .
geo:Asia a geo:Continent ;
    rdfs:label "Asia"^^xsd:string .
geo:Europe a geo:Continent ;
    rdfs:label "Europe"^^xsd:string .
geo:India a geo:Country ;
    rdfs:label "India"^^xsd:string ;
    rdfs:subClassOf geo:Asia .
geo:Germany a geo:Country ;
    rdfs:label "Germany"^^xsd:string ;
    rdfs:subClassOf geo:Europe .
