@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix year: <http://example.org/year#> .
year:Year a rdfs:Class ;
  rdfs:label "Year"@en ;
  rdfs:comment "Represents a year"@en ;
  rdfs:subClassOf time:TemporalEntity ;
.year:hasMonth a rdf:Property ;
  rdfs:label "has month"@en ;
  rdfs:comment "The property that links a year to its months"@en ;
  rdfs:range year:Month ;
  rdfs:domain year:Year ;
.year:Month a rdfs:Class ;
  rdfs:label "Month"@en ;
  rdfs:comment "Represents a month"@en ;
  rdfs:subClassOf time:TemporalEntity ;
.year:Month rdf:type owl:Class ;
  rdfs:subClassOf time:TemporalUnit ;
.year:hasDay a rdf:Property ;
  rdfs:label "has day"@en ;
  rdfs:comment "The property that links a month to its days"@en ;
  rdfs:range year:Day ;
  rdfs:domain year:Month ;
.year:Day a rdfs:Class ;
  rdfs:label "Day"@en ;
  rdfs:comment "Represents a day"@en ;
  rdfs:subClassOf time:TemporalEntity ;
.year:Day rdf:type owl:Class ;
  rdfs:subClassOf time:TemporalUnit ;
.year:hasYearNumber a rdf:Property ;
  rdfs:label "has year number"@en ;
  rdfs:comment "The property that links a year to its numerical representation"@en ;
  rdfs:range xsd:integer ;
  rdfs:domain year:Year ;
.