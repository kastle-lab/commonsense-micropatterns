@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
base:Year a rdfs:Class ;
          rdfs:label "Year"@en ;
          rdfs:comment "A particular year in the Gregorian calendar"@en .
base:hasMonth a rdf:Property ;
          rdfs:domain base:Year ;
          rdfs:range base:Month ;
          rdfs:label "has month"@en ;
          rdfs:comment "Relates a year to its months"@en .
base:Month a rdfs:Class ;
          rdfs:label "Month"@en ;
          rdfs:comment "A particular month of a year"@en .
base:hasDay a rdf:Property ;
          rdfs:domain base:Month ;
          rdfs:range base:Day ;
          rdfs:label "has day"@en ;
          rdfs:comment "Relates a month to its days"@en .
base:Day a rdfs:Class ;
          rdfs:label "Day"@en ;
          rdfs:comment "A particular day of a month"@en .
base:inYear a rdf:Property ;
          rdfs:domain base:Day ;
          rdfs:range base:Year ;
          rdfs:label "in year"@en ;
          rdfs:comment "Provides the year of a given day"@en .
base:Year2022 a base:Year ;
          rdfs:label "2022" .
base:January a base:Month ;
          rdfs:label "January" ;
          base:inYear base:Year2122 .
base:Day1 a base:Day ;
          rdfs:label "1" ;
          base:inYear base:Year2022 ;
          base:inMonth base:January .
