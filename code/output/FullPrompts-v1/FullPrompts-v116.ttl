@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix person: <http://example.org/Person#> .
person:     rdf:type rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A class for representing persons in our ontology" .
person:name     rdf:type rdf:Property ;
    rdfs:domain person: ;
    rdfs:range xsd:string ;
    rdfs:label "name" ;
    rdfs:comment "The full name of the person" .
person:age     rdf:type rdf:Property ;
    rdfs:domain person: ;
    rdfs:range xsd:integer ;
    rdfs:label "age" ;
    rdfs:comment "The age of the person" .
person:gender     rdf:type rdf:Property ;
    rdfs:domain person: ;
    rdfs:range xsd:string ;
    rdfs:label "gender" ;
    rdfs:comment "The gender of the person".
