@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://example.com/life#> .
life:Life rdfs:subClassOf rdfs:Resource .
life:HumanLife rdfs:subClassOf life:Life ;
    rdfs:label "Human Life"@en .
life:AnimalLife rdfs:subClassOf life:Life ;
    rdfs:label "Animal Life"@en .
life:PlantLife rdfs:subClassOf life:Life ;
    rdfs:label "Plant Life"@en .
life:hasCycle rdfs:domain life:Life ;
    rdfs:range xsd:string ;
    rdfs:label "has life cycle"@en ;
    rdfs:comment "Represents the life cycle of a given form of life."@en .
life:hasLifeSpan rdfs:domain life:Life ;
    rdfs:range xsd:integer ;
    rdfs:label "has life span"@en ;
    rdfs:comment "Represents the average life span of a given form of life."@en .
life:Life rdf:type rdfs:Class ;
    rdfs:comment "The class of all types of life."@en ;
    rdfs:label "Life"@en .
life:HumanLife rdf:type rdfs:Class ;
    rdfs:comment "The class of human life."@en ;
    rdfs:label "Human Life"@en .
life:AnimalLife rdf:type rdfs:Class ;
    rdfs:comment "The class of animal life excluding human."@en ;
    rdfs:label "Animal Life"@en .
 life:PlantLife rdf:type rdfs:Class ;
    rdfs:comment "The class of plant life."@en ;
    rdfs:label "Plant Life"@en .
 life:hasCycle rdf:type rdf:Property ;
    rdfs:comment "Represents a property about the life cycle of a life form."@en ;
    rdfs:label "has life cycle"@en .
life:hasLifeSpan rdf:type rdf:Property ;
    rdfs:comment "Represents a property about the average life span of a life form."@en ;
    rdfs:label "has life span"@en .
