@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://www.example.org/lifeontology#> .
life:Life   rdf:type owl:Class ;
  rdfs:comment "Represents the concept of life" .
life:Mammals   rdf:type owl:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:comment "Represents the class of mammals within life" .
life:Plants   rdf:type owl:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:comment "Represents the class of plants within life" .
life:Insects   rdf:type owl:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:comment "Represents the class of insects within life" .
life:Birds  rdf:type owl:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:comment "Represents the class of birds within life" .
life:Fish   rdf:type owl:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:comment "Represents the class of fish within life" .
life:hasLifeSpan   rdf:type owl:DatatypeProperty ;
  rdfs:domain life:Life ;
  rdfs:range xsd:integer ;
  rdfs:comment "Represents the lifespan of a life form" .
life:hasScientificName   rdf:type owl:DatatypeProperty ;
  rdfs:domain life:Life ;
  rdfs:range xsd:string ;
  rdfs:comment "Represents the scientific name of a life form" .
  life:consumes   rdf:type owl:ObjectProperty ;
  rdfs:domain life:Life ;
  rdfs:range life:Life ;
  rdfs:comment "Represents the feeding habits between life forms" .
 life:habitat  rdf:type owl:DatatypeProperty ;
  rdfs:domain life:Life ;
  rdfs:range xsd:string ;
  rdfs:comment "Represents the habitat of a life form" .
