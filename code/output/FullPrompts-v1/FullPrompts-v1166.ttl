@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix my: <http://www.myontology.com#> .
my:Thing rdf:type owl:Class .
my:PhysicalObject rdf:type owl:Class ;
    rdfs:subClassOf my:Thing .
my:Liquid rdf:type owl:Class ;
    rdfs:subClassOf my:PhysicalObject .
my:Gas rdf:type owl:Class ;
    rdfs:subClassOf my:PhysicalObject .
my:Solid rdf:type owl:Class ;
    rdfs:subClassOf my:PhysicalObject .
my:AbstractThing rdf:type owl:Class ;
    rdfs:subClassOf my:Thing .
my:Idea rdf:type owl:Class ;
    rdfs:subClassOf my:AbstractThing .
my:Theory rdf:type owl:Class ;
    rdfs:subClassOf my:AbstractThing .
my:hasColor rdf:type owl:DatatypeProperty ;
    rdfs:domain my:PhysicalObject ;
    rdfs:range xsd:string .
