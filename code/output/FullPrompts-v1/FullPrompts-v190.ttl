@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://example.org/way> .
<#Way> rdf:type owl:Class ;
    rdfs:label "Way"@en ;
    rdfs:comment "A method, style or manner of doing something"@en .
<#Highway> rdf:type owl:Class ;
    rdfs:subClassOf <#Way> ;
    rdfs:label "Highway"@en ;
    rdfs:comment "A major road for any form of motor transport"@en .
<#Footpath> rdf:type owl:Class ;
    rdfs:subClassOf <#Way> ;
    rdfs:label "Footpath"@en ;
    rdfs:comment "A type of way intended for pedestrians"@en .
<#Route> rdf:type owl:Class ;
    rdfs:subClassOf <#Way> ;
    rdfs:label "Route"@en ;
    rdfs:comment "A way or course taken in getting from a starting point to a destination"@en .
<#Length> rdf:type owl:DatatypeProperty ;
    rdfs:label "length"@en ;
    rdfs:comment "The length of the way in meters"@en ;
    rdfs:domain <#Way> ;
    rdfs:range xsd:integer .
<#hasWayType> rdf:type owl:ObjectProperty ;
    rdfs:label "has way type"@en ;
    rdfs:comment "The type of the way, e.g. road, path, route"@en ;
    rdfs:domain <#Way> ;
    rdfs:range <#WayType> .
