@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix example: <http://example.org/person#> .
# Person Classexample:Person rdf:type rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "Person represents any human being" .
# Subclass of Personexample:Man rdf:type rdfs:Class ;
    rdfs:label "Man" ;
    rdfs:comment "Man represents a male human being" ;
    rdfs:subClassOf example:Person .
example:Woman rdf:type rdfs:Class ;
    rdfs:label "Woman" ;
    rdfs:comment "Woman represents a female human being" ;
    rdfs:subClassOf example:Person .
# First Name Propertyexample:firstName rdf:type rdf:Property ;
    rdfs:label "first name" ;
    rdfs:comment "The first name of a person" ;
    rdfs:domain example:Person ;
    rdfs:range xsd:string .
# Last Name Propertyexample:lastName rdf:type rdf:Property ;
    rdfs:label "last name" ;
    rdfs:comment "The last name of a person" ;
    rdfs:domain example:Person ;
    rdfs:range xsd:string .
# Age Propertyexample:age rdf:type rdf:Property ;
    rdfs:label "age" ;
    rdfs:comment "The age of a person" ;
    rdfs:domain example:Person ;
    rdfs:range xsd:integer .
# Example usage of the ontology for a Personexample:JohnDoe rdf:type example:Man ;
    example:firstName "John" ;
    example:lastName "Doe" ;
    example:age 30 .
