@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/person-ontology#> .
:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "An individual human being."@en .
:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en ;
    rdfs:comment "Relates a person to their full name."@en ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .
:bornOnDate a owl:DatatypeProperty ;
    rdfs:label "born on date"@en ;
    rdfs:comment "Relates a person to their birth date."@en ;
    rdfs:domain :Person ;
    rdfs:range xsd:date .
:hasGender a owl:ObjectProperty ;
    rdfs:label "has gender"@en ;
    rdfs:comment "Relates a person to their gender."@en ;
    rdfs:domain :Person ;
    rdfs:range :Gender .
    :Gender a owl:Class ;
    rdfs:label "Gender"@en ;
    rdfs:comment "Classifies a person's gender."@en .
:Male a :Gender ;
    rdfs:label "Male"@en ;
    rdfs:comment "Male gender."@en .
:Female a :Gender ;
    rdfs:label "Female"@en ;
    rdfs:comment "Female gender."@en .
