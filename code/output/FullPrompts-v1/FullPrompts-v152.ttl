@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
# Defining the class 'Year':Year rdf:type rdfs:Class ;
      rdfs:label "Year"@en ;
      rdfs:comment "A particular period of 365 (or 366) days in the Gregorian calendar"@en .
# Defining the property 'hasMonth':hasMonth rdf:type rdf:Property ;
          rdfs:domain :Year ;
          rdfs:range :Month ;
          rdfs:label "has month"@en ;
          rdfs:comment "The relationship between a year and a month that it contains"@en .
# Defining the class 'Month':Month rdf:type rdfs:Class ;
       rdfs:label "Month"@en ;
       rdfs:comment "One of the twelve named periods into which a year is divided"@en .
# Defining the property 'hasDay':hasDay rdf:type rdf:Property ;
        rdfs:domain :Month ;
        rdfs:range :Day ;
        rdfs:label "has day"@en ;
        rdfs:comment "The relationship between a month and a day that it contains"@en .
# Defining the class 'Day':Day rdf:type rdfs:Class ;
     rdfs:label "Day"@en ;
     rdfs:comment "A period of 24 hours, as a unit of time, reckoned from one midnight to the next"@en .
