@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix person: <http://example.org/Person#> .
person:Person rdf:type owl:Class .
person:name rdf:type owl:DatatypeProperty ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .
person:hasAge rdf:type owl:DatatypeProperty ;
    rdfs:domain person:Person ;
    rdfs:range xsd:integer .
person:bornIn rdf:type owl:ObjectProperty ;
    rdfs:domain person:Person ;
    rdfs:range person:Place .
person:Place rdf:type owl:Class .
person:livesIn rdf:type owl:ObjectProperty ;
    rdfs:domain person:Person ;
    rdfs:range person:Place .
person:hasGender rdf:type owl:ObjectProperty ;
    rdfs:domain person:Person ;
    rdfs:range person:Gender .
person:Gender rdf:type owl:Class .
person:Male rdf:type owl:NamedIndividual ;
    rdf:type person:Gender .
person:Female rdf:type owl:NamedIndividual ;
    rdf:type person:Gender .
