@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix manOnt: <http://www.semanticweb.org/man_ontology#> .
manOnt:Man rdf:type rdfs:Class .
manOnt:hasAge rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range xsd:integer .
manOnt:hasName rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range xsd:string .
manOnt:hasGender rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range manOnt:Gender .
manOnt:Gender rdf:type rdfs:Class .
manOnt:Male rdf:type manOnt:Gender .
manOnt:Female rdf:type manOnt:Gender .
manOnt:isEmployed rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range xsd:boolean .
manOnt:hasProfession rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range manOnt:Profession .
manOnt:Profession rdf:type rdfs:Class .
manOnt:Engineer rdf:type manOnt:Profession .
manOnt:Doctor rdf:type manOnt:Profession .
manOnt:hasHeight rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range xsd:double .
manOnt:hasWeight rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range xsd:double .
manOnt:hasNationality rdf:type rdf:Property ;
    rdfs:domain manOnt:Man ;
    rdfs:range manOnt:Country .
manOnt:Country rdf:type rdfs:Class .
manOnt:UnitedStates rdf:type manOnt:Country .
