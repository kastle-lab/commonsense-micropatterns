@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix man: <http://example.org/man#> .
man:Man a rdfs:Class ;
rdfs:label "Man"@en ;
rdfs:seeAlso <http://dbpedia.org/page/Man> ;
rdfs:subClassOf man:Mammal .
man:name a rdf:Property ;
    rdfs:domain man:Man ;
    rdfs:range xsd:string .
man:age a rdf:Property ;
    rdfs:domain man:Man ;
    rdfs:range xsd:integer .
    man:hasGender a rdf:Property ;
    rdfs:domain man:Man ;
    rdfs:range man:Gender.man:Gender a rdfs:Class.man:Male a rdfs:Class ;
    rdfs:subClassOf man:Gender.man:Female a rdfs:Class ;
    rdfs:subClassOf man:Gender.    man:John a man:Man ;
    man:name "John" ;
    man:age 42 ;
    man:hasGender man:Male .
man:Mammal a rdfs:Class .
