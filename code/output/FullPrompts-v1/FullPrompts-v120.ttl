@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix : <http://example.org/#> .
:Person a rdfs:Class ;
    rdfs:comment "A class representing a person"^^xsd:string ;
    rdfs:label "Person"^^xsd:string .
:hasName a rdf:Property ;
    rdfs:comment "The name of the person"^^xsd:string ;
    rdfs:label "has name"^^xsd:string ;
    rdfs:domain :Person ;
    rdfs:range xsd:string .
:hasAge a rdf:Property ;
    rdfs:comment "The age of the person"^^xsd:string ;
    rdfs:label "has age"^^xsd:string ;
    rdfs:domain :Person ;
    rdfs:range xsd:int .
:hasGender a rdf:Property ;
    rdfs:comment "The gender of the person"^^xsd:string ;
    rdfs:label "has gender"^^xsd:string ;
    rdfs:domain :Person ;
    rdfs:range :Gender .
:Male a :Gender ;
    rdfs:comment "Male gender"^^xsd:string ;
    rdfs:label "Male"^^xsd:string .
:Female a :Gender ;
    rdfs:comment "Female gender"^^xsd:string ;
    rdfs:label "Female"^^xsd:string .
