@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix : <http://example.org/> .
:Man a rdfs:Class ;
    rdfs:label "Man"@en ;
    rdfs:comment "The class of all men"@en ;
    rdfs:subClassOf :Human .
:hasAge a rdf:Property ;
    rdfs:label "has age"@en ;
    rdfs:comment "The age of a man"@en ;
    rdfs:domain :Man ;
    rdfs:range xsd:integer .
:hasName a rdf:Property ;
    rdfs:label "has name"@en ;
    rdfs:comment "The name of a man"@en ;
    rdfs:domain :Man ;
    rdfs:range xsd:string .
:Human a rdfs:Class ;
    rdfs:label "Human"@en ;
    rdfs:comment "The class of all humans"@en .
