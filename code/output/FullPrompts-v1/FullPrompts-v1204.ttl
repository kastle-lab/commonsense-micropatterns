@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
# Class
:Man a rdfs:Class ;
    rdfs:label "Man"@en ;
    rdfs:subClassOf :Person ;
    rdfs:comment "A human male"@en .
:Human a rdfs:Class ;
    rdfs:label "Human"@en ;
    rdfs:comment "A Homo Sapien"@en .
:Person a rdfs:Class ;
    rdfs:label "Person"@en ;
    rdfs:subClassOf :Human ;
    rdfs:comment "A human individual"@en .
# Object Properties:hasFather a rdf:Property ;
    rdfs:label "has father"@en ;
    rdfs:domain :Person ;
    rdfs:range :Man ;
    rdfs:comment "A person's biological father"@en .
:hasBrother a rdf:Property ;
    rdfs:label "has brother"@en ;
    rdfs:domain :Person ;
    rdfs:range :Man ;
    rdfs:comment "A person's male sibling"@en .
# Datatype Properties:hasAge a rdf:Property ;
    rdfs:label "has age"@en ;
    rdfs:domain :Person ;
    rdfs:range xsd:integer ;
    rdfs:comment "A person's age"@en .
:hasName a rdf:Property ;
    rdfs:label "has name"@en ;
    rdfs:domain :Person ;
    rdfs:range xsd:string ;
    rdfs:comment "A person's name"@en .
