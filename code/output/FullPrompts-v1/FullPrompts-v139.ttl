@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/exampleOntology#> .
# Class definitionex:Person  rdf:type rdfs:Class ;
  rdfs:label "Person" ;
  rdfs:comment "Class representing a person" .
# Properties
 definitionsex:hasName  rdf:type rdf:Property ;
  rdfs:label "has name" ;
  rdfs:comment "Property representing a person's name" ;
  rdfs:domain ex:Person ;
  rdfs:range xsd:string .
ex:hasAge  rdf:type rdf:Property ;
  rdfs:label "has age" ;
  rdfs:comment "Property representing a person's age" ;
  rdfs:domain ex:Person ;
  rdfs:range xsd:integer .
