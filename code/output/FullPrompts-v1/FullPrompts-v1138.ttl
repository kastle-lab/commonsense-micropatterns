@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
ex:Day rdf:type rdfs:Class ;
rdfs:comment "Class representing a Day" .
ex:hasDate rdf:type rdf:Property ;
rdfs:range xsd:date ;
rdfs:domain ex:Day ;
rdfs:comment "Property that holds the calendar date of the Day" .
ex:hasDayName rdf:type rdf:Property ;
rdfs:range rdfs:Literal ;
rdfs:domain ex:Day ;
rdfs:comment "Property that holds the common name of the Day (e.g., Monday, Tuesday, etc.)" .
ex:hasDayOfYear rdf:type rdf:Property ;
rdfs:range xsd:integer ;
rdfs:domain ex:Day ;
rdfs:comment "Property that holds the day of the year (e.g., 1 for January 1, 365 for December 31, etc.)" .
ex:hasWeekday rdf:type rdf:Property ;
rdfs:range xsd:boolean ;
rdfs:domain ex:Day ;
rdfs:comment "Property indicating if the Day is a weekday (true) or weekend (false)" .
