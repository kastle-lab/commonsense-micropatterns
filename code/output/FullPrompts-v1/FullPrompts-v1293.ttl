@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix life: <http://example.org/life#> .
life:     rdf:type rdfs:Class ;
    rdfs:comment "The class of all life entities".
life:Animal    rdf:type rdfs:Class ;
    rdfs:subClassOf life: ;
    rdfs:comment "The class of all animal entities".
life:Plant    rdf:type rdfs:Class ;
    rdfs:subClassOf life: ;
    rdfs:comment "The class of all plant entities".
life:Bacteria    rdf:type rdfs:Class ;
    rdfs:subClassOf life: ;
    rdfs:comment "The class of all bacterial entities".
life:Virus    rdf:type rdfs:Class ;
    rdfs:subClassOf life: ;
    rdfs:comment "The class of all viral entities".
life:birth    rdf:type rdf:Property ;
    rdfs:domain life: ;
    rdfs:range xsd:date ;
    rdfs:comment "The date of when a life entity is born or otherwise originates".
life:death    rdf:type rdf:Property ;
    rdfs:domain life: ;
    rdfs:range xsd:date ;
    rdfs:comment "The date of when a life entity dies or otherwise ceases to exist".
life:species    rdf:type rdf:Property ;
    rdfs:domain life: ;
    rdfs:comment "The species that a life entity belongs to".
