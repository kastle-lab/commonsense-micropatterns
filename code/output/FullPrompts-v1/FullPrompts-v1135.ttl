@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix day: <http://example.com/ontology/day/> .
day:Day a owl:Class ;
  rdfs:label "Day"@en ;
  rdfs:comment "A period of 24 hours from midnight to the following midnight."@en ;
  rdfs:subClassOf owl:Thing ;
.day:hasTime a owl:ObjectProperty ;
  rdfs:label "has time"@en ;
  rdfs:range xsd:dateTime ;
  rdfs:domain day:Day ;
.day:hasDaytime a owl:ObjectProperty ;
  rdfs:label "has daytime"@en ;
  rdfs:range day:Daytime ;
  rdfs:domain day:Day ;
.day:hasNighttime a owl:ObjectProperty ;
  rdfs:label "has nighttime"@en ;
  rdfs:range day:Nighttime ;
  rdfs:domain day:Day ;
.day:Daytime a owl:Class ;
  rdfs:label "Daytime"@en ;
  rdfs:comment "The period of a day during which it is light."@en ;
  rdfs:subClassOf day:Day ;
.day:Nighttime a owl:Class ;
  rdfs:label "Nighttime"@en ;
  rdfs:comment "The period of a day during which it is dark."@en ;
  rdfs:subClassOf day:Day ;
.
