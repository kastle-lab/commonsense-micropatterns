@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix world: <http://example.com/world#> .
world:Country rdf:type rdfs:Class .
world:City rdf:type rdfs:Class .
world:Person rdf:type rdfs:Class .
world:Animal rdf:type rdfs:Class .
world:Plant rdf:type rdfs:Class .
world:livesIn rdf:type owl:ObjectProperty ;
  rdfs:range world:City ;
  rdfs:domain world:Person .
world:locatedIn rdf:type owl:ObjectProperty ;
  rdfs:range world:Country ;
  rdfs:domain world:City .
world:exists rdf:type owl:ObjectProperty ;
  rdfs:range world:Country ;
  rdfs:domain world:Animal .
world:grows rdf:type owl:ObjectProperty ;
  rdfs:range world:Country ;
  rdfs:domain world:Plant .
world:name rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:string ;
  rdfs:domain rdfs:Resource .
world:population rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:int ;
  rdfs:domain world:City .
