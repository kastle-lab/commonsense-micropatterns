@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix life: <http://www.example.org/life#> .
life:    rdf:type owl:Ontology ;
    rdfs:comment "An ontology for life" ;
    rdfs:label "Life" .
life:LivingBeing    rdf:type owl:Class ;
    rdfs:comment "Entities that have life" ;
    rdfs:label "Living Being" .
life:Human    rdf:type owl:Class ;
    rdfs:comment "A human being" ;
    rdfs:subClassOf life:LivingBeing ;
    rdfs:label "Human" .
life:Animal    rdf:type owl:Class ;
    rdfs:comment "An animal, non-human being" ;
    rdfs:subClassOf life:LivingBeing ;
    rdfs:label "Animal" .
life:Plant    rdf:type owl:Class ;
    rdfs:comment "A plant, non-animal living being" ;
    rdfs:subClassOf life:LivingBeing ;
    rdfs:label "Plant" .
life:hasLife    rdf:type owl:ObjectProperty ;
    rdfs:domain life:LivingBeing ;
    rdfs:range xsd:boolean ;
    rdfs:label "has life" ;
    rdfs:comment "Indicates whether a living being currently has life" .
life:age    rdf:type owl:DatatypeProperty ;
    rdfs:domain life:LivingBeing ;
    rdfs:range xsd:integer ;
    rdfs:label "age" ;
    rdfs:comment "The age of a living being" .
