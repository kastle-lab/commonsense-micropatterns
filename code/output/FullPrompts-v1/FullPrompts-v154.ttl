@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix year: <http://example.org/year#> .
year:Year rdf:type owl:Class ;
          rdfs:comment "A class representing a specific year" ;
          rdfs:label "Year" .
year:hasMonth rdf:type owl:ObjectProperty ;
              rdfs:domain year:Year ;
              rdfs:range year:Month ;
              rdfs:comment "Connects a year with its months" ;
              rdfs:label "has month" .
year:Month rdf:type owl:Class ;
           rdfs:comment "A class representing a specific month within a year" ;
           rdfs:label "Month" .
year:inYear rdf:type owl:DatatypeProperty ;
            rdf:range xsd:integer ;
            rdfs:domain year:Year ;
            rdfs:comment "Specifies the calendar year" ;
            rdfs:label "in year" .
year:hasDay rdf:type owl:ObjectProperty ;
            rdfs:domain year:Month ;
            rdfs:range year:Day ;
            rdfs:comment "Connects a month with the days in it" ;
            rdfs:label "has day" .
year:Day rdf:type owl:Class ;
         rdfs:comment "A class representing a specific day within a month" ;
         rdfs:label "Day" .
year:inMonth rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:domain year:Month ;
             rdfs:comment "Specifies the specific month" ;
             rdfs:label "in month" .
year:onDay rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:integer ;
           rdfs:domain year:Day ;
           rdfs:comment "Specifies the specific day within a month" ;
           rdfs:label "on day" .
