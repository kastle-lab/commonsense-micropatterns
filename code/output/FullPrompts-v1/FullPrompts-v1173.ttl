@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org#> .
ex:Thing rdf:type rdfs:Class .
ex:PhysicalThing rdf:type rdfs:Class ;
    rdfs:subClassOf ex:Thing .
ex:AbstractThing rdf:type rdfs:Class ;
    rdfs:subClassOf ex:Thing .
ex:size rdf:type rdf:Property ;
    rdfs:domain ex:PhysicalThing ;
    rdfs:range xsd:string .
ex:color rdf:type rdf:Property ;
    rdfs:domain ex:PhysicalThing ;
    rdfs:range xsd:string .
ex:hasPart rdf:type rdf:Property ;
    rdfs:domain ex:PhysicalThing ;
    rdfs:range ex:PhysicalThing .
ex:concept rdf:type rdf:Property ;
    rdfs:domain ex:AbstractThing;
    rdfs:range xsd:string .
 ex:description rdf:type rdf:Property ;
    rdfs:domain ex:AbstractThing ;
    rdfs:range xsd:string .
ex:relatedTo rdf:type rdf:Property ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    rdfs:domain ex:Thing ;
    rdfs:range ex:Thing .
