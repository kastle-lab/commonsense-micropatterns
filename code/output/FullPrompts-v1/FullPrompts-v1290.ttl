`@base <http://example.org/> .
@prefix life: <http://example.org/life#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
life:Life a rdfs:Class ;
  rdfs:label "Life"@en ;
  rdfs:comment "Top class representing a base concept of life."@en .
life:Human a rdfs:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:label "Human"@en ;
  rdfs:comment "Class representing humans as a specific form of life."@en .
life:Animal a rdfs:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:label "Animal"@en ;
  rdfs:comment "Class representing animals as a specific form of life."@en .
life:Plant a rdfs:Class ;
  rdfs:subClassOf life:Life ;
  rdfs:label "Plant"@en ;
  rdfs:comment "Class representing plants as a specific form of life."@en .
life:hasLifeSpan rdfs:Property ;
  rdfs:domain life:Life ;
  rdfs:range xsd:integer ;
  rdfs:label "has life span"@en ;
  rdfs:comment "Property representing the life span of a life form."@en .
life:hasName rdfs:Property ;
  rdfs:domain life:Life ;
  rdfs:range xsd:string ;
  rdfs:label "has name"@en ;
  rdfs:comment "Property representing the name of a life form."@en .
`