@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://example.org/life#> .
life:Life rdfs:subClassOf rdfs:Resource .
life:PlantLife rdfs:subClassOf life:Life .
life:AnimalLife rdfs:subClassOf life:Life .
life:hasDNA a rdf:Property ;
    rdfs:range xsd:string ;
    rdfs:domain life:Life .
life:hasCellType a rdf:Property ;
    rdfs:range xsd:string ;
    rdfs:domain life:Life .
life:Animal rdfs:subClassOf life:AnimalLife .
life:Plant rdfs:subClassOf life:PlantLife .
_:a life:Animal ;
    life:hasDNA "ATCG" ;
    life:hasCellType "Eukaryotic" .
_:b life:Plant ;
    life:hasDNA "ATCG" ;
    life:hasCellType "Eukaryotic" .
