@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
ex:World rdf:type owl:Class .
ex:human rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
ex:animal rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
ex:plant rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
ex:continent rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
ex:Country rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
ex:City rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
ex:Ocean rdf:type owl:Class ;
  rdfs:subClassOf ex:World .
