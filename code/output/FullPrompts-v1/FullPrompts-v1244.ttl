@prefix : <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
:World rdf:type owl:Class .
:Continent rdf:type owl:Class .
:Ocean rdf:type owl:Class .
:Island rdf:type owl:Class .
:Country rdf:type owl:Class .
:City rdf:type owl:Class .
:hasContinent rdf:type owl:ObjectProperty ;
              rdfs:domain :World ;
              rdfs:range :Continent .
:hasOcean rdf:type owl:ObjectProperty ;
          rdfs:domain :World ;
          rdfs:range :Ocean .
:hasIsland rdf:type owl:ObjectProperty ;
           rdfs:domain :World ;
           rdfs:range :Island .
:locatedInContinent rdf:type owl:ObjectProperty ;
                    rdfs:domain :Country ;
                    rdfs:range :Continent .
:locatedInCountry rdf:type owl:ObjectProperty ;
                  rdfs:domain :City ;
                  rdfs:range :Country .
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :{ :Country, :City } ;
           rdfs:range :World .
