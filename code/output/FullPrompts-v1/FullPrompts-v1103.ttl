@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
# Define "Way" as a classex:Way rdf:type owl:Class ;
    rdfs:comment "Way in which something is done or occurs" .
# Define properties associated with "Way"ex:hasMethod rdf:type owl:ObjectProperty ;
    rdfs:domain ex:Way ;
    rdfs:comment "A specific method in which way is represented" .
ex:hasDuration rdf:type owl:DatatypeProperty ;
    rdfs:domain ex:Way ;
    rdfs:range xsd:duration ;
    rdfs:comment "The duration of the way" .
# Example instances of `Way` ex:Way1 rdf:type ex:Way ;
     ex:hasMethod ex:Method1 ;
     ex:hasDuration "P2H"^^xsd:duration .
ex:Method1 rdf:type owl:NamedIndividual ;
   rdfs:comment "One method of performing way" .
