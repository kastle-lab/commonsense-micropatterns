@prefix ex: <http://example.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
ex:Year a rdfs:Class ;
    rdfs:comment "Class representing a specific year" .
ex:hasYear a rdf:Property ;
   rdfs:domain ex:Event ;
   rdfs:range ex:Year ;
   rdfs:comment "Property linking an event to the year it occurred" .
ex:Event a rdfs:Class ;
   rdfs:comment "Class representing an event that occurred in a specific year" .
 ex:yearValue a rdf:Property ;
    rdfs:domain ex:Year ;
    rdfs:range xsd:integer ;
    rdfs:comment "Property representing the numeric value of the year" .
