@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://example.org/> .
:Way a rdfs:Class ;
    rdfs:label "Way"@en ;
    rdfs:comment "A method, technique, or process to achieve something" .
:hasWay a rdf:Property ;
    rdfs:label "has way"@en ;
    rdfs:range :Way ;
    rdfs:domain :Entity ;
    rdfs:comment "Relates an entity to a way method, technique or process associated with it" .
:Entity a rdfs:Class ;
    rdfs:label "Entity"@en .
# example of usage::Entity1 a :Entity ;
    :hasWay :Way1 .
:Way1 a :Way ;
    rdfs:label "Walking"@en ;
    rdfs:comment "A way of moving by using one's feet, each foot being lifted before the next step is made." .
