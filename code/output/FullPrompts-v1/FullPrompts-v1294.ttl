@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
 @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix life: <http://example.org/life#> .
life:     rdf:type rdfs:Class .
life:Plant     rdf:type rdfs:Class ;
    rdfs:subClassOf life: .
life:Animal     rdf:type rdfs:Class ;
    rdfs:subClassOf life: .
life:Mammal     rdf:type rdfs:Class ;
    rdfs:subClassOf life:Animal .
life:Bird     rdf:type rdfs:Class ;
    rdfs:subClassOf life:Animal .
life:Insect     rdf:type rdfs:Class ;
    rdfs:subClassOf life:Animal .
life:Fish     rdf:type rdfs:Class ;
    rdfs:subClassOf life:Animal .
life:Tree     rdf:type rdfs:Class ;
    rdfs:subClassOf life:Plant .
life:Flower     rdf:type rdfs:Class ;
    rdfs:subClassOf life:Plant .
life:hasLifeSpan    rdf:type rdf:Property ;
    rdfs:domain life: ;
    rdfs:range xsd:decimal .
life:Human    rdf:type life:Mammal ;
    life:hasLifeSpan "79.2"^^xsd:decimal .
life:OakTree    rdf:type life:Tree ;
    life:hasLifeSpan "300.0"^^xsd:decimal .
