@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix world: <http://example.org/world#> .
world:World rdf:type rdfs:Class .
world:Countries rdf:type rdfs:Class ;
    rdfs:subClassOf world:World .
    world:Cities rdf:type rdfs:Class ;
    rdfs:subClassOf world:Countries .
    world:Rivers rdf:type rdfs:Class ;
    rdfs:subClassOf world:World .
world:Mountains rdf:type rdfs:Class ;
    rdfs:subClassOf world:World .
world:Oceans rdf:type rdfs:Class ;
    rdfs:subClassOf world:World .
world:Forests rdf:type rdfs:Class ;
    rdfs:subClassOf world:World .
world:continent rdf:type rdf:Property ;
    rdfs:domain world:Countries ;
    rdfs:range xsd:string .
world:population rdf:type rdf:Property ;
    rdfs:domain world:Countries ;
    rdfs:range xsd:integer .
world:isIn rdf:type rdf:Property ;
    rdfs:domain world:Cities ;
    rdfs:range world:Countries .
world:length rdf:type rdf:Property ;
    rdfs:domain world:Rivers ;
    rdfs:range xsd:integer .
world:height rdf:type rdf:Property ;
    rdfs:domain world:Mountains ;
    rdfs:range xsd:integer .
world:depth rdf:type rdf:Property ;
    rdfs:domain world:Oceans ;
    rdfs:range xsd:integer .
world:area rdf:type rdf:Property ;
    rdfs:domain world:Forests ;
    rdfs:range xsd:integer .
