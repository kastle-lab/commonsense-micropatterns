@prefix : <http://example.org/way#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:Way rdf:type rdfs:Class .
:hasDirection rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range :Direction .
:hasLength rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range rdf:Literal .
:hasTerrain rdf:type rdf:Property ;
    rdfs:domain :Way ;
    rdfs:range :Terrain .
:Direction rdf:type rdfs:Class .
:Terrain rdf:type rdfs:Class .
:North rdf:type :Direction .
:South rdf:type :Direction .
:East rdf:type :Direction .
:West rdf:type :Direction .
:Urban rdf:type :Terrain .
:Rural rdf:type :Terrain .
:Mountainous rdf:type :Terrain .
:Forested rdf:type :Terrain .
