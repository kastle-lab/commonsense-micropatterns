@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
ex:year a rdfs:Class ;
    rdfs:label "Year"@en ;
    rdfs:comment "A period of 365 or 366 days, divided into 12 calendar months, now understood to be referenced to the Gregorian calendar."@en .
ex:year2020 a ex:year ;
    ex:hasStart "2020-01-01"^^xsd:date ;
    ex:hasEnd "2020-12-31"^^xsd:date .
ex:year2021 a ex:year ;
    ex:hasStart "2021-01-01"^^xsd:date ;
    ex:hasEnd "2021-12-31"^^xsd:date .
