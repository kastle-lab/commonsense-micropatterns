@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix way: <http://www.example.org/way#> .
way:     rdf:type    owl:Ontology ;
    rdfs:label  "Way Ontology"@en .
way:Road     rdf:type    owl:Class ;
    rdfs:label  "Road"@en ;
    rdfs:comment "A distinct route or direction."way:Pathway    rdf:type    owl:Class ;
    rdfs:label  "Pathway"@en ;
    rdfs:comment "A way that constitutes or serves as a path."way:Highway    rdf:type    owl:Class ;
    rdfs:label  "Highway"@en ;
    rdfs:comment "A main road or thoroughfare."way:Street    rdf:type    owl:Class ;
    rdfs:label  "Street"@en ;
    rdfs:comment "A paved public road in a city or town."way:Trail    rdf:type    owl:Class ;
    rdfs:label  "Trail"@en ;
    rdfs:comment "A beaten path through countryside or wilderness.".
way:Boulevard    rdf:type    owl:Class ;
    rdfs:label  "Boulevard"@en ;
    rdfs:comment "A wide street in a town or city, often lined with trees."way:Avenue    rdf:type    owl:Class ;
    rdfs:label  "Avenue"@en ;
    rdfs:comment "A broad road in a town or city, often tree-lined."way:Lane    rdf:type    owl:Class ;
    rdfs:label  "Lane"@en ;
    rdfs:comment "A small street or passageway, often narrow or rural."way:isPartOf    rdf:type    owl:ObjectProperty ;
    rdfs:label  "is part of"@en ;
    rdfs:domain way:Road ;
    rdfs:range  way:Pathway .
way:leadsTo    rdf:type    owl:ObjectProperty ;
    rdfs:label  "leads to"@en ;
    rdfs:domain way:Road ;
    rdfs:range  way:Street .
