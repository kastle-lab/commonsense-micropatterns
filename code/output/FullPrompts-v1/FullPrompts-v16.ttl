@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix atp: <http://www.example.org/ontology/person#> .
atp:Person     rdf:type rdfs:Class ;
    rdfs:comment "Represents a person"^^xsd:string ;
    rdfs:label "Person"^^xsd:string .
atp:hasFullName     rdf:type rdf:Property ;
    rdfs:comment "The full name of a person"^^xsd:string ;
    rdfs:label "hasFullName"^^xsd:string ;
    rdfs:domain atp:Person ;
    rdfs:range xsd:string .
atp:hasAge     rdf:type rdf:Property ;
    rdfs:comment "The age of a person"^^xsd:string ;
    rdfs:label "hasAge"^^xsd:string ;
    rdfs:domain atp:Person ;
    rdfs:range xsd:integer .
atp:hasGender     rdf:type rdf:Property ;
    rdfs:comment "The gender of a person"^^xsd:string ;
    rdfs:label "hasGender"^^xsd:string ;
    rdfs:domain atp:Person ;
    rdfs:range xsd:string .
atp:hasOccupation     rdf:type rdf:Property ;
    rdfs:comment "The occupation of a person"^^xsd:string ;
    rdfs:label "hasOccupation"^^xsd:string ;
    rdfs:domain atp:Person ;
    rdfs:range xsd:string .
