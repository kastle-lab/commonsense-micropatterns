@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix man: <http://example.org/ontology/man#> .
man:Man rdf:type owl:Class ;
        rdfs:comment "A class representing a human male."@en ;
        rdfs:label "Man"@en .
man:hasName rdf:type owl:DatatypeProperty ;
       rdfs:comment "The name of the Man."@en ;
       rdfs:label "has name"@en ;
       rdfs:domain man:Man ;
       rdfs:range xsd:string .
man:hasAge rdf:type owl:DatatypeProperty ;
       rdfs:comment "The age of the Man."@en ;
       rdfs:label "has age"@en ;
       rdfs:domain man:Man ;
       rdfs:range xsd:integer .
       man:isMarriedTo rdf:type owl:ObjectProperty ;
       rdfs:comment "A property representing the Man's spouse, if any."@en ;
       rdfs:label "is married to"@en ;
       rdfs:domain man:Man ;
       rdfs:range man:Woman .
 man:hasOccupation rdf:type owl:ObjectProperty ;
       rdfs:comment "A property representing the Man's occupation."@en ;
       rdfs:label "has occupation"@en ;
       rdfs:domain man:Man ;
       rdfs:range man:Occupation .
man:Woman rdf:type owl:Class ;
       rdfs:comment "A class representing a female human."@en ;
       rdfs:label "Woman"@en .
man:Occupation rdf:type owl:Class ;
       rdfs:comment "A class representing an occupation of a human."@en ;
       rdfs:label "Occupation"@en .
