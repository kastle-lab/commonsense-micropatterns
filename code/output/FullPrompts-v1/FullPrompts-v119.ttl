rdfs:range rdf:Literal .
:residesIn a rdf:Property ;
rdfs:label "resides in"@en ;
rdfs:domain :Person ;
rdfs:range rdf:Literal .
 :isMarriedTo a rdf:Property ;
rdfs:label "is married to"@en ;
rdfs:domain :Person ;
rdfs:range :Person .
:isSiblingOf a rdf:Property ;
rdfs:label "is sibling of"@en ;
rdfs:domain :Person ;
rdfs:range :Person .
:isParentOf a rdf:Property ;
rdfs:label "is parent of"@en ;
