@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix thing: <http://www.example.org/ontology/thing#> .
thing: a owl:Ontology ;
    dc:title "Ontology of Thing"@en ;
    dc:description "An ontology defining the concept of 'thing'."@en .
thing:Thing a rdfs:Class ;
    rdfs:label "Thing"@en ;
    rdfs:comment "A representational unit of something that exists."@en ;
    rdfs:subClassOf owl:Thing .
thing:hasProperty a rdf:Property ;
    rdfs:label "has property"@en ;
    rdfs:comment "Relates a thing to its properties."@en ;
    rdfs:domain thing:Thing ;
    rdfs:range thing:Property .
thing:Property a rdfs:Class ;
    rdfs:label "Property"@en ;
    rdfs:comment "A characteristic or attribute of a thing."@en ;
    rdfs:subClassOf owl:Thing .
thing:hasValue a rdf:Property ;
    rdfs:label "has value"@en ;
    rdfs:comment "Relates a property to its value."@en ;
    rdfs:domain thing:Property ;
    rdfs:range xsd:string .
thing:name a rdf:Property ;
    rdfs:label "name"@en ;
    rdfs:comment "The name of a thing."@en ;
    rdfs:domain thing:Thing ;
    rdfs:range xsd:string .
