@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/#> .
# Defining 'Country' as a classex:Country rdf:type rdfs:Class ;
# Properties of a countryex:hasName rdf:type rdf:Property ;
    rdfs:domain ex:Country ;
    rdfs:range xsd:string .
  ex:hasPopulation rdf:type rdf:Property ;
    rdfs:domain ex:Country ;
    rdfs:range xsd:integer .
              ex:hasArea rdf:type rdf:Property ;
    rdfs:domain ex:Country ;
    rdfs:range xsd:integer .
  ex:hasCapital rdf:type rdf:Property ;
    rdfs:domain ex:Country ;
    rdfs:range ex:City .
# Defining 'City' as a classex:City rdf:type rdfs:Class ;
  # Properties of a cityex:hasName rdf:type rdf:Property ;
    rdfs:domain ex:City ;
    rdfs:range xsd:string .
  ex:isCapitalOf rdf:type rdf:Property ;
    rdfs:domain ex:City ;
    rdfs:range ex:Country .
