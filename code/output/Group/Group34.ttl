@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.com/> .
ex:Group1 a ex:Group ;
    ex:hasMember ex:Member1 , ex:Member2 ;
    ex:groupName "Sample Group 1"^^xsd:string .
ex:Member1 a ex:Member ;
    ex:memberName "John Doe"^^xsd:string .
ex:Member2 a ex:Member ;
    ex:memberName "Jane Doe"^^xsd:string .
