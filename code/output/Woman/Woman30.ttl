@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix person: <http://www.example.org/person#> .
person:Woman rdf:type owl:Class ;
    rdfs:subClassOf person:Person ;
    rdfs:label "Woman"@en ;
    rdfs:comment "A person who identifies as a female adult."@en .
person:Person rdf:type owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "An individual human being"@en .
