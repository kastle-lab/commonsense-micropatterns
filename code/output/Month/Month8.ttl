@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix month: <http://example.org/month/> .
month:     rdf:type owl:Class ;
    rdfs:label "Month"@en ;
    rdfs:subClassOf xsd:string .
month:January    rdf:type month: ;
    rdfs:label "January"@en .
month:February    rdf:type month: ;
    rdfs:label "February"@en .
month:March    rdf:type month: ;
    rdfs:label "March"@en .
month:April    rdf:type month: ;
    rdfs:label "April"@en .
month:May    rdf:type month: ;
    rdfs:label "May"@en .
month:June    rdf:type month: ;
    rdfs:label "June"@en .
month:July    rdf:type month: ;
    rdfs:label "July"@en .
month:August    rdf:type month: ;
    rdfs:label "August"@en .
month:September    rdf:type month: ;
    rdfs:label "September"@en .
month:October    rdf:type month: ;
    rdfs:label "October"@en .
month:November    rdf:type month: ;
    rdfs:label "November"@en .
month:December    rdf:type month: ;
    rdfs:label "December"@en .
