@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
ex:Day a rdfs:Class ;
     rdfs:label "Day" ;
     rdfs:comment "Represents a day in a week or a calendar" ;
     rdfs:subClassOf xsd:string .
ex:Monday a ex:Day ;
  rdfs:label "Monday" ;
  rdfs:comment "Represents Monday, first day of the week in many cultures" .
ex:Tuesday a ex:Day ;
  rdfs:label "Tuesday" ;
  rdfs:comment "Represents Tuesday, the second day of the week in many cultures" .
ex:Wednesday a ex:Day ;
  rdfs:label "Wednesday" ;
  rdfs:comment "Represents Wednesday, the third day of the week in many cultures" .
ex:Thursday a ex:Day ;
  rdfs:label "Thursday" ;
  rdfs:comment "Represents Thursday, the fourth day of the week in many cultures" .
ex:Friday a ex:Day ;
  rdfs:label "Friday" ;
  rdfs:comment "Represents Friday, the fifth day of the week in many cultures" .
ex:Saturday a ex:Day ;
  rdfs:label "Saturday" ;
  rdfs:comment "Represents Saturday, the sixth day of the week in many cultures" .
ex:Sunday a ex:Day ;
  rdfs:label "Sunday" ;
  rdfs:comment "Represents Sunday, the seventh day of the week in many cultures" .
