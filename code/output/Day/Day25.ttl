@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .
ex:day  a rdfs:Class ;
  rdfs:label "Day" ;
  rdfs:comment "A duration of 24 hours, usually from midnight to the next midnight." ;
.ex:Monday  a ex:day ;
  rdfs:label "Monday" ;
.ex:Tuesday  a ex:day ;
  rdfs:label "Tuesday" ;
.ex:Wednesday  a ex:day ;
  rdfs:label "Wednesday" ;
.ex:Thursday  a ex:day ;
  rdfs:label "Thursday" ;
.ex:Friday  a ex:day ;
  rdfs:label "Friday" ;
.ex:Saturday  a ex:day ;
  rdfs:label "Saturday" ;
.ex:Sunday  a ex:day ;
  rdfs:label "Sunday" ;
.
