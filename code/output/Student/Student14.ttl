@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <http://www.example.com/studentOntology#> .
:Student a rdfs:Class .
:hasName a rdf:Property ;
    rdfs:domain :Student ;
    rdfs:range rdfs:Literal .
 :hasAge a rdf:Property ;
    rdfs:domain :Student ;
    rdfs:range rdfs:Literal .
 :studiesSubject a rdf:Property ;
    rdfs:domain :Student ;
    rdfs:range rdfs:Literal .
:hasStudentID a rdf:Property ;
    rdfs:domain :Student ;
    rdfs:range rdfs:Literal .
 :attendsUniversity a rdf:Property ;
    rdfs:domain :Student ;
    rdfs:range rdfs:Literal .
