@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fo: <http://www.example.com/faceOntology#> .
fo:Face rdf:type rdfs:Class .
fo:hasEyes rdf:type rdf:Property ;
    rdfs:domain fo:Face ;
    rdfs:range rdfs:Literal .
fo:hasNose rdf:type rdf:Property ;
    rdfs:domain fo:Face ;
    rdfs:range rdfs:Literal .
fo:hasMouth rdf:type rdf:Property ;
    rdfs:domain fo:Face ;
    rdfs:range rdfs:Literal .
fo:hasSkinColor rdf:type rdf:Property ;
    rdfs:domain fo:Face ;
    rdfs:range rdfs:Literal .
