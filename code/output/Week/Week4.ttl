@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix week: <http://example.org/week#> .
week:Week rdf:type rdfs:Class .
week:dayOfWeek rdf:type rdf:Property ;
    rdfs:domain week:Week ;
    rdfs:range rdfs:Resource .
week:Monday rdf:type week:Week ;
    rdfs:label "Monday"^^xsd:string .
week:Tuesday rdf:type week:Week ;
    rdfs:label "Tuesday"^^xsd:string .
week:Wednesday rdf:type week:Week ;
    rdfs:label "Wednesday"^^xsd:string .
week:Thursday rdf:type week:Week ;
    rdfs:label "Thursday"^^xsd:string .
week:Friday rdf:type week:Week ;
    rdfs:label "Friday"^^xsd:string .
week:Saturday rdf:type week:Week ;
    rdfs:label "Saturday"^^xsd:string .
week:Sunday rdf:type week:Week ;
    rdfs:label "Sunday"^^xsd:string .
