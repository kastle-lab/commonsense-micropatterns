@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
ex:Level a rdfs:Class ;
    rdfs:label "Level"@en ;
    rdfs:comment "A level in a hierarchical system."@en .
ex:beginner a ex:Level ;
    rdfs:label "Beginner"@en ;
    rdfs:comment "Basic level in hierarchical system."@en .
    ex:intermediate a ex:Level ;
    rdfs:label "Intermediate"@en ;
    rdfs:comment "Middle level in hierarchical system."@en .
    ex:advanced a ex:Level ;
    rdfs:label "Advanced"@en ;
    rdfs:comment "High level in hierarchical system."@en .
