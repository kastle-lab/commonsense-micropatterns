@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix level: <http://example.com/ontology/level#> .
level:Level rdf:type rdfs:Class ;
    rdfs:comment "A class representing different levels or stages" .
level:easy rdf:type level:Level ;
    rdfs:label "easy" ;
    rdfs:comment "An easy level or stage" .
level:medium rdf:type level:Level ;
    rdfs:label "medium" ;
    rdfs:comment "A medium level or stage" .
level:hard rdf:type level:Level ;
    rdfs:label "hard" ;
    rdfs:comment "A hard level or stage" .
level:difficult rdf:type level:Level ;
    rdfs:label "difficult" ;
    rdfs:comment "A difficult level or stage" .
level:extreme rdf:type level:Level ;
    rdfs:label "extreme" ;
    rdfs:comment "An extreme level or stage" .
