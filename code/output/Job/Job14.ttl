@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix job: <http://example.com/job#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
job:Job rdf:type rdf:Class .
job:id rdf:type rdf:Property ;
        rdf:domain job:Job ;
        rdf:range xsd:string .
job:title rdf:type rdf:Property ;
        rdf:domain job:Job ;
        rdf:range xsd:string .
job:description rdf:type rdf:Property ;
        rdf:domain job:Job ;
        rdf:range xsd:string .
job:employer rdf:type rdf:Property ;
        rdf:domain job:Job ;
        rdf:range xsd:string .
job:location rdf:type rdf:Property ;
        rdf:domain job:Job ;
        rdf:range xsd:string .
job:salary rdf:type rdf:Property ;
        rdf:domain job:Job ;
        rdf:range xsd:integer .
