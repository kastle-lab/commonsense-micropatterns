@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/eye_ontology/> .
ex:Eye rdf:type owl:Class .
ex:Color rdf:type owl:Class .
ex:hasColor rdf:type owl:ObjectProperty ;
          rdfs:domain ex:Eye ;
          rdfs:range ex:Color .
ex:Function rdf:type owl:Class .
ex:hasFunction rdf:type owl:ObjectProperty ;
              rdfs:domain ex:Eye ;
              rdfs:range ex:Function .
ex:Shape rdf:type owl:Class .
ex:hasShape rdf:type owl:ObjectProperty ;
           rdfs:domain ex:Eye ;
           rdfs:range ex:Shape .
ex:Size rdf:type owl:Class .
ex:hasSize rdf:type owl:ObjectProperty ;
          rdfs:domain ex:Eye ;
          rdfs:range ex:Size .
ex:Vision rdf:type owl:Class .
ex:hasVision rdf:type owl:ObjectProperty ;
            rdfs:domain ex:Eye ;
            rdfs:range ex:Vision .
ex:Anatomy rdf:type owl:Class .
ex:hasAnatomy rdf:type owl:ObjectProperty ;
             rdfs:domain ex:Eye ;
             rdfs:range ex:Anatomy .
