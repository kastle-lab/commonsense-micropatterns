@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://purl.org/temporal/time#> .
<http://purl.org/temporal/time#>        rdf:type owl:Ontology ;
        rdfs:comment "An ontology model for Time."@en ;
        rdfs:label "Time Ontology"@en .
        time:Instant rdf:type owl:Class ;
        rdfs:comment "A specific point in time."@en ;
        rdfs:label "Instant"@en .
time:Interval rdf:type owl:Class ;
        rdfs:comment "An interval of time, between two instants."@en ;
        rdfs:label "Interval"@en .
time:Duration rdf:type owl:Class ;
        rdfs:comment "A measure of elapsed time."@en ;
        rdfs:label "Duration"@en .
time:DateTime rdf:type owl:DatatypeProperty ;
        rdfs:comment "Date and time of the instant."@en ;
        rdfs:range xsd:dateTime ;
        rdfs:domain time:Instant ;
        rdfs:label "DateTime"@en .
time:hasBeginning rdf:type owl:ObjectProperty ;
        rdfs:comment "The instant at which the interval begins."@en ;
        rdfs:range time:Instant ;
        rdfs:domain time:Interval ;
        rdfs:label "hasBeginning"@en .
        time:hasEnd rdf:type owl:ObjectProperty ;
        rdfs:comment "The instant at which the interval ends."@en ;
        rdfs:range time:Instant ;
        rdfs:domain time:Interval ;
        rdfs:label "hasEnd"@en .
        time:hasDuration rdf:type owl:ObjectProperty ;
        rdfs:comment "The duration of the interval."@en ;
        rdfs:range time:Duration ;
        rdfs:domain time:Interval ;
        rdfs:label "hasDuration"@en .
