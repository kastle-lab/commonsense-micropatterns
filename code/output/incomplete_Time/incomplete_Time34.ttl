@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
<http://www.example.org/time#>    a owl:Ontology ;
    rdfs:comment "An ontology representing Time" ;
    rdfs:label "TimeOntology" .
time:TemporalEntity    a owl:Class ;
    rdfs:comment "A temporal entity that occurs in time" ;
    rdfs:label "TemporalEntity" .
time:Instant    a owl:Class ;
    rdfs:subClassOf time:TemporalEntity ;
    rdfs:comment "An instant of time" ;
    rdfs:label "Instant" .
time:Interval    a owl:Class ;
    rdfs:subClassOf time:TemporalEntity ;
    rdfs:comment "A time interval" ;
    rdfs:label "Interval" .
time:inXSDDateTime    a owl:DatatypeProperty ;
    rdfs:domain time:Instant ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Expresses an instant of time in the xsd:dateTime format" ;
    rdfs:label "inXSDDateTime" .
time:hasBeginning    a owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:comment "The beginning instant of a time interval" ;
    rdfs:label "hasBeginning" .
time:hasEnd    a owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:comment "The end instant of a time interval" ;
    rdfs:label "hasEnd" .
time:hasDuration    a owl:DatatypeProperty ;
    rdfs:domain time:Interval ;
    rdfs:range xsd:duration ;
    rdfs:comment "The duration of a time interval" ;
    rdfs:label "hasDuration" .
