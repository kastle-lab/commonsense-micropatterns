@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.example.org/time#> .
# Classestime:TimeUnit a rdfs:Class .
time:Second a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
time:Minute a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
time:Hour a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
time:Day a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
time:Week a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
time:Month a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
time:Year a rdfs:Class ;
  rdfs:subClassOf time:TimeUnit .
# Propertiestime:hasTime a rdf:Property ;
  rdfs:domain rdfs:Resource ;
  rdfs:range time:TimeUnit .
time:inSeconds a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
time:inMinutes a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
time:inHours a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
time:inDays a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
time:inWeeks a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
time:inMonths a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
time:inYears a rdf:Property ;
  rdfs:domain time:TimeUnit ;
  rdfs:range xsd:integer .
