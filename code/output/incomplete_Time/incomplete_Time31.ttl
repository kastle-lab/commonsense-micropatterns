@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.w3.org/2006/time#> .
# Class Definitions:TimeEntity rdf:type owl:Class ;
    rdfs:subClassOf rdfs:Resource ;
    rdfs:comment "Generalized concept of time entities" ;
    rdfs:label "Time Entity" .
:Instant rdf:type owl:Class ;
    rdfs:subClassOf :TimeEntity ;
    rdfs:comment "A point in time" ;
    rdfs:label "Instant" .
:Interval rdf:type owl:Class ;
    rdfs:subClassOf :TimeEntity ;
    rdfs:comment "A span of time" ;
    rdfs:label "Interval" .
:DateTimeDescription rdf:type owl:Class ;
    rdfs:subClassOf :TimeEntity ;
    rdfs:comment "A time description in terms of calendar and clock" ;
    rdfs:label "DateTimeDescription" .
# Property Definitions:inXSDDateTime rdf:type owl:DatatypeProperty ;
    rdfs:domain :Instant ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "The date and time in xsd:dateTime format" ;
    rdfs:label "in xsd:dateTime" .
:hasBeginning rdf:type owl:ObjectProperty ;
    rdfs:domain :Interval ;
    rdfs:range :Instant ;
    rdfs:comment "The beginning of a time interval" ;
    rdfs:label "has beginning" .
:hasEnd rdf:type owl:ObjectProperty ;
    rdfs:domain :Interval ;
    rdfs:range :Instant ;
    rdfs:comment "The end of a time interval" ;
    rdfs:label "has end" .
:hasDateTimeDescription rdf:type owl:ObjectProperty ;
    rdfs:domain :Instant ;
    rdfs:range :DateTimeDescription ;
    rdfs:comment "The date and time description of an instant" ;
    rdfs:label "has date and time description" .
