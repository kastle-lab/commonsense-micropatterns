@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://myOntology.org/time#> .
time:    a owl:Ontology ;
    rdfs:comment "A time-based ontology"@en .
time:DateTime    a owl:Class ;
    rdfs:comment "A specific point in time."@en ;
    rdfs:subClassOf rdfs:Resource .
time:DateTimeStamp    a owl:DatatypeProperty ;
    rdfs:domain time:DateTime ;
    rdfs:range xsd:dateTimeStamp ;
    rdfs:comment "The DateTime represented in ISO 8601 format."@en.time:Interval    a owl:Class ;
    rdfs:comment "A duration of time."@en ;
    rdfs:subClassOf rdfs:Resource .
time:hasBeginning    a owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:DateTime ;
    rdfs:comment "The beginning of a time interval."@en .
time:hasEnd    a owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:DateTime ;
    rdfs:comment "The end of a time interval."@en .
time:Duration    a owl:Class ;
    rdfs:subClassOf time:Interval ;
    rdfs:comment "A specific length of time."@en .
time:inSeconds    a owl:DatatypeProperty ;
    rdfs:domain time:Duration ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The duration in seconds."@en.time:inMinutes    a owl:DatatypeProperty ;
    rdfs:domain time:Duration ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The duration in minutes."@en.time:inHours    a owl:DatatypeProperty ;
    rdfs:domain time:Duration ;
    rdfs:range xsd:decimal ;
    rdfs:comment "The duration in hours."@en.
