@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.ontology.org/time#> .
# Classestime:Time a rdfs:Class ;
    rdfs:label "Time" ;
    rdfs:comment "A specific point in time." .
time:Interval a rdfs:Class ;
    rdfs:label "Interval" ;
    rdfs:comment "A period of time between two times." ;
    rdfs:subClassOf time:Time .
time:Instant a rdfs:Class ;
    rdfs:label "Instant" ;
    rdfs:comment "A point in time." ;
    rdfs:subClassOf time:Time .
# Propertiestime:hasTime a rdf:Property ;
    rdfs:label "hasTime" ;
    rdfs:domain time:Time ;
    rdfs:range xsd:dateTime .
time:before a rdf:Property ;
    rdfs:label "before" ;
    rdfs:domain time:Time ;
    rdfs:range time:Time .
time:after a rdf:Property ;
    rdfs:label "after" ;
    rdfs:domain time:Time ;
    rdfs:range time:Time .
time:hasBeginning a rdf:Property ;
    rdfs:label "hasBeginning" ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant .
time:hasEnd a rdf:Property ;
    rdfs:label "hasEnd" ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant .
time:inDateTime a rdf:Property ;
    rdfs:label "inDateTime" ;
    rdfs:domain time:Instant ;
    rdfs:range xsd:dateTime .
