@prefix : <http://www.example.org/time#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:TimeUnit a owl:Class .
:Second a :TimeUnit .
:Minute a :TimeUnit ;
    :containsExactly60 :Second .
:Hour a :TimeUnit ;
    :containsExactly60 :Minute .
:Day a :TimeUnit ;
    :containsExactly24 :Hour .
:Week a :TimeUnit ;
    :containsExactly7 :Day .
:Year a :TimeUnit ;
    :containsApproximately365 :Day .
:TimePoint a owl:Class .
:Interval a owl:Class ;
    owl:disjointWith :TimePoint .
:Instant a owl:Class ;
    owl:disjointWith :Interval ;
    owl:equivalentClass :TimePoint .
:Duration a owl:Class .
:TemporalEntity a owl:Class .
:TemporalUnit a owl:Class .
:GeneralizedInstant a owl:Class .
:TemporalEntity owl:unionOf (:Duration :Instant).:hasTemporalDuration a rdf:Property ;
    rdfs:domain :TemporalEntity ;
    rdfs:range :Duration .
:inTemporalUnit a rdf:Property ;
    rdfs:domain :Duration ;
    rdfs:range :TemporalUnit .
:TimeStandard a owl:Class ;
    owl:oneOf (:UTC :TAI :TT :TCG :TCB :GPS) .
