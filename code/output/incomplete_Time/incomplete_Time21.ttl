@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .
time:  rdf:type owl:Ontology ;
  rdfs:comment "An ontology of time for RDF" ;
  rdfs:seeAlso <http://www.w3.org/TR/owl-time/> ;
  owl:imports <http://www.w3.org/2002/07/owl#> .
time:TemporalEntity  rdf:type owl:Class ;
  rdfs:label "Temporal Entity" ;
  rdfs:comment "The concepts of events and time intervals" .
time:Instant  rdf:type owl:Class ;
  rdfs:label "Instant" ;
  rdfs:comment "A zero-dimensional entity that has position on a temporal axis" ;
  rdfs:subClassOf time:TemporalEntity .
time:Interval  rdf:type owl:Class ;
  rdfs:label "Interval" ;
  rdfs:comment "A one-dimensional entity that has extent on a temporal axis" ;
  rdfs:subClassOf time:TemporalEntity .
time:inXSDDateTimeStamp  rdf:type owl:DatatypeProperty ;
  rdfs:label "in xsd:dateTimeStamp" ;
  rdfs:comment "The xsd:dateTimeStamp of a temporal entity, as a string in the xsd:dateTimeStamp datatype" ;
  rdfs:domain time:TemporalEntity ;
  rdfs:range xsd:dateTimeStamp .
  time:hasBeginning  rdf:type owl:ObjectProperty ;
  rdfs:label "has beginning" ;
  rdfs:comment "The beginning of a temporal interval" ;
  rdfs:domain time:Interval ;
  rdfs:range time:Instant .
time:hasEnd  rdf:type owl:ObjectProperty ;
  rdfs:label "has end" ;
  rdfs:comment "The end of a temporal interval" ;
  rdfs:domain time:Interval ;
  rdfs:range time:Instant .
