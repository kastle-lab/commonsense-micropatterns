@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
<http://example.org/ontology#> a owl:Ontology .
# Classes:TimePoint a owl:Class ;
  rdfs:label "Time Point"@en ;
  rdfs:subClassOf [    a owl:Restriction ;
    owl:onProperty :hasTimeStamp ;
    owl:allValuesFrom xsd:dateTime  ] .
:TimeInterval a owl:Class ;
  rdfs:label "Time Interval"@en ;
  rdfs:subClassOf [    a owl:Restriction ;
    owl:onProperty :hasStartTime ;
    owl:allValuesFrom :TimePoint  ] , [    a owl:Restriction ;
    owl:onProperty :hasEndTime ;
    owl:allValuesFrom :TimePoint  ] .
# Object Properties:hasStartTime a owl:ObjectProperty ;
  rdfs:label "has start time"@en ;
  rdfs:domain :TimeInterval ;
  rdfs:range :TimePoint .
:hasEndTime a owl:ObjectProperty ;
  rdfs:label "has end time"@en ;
  rdfs:domain :TimeInterval ;
  rdfs:range :TimePoint .
# Data Properties:hasTimeStamp a rdf:Property ;
  rdfs:label "has timestamp"@en ;
  rdfs:domain :TimePoint ;
  rdfs:range xsd:dateTime .
