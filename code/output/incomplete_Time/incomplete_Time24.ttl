@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.w3.org/2006/time#> .
time:     a owl:Ontology ;
    owl:imports <http://www.w3.org/2006/time#> .
time:Instant     a rdfs:Class ;
    rdfs:label "Instant" ;
    rdfs:comment "A point in time." .
time:Interval     a rdfs:Class ;
    rdfs:label "Interval" ;
    rdfs:comment "A span of time." ;
time:hasBeginning     a rdf:Property ;
    rdfs:label "has beginning" ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:comment "The point in time at which an Interval begins." ;
time:hasEnd     a rdf:Property ;
    rdfs:label "has end" ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:comment "The point in time at which an Interval ends." ;
time:inXSDDateTime     a rdf:Property ;
    rdfs:label "in XSD Date Time" ;
    rdfs:domain time:Instant ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "The XSD DateTime representation of an Instant." ;
time:duration     a rdf:Property ;
    rdfs:label "duration" ;
    rdfs:domain time:Interval ;
    rdfs:range xsd:duration ;
    rdfs:comment "The duration of an Interval, represented in XSD duration format." .
