@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.example.com/time#> .
time: a owl:Ontology ;
    rdfs:comment "An ontology for Time representation" .
time:Time a rdfs:Class ;
    rdfs:label "Time" ;
    rdfs:comment "Class representing time" .
time:hasHour a rdf:Property ;
    rdfs:label "has Hour" ;
    rdfs:comment "Relates a time to its hour" ;
    rdfs:domain time:Time ;
    rdfs:range rdf:integer .
time:hasMinute a rdf:Property ;
    rdfs:label "has Minute" ;
    rdfs:comment "Relates a time to its minute" ;
    rdfs:domain time:Time ;
    rdfs:range rdf:integer .
time:hasSecond a rdf:Property ;
    rdfs:label "has Second" ;
    rdfs:comment "Relates a time to its second" ;
    rdfs:domain time:Time ;
    rdfs:range rdf:integer .
 time:isPartOf a rdf:Property ;
    rdfs:label "is part of" ;
    rdfs:comment "Relates a time to the day, month, year it is part of" ;
    rdfs:domain time:Time ;
    rdfs:range rdfs:Resource .
 time:Day a rdfs:Class ;
    rdfs:label "Day" ;
    rdfs:comment "Class representing the day" .
time:Month a rdfs:Class ;
    rdfs:label "Month" ;
    rdfs:comment "Class representing the month" .
time:Year a rdfs:Class ;
    rdfs:label "Year" ;
    rdfs:comment "Class representing the year" .
 time:hasDate a rdf:Property ;
    rdfs:label "has date" ;
    rdfs:comment "Relates a Time to its specific date (day, month, year)" ;
    rdfs:domain time:Time ;
    rdfs:range rdfs:Resource .
