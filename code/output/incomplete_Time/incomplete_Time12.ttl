@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://purl.org/NET/c4dm/timeline.owl#> .
# Class Definitionstime:TimePoint a owl:Class ;
    rdfs:comment "A specific point in time" ;
    rdfs:label "Time Point" ;
    rdfs:subClassOf time:Instant .
time:TimeInterval a owl:Class ;
    rdfs:comment "A specific interval in time" ;
    rdfs:label "Time Interval" ;
    rdfs:subClassOf time:Interval .
# Property Definitionstime:inDateTime a owl:DatatypeProperty ;
    rdfs:comment "The date-time associated with a time point" ;
    rdfs:label "in DateTime" ;
    rdfs:domain time:TimePoint ;
    rdfs:range xsd:dateTime .
time:before a owl:ObjectProperty ;
    rdfs:comment "Indicates a time point occurred before another" ;
    rdfs:label "before" ;
    rdfs:domain time:TimePoint ;
    rdfs:range time:TimePoint .
time:after a owl:ObjectProperty ;
    rdfs:comment "Indicates a time point occurred after another" ;
    rdfs:label "after" ;
    rdfs:domain time:TimePoint ;
    rdfs:range time:TimePoint .
time:duration a owl:DatatypeProperty ;
    rdfs:comment "The duration of a time interval" ;
    rdfs:label "duration" ;
    rdfs:domain time:TimeInterval ;
    rdfs:range xsd:duration .
