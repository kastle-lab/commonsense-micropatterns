@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.example.org/time#> .
time:DateTime    a rdfs:Class ;
    rdfs:label "Date Time" ;
    rdfs:comment "Point in time with Date and Time" .
time:TimeUnit    a rdfs:Class ;
    rdfs:label "Time Unit" ;
    rdfs:comment "Unit for representing time like seconds, minutes, hours, etc." .
time:Interval    a rdfs:Class ;
    rdfs:label "Interval" ;
    rdfs:comment "Interval of time duration" .
time:DateTimeInterval    a rdfs:Class ;
    rdfs:label "Date Time Interval" ;
    rdfs:subClassOf time:Interval ;
    rdfs:comment "Interval of time with start and end DateTime" .
time:containsDateTime    a owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:DateTime ;
    rdfs:label "contains date time" ;
    rdfs:comment "Datetime contained in an interval" .
time:inTimeUnit    a owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:TimeUnit ;
    rdfs:label "in time unit" ;
    rdfs:comment "An interval represented in specific time unit" .
time:duration    a owl:DatatypeProperty ;
    rdfs:domain time:Interval ;
    rdfs:range xsd:double ;
    rdfs:label "duration" ;
    rdfs:comment "Duration of a time interval" .
 time:hasDateTime    a owl:DatatypeProperty ;
    rdfs:domain time:DateTime ;
    rdfs:range xsd:dateTime ;
    rdfs:label "has date time" ;
    rdfs:comment "The actual date time of the DateTime instance" .
 time:hasTimeUnit    a owl:DatatypeProperty ;
    rdfs:domain time:TimeUnit ;
    rdfs:range xsd:string ;
    rdfs:label "has time unit" ;
    rdfs:comment "The unit of the TimeUnit instance, e.g. 'second', 'minute', 'hour' etc." .
