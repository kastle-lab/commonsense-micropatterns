@prefix : <http://example.org/time#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:Time a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Time"@en ;
    rdfs:comment "This class represents time"@en .
    :Date a owl:Class ;
    rdfs:subClassOf :Time ;
    rdfs:label "Date"@en ;
    rdfs:comment "A specific day represented in calendar time"@en .
    :DateTime a owl:Class ;
    rdfs:subClassOf :Date ;
    rdfs:label "DateTime"@en ;
    rdfs:comment "A specific moment represented in calendar time and clock time"@en .
:hasHour a owl:ObjectProperty ;
    rdfs:subClassOf owl:topObjectProperty ;
    rdfs:domain :DateTime ;
    rdfs:range xsd:integer ;
    rdfs:label "hasHour"@en ;
    rdfs:comment "Represents the hour of a datetime"@en .
    :hasMinute a owl:ObjectProperty ;
    rdfs:subClassOf owl:topObjectProperty ;
    rdfs:domain :DateTime ;
    rdfs:range xsd:integer ;
    rdfs:label "hasMinute"@en ;
    rdfs:comment "Represents the minute of a datetime"@en .
:hasSecond a owl:ObjectProperty ;
    rdfs:subClassOf owl:topObjectProperty ;
    rdfs:domain :DateTime ;
    rdfs:range xsd:integer ;
    rdfs:label "hasSecond"@en ;
    rdfs:comment "Represents the second of a datetime"@en .
:hasDay a owl:ObjectProperty ;
    rdfs:subClassOf owl:topObjectProperty ;
    rdfs:domain :Date ;
    rdfs:range xsd:integer ;
    rdfs:label "hasDay"@en ;
    rdfs:comment "Represents the day of a date"@en .
:hasMonth a owl:ObjectProperty ;
    rdfs:subClassOf owl:topObjectProperty ;
    rdfs:domain :Date ;
    rdfs:range xsd:integer ;
    rdfs:label "hasMonth"@en ;
    rdfs:comment "Represents the month of a date"@en .
:hasYear a owl:ObjectProperty ;
    rdfs:subClassOf owl:topObjectProperty ;
    rdfs:domain :Date ;
    rdfs:range xsd:integer ;
    rdfs:label "hasYear" ;
    rdfs:comment "Represents the year of a date".
