@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.example.org/time#> .
time:Time    rdf:type owl:Class ;
    rdfs:comment "A period of time" ;
    rdfs:label "Time"@en .
time:Interval    rdf:type owl:Class ;
    rdfs:subClassOf time:Time ;
    rdfs:label "Interval"@en ;
    rdfs:comment "A duration of time with start and end points" .
time:Instant    rdf:type owl:Class ;
    rdfs:subClassOf time:Time ;
    rdfs:label "Instant"@en ;
    rdfs:comment "A specific point in time" .
time:DateTime    rdf:type owl:DatatypeProperty ;
    rdfs:domain time:Instant ;
    rdfs:range xsd:dateTime ;
    rdfs:label "DateTime"@en ;
    rdfs:comment "The date time of a specific point in time" .
time:hasStart    rdf:type owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:label "has start"@en ;
    rdfs:comment "The starting moment of a time interval" .
time:hasEnd    rdf:type owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:label "has end"@en ;
    rdfs:comment "The ending moment of a time interval" .
