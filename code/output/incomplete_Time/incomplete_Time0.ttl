@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix time: <http://www.w3.org/2006/time#> .
time:     rdf:type owl:Ontology ;
    rdfs:comment "An ontology for describing time" ;
    rdfs:label "Time Ontology" .
time:Instant     rdf:type owl:Class ;
    rdfs:comment "A point in time" ;
    rdfs:label "Instant" .
time:Interval     rdf:type owl:Class ;
    rdfs:comment "A span of time" ;
    rdfs:label "Interval" .
time:DateTimeDescription     rdf:type owl:Class ;
    rdfs:comment "A specific date and time" ;
    rdfs:label "DateTimeDescription" .
time:hasDateTimeDescription    rdf:type owl:ObjectProperty ;
    rdfs:comment "The DateTimeDescription of an Instant" ;
    rdfs:domain time:Instant ;
    rdfs:range time:DateTimeDescription .
time:inXSDDateTime     rdf:type owl:DatatypeProperty ;
    rdfs:comment "The xsd:dateTime that represents an Instant" ;
    rdfs:domain time:Instant ;
    rdfs:range xsd:dateTime .
time:hasBeginning     rdf:type owl:ObjectProperty ;
    rdfs:comment "The beginning instant of an Interval" ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant .
time:hasEnd     rdf:type owl:ObjectProperty ;
    rdfs:comment "The end instant of an Interval" ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant .
