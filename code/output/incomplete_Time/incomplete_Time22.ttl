@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://purl.org/time/> .
time:     rdf:type owl:Ontology ;
    rdfs:label "Time ontology" ;
    rdfs:comment "An ontology to describe the temporal concepts" .
time:Instant    rdf:type rdfs:Class ;
    rdfs:label "Instant" ;
    rdfs:comment "A point in time" .
time:Interval    rdf:type rdfs:Class ;
    rdfs:label "Interval" ;
    rdfs:comment "A duration of time" .
time:DateTime     rdf:type rdfs:Datatype ;
    rdfs:label "DateTime" ;
    rdfs:comment "A specific date and time" ;
    rdfs:subClassOf xsd:dateTime .
time:inXSDDateTime    rdf:type owl:DatatypeProperty ;
    rdfs:domain time:Instant ;
    rdfs:range time:DateTime ;
    rdfs:label "in XSD Date Time" ;
    rdfs:comment "The DateTime of an Instant in XSD Date Time format" .
time:hasBeginning    rdf:type owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:label "has beginning" ;
    rdfs:comment "The start of a time Interval" .
time:hasEnd    rdf:type owl:ObjectProperty ;
    rdfs:domain time:Interval ;
    rdfs:range time:Instant ;
    rdfs:label "has end" ;
    rdfs:comment "The end of a time Interval" .
time:duration    rdf:type owl:DatatypeProperty ;
    rdfs:domain time:Interval ;
    rdfs:range xsd:duration ;
    rdfs:label "duration" ;
    rdfs:comment "The duration of a time Interval" .
