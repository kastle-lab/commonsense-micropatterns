@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix lineOnt: <http://www.example.org/lineOnt#> .
lineOnt:Line rdf:type owl:Class .
lineOnt:hasStartPoint rdf:type owl:ObjectProperty .
lineOnt:hasEndPoint rdf:type owl:ObjectProperty .
lineOnt:hasLength rdf:type owl:DatatypeProperty .
lineOnt:hasLength rdf:range xsd:decimal .
lineOnt:Point rdf:type owl:Class .
lineOnt:hasStartPoint rdfs:domain lineOnt:Line .
lineOnt:hasStartPoint rdfs:range lineOnt:Point .
lineOnt:hasEndPoint rdfs:domain lineOnt:Line .
lineOnt:hasEndPoint rdfs:range lineOnt:Point .
lineOnt:hasLength rdfs:domain lineOnt:Line .
