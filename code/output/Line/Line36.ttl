@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.com/> .
ex:Line a rdfs:Class .
ex:hasStartPoint a rdf:Property ;
    rdfs:domain ex:Line ;
    rdfs:range xsd:string .
ex:hasEndPoint a rdf:Property ;
    rdfs:domain ex:Line ;
    rdfs:range xsd:string .
ex:hasLength a rdf:Property ;
    rdfs:domain ex:Line ;
    rdfs:range xsd:integer .
