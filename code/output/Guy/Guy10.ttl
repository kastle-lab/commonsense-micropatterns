@prefix : <http://example.org/guy#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
:Guy a foaf:Person ;
     foaf:name "Guy"^^xsd:string ;
     foaf:givenName "Guy"^^xsd:string ;
     foaf:gender "male"^^xsd:string ;
     :age ? ;
     :hasPet ? ;
     :hobbies ? ;
     :isEmployed ? ;
     :livesIn ? ;
     :isMarried ? .
:Pet a :Animal ;
     foaf:name ? .
:Hobby a :Activity ;
       foaf:name ? .
:Employed a :EmploymentStatus ;
           foaf:name ? .
:Location a :Place ;
          foaf:name ? .
:MaritalStatus a :RelationshipStatus ;
               foaf:name ? .
