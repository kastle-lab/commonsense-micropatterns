@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ex: <http://example.com#> .
ex:Lot rdf:type owl:Class .
ex:Lot rdfs:subClassOf ex:Property .
ex:ParkingLot rdf:type owl:Class .
ex:ParkingLot rdfs:subClassOf ex:Lot .
ex:VacantLot rdf:type owl:Class .
ex:VacantLot rdfs:subClassOf ex:Lot .
ex:LotNumber rdf:type owl:DatatypeProperty .
ex:LotNumber rdfs:domain ex:Lot .
ex:LotNumber rdfs:range rdf:literal .
ex:HasParkingLot rdf:type owl:ObjectProperty .
ex:HasParkingLot rdfs:domain ex:Building .
ex:HasParkingLot rdfs:range ex:ParkingLot .
ex:Building rdf:type owl:Class .
ex:Location rdf:type owl:DatatypeProperty .
ex:Location rdfs:domain ex:Lot .
ex:Location rdfs:range rdf:literal .
ex:Size rdf:type owl:DatatypeProperty .
ex:Size rdfs:domain ex:Lot .
ex:Size rdfs:range rdf:literal .
