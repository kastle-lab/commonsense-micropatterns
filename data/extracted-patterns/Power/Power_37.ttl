@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
ex:Power a owl:Class ;
   rdfs:label "Power"@en ;
   rdfs:comment "The capacity or ability to direct or influence the behavior of others or the course of events"@en .
ex:ElectricalPower a owl:Class ;
   rdfs:subClassOf ex:Power ;
   rdfs:label "Electrical Power"@en ;
   rdfs:comment "Power that is generated by electricity"@en .
ex:PoliticalPower a owl:Class ;
   rdfs:subClassOf ex:Power ;
   rdfs:label "Political Power"@en ;
   rdfs:comment "Power that is related togovt"@en .
ex:PhysicalPower a owl:Class ;
   rdfs:subClassOf ex:Power ;
   rdfs:label "Physical Power"@en ;
   rdfs:comment "Power that is related to physical strength and energy"@en .
ex:SolarPower a owl:Class ;
   rdfs:subClassOf ex:ElectricalPower ;
   rdfs:label "Solar Power"@en ;
   rdfs:comment "Power that is generated by the sun's rays"@en .
ex:WindPower a owl:Class ;
   rdfs:subClassOf ex:ElectricalPower ;
   rdfs:label "Wind Power"@en ;
   rdfs:comment "Power that is generated by wind"@en .
