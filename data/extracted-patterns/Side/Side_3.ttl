@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix side: <http://example.org/side#> .
side:     rdf:type rdfs:Class ;
    rdfs:label "Side"@en ;
    rdfs:comment "This class represents different types of sides"@en .
side:LeftSide    rdf:type side: ;
    rdfs:label "Left Side"@en ;
    rdfs:comment "This represents the left side"@en .
side:RightSide    rdf:type side: ;
    rdfs:label "Right Side"@en ;
    rdfs:comment "This represents the right side"@en .
side:TopSide    rdf:type side: ;
    rdfs:label "Top Side"@en ;
    rdfs:comment "This represents the top side"@en .
side:BottomSide    rdf:type side: ;
    rdfs:label "Bottom Side"@en ;
    rdfs:comment "This represents the bottom side"@en .
side:FrontSide    rdf:type side: ;
    rdfs:label "Front Side"@en ;
    rdfs:comment "This represents the front side"@en .
side:BackSide    rdf:type side: ;
    rdfs:label "Back Side"@en ;
    rdfs:comment "This represents the back side"@en .
side:Inside    rdf:type side: ;
    rdfs:label "Inside"@en ;
    rdfs:comment "This represents the inside"@en .
side:Outside    rdf:type side: ;
    rdfs:label "Outside"@en ;
    rdfs:comment "This represents the outside"@en .
side:Side    rdfs:subClassOf rdfs:Class ;
    rdfs:label "Side"@en ;
    rdfs:comment "A class representing all sides"@en .
