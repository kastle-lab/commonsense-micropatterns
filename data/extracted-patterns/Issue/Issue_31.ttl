@prefix : <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
:Issue rdf:type rdf:Class .
:hasDescription rdf:type rdf:Property ;
  rdf:domain :Issue ;
  rdf:range rdf:Literal .
:hasSolution rdf:type rdf:Property ;
  rdf:domain :Issue ;
  rdf:range rdf:Literal .
:isOpen rdf:type rdf:Property ;
  rdf:domain :Issue ;
  rdf:range rdf:Literal .
:hasAssignedPerson rdf:type rdf:Property ;
  rdf:domain :Issue ;
  rdf:range :Person .
:Person rdf:type rdf:Class .
