@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
 @prefix ex: <http://example.org/> .
ex:Father a rdfs:Class ;
    rdfs:comment "The class of individuals who are male parents."@en ;
    rdfs:label "Father"@en ;
    rdfs:subClassOf ex:Parent .
    ex:Parent a rdfs:Class ;
    rdfs:comment "The class of individuals who are parents."@en ;
    rdfs:label "Parent"@en .
