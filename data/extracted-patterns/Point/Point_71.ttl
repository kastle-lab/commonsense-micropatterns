@prefix : <http://example.org/ontology#> .
:Point a rdfs:Class ;
    rdfs:comment "A class representing a point in a 2D space" ;
    rdfs:label "Point" .
:XCoordinate a rdfs:Property ;
    rdfs:domain :Point ;
    rdfs:range xsd:float ;
    rdfs:comment "The X coordinate of the point." ;
    rdfs:label "XCoordinate" .
:YCoordinate a rdfs:Property ;
    rdfs:domain :Point ;
    rdfs:range xsd:float ;
    rdfs:comment "The Y coordinate of the point." ;
    rdfs:label "YCoordinate" .
