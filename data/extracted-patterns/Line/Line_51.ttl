@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .
ex:Line a rdfs:Class .
ex:hasStartPoint a rdf:Property ;
  rdfs:domain ex:Line ;
  rdfs:range ex:Point .
ex:hasEndPoint a rdf:Property ;
  rdfs:domain ex:Line ;
  rdfs:range ex:Point .
ex:Point a rdfs:Class .
