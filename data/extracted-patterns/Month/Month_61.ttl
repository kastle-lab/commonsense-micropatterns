@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix month: <http://example.com/month#> .
month:January rdf:type owl:Class ;
              rdfs:subClassOf month:Month ;
              rdfs:label "January"@en ;
              rdfs:comment "The first month of a year."@en .
month:February rdf:type owl:Class ;
               rdfs:subClassOf month:Month ;
               rdfs:label "February"@en ;
               rdfs:comment "The second month of a year."@en .
month:March rdf:type owl:Class ;
             rdfs:subClassOf month:Month ;
             rdfs:label "March"@en ;
             rdfs:comment "The third month of a year."@en .
