    rdfs:label "June"^^xsd:string ;
    ex:isMonthNumber 6^^xsd:integer .
ex:July rdf:type ex:Month ;
    rdfs:label "July"^^xsd:string ;
    ex:isMonthNumber 7^^xsd:integer .
ex:August rdf:type ex:Month ;
    rdfs:label "August"^^xsd:string ;
    ex:isMonthNumber 8^^xsd:integer .
ex:September rdf:type ex:Month ;
