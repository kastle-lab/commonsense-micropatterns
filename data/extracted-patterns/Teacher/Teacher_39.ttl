@prefix : <http://www.example.com/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:Teacher rdf:type rdfs:Class .
:hasName rdf:type rdf:Property ;
         rdfs:domain :Teacher ;
         rdfs:range xsd:string .
:hasAge rdf:type rdf:Property ;
        rdfs:domain :Teacher ;
        rdfs:range xsd:integer .
:teachesSubject rdf:type rdf:Property ;
                rdfs:domain :Teacher ;
                rdfs:range :Subject .
:Subject rdf:type rdfs:Class .
:hasExperienceInYears rdf:type rdf:Property ;
                      rdfs:domain :Teacher ;
                      rdfs:range xsd:integer .
