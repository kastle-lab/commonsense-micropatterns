@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix level: <http://example.com/level#> .
level:Level    a rdfs:Class ;
    rdfs:label "Level" ;
    rdfs:comment "Class representing a general concept of level or stages." .
level:beginner    a level:Level ;
    rdfs:label "Beginner" ;
    rdfs:comment "Level indicating an introductory or starting stage." .
level:intermediate    a level:Level ;
    rdfs:label "Intermediate" ;
    rdfs:comment "Level indicating a medium or mid-point stage." .
level:advanced     a level:Level ;
    rdfs:label "Advanced" ;
    rdfs:comment "Level indicating a level for proficient or experts." .
level:master     a level:Level ;
    rdfs:label "Master" ;
    rdfs:comment "Highest level indicating mastery or expert knowledge about something." .
