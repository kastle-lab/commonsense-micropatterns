@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
ex:Number rdf:type rdf:Class .
ex:integer rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:realNumber rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:naturalNumber rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:floatingPoint rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:complexNumber rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:negativeNumber rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:positiveNumber rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
ex:zero rdf:type rdf:Class ;
    rdfs:subClassOf ex:Number .
