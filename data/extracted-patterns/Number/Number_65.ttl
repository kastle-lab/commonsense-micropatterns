@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
:nonNegativeInteger rdf:type rdfs:Class ;
    rdfs:subClassOf :Number .
:NaturalNumber rdf:type rdfs:Class ;
    rdfs:subClassOf :nonNegativeInteger .
:wholeNumber rdf:type rdfs:Class ;
    rdfs:subClassOf :Number .
:integer rdf:type rdfs:Class ;
    rdfs:subClassOf :wholeNumber .
:realNumber rdf:type rdfs:Class ;
    rdfs:subClassOf :Number .
:complexNumber rdf:type rdfs:Class ;
    rdfs:subClassOf :Number .
:evenNumber rdf:type rdfs:Class ;
    rdfs:subClassOf :integer .
:oddNumber rdf:type rdfs:Class ;
    rdfs:subClassOf :integer .
