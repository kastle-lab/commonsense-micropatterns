	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Slave -> date [label="kastle:dateOfManumission"]
	Human [label="kastle:Human"]
	Manumission -> Human [label="kastle:grantedBy"]
	SlaveOwner [label="kastle:SlaveOwner"]
	SlaveOwner -> Slave [label="kastle:hasManumitted"]
	Human [label="kastle:Human"]
	Manumission -> Human [label="kastle:involvedSlave"]
	Literal [label="http://www.w3.org/2000/01/rdf-schema#Literal"]
	Slave -> Literal [label="kastle:legalDocument"]
	SlaveOwner -> Slave [label="kastle:manumission"]
	Slave -> SlaveOwner [label="kastle:wasManumittedBy"]
