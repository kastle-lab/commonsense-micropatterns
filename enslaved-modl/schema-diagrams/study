digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Study [label="kastle:Study"]
	Researcher [label="kastle:Researcher"]
	Study -> Researcher [label="kastle:conductedBy"]
	AcademicStudy [label="kastle:AcademicStudy"]
	Literal [label="http://www.w3.org/2000/01/rdf-schema#Literal"]
	AcademicStudy -> Literal [label="kastle:evaluate"]
	Literal [label="http://www.w3.org/2000/01/rdf-schema#Literal"]
	AcademicStudy -> Literal [label="kastle:evaluateResult"]
	Time [label="kastle:Time"]
	Study -> Time [label="kastle:hasDuration"]
	Researcher [label="kastle:Researcher"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Researcher -> string [label="kastle:hasName"]
	Study -> Researcher [label="kastle:hasResearcher"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Study -> string [label="kastle:hasStudyField"]
	Subject [label="kastle:Subject"]
	Study -> Subject [label="kastle:hasSubject"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Study -> string [label="kastle:hasTitle"]
	StudyTool [label="kastle:StudyTool"]
	Study -> StudyTool [label="kastle:usesTool"]
}
