digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Party [label="kastle:Party"]
	Datetime [label="kastle:Datetime"]
	Party -> Datetime [label="kastle:atDate"]
	Location [label="kastle:Location"]
	Party -> Location [label="kastle:atLocation"]
	Person [label="kastle:Person"]
	Person -> Party [label="kastle:attends"]
	Integer [label="kastle:Integer"]
	Person -> Integer [label="kastle:hasAge"]
	Attendee [label="kastle:Attendee"]
	Party -> Person [label="kastle:hasAttendee"]
	Datetime [label="kastle:Datetime"]
	Party -> Datetime [label="kastle:hasDate"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Party -> date [label="kastle:hasEndDate"]
	Party -> Person [label="kastle:hasGuest"]
	Party -> Person [label="kastle:hasHost"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Resource [label="kastle:Resource"]
	Party -> Resource [label="kastle:hasLocation"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:hasName"]
	Organizer [label="kastle:Organizer"]
	Party -> Person [label="kastle:hasOrganizer"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Party -> date [label="kastle:hasStartDate"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Theme [label="kastle:Theme"]
	Party -> Theme [label="kastle:hasTheme"]
	Person -> Party [label="kastle:organizes"]
}
