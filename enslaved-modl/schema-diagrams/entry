digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Entry [label="kastle:Entry"]
	Person [label="kastle:Person"]
	Entry -> Person [label="kastle:entryAuthor"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Entry -> date [label="kastle:entryDate"]
	Database [label="kastle:Database"]
	Database -> Entry [label="kastle:hasEntry"]
}
