digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Story [label="kastle:Story"]
	Author [label="kastle:Author"]
	Story -> Author [label="kastle:hasAuthor"]
	Character [label="kastle:Character"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Character -> string [label="kastle:hasCharacterName"]
	Story -> Character [label="kastle:hasCharacters"]
	Genre [label="kastle:Genre"]
	Story -> Genre [label="kastle:hasGenre"]
	Genre [label="kastle:Genre"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Genre -> string [label="kastle:hasGenreName"]
	Author [label="kastle:Author"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Author -> string [label="kastle:hasName"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Story -> string [label="kastle:hasPlot"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Story -> string [label="kastle:hasTitle"]
}
