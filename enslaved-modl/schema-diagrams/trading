	Trade -> Asset [label="kastle:hasAsset"]
	Number [label="kastle:Number"]
	Trade -> Number [label="kastle:hasPrice"]
	Role [label="kastle:Role"]
	Trader -> Role [label="kastle:hasRole"]
	Number [label="kastle:Number"]
	Trade -> Number [label="kastle:hasVolume"]
	Trader -> Trade [label="kastle:trades"]
	Market [label="kastle:Market"]
	Trader -> Market [label="kastle:tradesIn"]
	TradingPlatform [label="kastle:TradingPlatform"]
