	HistoricalEvent [label="kastle:HistoricalEvent"]
	HistoricalEvent -> TimePeriod [label="kastle:happenedDuring"]
	Person [label="kastle:Person"]
	HistoricalEvent -> Person [label="kastle:involvedPerson"]
	Person [label="kastle:Person"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:personName"]
	Place [label="kastle:Place"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Place -> string [label="kastle:placeName"]
	Datetime [label="kastle:Datetime"]
	TimePeriod -> Datetime [label="kastle:startTime"]
	HistoricalEvent -> Place [label="kastle:tookPlaceIn"]
}
