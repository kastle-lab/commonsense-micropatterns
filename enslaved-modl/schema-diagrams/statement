	Resource [label="kastle:Resource"]
	Statement -> Resource [label="kastle:hasSubject"]
	boolean [label="http://www.w3.org/2001/XMLSchema#boolean"]
	Statement -> boolean [label="kastle:hasTruthValue"]
}
