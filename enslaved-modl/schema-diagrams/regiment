digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Regiment [label="kastle:Regiment"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Regiment -> date [label="kastle:activeSince"]
	Brigade [label="kastle:Brigade"]
	Regiment -> Brigade [label="kastle:assignedTo"]
	Brigade [label="kastle:Brigade"]
	Brigade -> Regiment [label="kastle:comprisingOf"]
	Person [label="kastle:Person"]
	Regiment -> Person [label="kastle:hasCommander"]
	Person [label="kastle:Person"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:name"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:rank"]
	Integer [label="kastle:Integer"]
	Regiment -> Integer [label="kastle:regimentNumber"]
}
