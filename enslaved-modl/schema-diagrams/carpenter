	Carpenter -> float [label="kastle:hasExperience"]
	Tool [label="kastle:Tool"]
	Carpenter -> Tool [label="kastle:usesTool"]
	Project [label="kastle:Project"]
	Carpenter -> Project [label="kastle:worksInProject"]
}
