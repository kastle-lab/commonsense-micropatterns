digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Person [label="kastle:Person"]
	Servant [label="kastle:Servant"]
	Integer [label="kastle:Integer"]
	Nonnegativeinteger [label="kastle:Nonnegativeinteger"]
	Servant -> Nonnegativeinteger [label="kastle:age"]
	Servant -> Person [label="kastle:employer"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:firstName"]
	Employer [label="kastle:Employer"]
	Employer -> Servant [label="kastle:hires"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Servant -> string [label="kastle:jobTitle"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:lastName"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Servant -> string [label="kastle:name"]
	Servant -> Employer [label="kastle:worksFor"]
}
