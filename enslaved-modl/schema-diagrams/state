digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	State [label="kastle:State"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	State -> string [label="kastle:capital"]
	Double [label="kastle:Double"]
	State -> Double [label="kastle:hasArea"]
	City [label="kastle:City"]
	State -> City [label="kastle:hasLargestCity"]
	Integer [label="kastle:Integer"]
	State -> Integer [label="kastle:hasPopulation"]
	Person [label="kastle:Person"]
	State -> Person [label="kastle:hasReigningGovernor"]
	Country [label="kastle:Country"]
	State -> Country [label="kastle:locatedIn"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	State -> string [label="kastle:name"]
	Integer [label="kastle:Integer"]
	State -> Integer [label="kastle:population"]
}
