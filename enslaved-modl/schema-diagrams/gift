	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Gift -> string [label="kastle:hasGiftDescription"]
	float [label="http://www.w3.org/2001/XMLSchema#float"]
	Gift -> float [label="kastle:hasGiftPrice"]
	Gift -> Person [label="kastle:hasGiftReceiver"]
	Gift -> Person [label="kastle:hasGiftSender"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Gift -> string [label="kastle:hasGiftTitle"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:name"]
}
