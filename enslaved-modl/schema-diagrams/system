digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Component [label="kastle:Component"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Component -> string [label="kastle:componentType"]
	System [label="kastle:System"]
	System -> Component [label="kastle:hasComponent"]
	Hardware [label="kastle:Hardware"]
	System -> Hardware [label="kastle:hasHardware"]
	Software [label="kastle:Software"]
	System -> Software [label="kastle:hasSoftware"]
	Subcomponent [label="kastle:Subcomponent"]
	Component -> Subcomponent [label="kastle:hasSubcomponent"]
	Hardware [label="kastle:Hardware"]
	Hardware -> System [label="kastle:isComponentOf"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	System -> string [label="kastle:name"]
	Subcomponent [label="kastle:Subcomponent"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Subcomponent -> string [label="kastle:subcomponentType"]
}
