digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Document [label="kastle:Document"]
	Person [label="kastle:Person"]
	Document -> Person [label="kastle:hasAuthor"]
	Literal [label="http://www.w3.org/2000/01/rdf-schema#Literal"]
	Document -> Literal [label="kastle:hasCreationDate"]
	Literal [label="http://www.w3.org/2000/01/rdf-schema#Literal"]
	Document -> Literal [label="kastle:hasTitle"]
}
