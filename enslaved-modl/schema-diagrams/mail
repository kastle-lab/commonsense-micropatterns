	Mail -> Person [label="kastle:hasRecipient"]
	Mail -> Person [label="kastle:hasSender"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Mail -> string [label="kastle:hasSubject"]
	boolean [label="http://www.w3.org/2001/XMLSchema#boolean"]
	Mail -> boolean [label="kastle:isRead"]
	Datetime [label="kastle:Datetime"]
