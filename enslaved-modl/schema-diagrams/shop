digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Product [label="kastle:Product"]
	Category [label="kastle:Category"]
	Product -> Category [label="kastle:hasCategory"]
	Category [label="kastle:Category"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Category -> string [label="kastle:hasDescription"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Product -> string [label="kastle:hasName"]
	float [label="http://www.w3.org/2001/XMLSchema#float"]
	Product -> float [label="kastle:hasPrice"]
	Shop [label="kastle:Shop"]
	Shop -> Product [label="kastle:hasProduct"]
	decimal [label="http://www.w3.org/2001/XMLSchema#decimal"]
	Product -> decimal [label="kastle:price"]
	Shop -> Product [label="kastle:sells"]
}
