digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Doctor [label="kastle:Doctor"]
	Condition [label="kastle:Condition"]
	Doctor -> Condition [label="kastle:diagnosedBy"]
	Individual [label="kastle:Individual"]
	MedicalCondition [label="kastle:MedicalCondition"]
	Individual -> MedicalCondition [label="kastle:diagnosedWith"]
	Entity [label="kastle:Entity"]
	Condition [label="kastle:Condition"]
	Entity -> Condition [label="kastle:hasCondition"]
	Duration [label="kastle:Duration"]
	Condition [label="kastle:Condition"]
	Duration -> Condition [label="kastle:hasDuration"]
	Symptom [label="kastle:Symptom"]
	Condition [label="kastle:Condition"]
	Symptom -> Condition [label="kastle:hasSymptom"]
	Treatment [label="kastle:Treatment"]
	Condition [label="kastle:Condition"]
	Treatment -> Condition [label="kastle:hasTreatment"]
}
