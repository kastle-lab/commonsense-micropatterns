	InfluenceFactor [label="kastle:InfluenceFactor"]
	Influence -> InfluenceFactor [label="kastle:hasInfluenceFactor"]
	InfluencePower [label="kastle:InfluencePower"]
	Entity -> InfluencePower [label="kastle:hasInfluencePower"]
	Entity -> Influence [label="kastle:influencedBy"]
}
