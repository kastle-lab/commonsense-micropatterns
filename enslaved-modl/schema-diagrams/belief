	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Belief -> string [label="kastle:hasEvidence"]
	decimal [label="http://www.w3.org/2001/XMLSchema#decimal"]
	Belief -> decimal [label="kastle:hasStrength"]
	boolean [label="http://www.w3.org/2001/XMLSchema#boolean"]
	Belief -> boolean [label="kastle:hasTruthValue"]
}
