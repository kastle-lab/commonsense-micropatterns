	Building -> date [label="kastle:hasConstructionDate"]
	Integer [label="kastle:Integer"]
	Building -> Integer [label="kastle:hasFloors"]
	Integer [label="kastle:Integer"]
	Building -> Integer [label="kastle:hasNumberOfFloors"]
	Person [label="kastle:Person"]
	Building -> Person [label="kastle:hasOwner"]
	BuildingType [label="kastle:BuildingType"]
	Building -> BuildingType [label="kastle:hasType"]
}
