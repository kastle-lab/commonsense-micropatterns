	Reputation -> Resource [label="kastle:contributedBy"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Reputation -> string [label="kastle:hasComment"]
	float [label="http://www.w3.org/2001/XMLSchema#float"]
	Reputation -> float [label="kastle:hasRating"]
}
