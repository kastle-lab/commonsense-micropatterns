digraph {
	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Whole [label="kastle:Whole"]
	Portion [label="kastle:Portion"]
	Whole -> Portion [label="kastle:hasPortion"]
	Integer [label="kastle:Integer"]
	Whole -> Integer [label="kastle:hasQuantity"]
	Drink [label="kastle:Drink"]
	Resource [label="kastle:Resource"]
	Drink -> Resource [label="kastle:ofDrink"]
	Food [label="kastle:Food"]
	Resource [label="kastle:Resource"]
	Food -> Resource [label="kastle:ofFood"]
	Portion [label="kastle:Portion"]
	Portion -> Whole [label="kastle:portionOf"]
	decimal [label="http://www.w3.org/2001/XMLSchema#decimal"]
	Portion -> decimal [label="kastle:portionSize"]
	decimal [label="http://www.w3.org/2001/XMLSchema#decimal"]
	Portion -> decimal [label="kastle:servingSize"]
}
