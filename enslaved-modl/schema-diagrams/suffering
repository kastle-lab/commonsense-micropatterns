	Suffering -> Cause [label="kastle:hasCause"]
	Duration [label="kastle:Duration"]
	Suffering -> Duration [label="kastle:hasDuration"]
	Intensity [label="kastle:Intensity"]
	Suffering -> Intensity [label="kastle:hasIntensity"]
	Response [label="kastle:Response"]
	Suffering -> Response [label="kastle:hasResponse"]
}
