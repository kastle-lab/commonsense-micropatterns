	graph [rankdir=LR]
	node [fillcolor="#ffcc00" penwidth=2 shape=box style=filled]
	Publication [label="kastle:Publication"]
	Person [label="kastle:Person"]
	Publication -> Person [label="kastle:author"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Publication -> date [label="kastle:date"]
	Publisher [label="kastle:Publisher"]
	date [label="http://www.w3.org/2001/XMLSchema#date"]
	Publisher -> date [label="kastle:founded"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Publisher -> string [label="kastle:location"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Publisher -> string [label="kastle:name"]
	Publisher -> Publication [label="kastle:publishes"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Publication -> string [label="kastle:title"]
}
