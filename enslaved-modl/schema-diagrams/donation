	Donation -> date [label="kastle:dateOfDonation"]
	decimal [label="http://www.w3.org/2001/XMLSchema#decimal"]
	Donation -> decimal [label="kastle:donationAmount"]
	Person [label="kastle:Person"]
	Donation -> Person [label="kastle:donor"]
	Organization [label="kastle:Organization"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Organization -> string [label="kastle:hasEIN"]
	Person [label="kastle:Person"]
	string [label="http://www.w3.org/2001/XMLSchema#string"]
	Person -> string [label="kastle:hasName"]
	Donation -> Organization [label="kastle:recipient"]
