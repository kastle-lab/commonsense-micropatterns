@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix kastle: <https://kastle.cs.wright.edu/enslaved-modl#> .
@prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle:Church a rdfs:Class ;
    rdfs:subClassOf kastle:Congregation .

kastle:Congregation a rdfs:Class .

kastle:Member a rdfs:Class ;
    rdfs:subClassOf kastle:Congregation .

kastle:Service a rdfs:Class ;
    rdfs:subClassOf kastle:Congregation .

<https://archive.org/services/purl/domain/modular_ontology_design_library/congregation> a owl:Ontology ;
    opla-core:hasPatternName "congregation Pattern"^^xsd:string ;
    dc:contributor "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string ;
    dc:creator "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string .

kastle:conductsService a rdf:Property ;
    rdfs:domain kastle:Congregation ;
    rdfs:range kastle:Service .

kastle:hasMember a rdf:Property ;
    rdfs:domain kastle:Congregation ;
    rdfs:range kastle:Member .

kastle:location a rdf:Property ;
    rdfs:domain kastle:Congregation ;
    rdfs:range kastle:Church .

