@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix kastle: <https://kastle.cs.wright.edu/enslaved-modl#> .
@prefix opla-core: <http://ontologydesignpatterns.org/opla-core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

kastle:Absorbency a rdfs:Class .

kastle:Black a rdfs:Class ;
    rdfs:subClassOf kastle:Color .

kastle:Color a rdfs:Class .

kastle:Thing a rdfs:Class .

<https://archive.org/services/purl/domain/modular_ontology_design_library/black> a owl:Ontology ;
    opla-core:hasPatternName "black Pattern"^^xsd:string ;
    dc:contributor "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string ;
    dc:creator "Andrew Eells"^^xsd:string,
        "Brandon Dave"^^xsd:string,
        "Dr. Cogan Shimizu"^^xsd:string,
        "Dr. Pascal Hitzler"^^xsd:string .

kastle:hasAbsorbency a rdf:Property ;
    rdfs:domain kastle:Black ;
    rdfs:range kastle:Absorbency .

kastle:hasColor a rdf:Property ;
    rdfs:domain kastle:Thing ;
    rdfs:range kastle:Color .

kastle:isBlack a rdf:Property ;
    rdfs:domain kastle:Thing ;
    rdfs:range kastle:Black .

